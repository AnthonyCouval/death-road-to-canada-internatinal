	constant DULL-ONELINERS
	DULL-ONELINERS var dull-oneliners

: dull-oneliner
	dull-oneliners depths 0= if DULL-ONELINERS shuffles to dull-oneliners then
	blurbslot 0= if
		drawcard blurbsay
	then
;

: dull-question
	blurbslot 0= if
		"On y est presque?" blurbsay
	else
		blurbslot 2 < if
			"NON, ARRETEZ DE DEMANDER !" blurbsay
			STAT_MORALE 1st-char .stat--
		then
	then
;

: obama-this
2nd-char choosechar
	blurbslot 0= if
		"This wouldn't have happened if Obama..." blurbsay
	else
		blurbslot 2 < if
			cwits- not if
				($ "Ca n'a plus d'importance, " .. 1st-char .name@ .. "." $) blurbsay
				"PARLEZ DAUTRE CHOSE !" blurbsay
				STAT_MORALE 1st-char .stat--
		else
			"Je sais ! Merci beaucoup, Obama !" blurbsay
			STAT_MORALE me .stat++
			STAT_MORALE 1st-char .stat++
		then then
	then
;

: zombie-games
	2nd-char choosechar
	blurbslot 0= if
		"Tout ca ressemble a mes jeux video de zombies en ligne !" blurbsay
	else
		blurbslot 2 < if
			cwits- if
				($ "J'adore ces jeux, " .. 1st-char .name@ .. "!" $) blurbsay
			else
				STAT_MORALE me .stat--
			then
		then
	then
;

: dull-dullaggro1
2nd-char choosechar
	blurbslot 0= if
		"La prochaine fois quon s'arrete j'aimerais utiliser la batterie de la voiture pour alimenter ma console de jeu !" blurbsay
	else
		blurbslot 2 < if
			ctemper+ cwits- or if
				($ "Si tu me 1v1, " .. 1st-char .name@ .. "." $) blurbsay
				"Pas d'objet, pas de connerie." blurbsay
				STAT_MORALE me .stat++
				STAT_MORALE 1st-char .stat++
			else
				"Non." blurbsay
				STAT_MORALE 1st-char .stat--
			then
		then
	then
;

	deck-new
		card: zombie-games
		card: obama-this
		card: dull-question
		card: dull-dullaggro1
		card: dull-werewolf
		22 cards: dull-oneliner

