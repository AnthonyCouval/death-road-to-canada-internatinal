	constant DULL-ONELINERS
	DULL-ONELINERS var dull-oneliners

: dull-oneliner
	dull-oneliners depths 0= if DULL-ONELINERS shuffles to dull-oneliners then
	blurbslot 0= if
		drawcard blurbsay
	then
;

: dull-question
	blurbslot 0= if
		"Estamos la ja?" blurbsay
	else
		blurbslot 2 < if
			"NaO, PARA DE PERGUNTAR!" blurbsay
			STAT_MORALE 1st-char .stat--
		then
	then
;
	
: obama-this
2nd-char choosechar
	blurbslot 0= if
		"This wouldn't have happened if Obama..." blurbsay
	else
		blurbslot 2 < if
			cwits- not if
				($ " Nao importa mais, " .. 1st-char .name@ .. "." $) blurbsay
				"FALA DE OUTRA COISA!" blurbsay
				STAT_MORALE 1st-char .stat--
		else
			"'Eu sei! Muito obrigado, Obama!'" blurbsay
			STAT_MORALE me .stat++
			STAT_MORALE 1st-char .stat++
		then then
	then
;

: zombie-games
	2nd-char choosechar
	blurbslot 0= if 
		"Este e tudo como os meus jogos de video online de zumbis!" blurbsay
	else
		blurbslot 2 < if		
			cwits- if
				($ "Eu amo esses jogos " .. 1st-char .name@ .. "!" $) blurbsay
			else
				STAT_MORALE me .stat--
			then
		then
	then
;

: dull-dullaggro1
2nd-char choosechar
	blurbslot 0= if
		"'Da proxima vez que pararmos quero usar a bateria do carro para alimentar meu console de jogos!'" blurbsay
	else
		blurbslot 2 < if
			ctemper+ cwits- or if
				($ "Somente se voce 1v1 me, " .. 1st-char .name@ .. "." $) blurbsay
				"Sem itens, sem besteira." blurbsay
				STAT_MORALE me .stat++
				STAT_MORALE 1st-char .stat++
			else
				"Nao." blurbsay
				STAT_MORALE 1st-char .stat--
			then 
		then
	then
;	

	deck-new
		card: zombie-games
		card: obama-this
		card: dull-question
		card: dull-dullaggro1
		card: dull-werewolf
