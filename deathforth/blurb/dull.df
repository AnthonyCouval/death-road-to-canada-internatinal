	constant DULL-ONELINERS
	DULL-ONELINERS var dull-oneliners

: dull-oneliner
	dull-oneliners depths 0= if DULL-ONELINERS shuffles to dull-oneliners then
	blurbslot 0= if
		drawcard blurbsay
	then
;

: dull-question
	blurbslot 0= if
		"Somme-nous la deja ?" blurbsay
	else
		blurbslot 2 < if
			"ARRETE DE M'INTERROGER !" blurbsay
			STAT_MORALE 1st-char .stat--
		then
	then
;
	
: obama-this
2nd-char choosechar
	blurbslot 0= if
		"This wouldn't have happened if Obama..." blurbsay
	else
		blurbslot 2 < if
			cwits- not if
				($ "C'est plus irrelevant maintenant. " .. 1st-char .name@ .. "." $) blurbsay
				"PARLE DE QUELQUE CHOSE D'AUTRE !" blurbsay
				STAT_MORALE 1st-char .stat--
		else
			"Merci beaucoup, obama !" blurbsay
			STAT_MORALE me .stat++
			STAT_MORALE 1st-char .stat++
		then then
	then
;

: zombie-games
	2nd-char choosechar
	blurbslot 0= if 
		"C'est tout semblable a mes jeux video en ligne de zombies !" blurbsay
	else
		blurbslot 2 < if		
			cwits- if
				($ "Je adore ces jeux. " .. 1st-char .name@ .. "!" $) blurbsay
			else
				STAT_MORALE me .stat--
			then
		then
	then
;

: dull-dullaggro1
2nd-char choosechar
	blurbslot 0= if
		"La prochaine fois ou nous arretons, je veux utiliser la batterie de voiture pour alimenter mon console de jeu !" blurbsay
	else
		blurbslot 2 < if
			ctemper+ cwits- or if
				($ "Si tu me defies a une duel 1 contre 1, " .. 1st-char .name@ .. "." $) blurbsay
				"Aucune instruction donnee, rien qu'une blague ?" blurbsay
				STAT_MORALE me .stat++
				STAT_MORALE 1st-char .stat++
			else
				"Aucune" blurbsay
				STAT_MORALE 1st-char .stat--
			then 
		then
	then
;	

