	constant DULL-ONELINERS
	DULL-ONELINERS var dull-oneliners

: dull-oneliner
	dull-oneliners depths 0= if DULL-ONELINERS shuffles to dull-oneliners then
	blurbslot 0= if
		drawcard blurbsay
	then
;

: dull-question
	blurbslot 0= if
		"Sind wir schon da?" blurbsay
	else
		blurbslot 2 < if
			"NEIN, HOR AUF ZU FRAGEN!" blurbsay
			STAT_MORALE 1st-char .stat--
		then
	then
;
	
: obama-this
2nd-char choosechar
	blurbslot 0= if
		"This wouldn't have happened if Obama..." blurbsay
	else
		blurbslot 2 < if
			cwits- not if
				($ "'Es ist egal, " .. 1st-char .name@ .. "." $) blurbsay
				"REDEN SIE UBER ETWAS ANDERES!" blurbsay
				STAT_MORALE 1st-char .stat--
		else
			"Ich wei! Vielen Dank, Obama!" blurbsay
			STAT_MORALE me .stat++
			STAT_MORALE 1st-char .stat++
		then then
	then
;

: zombie-games
	2nd-char choosechar
	blurbslot 0= if 
		"Das ist alles wie meine Online Zombie Videospiele!" blurbsay
	else
		blurbslot 2 < if		
			cwits- if
				($ "Ich liebe diese Spiele, " .. 1st-char .name@ .. "!" $) blurbsay
			else
				STAT_MORALE me .stat--
			then
		then
	then
;

: dull-dullaggro1
2nd-char choosechar
	blurbslot 0= if
		"Wenn wir das nachste Mal anhalten mochte ich die Autobatterie verwenden um meine Spielkonsole mit Strom zu versorgen!" blurbsay
	else
		blurbslot 2 < if
			ctemper+ cwits- or if
				($ "Nur wenn du 1v1 mich, " .. 1st-char .name@ .. "." $) blurbsay
				"Keine Gegenstande, kein Bullcrap." blurbsay
				STAT_MORALE me .stat++
				STAT_MORALE 1st-char .stat++
			else
				"Nein." blurbsay
				STAT_MORALE 1st-char .stat--
			then 
		then
	then
;	

	deck-new
		card: zombie-games
		card: obama-this
		card: dull-question
		card: dull-dullaggro1
