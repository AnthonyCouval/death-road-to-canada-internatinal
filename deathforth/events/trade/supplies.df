	"Troca de Armas" cyoa-title!
//	iicon
	($ Thegroup .. " ve uma mulher parada perto de um estoque de caixas. As caixas parecem conter uma grande quantidade de comida e bebida." $) cyoa-text!
		($ "0x0000000000000000" pbold .. "0 means " .. pname .. " is carrying..." $) +crtext
	pchar-weapon-sell-check if
		choice( "'Nao de nada agora'" )choice back-esc
	else 
		"Ela nao ve nada que ela queira!" +crtext
		WEAPON_LIST_SELLABLE roadtrip-has-weapontype? if
			"  No entanto, ha algo que voce pode vender " +text
			WEAPON_LIST_SELLABLE team-has-weapontype? if
				"em um personagem diferente."
			else
				"'sentado no portamalas do carro'"
			then +text
		then
		choice( "Volte mais tarde" )choice back-esc
	then
	pchar ahole? if
		pchar charchoice( " rouba dela" )choice: trade-weapon-buy-rob
	then
	basic-tone 

