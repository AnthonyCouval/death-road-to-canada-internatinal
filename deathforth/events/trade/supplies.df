cyoa: trade-gas
	"' Gas Verkauf" cyoa-title! picon!
	($ Thegroup .. " 'Er findet einen Mann in der Nahe eines verrosteten Lastwagens. Der Wagen ist voller Benzintanks und fettiger Rageteile. Beide der Mann und der Wagen riechen nach Benzin." $) cyoa-text!
	gas-bought 15 < if
		"'Er bietet 10 Einheiten von Benzin gegen jedes Essenstuck ein." +crtext 
		foodamt
		choice( "'Nein danke" )choice back-esc
		most-ahole if
			charchoice( " 'beraubt den Handler" )choice: trade-gas-rob
		then
	else
		clear-cyoa "Er hat keinen Sprit mehr." +crtext 
	then basic-tone 
	// Possibly replace some with weaselest-pass if weaselest
	trunk-food 0 > if
		trunk-food 14 > gas-bought 5 < and if
			choice( "' tauscht 15 Lebensmittel" )choice: trade-gas-15
		then
		trunk-food 9 > gas-bought 10 < and if
			choice( "' tauscht 10 Lebensmittel" )choice: trade-gas-10
		then
		gas-bought 15 < if
			trunk-food 4 > if
				choice( "' tauscht 5 Lebensmittel" )choice: trade-gas-5
			else	
				choice( "'Handel " .. trunk-food .. " 'Lebensmittel" )choice: trade-gas-all
			then
		then
	then
;

