cyoa{ regiondef{

: trade-flee
	choosehuman alive? if
		($ Thegroup .. " 'es obligado a huir del campamento de comerciantes!" $) +crtext quitmission
	else 
		clear-streak delete-the-game
		' quit-to-title cyoa-choice-handler! 
	then
;

// Trading gas for food

0 value gas-to-food-bought

cyoa-choice: trade-gas-food-rob 
	($ cname .. " 'choca al hombre, saca una bolsa de comida y luego corre. " $) cyoa-text! badfx
	notsolo if
		($ Theothersare .. " arrastrado a ella." $) +text
	then
	6 10 rnd trunk-food+ rob-guilt
	aholeREV trade-flee
;

cyoa-choice: trade-gas-food-2
	($ Thegroup .. " 'intercambia 25 unidades de gasolina por 2 alimentos." $) cyoa-text! basic-tone
	-25 trunk-gas+ 2 trunk-food+ 2 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-4
	($ Thegroup .. " 'intercambia 50 unidades de gasolina por 4 alimentos." $) cyoa-text! basic-tone
	-50 trunk-gas+ 4 trunk-food+ 4 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-8
	($ Thegroup .. " 'intercambia 100 unidades de gasolina por 8 alimentos." $) cyoa-text! basic-tone
	-100 trunk-gas+ 8 trunk-food+ 8 ' gas-to-food-bought +!
;

cyoa: trade-gas-food
	"Conversion Gas a Alimentos" cyoa-title!
//	iicon
	($ Thegroup .. " 've a un hombre parado cerca de un SUV." $) cyoa-text!
	gas-to-food-bought 20 < if
		($ "Tienes algun gas extra? Estoy agotandome!		" pbold .. "Te ofrece 2 latas de comida por cada 25 unidades de gasolina." $)
		+crtext gasamt
		choice( "No gracias" )choice back-esc
		most-ahole if
			charchoice( " 'lo roba" )choice: trade-gas-food-rob
		then
	else
		clear-cyoa ($ "Gracias por todo el gas!" pbold $) +crtext
	then basic-tone 
	trunk-gas 25 < if
		($ "No hay suficiente a sparecer." $) +text
	else
		trunk-gas 99 > gas-to-food-bought 13 < and if
			choice( "Intercambia 100 unidades de gasolina" )choice: trade-gas-food-8
		then
		trunk-gas 49 > gas-to-food-bought 17 < and if
			choice( "1 mineral" )choice: trade-gas-food-4
		then
		trunk-gas 24 > gas-to-food-bought 19 < and if
			choice( "100 minerals" )choice: trade-gas-food-2
		then
	then
;

