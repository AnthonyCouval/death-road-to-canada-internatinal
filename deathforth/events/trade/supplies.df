	"Intercambio de Armas" cyoa-title!
//	iicon
	($ Thegroup .. " ve a una mujer parada cerca de un monton de cajas. Las cajas parecen contener una gran cantidad de comida y bebida." $) cyoa-text!
		($ "0x0000000000000000," pbold .. "Hace una rapida mirada a lo que " .. pname .. " is carrying..." $) +crtext
	pchar-weapon-sell-check if
		choice( "No da nada por ahi ahora" )choice back-esc
	else 
		"No ve nada que quiera!" +crtext
		WEAPON_LIST_SELLABLE roadtrip-has-weapontype? if
			" 'Pero hay algo que puedes vender " +text
			WEAPON_LIST_SELLABLE team-has-weapontype? if
				"en otro personaje."
			else
				"sentado en el maletero del coche."
			then +text
		then
		choice( "Vuelva mas tarde" )choice back-esc
	then
	pchar ahole? if
		pchar charchoice( " 'le roba" )choice: trade-weapon-buy-rob
	then
	basic-tone 

