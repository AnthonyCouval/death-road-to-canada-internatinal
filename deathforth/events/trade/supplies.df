cyoa{ regiondef{

: trade-flee
	choosehuman alive? if
		($ Thegroup .. " ' gezwungen ist, das Handlerlager zu verlassen!" $) +crtext quitmission
	else 
		clear-streak delete-the-game
		' quit-to-title cyoa-choice-handler! 
	then
;

// Trading gas for food

0 value gas-to-food-bought

cyoa-choice: trade-gas-food-rob 
	($ cname .. " ' stoßt den Mann um, greift sich einen Sack mit Nahrung und rennt weg. " $) cyoa-text! badfx
	notsolo if
		($ Theothersare .. " 'hineingezogen wird." $) +text
	then
	6 10 rnd trunk-food+ rob-guilt
	aholeREV trade-flee
;

cyoa-choice: trade-gas-food-2
	($ Thegroup .. " ' tauscht 25 Einheiten Benzin gegen 2 Snacks." $) cyoa-text! basic-tone
	-25 trunk-gas+ 2 trunk-food+ 2 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-4
	($ Thegroup .. " ' tauscht 50 Einheiten Benzin gegen 4 Snacks." $) cyoa-text! basic-tone
	-50 trunk-gas+ 4 trunk-food+ 4 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-8
	($ Thegroup .. " ' tauscht 100 Einheiten Benzin gegen 8 Snacks." $) cyoa-text! basic-tone
	-100 trunk-gas+ 8 trunk-food+ 8 ' gas-to-food-bought +!
;

cyoa: trade-gas-food
	"'Benzin zu Snacks Tausch" cyoa-title!
//	iicon
	($ Thegroup .. " 'Sie sehen einen Mann stehen, nahe bei einem SUV." $) cyoa-text!
	gas-to-food-bought 20 < if
		($ "'Hast du leeres Benzin? Ich bin aufgebraucht!	" pbold .. "'Er bietet dir 2 Kannen Essen gegen jedes 25 Einheiten von Benzin." $)
		+crtext gasamt
		choice( "'Nein danke" )choice back-esc
		most-ahole if
			charchoice( " 'er ihn uberfallen" )choice: trade-gas-food-rob
		then
	else
		clear-cyoa ($ "'Danke fur alle das Benzin!" pbold $) +crtext
	then basic-tone 
	trunk-gas 25 < if
		($ "'Nicht genug, um zu sparen." $) +text
	else
		trunk-gas 99 > gas-to-food-bought 13 < and if
			choice( "'Tausche 100 Benzin" )choice: trade-gas-food-8
		then
		trunk-gas 49 > gas-to-food-bought 17 < and if
			choice( "'Tausche 50 Benzin" )choice: trade-gas-food-4
		then
		trunk-gas 24 > gas-to-food-bought 19 < and if
			choice( "'Tausche 25 Benzin" )choice: trade-gas-food-2
		then
	then
;

