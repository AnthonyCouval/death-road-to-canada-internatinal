cyoa{ regiondef{

: trade-flee
	choosehuman alive? if
		($ Thegroup .. " e forcado a fugir do acampamento de comerciantes!" $) +crtext quitmission
	else 
		clear-streak delete-the-game
		' quit-to-title cyoa-choice-handler! 
	then
;

// Trading gas for food

0 value gas-to-food-bought

cyoa-choice: trade-gas-food-rob 
	($ cname .. "  derruba o homem, pega um saco de comida e sai correndo. " $) cyoa-text! badfx
	notsolo if
		($ Theothersare .. "  e arrastado nele." $) +text
	then
	6 10 rnd trunk-food+ rob-guilt
	aholeREV trade-flee
;

cyoa-choice: trade-gas-food-2
	($ Thegroup .. " 25 gas for 2 food" $) cyoa-text! basic-tone
	-25 trunk-gas+ 2 trunk-food+ 2 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-4
	($ Thegroup .. " 50 gas for 4 food" $) cyoa-text! basic-tone
	-50 trunk-gas+ 4 trunk-food+ 4 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-8
	($ Thegroup .. " 100 gas for 8 food" $) cyoa-text! basic-tone
	-100 trunk-gas+ 8 trunk-food+ 8 ' gas-to-food-bought +!
;

cyoa: trade-gas-food
	"Conversao de Gasolina para Lanches" cyoa-title!
//	iicon
	($ Thegroup .. " ve um homem parado perto de um SUV." $) cyoa-text!
	gas-to-food-bought 20 < if
		($ "'Voce tem alguma gasolina extra? Estou ficando sem!" pbold .. "Ele oferece 2 latas de comida para cada 25 unidades de gasolina." $)
		+crtext gasamt
		choice( "Nao obrigado." )choice back-esc
		most-ahole if
			charchoice( "  rouba ele" )choice: trade-gas-food-rob
		then
	else
		clear-cyoa ($ "Obrigado por toda a gasolina!" pbold $) +crtext
	then basic-tone 
	trunk-gas 25 < if
		($ "Nao tem o suficiente para poupar." $) +text
	else
		trunk-gas 99 > gas-to-food-bought 13 < and if
			choice( "Troca 100 de gasolina" )choice: trade-gas-food-8
		then
		trunk-gas 49 > gas-to-food-bought 17 < and if
			choice( "Troca 50 de gasolina" )choice: trade-gas-food-4
		then
		trunk-gas 24 > gas-to-food-bought 19 < and if
			choice( "Troca 25 de gasolina" )choice: trade-gas-food-2
		then
	then
;

