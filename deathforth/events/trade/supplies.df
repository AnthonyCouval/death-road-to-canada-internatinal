// Healing

cyoa-choice: trade-groupheal-rob 
	($ Thegroup .. " roba suministros al doctor. Las otras personas en el campamento comercial se lanzan a defenderla." $) cyoa-text! bitefx 6 10 rnd trunk-medical+ rob-fail rob-guilt2
		aholeREV trade-flee
;

: trade-groupheal-guy
	cchar damage 0 = if
		($ cname .. " 'se hace un chequeo, pero no le pasa nada " .. chim .. "." $)
	else
		($ cname .. " recibe tratamiento para " .. chis .. "  las heridas." $) 
	then cchar .fullheal
;

cyoa-choice: trade-groupheal-heal
	trade-groupheal-guy
	cyoa-text! goodfx
	-6 trunk-food+
	clear-cyoa
;

cyoa-choice: trade-groupheal-group
	
	team-damage 0 = if
		($ Thegroup .. " 'se hace un chequeo, pero no le pasa nada." $) cyoa-text!
	else
		7 groupheal
		($ Thegroup .. " recibe tratamiento para " $) cyoa-text!
		team-damage 1 < if 
			"todos" +text 
		else
			"la mayoria" +text
		then
			" de sus heridas." +text
	then
	goodfx
	trade-train-pay
	clear-cyoa
;

: trade-groupheal-give
	cyoa-start
	solo if
		soloer choosechar trade-groupheal-heal
	then;
	"Quien deberia recibir el tratamiento?" cyoa-text!
	' trade-groupheal-heal cyoa-team-choice
;

cyoa-choice: trade-groupheal-charm 
	charmroll if
		($ cname .. " convence a la doctora de ofrecer sus servicios gratuitas!	" .. cHe .. "'es tan encantador." $) cyoa-text! ' camp-charm ++ goodfx
		trade-groupheal-guy +crtext
	else
		($ cname .. "  accidentalmente simplemente resulta espeluznante. 		" .. gHe .. " es pedido que se vaya y nunca vuelva." $) cyoa-text! badfx
	then clear-cyoa
;

cyoa: trade-groupheal
	"Atencion Medica" cyoa-title! picon!
	($ Thegroup .. " encontro a una mujer rodeada de equipo medico. Ella se presenta como una doctora experimentada!	Ella ofrece atencion medica a cambio de comida." $) cyoa-text! basic-tone foodamt
	choice( "Dejalo por ahora" )choice back-esc
	trunk-food 6 < if
		($ "'No hay suficiente comida para pagar el tratamiento." $) +cr +text
	else
		choice( "Dar 6 alimentos para el tratamiento" )choice: trade-groupheal-give
		trunk-food 14 > notsolo and if
			choice( "Dar " .. trade-train-amt .. " comida para el cuidado colectivo" )choice: trade-groupheal-group
		then
	then
		most-charming camp-charm 1 < and if
			charchoice( "  utiliza encanto" )choice: trade-groupheal-charm
		then
		most-ahole if
			charchoice( "  roba al doctor" )choice: trade-groupheal-rob
		then
;

