cyoa{ regiondef{

: trade-flee
	choosehuman alive? if
		($ Thegroup .. "  se ve obligado a huir del campamento de comerciantes!" $) +crtext quitmission
	else 
		clear-streak delete-the-game
		' quit-to-title cyoa-choice-handler! 
	then
;

// Trading gas for food

0 value gas-to-food-bought

cyoa-choice: trade-gas-food-rob 
	($ cname .. "  derriba al hombre, agarra un saco de comida y luego corre. " $) cyoa-text! badfx
	notsolo if
		($ Theothersare .. "  arrastrado a ella." $) +text
	then
	6 10 rnd trunk-food+ rob-guilt
	aholeREV trade-flee
;

cyoa-choice: trade-gas-food-2
	($ Thegroup .. "  intercambia 25 unidades de gas por 2 alimentos." $) cyoa-text! basic-tone
	-25 trunk-gas+ 2 trunk-food+ 2 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-4
	($ Thegroup .. "  intercambia 50 unidades de gas por 4 alimentos." $) cyoa-text! basic-tone
	-50 trunk-gas+ 4 trunk-food+ 4 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-8
	($ Thegroup .. "  intercambia 100 unidades de gas por 8 alimentos." $) cyoa-text! basic-tone
	-100 trunk-gas+ 8 trunk-food+ 8 ' gas-to-food-bought +!
;

cyoa: trade-gas-food
	" Conversion de gas a bocadillos" cyoa-title!
//	iicon
	($ Thegroup .. "  ve a un hombre parado cerca de una camioneta." $) cyoa-text!
	gas-to-food-bought 20 < if
		($ "' Tienes gasolina de sobra? Me estoy quedando sin!" pbold .. " Ofrece darte 2 latas de comida por cada 25 unidades de gas." $)
		+crtext gasamt
		choice( "No gracias" )choice back-esc
		most-ahole if
			charchoice( "  lo roba" )choice: trade-gas-food-rob
		then
	else
		clear-cyoa ($ " Gracias por toda la gasolina!" pbold $) +crtext
	then basic-tone 
	trunk-gas 25 < if
		($ " No hay suficiente para gastar." $) +text
	else
		trunk-gas 99 > gas-to-food-bought 13 < and if
			choice( " Intercambiar 100 gas" )choice: trade-gas-food-8
		then
		trunk-gas 49 > gas-to-food-bought 17 < and if
			choice( " Intercambiar 50 gas" )choice: trade-gas-food-4
		then
		trunk-gas 24 > gas-to-food-bought 19 < and if
			choice( " Intercambiar 25 gas" )choice: trade-gas-food-2
		then
	then
;

