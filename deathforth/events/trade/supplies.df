cyoa{ regiondef{

: trade-flee
	choosehuman alive? if
		($ Thegroup .. "  e costretto a fuggire dal campo dei commercianti!" $) +crtext quitmission
	else 
		clear-streak delete-the-game
		' quit-to-title cyoa-choice-handler! 
	then
;

// Trading gas for food

0 value gas-to-food-bought

cyoa-choice: trade-gas-food-rob 
	($ cname .. " italiano " $) cyoa-text! badfx
	notsolo if
		($ Theothersare .. " viene trascinato dentro di esso." $) +text
	then
	6 10 rnd trunk-food+ rob-guilt
	aholeREV trade-flee
;

cyoa-choice: trade-gas-food-2
	($ Thegroup .. " 25 gas  2 food" $) cyoa-text! basic-tone
	-25 trunk-gas+ 2 trunk-food+ 2 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-4
	($ Thegroup .. " 50 gas for 4 food" $) cyoa-text! basic-tone
	-50 trunk-gas+ 4 trunk-food+ 4 ' gas-to-food-bought +!
;

cyoa-choice: trade-gas-food-8
	($ Thegroup .. " " $) cyoa-text! basic-tone
	-100 trunk-gas+ 8 trunk-food+ 8 ' gas-to-food-bought +!
;

cyoa: trade-gas-food
	"Trasformazione da carburante a snack" cyoa-title!
//	iicon
	($ Thegroup .. " vede un uomo in piedi vicino a un SUV." $) cyoa-text!
	gas-to-food-bought 20 < if
		($ "'Hai benzina di scorta? Sto finendo!" pbold .. "Si offre di darti 2 lattine di cibo per ogni 25 unita di benzina." $)
		+crtext gasamt
		choice( "No grazie" )choice back-esc
		most-ahole if
			charchoice( " lo deruba" )choice: trade-gas-food-rob
		then
	else
		clear-cyoa ($ "Grazie per tutta la benzina!" pbold $) +crtext
	then basic-tone 
	trunk-gas 25 < if
		($ "Non abbastanza da risparmiare." $) +text
	else
		trunk-gas 99 > gas-to-food-bought 13 < and if
			choice( "Scambia 100 gas" )choice: trade-gas-food-8
		then
		trunk-gas 49 > gas-to-food-bought 17 < and if
			choice( "Scambia 50 gas" )choice: trade-gas-food-4
		then
		trunk-gas 24 > gas-to-food-bought 19 < and if
			choice( "Scambia 25 gas" )choice: trade-gas-food-2
		then
	then
;

