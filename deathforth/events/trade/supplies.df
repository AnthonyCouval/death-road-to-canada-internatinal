// Healing

cyoa-choice: trade-groupheal-rob 
	($ Thegroup .. " 'asalta al doctor de suministros. La gente de campamento de comercio se apresura a defenderlo." $) cyoa-text! bitefx 6 10 rnd trunk-medical+ rob-fail rob-guilt2
		aholeREV trade-flee
;

: trade-groupheal-guy
	cchar damage 0 = if
		($ cname .. "  se hace un chequeo, pero no hay nada malo con " .. chim .. "." $)
	else
		($ cname .. " recibe tratamiento para " .. chis .. " heridas" $) 
	then cchar .fullheal
;

cyoa-choice: trade-groupheal-heal
	trade-groupheal-guy
	cyoa-text! goodfx
	-6 trunk-food+
	clear-cyoa
;

cyoa-choice: trade-groupheal-group
	
	team-damage 0 = if
		($ Thegroup .. " se hacen un chequeo, pero no hay nada malo con ellos." $) cyoa-text!
	else
		7 groupheal
		($ Thegroup .. " recibe tratamiento para " $) cyoa-text!
		team-damage 1 < if 
			"todo" +text 
		else
			"mas" +text
		then
			" de sus heridas." +text
	then
	goodfx
	trade-train-pay
	clear-cyoa
;

: trade-groupheal-give
	cyoa-start
	solo if
		soloer choosechar trade-groupheal-heal
	then;
	"Quien deberia recibir el tratamiento?" cyoa-text!
	' trade-groupheal-heal cyoa-team-choice
;

cyoa-choice: trade-groupheal-charm 
	charmroll if
		($ cname .. " 'conseguir que el doctor ofrezca sus servicios de forma gratuita!" .. cHe .. "s just that charming." $) cyoa-text! ' camp-charm ++ goodfx
		trade-groupheal-guy +crtext
	else
		($ cname .. " 'accidentalmente solo suena aterrador." .. gHe .. " 'le piden que se vaya y que nunca regrese." $) cyoa-text! badfx
	then clear-cyoa
;

cyoa: trade-groupheal
	"Atencion medica" cyoa-title! picon!
	($ Thegroup .. " 'encuentra a una mujer rodeada de equipos medicos. Ella afirma ser una doctora habil!' Ella ofrece proporcionar atencion medica a cambio de alimentos." $) cyoa-text! basic-tone foodamt
	choice( "Deja esto por ahora" )choice back-esc
	trunk-food 6 < if
		($ "No hay suficiente comida para pagar el tratamiento." $) +cr +text
	else
		choice( "Dejalos tener 6 alimentos para el tratamiento." )choice: trade-groupheal-give
		trunk-food 14 > notsolo and if
			choice( "Dame " .. trade-train-amt .. " spanish" )choice: trade-groupheal-group
		then
	then
		most-charming camp-charm 1 < and if
			charchoice( " 'utiliza encanto" )choice: trade-groupheal-charm
		then
		most-ahole if
			charchoice( " roba al doctor" )choice: trade-groupheal-rob
		then
;

