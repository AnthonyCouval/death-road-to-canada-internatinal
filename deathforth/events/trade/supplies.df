// Healing

cyoa-choice: trade-groupheal-rob 
	($ Thegroup .. " rouba o medico de suprimentos. As outras pessoas no acampamento de comercio se apressam para defendela." $) cyoa-text! bitefx 6 10 rnd trunk-medical+ rob-fail rob-guilt2
		aholeREV trade-flee
;

: trade-groupheal-guy
	cchar damage 0 = if
		($ cname .. "  gets a check-up, but there's nothing wrong with it " .. chim .. "." $)
	else
		($ cname .. "  recebe tratamento para " .. chis .. " feridas!" $) 
	then cchar .fullheal
;

cyoa-choice: trade-groupheal-heal
	trade-groupheal-guy
	cyoa-text! goodfx
	-6 trunk-food+
	clear-cyoa
;

cyoa-choice: trade-groupheal-group
	
	team-damage 0 = if
		($ Thegroup .. " ' recebe um exame, mas eles nao tem nada errado." $) cyoa-text!
	else
		7 groupheal
		($ Thegroup .. "  recebe tratamento para " $) cyoa-text!
		team-damage 1 < if 
			"todos" +text 
		else
			"maior parte" +text
		then
			"  de suas feridas." +text
	then
	goodfx
	trade-train-pay
	clear-cyoa
;

: trade-groupheal-give
	cyoa-start
	solo if
		soloer choosechar trade-groupheal-heal
	then;
	"Quem deve se tratar?" cyoa-text!
	' trade-groupheal-heal cyoa-team-choice
;

cyoa-choice: trade-groupheal-charm 
	charmroll if
		($ cname .. " 	convence o medico a oferecer seus servicos gratuitamente!" .. cHe .. "''e apenas encantador." $) cyoa-text! ' camp-charm ++ goodfx
		trade-groupheal-guy +crtext
	else
		($ cname .. "  acidentalmente parece assustador. 		" .. gHe .. " e pedido para sair e nunca mais voltar." $) cyoa-text! badfx
	then clear-cyoa
;

cyoa: trade-groupheal
	"Atendimento Medico" cyoa-title! picon!
	($ Thegroup .. " encontra uma mulher cercada por equipamentos medicos. Ela afirma ser uma medica habilidosa! Ela oferece cuidados medicos em troca de comida." $) cyoa-text! basic-tone foodamt
	choice( "Sai agora" )choice back-esc
	trunk-food 6 < if
		($ "Nao ha comida suficiente para pagar o tratamento." $) +cr +text
	else
		choice( "De 6 alimentos para o tratamento" )choice: trade-groupheal-give
		trunk-food 14 > notsolo and if
			choice( "De " .. trade-train-amt .. " alimento para cuidados grupais" )choice: trade-groupheal-group
		then
	then
		most-charming camp-charm 1 < and if
			charchoice( " " )choice: trade-groupheal-charm
		then
		most-ahole if
			charchoice( " rouba o medico" )choice: trade-groupheal-rob
		then
;

