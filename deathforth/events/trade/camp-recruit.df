cyoa-bridge: camp-goat-recruit
	ricon 4 recruitee .morale!
	clear-cyoa

	($ rname  .. "  starts following! It " .. "IMEDIATAMENTE" rbold .. "  crava os chifres em " .. cname .. "!" $) cyoa-text! bitefx
		recruitee .loyaltyREV recruitee .attitudeREV ouch--
		recruited
;

0 value camp-goat-milked

cyoa-choice: camp-goat-milk
	' camp-goat-milked ++
	camp-goat-milked 2 < if
		($ "0.5 " .. cname .. "  muito mal." $) 
	else
		($ cname .. " 	Attempts to do something again, even though it has already failed. " .. cname .. "  muito mal." $) 
	then
		text!
		ouch-- morale-- bitefx
		recruitee .female@ if 
			alive? if
				($ cname .. " 'consegue tirar leite da cabra!'" $)
			else
				($ cname .. " 'tira leite da cabra antes de morrer. Legal!'" $)
			then +crtext 2 trunk-food+ goodfx
		else
			"'Nao se pode tirar leite de macho cabrio!'" +crtext
			alive? if
				camp-goat-milked 2 < if
					($ "A cabra fere " .. cname .. "  muito dificil, entao " .. che .. " 'lembrase desta licao.'" $) 
				else
					($ cname .. " nao consegui lembrar essa licao da ultima vez." $) 
				then
					+crtext	ouch--
			else
				($ "A cabra esta decepcionada que " .. cname .. "  ja morreu, como ele queria thunk " .. chim .. "  duas vezes." $) +crtext 
			then
		then
;

cyoa-bridge: camp-goat-milk-pick
	solohuman if
		choosehuman camp-goat-milk
	then;
	($ "Quem deve ordenhar a cabra? " .. rHe .. "  parece " .. "ornery." rbold $) text!
	choice( "Mudei de ideia!" )choice
	' camp-goat-milk cyoa-human-choice
;

cyoa-bridge: camp-goat-rec-pick
	solohuman if
		choosehuman camp-goat-recruit
	then;
	($ "Quem deve recrutar a cabra? " .. rHe .. "  parece " .. "ornery." rbold $) text!
	' camp-goat-recruit cyoa-human-choice
;

