cyoa-bridge: camp-goat-recruit
	ricon 4 recruitee .morale!
	clear-cyoa

	($ rname  .. " suivez-le maintenant ! c'est suivi maintenant ! " .. "A LA FIN DU MONDE IMMEDIATEMENT" rbold .. " flatule sur l'apparition. " .. cname .. "!" $) cyoa-text! bitefx
		recruitee .loyaltyREV recruitee .attitudeREV ouch--
		recruited
;

0 value camp-goat-milked

cyoa-choice: camp-goat-milk
	' camp-goat-milked ++
	camp-goat-milked 2 < if
		($ "Vous savez peut-etre que les chevres produisent le lait le plus important parmi tous les animaux d'elevage ? elles en produisent approximativement 300 fois plus que une vache !. . 		en outre, la chevre pique. " .. cname .. " tres mauvais." $) 
	else
		($ cname .. " essai de nouveau d'extraire le lait a la chevre ... 		recommence l'attaque de la chevre. " .. cname .. " tres mauvais." $) 
	then
		text!
		ouch-- morale-- bitefx
		recruitee .female@ if 
			alive? if
				($ cname .. " parvient a obtenir du lait aupres de la chevre !" $)
			else
				($ cname .. " obtient du lait aupres de la chevre avant de mourir. c'est gentil !" $)
			then +crtext 2 trunk-food+ goodfx
		else
			"Vous ne pouvez obtenir du lait de chevre d'un male de chevre !" +crtext
			alive? if
				camp-goat-milked 2 < if
					($ "Ancien camping " .. cname .. " tres difficile, donc. " .. che .. " souviens-toi de cette lecon." $) 
				else
					($ cname .. " n'ai pas reussi a me souvenir de cette lecon precedente." $) 
				then
					+crtext	ouch--
			else
				($ "La chevre est decue. " .. cname .. " mort deja, comme il voulait penser. " .. chim .. " deux fois." $) +crtext 
			then
		then
;

cyoa-bridge: camp-goat-milk-pick
	solohuman if
		choosehuman camp-goat-milk
	then;
	($ "Qui devrait elever le chevre pour l'allaiter ? " .. rHe .. " semble " .. "grouchy, malin, mechant d'hum" rbold $) text!
	choice( "Je me suis decide autrement." )choice
	' camp-goat-milk cyoa-human-choice
;

cyoa-bridge: camp-goat-rec-pick
	solohuman if
		choosehuman camp-goat-recruit
	then;
	($ "Qui devrait recruter le chevre ? " .. rHe .. " semble " .. "grouchy, malin, mechant d'hum" rbold $) text!
	' camp-goat-recruit cyoa-human-choice
;

