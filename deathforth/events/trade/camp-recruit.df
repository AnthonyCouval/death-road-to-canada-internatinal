0 value camp-goat-milked

cyoa-choice: camp-goat-milk
	' camp-goat-milked ++
	camp-goat-milked 2 < if
		($ "Voce sabia que as cabras produzem mais leite do que qualquer outro animal de fazenda? Aproximadamente 300 vezes mais que uma vaca!		Tambem as cabras chifram " .. cname .. "  muito ruim." $) 
	else
		($ cname .. "  tenta ordenhar a cabra novamente.		Mais uma vez, a cabra chifra " .. cname .. "  muito ruim." $) 
	then
		text!
		ouch-- morale-- bitefx
		recruitee .female@ if 
			alive? if
				($ cname .. "  consegue tirar um pouco de leite da cabra!" $)
			else
				($ cname .. " tira leite da cabra antes de morrer. Legal!" $)
			then +crtext 2 trunk-food+ goodfx
		else
			"Nao se pode tirar leite de bode!" +crtext
			alive? if
				camp-goat-milked 2 < if
					($ "A cabra fere " .. cname .. " muito dificil, entao " .. che .. "  lembra desta licao." $) 
				else
					($ cname .. " ' nao conseguiu lembrar desta licao da ultima vez." $) 
				then
					+crtext	ouch--
			else
				($ "A cabra esta desapontada que " .. cname .. "  ja morreu, pois ele queria thunk " .. chim .. "  duas vezes." $) +crtext 
			then
		then
;

cyoa-bridge: camp-goat-milk-pick
	solohuman if
		choosehuman camp-goat-milk
	then;
	($ "Quem deve ordenhar a cabra? " .. rHe .. "  parece " .. "ornery." rbold $) text!
	choice( "Mudei de ideia!" )choice
	' camp-goat-milk cyoa-human-choice
;

cyoa-bridge: camp-goat-rec-pick
