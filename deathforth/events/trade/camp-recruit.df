cyoa{ road{ regiondef{

// Generic recruits

: .recruit-setup-default args( chara )
;

: .chara-link args( chara thing )

	<- chara character# <- thing .charid!
	<- thing thing# <- chara .cur_thingid!
;

: .recruit-setup-familiar .thing@ 0 args( thing newchara )

	// No familiars? Just go with the rando char
	draw-familiar dup 0= if drop then;

	// Save the familiar that we drew
	-> newchara

	// Free the old rando chara from the NPC
	<- thing .character@ char-free

	// Link the familiar character to the NPC
	<- newchara <- thing .chara-link
	<- newchara .ai_defaults
;

cyoa-bridge: recruit-campyes-familiar
	($ rname .. " est heureux de revenir !" $) cyoa-text! basic-tone
	clear-cyoa
	team-full? if
		($ "Vous devez laisser quelqu'un pour que place soit disponible. quel personne voulez-vous remplacer ? " .. rname .. "?" $) +crtext
		' recruit-familiarreplace cyoa-team-choice
	else
		recruited goodfx
		familiar-recruit-check
	then
;

cyoa-bridge: recruit-campyes
	($ rname .. " mange rapidement votre aliment !" $) cyoa-text! basic-tone
	-5 trunk-food+
	clear-cyoa
	team-full? if
		($ "Vous devez laisser quelqu'un pour que place soit disponible. quel personne voulez-vous remplacer ? " .. rname .. "?" $) +crtext
		' recruit-replace cyoa-team-choice
	else
		recruited goodfx
	then
;

: recruit-campyesno
	choice( "Quitter " .. rname .. " pour le moment :" )choice
	trunk-food 4 > if
		choice( "Recruter " .. rname .. " cinq aliments." )choice: recruit-campyes
	then
;

cyoa-bridge: recruit-finalcampyes
	($ rname .. " mange rapidement la nourriture ! c'est comme regarder une competition de mange-piffores, mais avec une seule personne." $) cyoa-text! basic-tone
	-10 trunk-food+
	clear-cyoa
	team-full? if
		($ "Vous devez laisser quelqu'un pour que place soit disponible. quel personne voulez-vous remplacer ? " .. rname .. "?" $) +crtext
		' recruit-replace cyoa-team-choice
	else
		recruited goodfx
	then
;

: recruit-finalcampyesno
	choice( "Quitter " .. rname .. " pour le moment :" )choice
	trunk-food 9 > if
		choice( "Recruter " .. rname .. " dix aliments" )choice: recruit-finalcampyes
	then
