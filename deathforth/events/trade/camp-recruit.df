// Special characters

: camp-ninja-vanish 
	interactee mission{ .smokepoof } deleteme
;

cyoa-choice: camp-ninja-flee
	ricon
	($ rname  .. " ' joga uma bomba de fumaca no chao! Ela se foi!" $) cyoa-text! badfx 
	camp-ninja-vanish
;

cyoa-bridge: camp-ninja-recruit
	ricon
	($ rname  .. " ' pega a comida e depois joga uma bomba de fumaca no chao. Ela se foi! Nao espere, ela esta te seguindo agora." $) cyoa-text! goodfx

	interactee mission{ .smokepoof } interactee .scatter 15 interactee .fade!

	5 recruitee .morale! clear-cyoa
		team-count 3 > if
			"'Ela gosta de seu espaco pessoal para balancar a espada, entao voce tera que chutar alguem." +crtext 
			-5 trunk-food+ ' recruit-replace cyoa-team-choice
		then;
	recruitee .strengthREV recruitee .fitnessREV recruitee .temperREV
	-5 trunk-food+
	recruited 
	road{ 6 8 rnd to ninja-countdown }
;

cyoa: camp-rec-ninja
