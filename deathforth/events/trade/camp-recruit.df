cyoa: camp-recruit-dog
	"Un chien sans raisons" cyoa-title!
	nearest .character@ choosechar
	cchar to recruitee ricon
	
	($ rname .. " se trouve autour du camp, dompte mais sans personne a suivre. " .. rHe .. " il devait avoir ete abandonne ici." $) cyoa-text! basic-tone
	
	choice( "Laisser le chien tranquille" )choice
	trunk-food 4 > if
		choice( "Recruter " .. rname .. " cinq aliments." )choice: recruit-campyes
	then
	dogpal? if
		choosedogpal cchar charchoice( " appelle le chien !" )choice: camp-recruit-dog-psych
	then
;

cyoa: camp-recruit-final
	"Si d'abord tu ne reussis pas, essaye encore et again." cyoa-title!
	nearest .character@ choosechar cchar to recruitee ricon
	($ rname .. " presque atteint le canada. " .. rHe .. " jusque dans la ville des espoirs ecrases. " .. rhis .. " bienvenue au canada !" .. rHe .. " offre de vous rejoindre pour diner pendant 10 jours. " .. rHe .. " cela ressent une vraie bonne atmosphere a ce essai !" $) cyoa-text! basic-tone
	WEAPON_HOCKEY recruitee .weaponc!
	
	trunk-food 10 < if
		camp-recruit-nofood
	then;
	
	regiondef{ camprecruit-final-talked } 1 < if 
		($ rname .. " une demonstration rapide de " .. rhis .. " " $) +crtext
		40 %chance if
			"competences de tir !" +text
			recruit-highstat .shooting!
			recruitee .shootingREV
		else
			50 %chance if
				"puissance enorme !" +text
				recruit-highstat .strength!
				recruitee .strengthREV
			else
				"sportive et physique en bonne forme !" +text
				recruit-highstat .fitness!
				recruitee .fitnessREV
			then
		then
	else
		($ "Je ne les estimais pas beaucoup mon equipe derniere, j'etais plutot un peu decevant! mais je pense faire beaucoup mieux cette fois-ci si vous m'accordez une seconde chance !" $) pbold +crtext
	then
	regiondef{ ' camprecruit-final-talked ++ }
	
	recruit-finalcampyesno
;

cyoa: camp-rec-familiar
	"Visage familiier" cyoa-title!
	nearest .character@ choosechar
	cchar to recruitee ricon
		
	($ rname .. " se trouve autour du camp. " .. rHe .. " tu attendais que je arrive ?" $) cyoa-text! basic-tone
	
	recruitee .trait@ $len 0= if
		($ rHe .. " claims to know you, but you swear you've never seen this person before..." $) +crtext
	else
		($ "TRAITE. " pbold .. recruitee .trait@ $) +crtext 
		($ " PERK : " pbold .. recruitee .perk@ $) +text 
	then
	
	choice( "Quitter " .. rname .. " pour le moment :" )choice

		choice( "Recruter " .. rname .. "!" )choice: recruit-campyes-familiar
;


