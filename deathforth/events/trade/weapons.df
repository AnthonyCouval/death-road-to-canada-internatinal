	most-grating if 
		charchoice( " lhe diz para ATRASAR" fcoolit )choice: trade-polearm-coolit
	then
	trunk-food 3 < if
		($ "'Isso nao e comida suficiente para comprar nada! Como consolo o comerciante da " .. thegroup .. " um taco de sinuca." $) +crtext goodfx 1 WEAPON_POOLCUE trunk.weapon+ clear-cyoa
	else
		trunk-food 24 > polearm-bardiche 1 < and if
			choice( "Bardiche por 25 de comida" )choice: trade-polearm-4
		then
		trunk-food 8 > polearm-spear 3 < and if
			choice( "Lanca Tatica por 9 de comida" )choice: trade-polearm-3
		then
		trunk-food 5 > polearm-boot 1 < and if
			choice( "Bota de Pau por 6 de comida" )choice: trade-polearm-2
		then
		trunk-food 2 > if
			choice( "Forcado por 3 de comida" )choice: trade-polearm-1
		then
	then
;

