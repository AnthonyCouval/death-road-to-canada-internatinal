// Fireworks Trader

0 value fireworks-4
0 value fireworks-3
0 value fireworks-2
0 value fireworks-1

cyoa-choice: trade-fireworks-4
	($ Thegroup .. "  compra um KO Boomer! Anteriormente ilegal em quase todos os paises!" $) cyoa-text! goodfx
		($ "'Ouvi dizer que isso ainda e realmente ilegal no Canada!		25% de chance de se explodir! Tente a sorte!		Se estiver quente em sua mao, tente largalo e fugir BEM rapido!" pbold $) +crtext 
	20 WEAPON_KOBOOMER trunk.weapon+ -5 trunk-food+ someicon ' fireworks-4 ++
;

cyoa-choice: trade-fireworks-3
	($ Thegroup .. "  compra uma vela romana!" .. "'E tao bonito! Zumbis adoram!" pbold $) cyoa-text! goodfx
	10 WEAPON_ROMANCANDLE trunk.weapon+ -3 trunk-food+ someicon ' fireworks-3 ++
;

cyoa-choice: trade-fireworks-2
	($ Thegroup .. "  compra um pacote duplo de Mount Dingus" .. "[TM]" pbold .. "!	O fogo de artificio mais decepcionante do mercado.	" .. "'Ele nem explode! Ele apenas solta pequenas faiscas!	Eu odeio isso!" pbold $) cyoa-text! goodfx
	2 WEAPON_MOUNTDINGUS trunk.weapon+ -3 trunk-food+ someicon ' fireworks-2 ++
;

cyoa-choice: trade-fireworks-1
	($ Thegroup .. "  compra 3 macos de foguetes." $) cyoa-text! goodfx
		($ "Acenda um maco e veja os zumbis correndo atras dele!" pbold $) +crtext 
	3 WEAPON_FIRECRACKERS trunk.weapon+ -2 trunk-food+ someicon ' fireworks-1 ++
;

cyoa: trade-firework
	"Entusiasta de fogos de artificio" cyoa-title!
	($ Thegroup .. " ' encontra uma mulher segurando uma vela romana. Nao esta aceso. Parece que ela esta pronta para acendelo em " .. "A QUALQUER MOMENTO.	" rbold
	.. "Tao forte! Tao bonita! Tao pratica!" pbold $) cyoa-text! basic-tone foodamt
	trunk-food 4 > fireworks-4 3 < and if
		"P" pbold +crtext
	then
	trunk-food 2 < if
		($ "'Voce esta sem comida! O comerciante da " .. thegroup .. " 'uma duzia de bombinhas'" $) +crtext goodfx 1 WEAPON_FIRECRACKERS trunk.weapon+ clear-cyoa
	else
		choice( "Sai agora" )choice back-esc
		trunk-food 4 > fireworks-4 3 < and if
			choice( "1 wood" )choice: trade-fireworks-4
		then
		trunk-food 2 > fireworks-3 3 < and if
			choice( "1 firework" )choice: trade-fireworks-3
		then
		trunk-food 2 > fireworks-2 2 < and if
			choice( "1 Mount Dingus" .. "[TM]" pbold .. "  - 3 comidas" )choice: trade-fireworks-2
		then
		fireworks-1 3 < if
			choice( "tres fogos de artificio - 2 comida" )choice: trade-fireworks-1
		then
	then
;

