			choice( "Slug Schrotflinte fur 25 Essen" )choice: trade-shotgun-3
		then
		trunk-food 29 > shotgun-2 1 < and if
			choice( "Autoshotty fur 30 Essen" )choice: trade-shotgun-2
		then
		trunk-food 11 > shotgun-4 2 < and if
			choice( "Doppellauf Schrotflinte fur 12 Essen" )choice: trade-shotgun-4
		then
		(* trunk-food 14 > if
			choice( "75 Patronen fur 15 Essen" )choice: trade-shotgun-shell-15
		then *)
		trunk-food 6 > shotgun-1 3 < and if
			choice( "Schrotflinte fur 7 Essen" )choice: trade-shotgun-1
		then
		(* trunk-food 9 > if
			choice( "50 Patronen fur 10 Essen" )choice: trade-shotgun-shell-10
		then
		trunk-food 4 > if
			choice( "25 Patronen fur 5 Essen" )choice: trade-shotgun-shell-5
		then *)

	then
;

