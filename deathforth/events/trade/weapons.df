	else
		($ cname .. "  diz ao comerciante de machados para " .. "SE ACALMAR." pbold $)
	then cyoa-text!
	cool-it++
	($ "'O comerciante de machados nao parece reagir.	" .. "....." rbold .. "
	" .. cname .. "  nao tem ideia se " .. che .. "  foi ouvido ou nao." $) 
