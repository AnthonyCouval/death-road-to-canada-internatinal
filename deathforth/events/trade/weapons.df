cyoa: trade-army
	"Fortaleza de Sobras do Exercito" cyoa-title!
	($ Thegroup .. " 	Encontra um container de lixo. Esta trancado com seguranca, com uma fenda na lateral. Ha nomes de armas e precos rabiscados perto do buraco." .. "'So colocar a comida aqui!'" pbold .. " 'diz uma vozinha no lixo.'" $) cyoa-text! basic-tone foodamt
	choice( "Deixeo sozinho" )choice back-esc 
	trunk-food 5 < if
		($ "'Isso nao e o suficiente para comer do lixo!" $) +crtext badfx // clear-cyoa
	else
		trunk-food 74 > army-7 1 < and if
			choice( "Metralhadora por 75 comidas" )choice: trade-army-7
		then
		trunk-food 49 > army-6 1 < and if
			choice( "Foguetefogo por 50 comida" )choice: trade-army-6
		then
		trunk-food 39 > army-5 1 < and if
			choice( "'12 granadas por 40 comida'" )choice: trade-army-5
		then
		trunk-food 24 > army-4 1 < and if
			choice( "'Fuzil de assalto por 25 comida'" )choice: trade-army-4
		then
		trunk-food 14 > army-3 1 < and if
			choice( "'Uzi por 15 comida'" )choice: trade-army-3
		then
		trunk-food 11 > army-2 1 < and if
			choice( "'3 granadas por 12 comida'" )choice: trade-army-2
		then
		trunk-food 3 > army-1 1 < and if
			choice( "'1 granada por 4 comida'" )choice: trade-army-1
		then
	then
;

