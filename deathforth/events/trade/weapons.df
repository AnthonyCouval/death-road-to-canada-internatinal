;

cyoa-choice: trade-army-6
	($ Thegroup .. " 	Alimenta uma quantidade massiva de comida no slot. Alguem do outro lado se esforca para empurrar um lanca chamas para fora da lixeira." .. "Acenda os!" pbold $) cyoa-text! goodfx
	1 WEAPON_FLAMETHROWER trunk.weapon+ -50 trunk-food+ picon ' army-6 ++
;

cyoa-choice: trade-army-7
	($ Thegroup .. " ' alimenta lentamente um carregamento de comida no conteiner. Ruidos de raspadas sao ouvidos e entao um cano de minigun e empurrado atraves da escuridao. A estrutura do slot e deformada devido ao tamanho da minigun quando ela e empurrada." .. "Muito obrigado! Posso me esconder aqui por meses agora!" pbold $) cyoa-text! goodfx
	1 WEAPON_MINIGUN trunk.weapon+ -75 trunk-food+ picon ' army-7 ++
;

cyoa: trade-army
	"Fortaleza de Sobras do Exercito" cyoa-title!
	($ Thegroup .. " ' encontra uma lixeira. Esta bem trancada, com uma fenda na lateral. Ha nomes e precos de armas rabiscados perto do buraco.	" .. "Basta deixar a comida aqui!" pbold .. "  diz uma pequena voz na lixeira." $) cyoa-text! basic-tone foodamt
	choice( "Deixe o sozinho" )choice back-esc 
	trunk-food 5 < if
		($ "'Isso nao e comida suficiente para a pessoa da lixeira!" $) +crtext badfx // clear-cyoa
	else
		trunk-food 74 > army-7 1 < and if
			choice( "Minigun por 75 comidas" )choice: trade-army-7
		then
		trunk-food 49 > army-6 1 < and if
			choice( "Lanca chamas por 50 alimentos" )choice: trade-army-6
		then
		trunk-food 39 > army-5 1 < and if
			choice( "12 granadas por 40 comidas" )choice: trade-army-5
		then
		trunk-food 24 > army-4 1 < and if
			choice( "Rifle de assalto por 25 comidas" )choice: trade-army-4
		then
		trunk-food 14 > army-3 1 < and if
			choice( "Uzi por 15 comidas" )choice: trade-army-3
		then
		trunk-food 11 > army-2 1 < and if
			choice( "3 granadas por 12 comidas" )choice: trade-army-2
		then
		trunk-food 3 > army-1 1 < and if
			choice( "1 granada por 4 comidas" )choice: trade-army-1
		then
	then
;

// Shotgun seller

0 value shotgun-1
0 value shotgun-2
0 value shotgun-3
