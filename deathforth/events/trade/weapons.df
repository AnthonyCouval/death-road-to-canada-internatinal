cyoa: trade-polearm
	"Paladao de Polearm" cyoa-title!
	($ Thegroup .. " encontra um homem empunhando uma lanca caseira. Apesar de ser uma faca amarrada a um pedaco de pau, parece que seria uma arma realmente eficaz." .. "Nada supera as armas de haste para cacar zumbis!" pbold $) cyoa-text! basic-tone foodamt
	choice( "Deixeo" )choice back-esc
	most-ahole if
		charchoice( " rouba o comerciante" )choice: trade-polearm-rob
	then
	most-grating if 
		charchoice( "  diz a ele para RELAXAR" fcoolit )choice: trade-polearm-coolit
	then
	trunk-food 3 < if
		($ "'Isso nao e suficiente comida para comprar nada! Como consolacao o vendedor da " .. thegroup .. " 'um taco de sinuca'" $) +crtext goodfx 1 WEAPON_POOLCUE trunk.weapon+ clear-cyoa
	else
		trunk-food 24 > polearm-bardiche 1 < and if
			choice( "1.5 gold per food" )choice: trade-polearm-4
		then
		trunk-food 8 > polearm-spear 3 < and if
			choice( "1 spear + 1 food" )choice: trade-polearm-3
		then
		trunk-food 5 > polearm-boot 1 < and if
			choice( "1 Stick Boot" )choice: trade-polearm-2
		then
		trunk-food 2 > if
			choice( "1 pitchfork for 3 food" )choice: trade-polearm-1
		then
	then
;

// Silent Gun Trader

0 value silent-pistol
0 value silent-rifle
0 value silent-shotgun

cyoa-choice: trade-silent-3
	($ Thegroup .. " 	Comprando uma espingarda silenciosa." .. "1.5" pbold $) cyoa-text! goodfx
	1 WEAPON_SILENTSHOTGUN trunk.weapon+ -30 trunk-food+ someicon ' silent-shotgun ++
;

cyoa-choice: trade-silent-2
	($ Thegroup .. " 0x0000000000000000" .. "'Escolha excelente. Isso e o meu favorito.	Tive modificado para ter um grande potencial de penetracao. Em maos de um especialista, pode destruir uma linha inteira de zumbis em uma unica disparo. A longa distancia, silenciosamente." pbold $) cyoa-text! goodfx
	1 WEAPON_SILENTRIFLE trunk.weapon+ -25 trunk-food+ someicon ' silent-rifle ++
;

cyoa-choice: trade-silent-1
	($ Thegroup .. " compra um pistola silenciada." $) cyoa-text! goodfx
		silent-pistol 0 = if 
			($ "Tenho alguns mais a vender, se quiser pistolas silenciosas para um grupo inteiro!" pbold $) +crtext 
		then
		silent-pistol 3 = if 
			($ "Este e o meu ultimo pistola. Divirtase com sua nova arsenal silencioso e mortal!" pbold $) +crtext 
		then
	1 WEAPON_SILENTPISTOL trunk.weapon+ -3 trunk-food+ someicon ' silent-pistol ++
;

