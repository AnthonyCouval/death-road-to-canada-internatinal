			choice( "75 cartuchos por 15 alimentos" )choice: trade-shotgun-shell-15
		then *)
		trunk-food 6 > shotgun-1 3 < and if
			choice( "7 food" )choice: trade-shotgun-1
		then
		(* trunk-food 9 > if
			choice( "50 cartuchos por 10 alimentos" )choice: trade-shotgun-shell-10
		then
		trunk-food 4 > if
			choice( "25 cartuchos por 5 alimentos" )choice: trade-shotgun-shell-5
