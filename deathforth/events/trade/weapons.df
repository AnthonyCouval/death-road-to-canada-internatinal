			choice( "Espingarda de bala por 25 comidas" )choice: trade-shotgun-3
		then
		trunk-food 29 > shotgun-2 1 < and if
			choice( "Espingarda automatica por 30 comida" )choice: trade-shotgun-2
		then
		trunk-food 11 > shotgun-4 2 < and if
			choice( "2x Double Barrel Shotgun for 2 food" )choice: trade-shotgun-4
		then
		(* trunk-food 14 > if
			choice( "75 balas por 15 comida" )choice: trade-shotgun-shell-15
		then *)
		trunk-food 6 > shotgun-1 3 < and if
			choice( "7 food" )choice: trade-shotgun-1
		then
		(* trunk-food 9 > if
			choice( "50 balas por 10 comida" )choice: trade-shotgun-shell-10
		then
		trunk-food 4 > if
			choice( "25 balas por 5 comida" )choice: trade-shotgun-shell-5
		then *)

	then
;

