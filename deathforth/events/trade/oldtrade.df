	($ "Ao primeiro sinal de problemas, o comerciante abre o casaco para revelar que ele esta coberto de bombas de tubo. Em um movimento rapido, ele acende um pavio em um deles." .. cname .. "  esta bem ao lado dele, e mal tem tempo para se arrepender profundamente de todo o assalto." $) cyoa-text! bitefx
		rob-megafail aholeREV
;
cyoa-choice: trade-pipebomb-coolit
	cpet? if
		($ cname .. "  peida bem alto no cara da bomba caseira." $)
	else
		($ cname .. "  diz ao cara da bomba caseira para " .. "SE ACALMAR." pbold $)
	then cyoa-text!
	cool-it-7 if
		($ "Ele interpreta isso como " .. cname .. "  jogando duro com o comercio.		" .. "NAO! PEGUE MINHAS BOMBAS! AMOSTRAS GRATIS!" pbold .. "		Ele da " .. chim .. " ' 25 bombas caseiras! Isso e uma quantidade enorme!" $) goodfx
		25 WEAPON_PIPEBOMB trunk.weapon+ cooled-it
	else cool-it++
		($ "SE ACALME! SE ACALME!!" pbold .. "		O clima fica bem tenso com o cara que vende explosivos, entao " .. thegroup .. " vai embora." $) badfx
	then +crtext attitudeREV witsREV
;
: charm-pipebomb
	most-charming if
		choosechar ($ "  O comerciante da " .. cname .. " ' uma bomba caseira extra em uma tentativa extremamente estranha de flertar ou ser amigavel. E dificil dizer com certeza." $) +cr +text 1 WEAPON_PIPEBOMB trunk.weapon+
	then
;
cyoa-choice: trade-pipebomb-20
	($ Thegroup .. "  compra 8 bombas caseiras." $) cyoa-text! basic-tone
	8 WEAPON_PIPEBOMB trunk.weapon+ -15 trunk-food+ charm-pipebomb someicon
;

cyoa-choice: trade-pipebomb-10
	($ Thegroup .. "  compra 5 bombas caseiras." $) cyoa-text! basic-tone
	5 WEAPON_PIPEBOMB trunk.weapon+ -10 trunk-food+ charm-pipebomb someicon
;

cyoa-choice: trade-pipebomb-5
	($ Thegroup .. "  compra 2 bombas caseiras." $) cyoa-text! basic-tone
	2 WEAPON_PIPEBOMB trunk.weapon+ -5 trunk-food+ charm-pipebomb someicon
;

cyoa: trade-pipebomb
	"Fornecedor de Pipebomb" cyoa-title!
	($ Thegroup .. " ' encontra um homem agitado vendendo bombas caseiras. Ele esta usando um casaco volumoso com carocos protuberantes por baixo.	" .. "COMPRE MINHAS BOMBAS!" pbold $) cyoa-text! basic-tone
	choice( "Va embora" )choice
	most-ahole if
		charchoice( "  rouba o comerciante" )choice: trade-pipebomb-rob
	then
	most-grating if 
		charchoice( " lhe diz para ATRASAR" fcoolit )choice: trade-pipebomb-coolit
	then
	trunk-food 5 < if
		($ "'Infelizmente, nao ha comida suficiente para troca, embora ele de uma pipebomb de graca!" $) +cr +text
		1 WEAPON_PIPEBOMB trunk.weapon+
	else
		trunk-food 14 > if
			choice( "8 pipebombs por 15 alimentos" )choice: trade-pipebomb-20
		then
		trunk-food 9 > if
			choice( "5 pipebombs por 10 alimentos" )choice: trade-pipebomb-10
		then
		trunk-food 4 > if
			choice( "2 pipebombs por 5 alimentos" )choice: trade-pipebomb-5
		then
	then
;

