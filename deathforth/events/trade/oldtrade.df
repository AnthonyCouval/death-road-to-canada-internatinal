cyoa: trade-army
	"Army Surplus Fort" cyoa-title!
	($ Thegroup .. " ' findet einen Armeeladen, der mit Barrikaden bedeckt ist. Er ist verschlossen. An einer Wand drauen, neben einem kleinen Fenster, hangt ein Schild mit Waffenpreisen.	" .. "Wirf das Essen einfach durch das Fenster!" pbold .. "  sagt eine kleine Stimme in der Dunkelheit." $) cyoa-text! basic-tone
	choice( "Abfahrt" )choice
	trunk-food 5 < if
		($ "'Es ist nicht genug Essen da um etwas von dem zu kaufen was sie verkaufen! " .. Thegroup .. "  muss gehen." $) +crtext badfx
	else
		trunk-food 74 > if
			choice( "Minigun fur 75 Lebensmittel" )choice: trade-army-7
		then
		trunk-food 59 > if
			choice( "Flammenwerfer fur 60 Lebensmittel" )choice: trade-army-6
		then
		trunk-food 39 > if
			choice( "12 Granaten fur 40 Lebensmittel" )choice: trade-army-5
		then
		trunk-food 24 > if
			choice( "Sturmgewehr fur 25 Lebensmittel" )choice: trade-army-4
		then
		trunk-food 14 > if
			choice( "Uzi fur 15 Lebensmittel" )choice: trade-army-3
		then
		trunk-food 11 > if
			choice( "3 Granaten fur 12 Lebensmittel" )choice: trade-army-2
		then
		trunk-food 3 > if
			choice( "1 Granate fur 4 Lebensmittel" )choice: trade-army-1
		then
	then
;

