	($ Thegroup .. "  troca 15 alimentos por uma variedade de municoes." $) cyoa-text! basic-tone
	food-15-to-allammo someicon
;

cyoa-choice: trade-allammo-all
	($ Thegroup .. "  troca " .. ghis .. "  restante de comida por alguma municao." $) cyoa-text! basic-tone
	food-all-to-allammo someicon
;

cyoa: trade-allammo
	"Deposito de Municao" cyoa-title!
	($ Thegroup .. "  encontra um comerciante rechonchudo em uma loja protegida cheia de municao. Ela esta cercada por guardas." .. "	BEM VINDO AO DEPOSITO DE MUNICAO! TUDO DEVE IR!" pbold .. "	Ela se oferece para dar 5 balas, 4 municoes de rifle e 3 cartuchos de espingarda em troca de cada item alimentar que receber." $) cyoa-text! basic-tone
	choice( "Va embora" )choice
	most-ahole if
		charchoice( "  rouba a loja" )choice: trade-allammo-rob
	then
	trunk-food 0 = if
		($ "'Infelizmente, nao ha comida suficiente para negociar." $) +cr +text
	else
		trunk-food 14 > if
			choice( "Troca 15 comidas" )choice: trade-allammo-15
		then
		trunk-food 9 > trunk-food and if
			choice( "Troca 10 comidas" )choice: trade-allammo-10
		then
		trunk-food 4 > if
			choice( "Negocie 5 alimentos" )choice: trade-allammo-5
		else	
			choice( "Negocie " .. trunk-food .. "  comida" )choice: trade-allammo-all
		then
	then
;

} }



