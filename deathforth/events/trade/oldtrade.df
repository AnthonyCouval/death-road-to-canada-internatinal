: trade-groupheal-guy
	cchar damage 0 = if
		($ cname .. " ' faz um check up, mas nao ha nada de errado com " .. chim .. "." $)
	else
		($ cname .. "  recebe tratamento para " .. chis .. "  ferimentos." $) 
	then cchar .fullheal
;

cyoa-choice: trade-groupheal-heal
	trade-groupheal-guy
	cyoa-text! goodfx
	-6 trunk-food+
;

cyoa-choice: trade-groupheal-group
	
	team-damage 0 = if
		($ Thegroup .. " ' faz um check up, mas nao ha nada de errado com ele." $) cyoa-text!
	else
		7 groupheal
		($ Thegroup .. "  recebe tratamento para " $) cyoa-text!
		team-damage 1 < if 
			"todos" +text 
		else
			"a maioria" +text
		then
			"  de seus ferimentos." +text
	then
	goodfx
	-15 trunk-food+
;

: trade-groupheal-give
	cyoa-start
	solo if
		soloer choosechar trade-groupheal-heal
	then;
	"Quem deve receber o tratamento?" cyoa-text!
	' trade-groupheal-heal cyoa-team-choice
;

cyoa-choice: trade-groupheal-charm 
	charmroll if
		($ cname .. "  convence a equipe do hospital a oferecer seus servicos gratuitamente!		" .. cHe .. "'' e tao charmoso." $) cyoa-text! goodfx
		trade-groupheal-guy +crtext
	else
		($ cname .. " acidental" .. gHe .. "  e convidado a deixar o hospital e nunca mais voltar." $) cyoa-text! badfx
	then
;

cyoa: trade-groupheal
	"Hospital Funcional" cyoa-title! someicon
	($ Thegroup .. " ' encontra um assentamento construido em um hospital. As luzes ainda estao acesas, alguns medicos ainda estao la e eles ainda estao abertos para negocios! Eles se oferecem para fornecer assistencia medica por comida. 10 alimentos para tratamento completo." $) cyoa-text! basic-tone
	trunk-food 6 < if
		($ "'Infelizmente, nao ha comida suficiente para seus servicos." $) +cr +text
	else
		choice( "De 6 alimentos para tratamento" )choice: trade-groupheal-give
		trunk-food 14 > notsolo and if
			choice( "Give 15 food for group care" )choice: trade-groupheal-group
		then
	then
		most-charming if
			charchoice( "  usa charme" )choice: trade-groupheal-charm
		then
		most-ahole if
			charchoice( "  rouba o hospital" )choice: trade-groupheal-rob
		then
		choice( "Va embora" )choice
;

