	food-10-to-allammo someicon
;

cyoa-choice: trade-allammo-15
	($ Thegroup .. "  intercambia 15 alimentos por una variedad de municiones." $) cyoa-text! basic-tone
	food-15-to-allammo someicon
;

cyoa-choice: trade-allammo-all
	($ Thegroup .. "  intercambia " .. ghis .. "  comida restante por algo de municion." $) cyoa-text! basic-tone
	food-all-to-allammo someicon
;

cyoa: trade-allammo
	"Emporio de municiones" cyoa-title!
	($ Thegroup .. "  encuentra a un comerciante regordete en una tienda tapiada llena de municiones. Esta rodeada de guardias." .. "bIENVENIDO AL EMPORIO DE MUNICIONES! TODO DEBE IRSE!" pbold .. "ella ofrece dar 5 balas, 4 municiones para rifle y 3 cartuchos de escopeta por cada alimento que recibe." $) cyoa-text! basic-tone
	choice( "Alejarse" )choice
	most-ahole if
		charchoice( "  roba la tienda" )choice: trade-allammo-rob
	then
	trunk-food 0 = if
		($ "'Lamentablemente, no queda suficiente comida para comerciar." $) +cr +text
	else
		trunk-food 14 > if
			choice( "Intercambie 15 alimentos" )choice: trade-allammo-15
		then
		trunk-food 9 > trunk-food and if
			choice( "Intercambie 10 alimentos" )choice: trade-allammo-10
		then
		trunk-food 4 > if
			choice( "Intercambie 5 alimentos" )choice: trade-allammo-5
		else	
			choice( "Intercambie " .. trunk-food .. "  comida" )choice: trade-allammo-all
		then
	then
;

} }



