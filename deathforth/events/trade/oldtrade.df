// Shotgun seller

cyoa-choice: trade-shotgun-coolit
	cpet? if
		($ cname .. " peida bem na frente do cara da espingarda." $)
	else
		($ cname .. " diz ao cara da espingarda para " .. "SE ACALMAR." pbold $)
	then cyoa-text!
	cool-it-7 if
		($ "Isso lhe causa algum tipo de crise emocional.		" .. "'Voce esta certo! O que estou fazendo com a minha vida?!" pbold .. "		Ele da " .. cname .. "  dele " $) +crtext goodfx
		50 %chance if "espingarda automatica e um monte de cartuchos!" 1 WEAPON_AUTOSHOT trunk.weapon+ else "espingarda de bala estriada e um monte de cartuchos!" 1 WEAPON_SLUG trunk.weapon+ then +text
		// used to also give a shotgun, a rifled slug shotgun, and the BIG DAWG. Too much. Busting down the value of mega coolit to about 40-50 food or so instead of like 150.
		50 trunk-shotgun-shell+ cooled-it
