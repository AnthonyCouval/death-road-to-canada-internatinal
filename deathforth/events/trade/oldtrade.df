		($ cname .. "  kommt versehentlich einfach nur gruselig ruber. 		" .. gHe .. "  wird gebeten das Krankenhaus zu verlassen und nie wieder zuruckzukehren." $) cyoa-text! badfx
	then
;

cyoa: trade-groupheal
	"Funktionales Krankenhaus" cyoa-title! someicon
	($ Thegroup .. " ' findet eine Siedlung, die in ein Krankenhaus eingebaut ist. Die Lichter brennen noch, ein paar Arzte sind noch da, und sie haben noch geoffnet! Sie bieten an, Gesundheitsversorgung gegen Essen anzubieten. 10 Essen fur eine vollstandige Behandlung." $) cyoa-text! basic-tone
	trunk-food 6 < if
		($ "'Leider gibt es nicht genug Essen fur ihre Dienste." $) +cr +text
	else
		choice( "Gib 6 Essen fur die Behandlung" )choice: trade-groupheal-give
		trunk-food 14 > notsolo and if
			choice( "Gib 15 Essen fur Gruppenpflege" )choice: trade-groupheal-group
		then
	then
		most-charming if
			charchoice( "  nutzt Charme" )choice: trade-groupheal-charm
		then
		most-ahole if
			charchoice( "  raubt das Krankenhaus aus" )choice: trade-groupheal-rob
		then
		choice( "Abfahrt" )choice
;

// All Ammo

cyoa-choice: trade-allammo-rob 
	60 %chance if
		($ cname .. "  raubt einen Teil des Munitionslagers aus, aber nicht ohne einen groen Kampf. " $) cyoa-text! bitefx
		notsolo if
			($ Theothersare .. "  hineingezogen." $) +text
		then
		50 60 rnd trunk-bullet+ 20 30 rnd trunk-shotgun-shell+ 30 40 rnd trunk-rifle-shell+ rob-fail
	else
		($ Thegroup .. "  erwies sich als eine wirklich schlechte Wahl. Die Wachen sind nicht glucklich." $) cyoa-text! bitefx
		rob-megafail
	then aholeREV
;

cyoa-choice: trade-allammo-5
	($ Thegroup .. "  handelt 5 Lebensmittel gegen ein bisschen Munition." $) cyoa-text! basic-tone
	food-5-to-allammo someicon
;

cyoa-choice: trade-allammo-10
	($ Thegroup .. "  handelt 10 Lebensmittel gegen einen Haufen Munition." $) cyoa-text! basic-tone
	food-10-to-allammo someicon
;

cyoa-choice: trade-allammo-15
	($ Thegroup .. "  handelt 15 Lebensmittel gegen eine Auswahl an Munition." $) cyoa-text! basic-tone
	food-15-to-allammo someicon
;

cyoa-choice: trade-allammo-all
	($ Thegroup .. "  tauscht " .. ghis .. "  verbleibende Lebensmittel gegen etwas Munition." $) cyoa-text! basic-tone
	food-all-to-allammo someicon
;

cyoa: trade-allammo
	"Munitionslager" cyoa-title!
	($ Thegroup .. "  findet einen wohlhabenden Handler in einem verbarrikadierten Laden voller Munition. Sie ist von Wachen umgeben." .. "	WILLKOMMEN IM MUNITIONSLAGER! ALLES MUSS RAUS!" pbold .. "	Sie bietet an, 5 Kugeln, 4 Gewehrmunition und 3 Schrotflintenpatronen im Austausch fur jeden Lebensmittelartikel zu geben, den sie bekommt." $) cyoa-text! basic-tone
	choice( "Abfahrt" )choice
	most-ahole if
		charchoice( "  raubt den Laden aus" )choice: trade-allammo-rob
	then
	trunk-food 0 = if
		($ "'Leider gibt es nicht genug Essen zum Handeln." $) +cr +text
	else
		trunk-food 14 > if
			choice( "Tausche 15 Lebensmittel" )choice: trade-allammo-15
		then
		trunk-food 9 > trunk-food and if
			choice( "Tausche 10 Lebensmittel" )choice: trade-allammo-10
		then
		trunk-food 4 > if
			choice( "Tausche 5 Lebensmittel" )choice: trade-allammo-5
		else	
			choice( "Tausche " .. trunk-food .. "  essen" )choice: trade-allammo-all
		then
	then
;

} }



