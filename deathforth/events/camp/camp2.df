// Zombie singalong

: camp2-sing-team args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		<- @ .morale++
	then
	<- @ .cattitude- if
		<- @ .morale--
	then	
;

cyoa-choice: camp2-sing-sing someicon
	($ Thegroup .. "  tenta manter o astral alto cantando. Se o canto era bom ou nao depende principalmente da sua atitude." $) cyoa-text! 3 700 tone 2 500 tone
		' camp2-sing-team foreach-team-char
;

cyoa-choice: camp2-sing-plan
	cwits- if
		($ "Acontece que " .. thegroup .. " ' realmente nao e tao bom em planejar as coisas." $) cyoa-text! morale--all ' .witsREV foreach-team-char badfx
	then;	
	cwits+ witsroll or if
		($ cname .. " ' planeja o dia seguinte. E um bom uso do tempo e ajuda " .. ghim .. "  foco no panorama geral." $) cyoa-text!
			morale++ goodfx
	else
		($ cname .. " ' faz planos elaborados para o dia seguinte. So de manha que " .. ghe .. "  percebe que os planos eram completamente ridiculos." $) cyoa-text!
			morale--all badfx
	then witsREV
;

cyoa: camp2-sing
	"Death Road Sing a Long" cyoa-title!
	($ Thegroup .. "  esta sentado ao redor de uma fogueira. Encontrar este acampamento foi dificil, entao " .. gtheyare .. "  ficando de mau humor." $) cyoa-text! +cr fullcamp
		choice( "Cante uma musica" )choice: camp2-sing-sing
		smartest charchoice( ": Planeje o amanha" )choice: camp2-sing-plan
