// Night watch for bandits, or rather thieves

cyoa-choice: camp2-bwatch-drive
	($ Thegroup .. " se refuse de camper dans une zone dangereuse et passe une nuit tendue et sans sommeil en conduisant a travers le noir... ." $) cyoa-text! morale--all +cr tiredcamp
	50 %chance if "La voiture derive de la route et heurte un arbre." +crtext  -2 car-chassis+ then
;
cyoa-choice: camp2-bwatch-pick
	($ cname .. " garde la veille durant la nuit." $) cyoa-text!
	cloyalty- if
		($ cHe .. " l'ordre ignore la tache et tombe rapidement endormi durant la nuit. le matin suivant, quelques approvisionnements manquent." $) +crtext sleep bandit-supplies2
	else
		($ cHe .. " se reveille avec peine et se sent mal dans le matin." $) +crtext
		sleep-except morale--
	then loyaltyREV
	+cr chowtime
;

cyoa-bridge: camp2-bwatch-pick4
	someicon
	"Quelqu'un devrait rester veiller." cyoa-text!
	' camp2-bwatch-pick cyoa-team-choice
;

cyoa: camp2-bwatch
	image-bandit-watch
	"Observation de bandit" cyoa-title! 
	($ "Il faudrait qu'une personne reste reveillee en cas d'attaque des voleurs. 	" $) cyoa-text! basic-tone
		team-count 3 > if
			' camp2-bwatch-pick 2 #cyoa-team-choice
			choice( "Choisissez quelqu'un d'autre." )choice: camp2-bwatch-pick4
		else
			' camp2-bwatch-pick 3 #cyoa-team-choice
		then
		choice( "Continue a conduire." )choice: camp2-bwatch-drive
;

// Night watch for zombies

cyoa-choice: camp2-zwatch-awake
	($ cname .. " se decide a s'endorser, pariant qu'il ne seront pas decouverts par les zombies. " .. chim .. "." $) cyoa-text!
	50 %chance if
		($ cHe .. " se reveille au milieu d'etre devore par un petit groupe de zombies. sans personne pour l'aider. " .. chim .. "c'est difficile de s'en liberer." $) +crtext despairbite
	else
		($ cHe .. " passe la nuit entiere sans interruption." $) +crtext
	then
	someone choosechar alive? if +cr fullcamp then
;
cyoa-choice: camp2-zwatch-shift
	someone choosechar
	($ Thegroup .. " s'alternent pour sommeil durant la nuit." $) text!
	team-count 4 < if 
		($ cname .. " est tres fatigue le matin. " .. theothers .. " pas dormi tres bien non plus." $) +crtext sleep-except 5050nosleep morale--all +cr chowtime
	else
		($ "Dans un groupe complet, il y a assez de personnes pour encore avoir beaucoup de repos !" $) +crtext +cr fullcamp
	then
;
cyoa-choice: camp2-zwatch-pick
	($ cname .. " veille a la recherche de zombies errants." $) cyoa-text!
	cloyalty- if
		cchar statmod{ .soundsleep? } if
			($ cHe .. " il perd la concentration et s'eloigne en plein milieu de garder la garde. " .. gHe .. " est attaque par des zombies, mais parvient a les combattre." $) +crtext
			cchar ' .sting-- foreach-team-char-except
		else
			($ cHe .. " tombe a dormir presque immediatement. " .. gHe .. " se reveille au milieu d'etre devore." $) +crtext ouch--all
		then sleep
	else
		($ cHe .. " il parvient a rester eveille, mais se sent lourd le matin." $)
		+crtext sleep-except morale--
	then loyaltyREV
	someone choosechar alive? if +cr chowtime then
;

cyoa-bridge: camp2-zwatch-pick4
	someicon
	"Quelqu'un devrait rester veiller." cyoa-text!
	' camp2-zwatch-pick cyoa-team-choice
;

cyoa: camp2-zwatch
	image-zombie-watch
	"Observation de zombies" cyoa-title! 
	($ "Dans la confusion de la nuit, il n'y a pas de bon endroit pour camper. des zombis seuls ralentissent lointainement, et un d'eux peut nous surprendre. " .. thegroup .. " ecrasons-le! " .. gthey .. " dormir" .. g1s .. "quelqu'un doit rester debout pour veiller." $) cyoa-text! basic-tone
		team-count 3 > if
			' camp2-zwatch-pick 2 #cyoa-team-choice
			choice( "Choisissez quelqu'un d'autre." )choice: camp2-zwatch-pick4
		else
			' camp2-zwatch-pick 3 #cyoa-team-choice
		then		
		solo if
			choice( "Dormez quand meme." )choice: camp2-zwatch-awake
		else
			choice( "Prendre des tours de garde" )choice: camp2-zwatch-shift
		then
;

