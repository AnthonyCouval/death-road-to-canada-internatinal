cyoa{ road{

// Minigolf, no choices

cyoa: camp2-minigolf
	image-golf-course
	"Mini Golfe!" cyoa-title!
	($ Thegroup .. "  monta acampamento no lugar mais seguro que conseguiu encontrar: um parque de minigolfe abandonado. " $) cyoa-text! basic-tone
	+cr fullcamp
	($ "De manha, " .. gthey .. "  encontrar" .. g1s .. "  uma bola de golfe e um taco enquanto procurava pela area. " .. Thegroup .. "  decide jogar uma partida rapida!" $) +crtext
	unfittest choosechar cfitness- if
		cpet? if
			($ cname .. "  corre por ai animado!" $) +crtext morale++
		else
			($ cname .. "  faz alguns exercicios necessarios!" $) +crtext fitness++
		then	
	then
	most-ahole if
		choosechar ($ cname .. "  se cansa do jogo e entao perde a bola de proposito." $) +crtext morale--all
	then;
	strongest-pass if
		cpet? if
		 	($ cname .. "  acidentalmente morde a bola." $) +crtext morale++ 1 WEAPON_GOLF trunk.weapon+
		then;
		strongest choosechar 50 %chance if
			($ cname .. "  acidentalmente destroi o taco." $) +crtext morale++
		else
			($ cname .. "  acidentalmente bate na bola tao longe que ela se perde." $) +crtext fitness++ 1 WEAPON_GOLF trunk.weapon+
		then
	then;
	morale++all 1 WEAPON_GOLF trunk.weapon+
;

// Driving range with different choices

// previously they find a less dangerous and less fun place to camp - fullcamp
cyoa-choice: camp2-fore-flee
	($ Thegroup .. "  decide fugir do campo de golfe. " .. gThey .. "  acabam nao encontrando outro bom lugar para dormir e dirigem durante a maior parte da noite. " .. gThey .. "  ficam muito cansados no dia seguinte." $) cyoa-text! +cr tiredcamp
;
cyoa-choice: camp2-fore-fore
	solo if
		soloer choosechar
		cpet? if
			($ cname .. " ' tenta jogar golfe, mas e dificil fazer isso como um " .. cdogcat .. ". " .. cHe .. " maybe could have done it with a caddy.
			" .. cname .. "  esta completamente sobrecarregado." $) cyoa-text! morale3- ouch-- sting-- bitefx 
		else
			($ cname .. " 'acerta varias bolas de golfe em cabecas de zumbis. E um bom exercicio, mas logo " .. che .. " is overwhelmed by them.
			" .. cHe .. "  escapa, mas passa a noite escondido em um armario. " .. cHe .. "  esta exausto pela manha." $) cyoa-text! fitness++ fitnessREV morale-- sting-- bitefx  
		then someone choosechar alive? if +cr tiredcamp then
	then;
	($ Thegroup .. "  pega varios tacos e alguns baldes de bolas de golfe. Eles comecam a dar tacadas no campo enquanto os zumbis se aproximam." $) cyoa-text! 
	fitness-total 7 > if
		"Eles enviam bola de golfe apos bola de golfe para a horda que se aproxima." +crtext
		strength-total 7 > if
			"A equipe acerta os zumbis do outro lado do campo. Eles nem chegam perto!" +crtext goodfx morale++all
		else
			"'Os zumbis sao atingidos por bolas de golfe, mas nao sao atingidos com forca suficiente para serem desacelerados muito. A equipe acaba lutando contra eles normalmente." +crtext someone .sting-- someone .sting-- bitefx
		then
		unfittest .fitness++ ' .fitnessREV foreach-team-char 
		' .strengthREV foreach-team-char +cr fullcamp
	else
		($ "'E um trabalho arduo, e a equipe acaba se cansando. " .. gThey .. "  sao dominados pelos zumbis." $) +crtext ouch--all ' .fitnessREV foreach-team-char +cr choosehuman alive? if fullcamp then bitefx
	then
	1 WEAPON_GOLF trunk.weapon+
;
cyoa-choice: camp2-fore-mech
	($ cname .. " ' pula em um coletor de bolas de golfe e comeca a atropelar zumbis. E surpreendentemente eficaz. Quando o coletor quebra, a maioria dos zumbis e eliminada." $) cyoa-text! morale++all mechanicREV mechanic++ +cr fullcamp
;
cyoa-bridge: camp2-fore-fight
	($ "'Os zumbis comecam a bater na porta da loja de golfe. Nao ha muito tempo para se preparar antes que a porta quebre!" $) cyoa-text!
		2.4 20 22 rnd 1 siegecast // Formerly 1.3 then 1.7 diff
		"events/siege/region/golfcamp.df" $load-region
		choice( "Tente Sobreviver" )choice: premission
;

cyoa: camp2-fore
	image-golf-course
	"Ultima Resistencia no Campo de Golfe" cyoa-title! 
	choosehuman
	($ Thegroup .. " acampa em uma loja de artigos esportivos perto de um campo de golfe. " .. cname-He .. " 	Olha pela janela e ve um grande grupo de zumbis se aproximando do campo de golfe!" $) cyoa-text! basic-tone 3 WEAPON_GOLF trunk.weapon+
	mechanicest-pass if
		mechanicest charchoice( " liga um coletor de bolas de golfe" )choice: camp2-fore-mech
	then
		choice( "Atire bolas de golfe nos zumbis" )choice: camp2-fore-fore
		choice( "Prepare se para o Cerco!" )choice: camp2-fore-fight
		choice( "Dirija sem dormir" )choice: camp2-fore-flee
;

