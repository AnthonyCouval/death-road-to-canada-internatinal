
cyoa{ mission{

cyoa: camp2-fightdone
	($ "La pire partie de siege des zombies est terminee. " .. Thegroup .. " elimine toute la horde de zombis restante a l'exterieur avant de retourner pour ecraser les survivants interieurs. " .. gthey .. " vous pouvez etablir un campement." $) cyoa-text! morale++all +cr road{ fullcamp }
;

} }

cyoa{ road{

// Minigolf, no choices

cyoa: camp2-minigolf
	image-golf-course
	"Mini golf : jeu de golf en mineur." cyoa-title!
	($ Thegroup .. " detablissez le camp au endroit le plus sur que vous pouvez trouver autour : un parc mini-golf abandonne. " $) cyoa-text! basic-tone
	+cr fullcamp
	($ "Dans le matin, " .. gthey .. " trouver" .. g1s .. " une balle de golf et une jumelle pendant que vous explorez l'aire. " .. Thegroup .. " joue une petite partie !" $) +crtext
	unfittest choosechar cfitness- if
		cpet? if
			($ cname .. " court autour en s'excitant !" $) +crtext morale++
		else
			($ cname .. " faites quelque exercice necessaire !" $) +crtext fitness++
		then	
	then
	most-ahole if
		choosechar ($ cname .. " perd intentionnellement la balle lorsqu'il en a assez du jeu." $) +crtext morale--all
	then;
	strongest-pass if
		cpet? if
		 	($ cname .. " mord accidentellement le ballon." $) +crtext morale++ 1 WEAPON_GOLF trunk.weapon+
		then;
		strongest choosechar 50 %chance if
			($ cname .. " l'accident fait que le putter est detruit." $) +crtext morale++
		else
			($ cname .. " la balle est perdue par inadvertance apres qu'elle ait ete frappee a une grande distance." $) +crtext fitness++ 1 WEAPON_GOLF trunk.weapon+
		then
	then;
	morale++all 1 WEAPON_GOLF trunk.weapon+
;

// Driving range with different choices

// previously they find a less dangerous and less fun place to camp - fullcamp
cyoa-choice: camp2-fore-flee
	($ Thegroup .. " s'en va fuir du terrain de golf. " .. gThey .. " n'arrivant plus a trouver une bonne place pour dormir supplementairement, et conduire pendant la majorite de la nuit. " .. gThey .. " nous sommes tres fatigues le lendemain." $) cyoa-text! +cr tiredcamp
;
cyoa-choice: camp2-fore-fore
	solo if
		soloer choosechar
		cpet? if
			($ cname .. " essayer de jouer au golf, mais cela est difficile a faire cela " .. cdogcat .. ". " .. cHe .. " maybe could have done it with a caddy.
			" .. cname .. " est totalement deborde" $) cyoa-text! morale3- ouch-- sting-- bitefx 
		else
			($ cname .. " joue au golf en frappant des boules dans les tetes de zombies. c'est une bonne exercice, mais bientot " .. che .. " is overwhelmed by them.
			" .. cHe .. " s'echappe, mais passe la nuit cache dans un placard. " .. cHe .. " est epuise" $) cyoa-text! fitness++ fitnessREV morale-- sting-- bitefx  
		then someone choosechar alive? if +cr tiredcamp then
	then;
	($ Thegroup .. " prend des clubettes et ramasse des sacs a balles de golf. commencent a frapper des coups vers la plage de pratique alors que les zombies s'approchent." $) cyoa-text! 
	fitness-total 7 > if
		"Ils lancent des boules de golf apres les unes sur les autres dans l'horde envenimee." +crtext
		strength-total 7 > if
			"La equipe elimine les zombies a partir d'une portee. ils n'ont jamais approche !" +crtext goodfx morale++all
		else
			"Les zombies sont frappes de boules de golf, mais ca ne les ralentit pas beaucoup. l'equipe combat donc normalement ces derniers." +crtext someone .sting-- someone .sting-- bitefx
		then
		unfittest .fitness++ ' .fitnessREV foreach-team-char 
		' .strengthREV foreach-team-char +cr fullcamp
	else
		($ "Travailler durcie et l'equipe s'epuise principalement eux-memes. " .. gThey .. " sont envahis par les zombies." $) +crtext ouch--all ' .fitnessREV foreach-team-char +cr choosehuman alive? if fullcamp then bitefx
	then
	1 WEAPON_GOLF trunk.weapon+
;
cyoa-choice: camp2-fore-mech
	($ cname .. " saute dans un recolteur de boules de golf et commence a les ecraser sur des zombies... cela fonctionne etrangement bien. au moment ou le recolteur cede, la plupart des zombies ont ete neutralises." $) cyoa-text! morale++all mechanicREV mechanic++ +cr fullcamp
;
cyoa-bridge: camp2-fore-fight
	($ "Les zombies commencent a frapper a la porte du magasin de golf. il n'y a plus beaucoup de temps pour se preparer avant que la porte ne se brise !" $) cyoa-text!
		2.4 20 22 rnd 1 siegecast // Formerly 1.3 then 1.7 diff
		"events/siege/region/golfcamp.df" $load-region
		choice( "Tente de survivre." )choice: premission
;

cyoa: camp2-fore
	image-golf-course
	"Stand de tir a balles : derniere resistance" cyoa-title! 
	choosehuman
	($ Thegroup .. " s'installe en raffinerie pres d'un parc de golf. " .. cname-He .. " voici une grande horde de zombies qui approchent de la terrasse de tir !" $) cyoa-text! basic-tone 3 WEAPON_GOLF trunk.weapon+
	mechanicest-pass if
		mechanicest charchoice( " debrancher le fil de soudage pour brancher les filaments du collecteur de golf balls." )choice: camp2-fore-mech
	then
		choice( "Tirez des boules de golf sur les zombies." )choice: camp2-fore-fore
		choice( "Preparez-vous au siege !" )choice: camp2-fore-fight
		choice( "Fuite sans sommeil : conduisez loin de vos yeux fermer." )choice: camp2-fore-flee
;

