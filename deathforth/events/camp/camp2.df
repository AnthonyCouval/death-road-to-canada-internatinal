// Night watch for bandits, or rather thieves

cyoa-choice: camp2-bwatch-drive
	($ Thegroup .. "  se recusa a acampar em uma area perigosa e passa uma noite tensa e sem dormir dirigindo no escuro." $) cyoa-text! morale--all +cr tiredcamp
	50 %chance if "O carro sai da estrada e bate em uma arvore." +crtext  -2 car-chassis+ then
;
cyoa-choice: camp2-bwatch-pick
	($ cname .. "  fica de guarda durante a noite." $) cyoa-text!
	cloyalty- if
		($ cHe .. "  ignora a tarefa e adormece quase instantaneamente. Na manha seguinte, varios suprimentos estao faltando." $) +crtext sleep bandit-supplies2
	else
		($ cHe .. "  consegue ficar acordado, mas se sente muito mal pela manha." $) +crtext
		sleep-except morale--
	then loyaltyREV
	+cr chowtime
;

cyoa-bridge: camp2-bwatch-pick4
	someicon
	"Portugues" cyoa-text!
	' camp2-bwatch-pick cyoa-team-choice
;

cyoa: camp2-bwatch
	image-bandit-watch
	"Vigilancia de Bandidos" cyoa-title! 
	($ "'Esta se aproximando do tempo de acampar, mas houve muitas sinais de bandidos hoje. Alguem precisa ficar acordado em caso de um ataque.	Quem deve ficar de guarda?" $) cyoa-text! basic-tone
		team-count 3 > if
			' camp2-bwatch-pick 2 #cyoa-team-choice
			choice( "Escolha outra pessoa" )choice: camp2-bwatch-pick4
		else
			' camp2-bwatch-pick 3 #cyoa-team-choice
		then
		choice( "Continue dirigindo" )choice: camp2-bwatch-drive
;

// Night watch for zombies

cyoa-choice: camp2-zwatch-awake
	($ cname .. " 'escolhe dormir, apostando que os zumbis nao o encontrarao " .. chim .. "." $) cyoa-text!
	50 %chance if
		($ cHe .. " acorda no meio de ser engolido por um pequeno grupo de zumbis. Sem ninguem para ajuda lo " .. chim .. "', e dificil se soltar." $) +crtext despairbite
	else
		($ cHe .. " dorme durante a noite sem interrupcao." $) +crtext
	then
	someone choosechar alive? if +cr fullcamp then
;
cyoa-choice: camp2-zwatch-shift
	someone choosechar
	($ Thegroup .. " toma turnos dormindo durante a noite." $) text!
	team-count 4 < if 
		($ cname .. " esta muito cansado na manha, e " .. theothers .. " 'nao dormiu bem tambem." $) +crtext sleep-except 5050nosleep morale--all +cr chowtime
	else
		($ "'Com um grupo completo, ha suficientes pessoas para ainda obter muita dormida!" $) +crtext +cr fullcamp
	then
;
cyoa-choice: camp2-zwatch-pick
	($ cname .. " mantem de vigilia contra zumbis errantes." $) cyoa-text!
	cloyalty- if
		cchar statmod{ .soundsleep? } if
			($ cHe .. " deixa de se concentrar e se desvia no meio do cumprimento da guarda. " .. gHe .. "  e emboscado por zumbis, mas consegue lutar contra eles." $) +crtext
			cchar ' .sting-- foreach-team-char-except
		else
			($ cHe .. "  adormece quase que instantaneamente " .. gHe .. "  acorda no meio de ser comido." $) +crtext ouch--all
		then sleep
	else
		($ cHe .. "  consegue ficar acordado, mas se sente grogue pela manha." $)
		+crtext sleep-except morale--
	then loyaltyREV
	someone choosechar alive? if +cr chowtime then
;

cyoa-bridge: camp2-zwatch-pick4
	someicon
	"Portugues" cyoa-text!
	' camp2-zwatch-pick cyoa-team-choice
;

cyoa: camp2-zwatch
	image-zombie-watch
	"Vigia Zumbi" cyoa-title! 
	($ "'Esta ficando tarde e nao ha um bom lugar para acampar. Zumbis solitarios mancam ao redor a distancia, e um pode se aproximar furtivamente de " .. thegroup .. " enquanto " .. gthey .. "  dormir" .. g1s .. ". Quem deve ficar acordado de guarda?" $) cyoa-text! basic-tone
		team-count 3 > if
			' camp2-zwatch-pick 2 #cyoa-team-choice
			choice( "Escolha outra pessoa" )choice: camp2-zwatch-pick4
		else
			' camp2-zwatch-pick 3 #cyoa-team-choice
		then		
		solo if
			choice( "Durma mesmo assim" )choice: camp2-zwatch-awake
		else
			choice( "Fazer turnos" )choice: camp2-zwatch-shift
		then
;

