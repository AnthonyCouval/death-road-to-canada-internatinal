// Bad Weather camping

: camp2-badweather-check args( @ )
	<- @ oblivious? if
		($ <- @ .colourname .. " le temps etait agreable !" $) +crtext 
		<- @ choosechar morale++ witsattREV
	then;
	<- @ .cattitude+ if 
		($ <- @ .colourname .. " cela m'a pas derange beaucoup." $) +crtext 
		<- @ .attitudeREV
	then;
	<- @ .morale--
;

cyoa: camp2-badweather
	image-camp-2
	"Camping deplaisant" cyoa-title!
	($ Thegroup .. " n'ai pu trouver de abris surs, mais j'ai reussi a mettre en place un camping. la nuit est horrible toute cette nuit !" $) text! ' camp2-badweather-check foreach-team-char +cr fullcamp
	0.4 1 frnd weather! -1 1 frnd wind_force!
;


// Woodstove camping

: camp2-wood-fire args( @ )
	<- @ .cattitude- if <- @ .attitudeREV then;
	<- @ .morale++
;

cyoa-choice: camp2-wood-chopped
	($ cname .. " couper beaucoup de bois et mettre en marche le four a bois avant d'epuisement... ." .. cHe .. " demain je serai tres fatigue !" $) text!
	strengthREV strength++ cchar tired goodfx
	"La flamme est agreable !" +crtext
	' camp2-wood-fire foreach-team-char
;

cyoa: camp2-wood-chop
	solohuman if
		choosehuman camp2-wood-chopped
	then;
	"Quelqu'un devrait aller couper la bois. cela est reellement du travail intense !" cyoa-text!
	' camp2-wood-chopped cyoa-human-choice
;

cyoa: camp2-wood
	image-cabin-in-woods
	"Boite de cuir avec poele a bois" cyoa-title!
	($ Thegroup .. " cherche une cabane dans la foret, sans aucun zombie en vue. elle dispose d'une cheminee a bois dedans, mais pas de bois !" $) text! +cr fullcamp
		choice( "Sortez et coupez du bois" )choice: camp2-wood-chop
		choice( "Alors, va dormir." )choice
;

// Creepy inn woman

: camp2-airbnb-eat args( @ )
	<- @ paranoid? if
		($ <- @ .colourname .. " il ne veut pas en manger !" $) +crtext <- @ .morale-- 
		<- @ .witsREV <- @ .attitudeREV
	then;
	<- @ .morale2+
;

: camp2-airbnb-breakfast
	($ "L'aubergiste sert une delicieuse petit dejeuner !" $) +crtext
	' camp2-airbnb-eat foreach-team-char goodfx	sleep
;

cyoa-choice: camp2-airbnb-pay
	($ Thegroup .. " paiement d'un sejour a l'auberge pendant une nuit. elle est confortable..." .. "No one wants zombies creepin while they're sleepin hhhHHHHHHeheh." rbold $) text! -10 trunk-food+
	camp2-airbnb-breakfast
;

cyoa-choice: camp2-airbnb-charm
	($ cname .. " obtenir une meilleure affaire pour la nuit..." .. "You're a sly one hhhhhhhhhhHHHHHHeheh." rbold $) text! -5 trunk-food+
	camp2-airbnb-breakfast
;

: camp2-airbnb-pres
		($ "...hhhhhhhhhhHHHHHHeheh you must be a big shot.
		I'll show you the presidential suite." rbold $)
		+crtext
;

cyoa-choice: camp2-airbnb-coolit
	cpet? if
		($ cname .. " fait une instruction " .. "hhhHHHHHeh" rbold .. " son." $)
	else
		($ cname .. " 'assurez-vous de faire ceci pour la femme :' " .. "DIT PAS SPRAYEZ PAS." pbold $)
	then text!
	cool-it-7 if
		camp2-airbnb-pres
		"Elle montre la suite presidentielle ! elle dispose d'un spa !" +crtext
		morale3+all 
