// Bad Weather camping

: camp2-badweather-check args( @ )
	<- @ oblivious? if
		($ <- @ .colourname .. "  achei que o clima estava bom!" $) +crtext 
		<- @ choosechar morale++ witsattREV
	then;
	<- @ .cattitude+ if 
		($ <- @ .colourname .. " ' nao se importou muito com isso." $) +crtext 
		<- @ .attitudeREV
	then;
	<- @ .morale--
;

cyoa: camp2-badweather
	image-camp-2
	"Camping Miseravel" cyoa-title!
	($ Thegroup .. " ' nao conseguiu encontrar nenhum abrigo seguro, mas conseguiu improvisar um acampamento. O tempo esta terrivel a noite toda!" $) text! ' camp2-badweather-check foreach-team-char +cr fullcamp
	0.4 1 frnd weather! -1 1 frnd wind_force!
;


// Woodstove camping

: camp2-wood-fire args( @ )
	<- @ .cattitude- if <- @ .attitudeREV then;
	<- @ .morale++
;

cyoa-choice: camp2-wood-chopped
	($ cname .. "  corta um monte de lenha e acende o fogao a lenha antes de desmaiar de exaustao.	" .. cHe .. "  ficara muito cansado amanha!" $) text!
	strengthREV strength++ cchar tired goodfx
	"O fogo esta otimo!" +crtext
	' camp2-wood-fire foreach-team-char
;

cyoa: camp2-wood-chop
	solohuman if
		choosehuman camp2-wood-chopped
	then;
	"'Quem deveria ir cortar a lenha? E um trabalho muito duro!" cyoa-text!
	' camp2-wood-chopped cyoa-human-choice
