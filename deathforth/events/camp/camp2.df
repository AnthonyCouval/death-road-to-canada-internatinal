		($ "Elle donne un coup de pied. " .. thegroup .. " trouver " .. gThey .. " gaz" .. g1s .. " sur la route, mais jamais la trouverez" .. g1s .. " un endroit sur pour dormir." $) +crtext badfx +cr tiredcamp
	then witsattREV 
;

cyoa-choice: camp2-airbnb-nosleep
	($ Thegroup .. " s'en retire rapidement de la. " .. gThey .. " gaz" .. g1s .. " sur la route, mais jamais la trouverez" .. g1s .. " un endroit sur pour dormir." $) text! +cr tiredcamp
;

cyoa: camp2-airbnb
	image-old-house
	"Auberge et dejeuner" cyoa-title!
	($ Thegroup .. " recherchez un auberge ouverte pour affaires! la proprietaire froisse ses mains: . . ." .. "Give me 10 food, I'll watch over you while you sleep hhhHHHHHeh." rbold .. "elle est tres effrayante." $) text!
		trunk-food 9 > if	
			choice( "Paiement de 10 alimentations" )choice: camp2-airbnb-pay
		then
		most-grating if	
			charchoice( ": DIRE PAS L'ARROSAGE" fcoolit )choice: camp2-airbnb-coolit
		then
		most-charming trunk-food 5 > and if
			charchoice( " negocie sur le prix jusqu'a en obtenir cinq alimentations." )choice: camp2-airbnb-charm
		then
		choice( "Conduise sans sommeil" )choice: camp2-airbnb-nosleep
;

// Zombie singalong

: camp2-sing-team args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		<- @ .morale++
	then
	<- @ .cattitude- if
		<- @ .morale--
	then	
;

cyoa-choice: camp2-sing-sing someicon
	($ Thegroup .. " essaye de tenir les esprits en haut en chantant. la qualite du chant depend essentiellement de ton etat d'esprit." $) cyoa-text! 3 700 tone 2 500 tone
		' camp2-sing-team foreach-team-char
;

cyoa-choice: camp2-sing-plan
	cwits- if
		($ "C'est venu a la lumiere que " .. thegroup .. " pas tres bon organisateur en fait." $) cyoa-text! morale--all ' .witsREV foreach-team-char badfx
	then;	
	cwits+ witsroll or if
		($ cname .. " prepare le lendemain. c'est une bonne utilisation du temps, et cela aide. " .. ghim .. " focaliser sur le tableau d'ensemble." $) cyoa-text!
			morale++ goodfx
	else
		($ cname .. " elabore des plans detailles pour le lendemain. cela ne sera reellement clair qu'au matin. " .. ghe .. " comprend que les plans etaient totalement brouilles." $) cyoa-text!
			morale--all badfx
	then witsREV
;

cyoa: camp2-sing
	"Rendez-vous sur la route fatale : chanson a chansons" cyoa-title!
	($ Thegroup .. " il est assis autour d'un feu de camp. la decouverte de ce camping etait difficile. " .. gtheyare .. " se mettre en mauvais humeur." $) cyoa-text! +cr fullcamp
		choice( "Chante une chanson." )choice: camp2-sing-sing
		smartest charchoice( ": preparer demain" )choice: camp2-sing-plan
;
		
