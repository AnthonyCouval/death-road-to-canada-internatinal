
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " recupere les twinkies qui n'etaient pas tous ecrases et saccages par des fourmis apres que le camion a renverse. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. " il commence a travailler sur le verrou. " $) cyoa-text!
		cmechanic+ if
			($ "C'est tout a fait possible pour moi. " .. chim .. " ouvrez-le pour le faire fonctionner." $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Cela prend un peu de temps, mais le verrou finit par s'ouvrir." +text common1-twinkie-get mechanic++
		else
			($ cHe .. " il ne parvient pas a avancer et se rend compte qu'il est incapable de continuer." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. " mord a la serrure. " $) 
	else
		($ cname .. " trouve une grande pierre et frappe durement sur le verrou. " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. " La serrure se brise. " $) +text common1-twinkie-get
		then;
		strengthroll if
			"La serrure cede !" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. " gache les choses " .. chis .. " les dents contre l'acier dur!" $)
			else
				($ cHe .. " finit par ecraser " .. chis .. " leve les mains mal avec peine au lieu d'ouvrir le camion !" $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. " visadelle verrou en arriere du camion. " $) cyoa-text!
		cshooting+ if
			"Une photo parfaite ! le relache s'ouvre. " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. " parvient a faire le tir, detruisant le verrou !" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "TRES INFLAMMABLE" rbold .. " aller interieurement. le camion est devore par une immense explosion !" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Camion de twinkies en route" cyoa-title!
	($ "Il y a une camionnette twinkie sur la route. elle est renversee et la porte arriere est verrouillee etanchement. " .. Thegroup .. " salue abondamment le gateau de sirop de mais traite qui doit se trouver dedans." $) cyoa-text!
		choice( "Ignorer pour des raisons de sante" )choice
		mechanicest charchoice( " inserer l'effet de teleportation ici" )choice: common1-twinkie-mech
		strongest charchoice( " il le brise avec quelque chose lourd" )choice: common1-twinkie-smash
		shootest charchoice( " fonce sur le verrou" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
	($ Thegroup .. " n'en sait pas beaucoup concernant la conservation de jeu. " .. gthey .. " preparer maladroitivement" .. g1s .. " des viandes a emballer dans la voiture." $) +cr +text 8 12 rnd trunk-food+ goodfx
;

cyoa-choice: common1-deer-leave
	($ "Le cerf s'enfuit rapidement. la rencontre silencieuse le fait fuir. " .. thegroup .. "mon dieu !" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. " accelere brutalement et percute le cerf avec la voiture." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. " prend une potache au cerf" $) cyoa-text!
	vehicle.which@ 0 > if " depuis une des fenetres d'une voiture. " else ". " then +text
		shooting+roll if
			($ cHe .. " fait le tir" $) +crtext 
			vehicle.which@ 0 > if 
				"capturer une faon en depit de toute ethique de chasseur."
			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. " rate totalement rate." $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Passage de mouflons" cyoa-title!
	($ "Un cerf s'est arrete au milieu de la route. " $) cyoa-text!
	vehicle.which@ 0 > if "Il regarde la voiture." else ($ "C'elabore sur toi. " .. thegroup .. "." $) then +text
		choice( "Laissez-le tranquille" )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Courir sur une faon" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( " essaye de le tirer." )choice: common1-deer-shoot
		then
;

// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. " decide " .. che .. " il n'a pas besoin de points de couture et il est preferable de ne rien faire face au probleme. " .. cHe .. " sa blessure saigne partout et son bras ne guerit jamais completement." $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. " essaie de le suturer " .. vname .. "musee" $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Sans approvisionnement en matiere de fournitures medicales, " .. cname .. " faites le meilleur. " .. che .. " il y a des problemes plus tard avec l'infection." $) +crtext victim .morale--
			else
				($ cHe .. " fait des pointes sur le bras avec un minimum de douleur." $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. " la blessure se termine par envenimer les points de couture et aggraver la plaie. l'hemorragie s'arrete, mais " .. vname .. "son bras guerit mal." $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. " imite quelque chose " .. che .. " j'ai vu la scene dans le film 'rambeaux'. " .. cHe .. " idee maladroite : chauffer un couteau jusqu'a ce qu'il soit reellement chaud et l'appliquer sur la blessure pour essayer de la coaguler... cela etait tres mal! bien que le bourrissage soit efficace et arrete la saignee, la douleur est extreme." $) cyoa-text! badfx morale3-
;

cyoa: common1-stitches
	image-arm-gash
	"Coupe profonde" cyoa-title!
	choosehuman cicon
	cchar to victim
	($ cname .. " coupe " .. chim .. "sois tres mauvais quand on se detend dans l'apres-apocalypses. " .. cHe .. " aura probablement besoin de sutures " .. chis .. " armez-vous." $) cyoa-text!
		choice( "Ignore it / Walk it off" )choice: common1-stitches-no
		medicalest charchoice( " essaie de la reparer en la collant ensemble." )choice: common1-stitches-med
		cchar charchoice( " guerit ce lui par une cauterisation." )choice: common1-stitches-cauter
;

// Lonely Gas Station

: common1-gas-get 
	($ Thegroup .. " recueillez cette precieuse gaz avec soin. " $) +crtext 70 110 rnd trunk-gas+ goodfx
;

cyoa-choice: common1-gas-mech
	($ cname .. " essaie de reparer les pompes. " $) cyoa-text!
		cmechanic+ if
			($ cHe .. " mettront le rythme en few minutes." $) +text common1-gas-get
		then;
		mechanicroll if
			($ "C'est un peu genant, mais " .. che .. " active les pompes." $) +text common1-gas-get mechanic++
		else
			($ cHe .. " essaie, mais ne parvient pas avec eux." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-gas-climb
	($ cname .. " instructions : monter dans les reservoirs. " $) cyoa-text!
		cpet? if
			($ cHe .. " s'y retrouve bloque et cela prend du temps pour en sortir. " .. cname .. " emerge totalement imbibe de gaz." $) +text
			morale2- sting-- 
			notsolo if
				($ notcname .. " s'enroule " .. chim .. " sortez et ramassez le gaz !" $)
				+crtext 70 110 rnd trunk-gas+ goodfx
			then
		then;
		fitnessroll if
			($ "Il y a beaucoup de peine et de malchance involvees. " .. che .. " se fait echouer dans le bassin." $) +text common1-gas-get fitness++
		else
			($ cHe .. " montre que la montee dans le reservoir est totalement epuisante. " .. cname .. " montrant son corps blesse, il entre dans le reservoir, mais se blesse en y entrant. sortir en est une autre affaire." $) +text
			badfx morale-- ouch--
		then fitnessREV
;

cyoa: common1-gas
	image-lonely-gas
	"Gare service isolee" cyoa-title!
	($ Thegroup .. " une station-service eloignee de la route principale. quelqu'un a vole le magasin, pas de zombis en vue. ... les pompes a essence sont anciennes et ne semblent pas fonctionner." $) cyoa-text!
		choice( "Laissez les pompes tranquilles" )choice
		mechanicest charchoice( " essaie de reparer les pompes." )choice: common1-gas-mech
		dumbest charchoice( " se fait introduire dans le reservoir." )choice: common1-gas-climb
;

// Jon's tooth pain event and revamped version of his radio event

: cardJ4-mcheck 
	medicalroll if
		($ cname .. " ils avaient reellement la maitrise de leurs taches. en utilisant les outils et les fournitures medicales a leur disposition, ils ont pu faire preuve d'une grande efficacite. " .. che .. " puisable de sortir nettement et avec un peu de douleur relatif la dents. " $) cyoa-text! goodfx
		medical++
	then;
	
		($ cname .. " eut des ennuis a grande echelle. l'extraction dentaire s'est averee geneuse, maladroite et douloureuse. pour aggraver les choses, " .. che .. " a eu mauvais raison de tirer la mauvaise dent et j'en ai du refaire une nouvelle. " $) cyoa-text! bitefx ouch-- morale3-
		
	dead? if
		+cr ($ "Apres avoir tire la deuxieme dent. " .. cname .. " a dit : a ete entendu hurler " .. "C'EST LA MERDE, JE VAIS PRENDRE CELA A LA GUEULE." pbold .. " et puis... " .. che .. " est decede de maniere spontanee." $) +text
	then
	
;

cyoa-choice: cardJ4-dentist cardJ4-mcheck medicalREV ;

: cardJ4-mecheck 
	mechanicroll if
		 ($ "En utilisant l'avantage supplementaire : " .. che .. " ferme violemment la porte de la voiture, extrayant nettement la dents." $) +text goodfx
		mechanic++ 
	then;
	
		($ "Sans preparation pour la brusque poussee de la porte qui s'est fermee brutalement, " .. che .. " est tire vers la voiture et heurte " .. chis .. " tete contre la porte. " $) +text bitefx ouch-- morale--
		
	dead? if
		+cr ($ "Ceci etait effectivement une situation courante avant l'apocalypse zombie pour deceder. ces accidents n'ont jamais fait la une a cause de : " .. "LES GRANDES ENTREPRISES AUTOMOBILES" rbold .. "." $) +text
	then
;

cyoa-choice: cardJ4-mechanical
	($ cname .. "un knot de television en se inspirant de dessins animes anciens pour lier. " .. chis .. " la dent sur la porte de voiture." $) cyoa-text! 
		+cr cardJ4-mecheck mechanicREV
;

: cardJ4-scheck 
	cpet? if
		($ cname .. " essaye de se debarrasser de la dent cassee en machonnant sur une pierre. quelques dents tombent a leur tour, et la douleur est extremement forte. mais au moins la dent mauvaise est egalement sortie." $) cyoa-text! badfx
		nonfatal if
			ouch--
		then morale3- attitude--
	then;
	strengthroll if
		($ cname .. " s'empare d'une paire de pince et va de l'avant, tirant sur la dent avec toute sa force. en utilisant seulement son volonte pour se battre contre la douleur. " .. che .. " retire la dent rapidement et avec une facilite etonnante." $) cyoa-text! goodfx
		strength++ 
	then;
		($ cname .. " extraction d'un dents avec des pincettes, mais le processus est difficile. il y a beaucoup de douleur et de saignement avant que le dent ne sorte finalement." $) cyoa-text! bitefx ouch-- morale2-
		
		dead? if
			+cr ($ "Quelque facon " .. cname .. " meurt de cela." $) +text
		then
		
;

cyoa-choice: cardJ4-force cardJ4-scheck strengthREV ;

