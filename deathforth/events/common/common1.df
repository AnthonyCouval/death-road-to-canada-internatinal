	"Ma Hooth" cyoa-title!
	choosehuman cicon
	($ cname .. " la personne souffre d'une douleur dentaire presque paralysante. l'option la plus viable sans l'acces aux outils de precision necessaires est de tirer la molaire. " .. cHe .. " pese en equilibre les options actuelles." $) cyoa-text! basic-tone
	
		choice( "Forcer cela hors!" )choice: cardJ4-force
		cnotpet? if
			choice( "Soins de sante appliques sur char" )choice: cardJ4-dentist
			vehicle.which@ 0 > if choice( "Je l'ai vu une fois dans un dessin anime !" )choice: cardJ4-mechanical then
		then
;

: cardJ2-broke "Le lecteur de cd reste casse et genant." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Reparation du lecteur de cd" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " une personne emet un long et puissant pet, qui ne detourne pas du tout de la musique enregistree sur le cd bloque. cependant, cela ne repare pas non plus le lecteur cd." $) cyoa-text! badfx morale2-all
		else
				($ cname .. " instructe : c'est une " .. everyone .. " vers" .. " ENTREPRISE INITECH" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "voix nasale de 'l a l'heure juste lors de la boucle de la chanson. " .. "FAITES FRAPPER UNE CHANSON HIP-HOP" rbold .. " une invention heureuse et inattendue s'est produite, augmentant considerablement le moral du groupe !" $) +crtext goodfx
				morale3+all
			else cool-it++
				"Cela ne contribue nullement a ameliorer la situation." +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. " regle le lecteur de cd a peine avec difficulte !" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. " arretez de demonter le lecteur audio de tableau de bord et l'expulsez par la fenetre... . 		cette solution fonctionne !" $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. " retire le lecteur de cd et se penche sur la reparation. " $) cyoa-text! 
	mechanicroll if
		($ cHe .. " repare ce qui est mal fonctionnant !" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " ne reussit pas a le reparer." $) +text cardJ2-broke
	then mechanicREV
;

