
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " ' sammelt die Twinkies ein die nicht alle zertrummert und von Ameisen uberfallen wurden nachdem der Lastwagen umgekippt war. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. "  fangt an mit dem Schloss herumzuspielen. " $) cyoa-text!
		cmechanic+ if
			($ "'Es ist kein Problem fur " .. chim .. "  sich zu offnen." $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Es dauert etwas, aber schlielich springt der Schlussel auf." +text common1-twinkie-get mechanic++
		else
			($ cHe .. " kommt nicht weiter und gibt auf aus Frustration." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. " beginnt den Schlussel zu zerbeissern " $) 
	else
		($ cname .. " findet ein groes Stein und schwinger dann hart gegen den Schlussel " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. " Der Schlussel bricht " $) +text common1-twinkie-get
		then;
		strengthroll if
			"Der Schlussel bricht!" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. " verrat sich " .. chis .. " leiht seinen Zahnen gegen den festen Metall!" $)
			else
				($ cHe .. " schlagt es anstatt das Lkw aufzuklappen " .. chis .. " beutelt seine Hand recht schlimm auf statt das Lkw aufzuziehen!" $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. " nimmt Schuss auf den Schlussel am Heck des Lkw " $) cyoa-text!
		cshooting+ if
			"Volltreffer! Der Hebel springt auf " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. " schafft es, den Schuss abzugeben, das Schloss zu zerstoren!" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "EXTREM ENTZUNDLICH" rbold .. "  goo drinnen. Der Lastwagen wird von einer gewaltigen Explosion verzehrt!" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Twinkie Truckin" cyoa-title!
	($ "'Auf der Strae steht ein Twinkie Lastwagen. Er steht auf dem Kopf, und die Tur zum Heck ist fest verschlossen. " .. Thegroup .. "  leckt sich die Lippen bei dem Gedanken an die Menge an verarbeitetem Maissirup die sich darin befinden muss." $) cyoa-text!
		choice( "Ignoriere es aus gesundheitlichen Grunden" )choice
		mechanicest charchoice( "  versucht es aufzubrechen" )choice: common1-twinkie-mech
		strongest charchoice( "  schlagt mit etwas Schwerem darauf ein" )choice: common1-twinkie-smash
		shootest charchoice( "  schiet auf das Schloss" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
	($ Thegroup .. " ' wei nicht viel uber die Konservierung von Wild, aber " .. gthey .. "  kocht" .. g1s .. "  etwas Fleisch um es in das Auto zu packen." $) +cr +text 8 12 rnd trunk-food+ goodfx
;

cyoa-choice: common1-deer-leave
	($ "Der Hirsch rennt schnell davon. Die ruhige Begegnung hebt " .. thegroup .. "''s Stimmung!" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. "  tritt aufs Gas und fahrt mit dem Auto durch den Hirsch." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. " schiet auf das Reh" $) cyoa-text!
	vehicle.which@ 0 > if " aus einem der Autofenster. " else ". " then +text
		shooting+roll if
			($ cHe .. "  macht den Schuss" $) +crtext 
			vehicle.which@ 0 > if 
				", das Hirsch erlegt hat, wahrend es auch jegliche Ethik des Jagers verletzt."
			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. "  komplett verfehlt" $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Hirschubergang" cyoa-title!
	($ "Ein Hirsch steht in der Mitte der Strae. " $) cyoa-text!
	vehicle.which@ 0 > if "Es schaut auf das Auto." else ($ "Schaut auf " .. thegroup .. "." $) then +text
		choice( "Lass es alleine" )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Das Hirsch uberfahren" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( "  versucht es zu schieen" )choice: common1-deer-shoot
		then
;

// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. "  entscheidet sich " .. che .. " 'keine Nahte brauchen, und dass es besser ist, das Problem einfach zu ignorieren. " .. cHe .. "  blutet uberall und der Arm heilt nie vollstandig." $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. "  versucht zu nahen " .. vname .. "'s arme Kind" $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Ohne medizinische Versorgung " .. cname .. " macht das Beste " .. che .. " es gibt einige Probleme spater mit einer Infektion." $) +crtext victim .morale--
			else
				($ cHe .. "  sticht den Arm mit minimalen Schmerzen." $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. "  verpfuscht die Stiche und verschlimmert die Wunde. Die Blutung hort auf, aber " .. vname .. "' Arm heilt schlecht." $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. "  ahmt etwas nach " .. che .. " ' sah als er den Film 'Rambeux' sah. " .. cHe .. " ' erhitzt ein Messer richtig und legt es dann direkt auf den Schnitt, um die Wunde zu kauterisieren. Es war eine ziemlich schlechte Idee! Der Schmerz ist extrem, obwohl die Blutung aufhort und keine Kraft im Arm verloren geht." $) cyoa-text! badfx morale3-
;

cyoa: common1-stitches
	image-arm-gash
	" Tiefer Schnitt" cyoa-title!
	choosehuman cicon
	cchar to victim
	($ cname .. "  schneidet " .. chim .. " selbst ziemlich schlecht wahrend er in der Postapokalypse herumalbert. " .. cHe .. "  wird wahrscheinlich Stiche brauchen fur " .. chis .. "  arm." $) cyoa-text!
		choice( "Ignore it / Walk it off" )choice: common1-stitches-no
		medicalest charchoice( "  versucht es zu nahen" )choice: common1-stitches-med
		cchar charchoice( "  kauterisiert es" )choice: common1-stitches-cauter
;

// Lonely Gas Station

: common1-gas-get 
	($ Thegroup .. "  sammelt dieses kostbare Gas " $) +crtext 70 110 rnd trunk-gas+ goodfx
;

cyoa-choice: common1-gas-mech
	($ cname .. " versucht die Pumpen zu reparieren. " $) cyoa-text!
		cmechanic+ if
			($ cHe .. " bringt sie im Handumdrehen zum Pumpen." $) +text common1-gas-get
		then;
		mechanicroll if
			($ "Es braucht etwas Fummelei, aber " .. che .. " bringt die Pumpen zum Laufen." $) +text common1-gas-get mechanic++
		else
			($ cHe .. " versucht es, kommt aber nicht weiter." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-gas-climb
	($ cname .. " meldet sich freiwillig in die Tanks zu klettern. " $) cyoa-text!
		cpet? if
			($ cHe .. " bleibt dort stecken und es dauert lange bis er wieder herauskommt. " .. cname .. " taucht vollig mit Benzin durchnasst auf." $) +text
			morale2- sting-- 
			notsolo if
				($ notcname .. " wringt " .. chim .. " aus und sammelt das Benzin!" $)
				+crtext 70 110 rnd trunk-gas+ goodfx
			then
		then;
		fitnessroll if
			($ "'Es gibt eine Menge Kampf und blindes Gluck, aber " .. che .. " quetscht sich in den Tank." $) +text common1-gas-get fitness++
		else
			($ cHe .. " stellt fest dass das Klettern in den Tank vollig erschopfend ist. " .. cname .. " steigt in den Tank, wird aber auf dem Weg verletzt. Rauszukommen ist eine andere Sache." $) +text
			badfx morale-- ouch--
		then fitnessREV
;

cyoa: common1-gas
	image-lonely-gas
	"Lonely Gas Station" cyoa-title!
	($ Thegroup .. " ' findet eine Tankstelle weit abseits der Hauptstrae. Jemand hat den Laden geplundert, und es sind keine Zombies in Sicht. Die Zapfsaulen sind alt und scheinen nicht zu funktionieren." $) cyoa-text!
		choice( "Pumpe in Ruhe lassen" )choice
		mechanicest charchoice( "  versucht die Pumpen zu reparieren" )choice: common1-gas-mech
		dumbest charchoice( "  kriecht in den Tank" )choice: common1-gas-climb
;

// Jon's tooth pain event and revamped version of his radio event

: cardJ4-mcheck 
	medicalroll if
		($ cname .. "  wusste wirklich, was sie taten. Mit den verfugbaren Werkzeugen und medizinischen Hilfsmitteln " .. che .. "  kann den Zahn sauber und mit relativ wenig Schmerzen ziehen. " $) cyoa-text! goodfx
		medical++
	then;
	
		($ cname .. "  hatte am Ende groe Probleme. Die Zahnextraktion war chaotisch, ungeschickt und schmerzhaft. Um es noch schlimmer zu machen, " .. che .. "  zog den falschen Zahn und musste es noch einmal machen.  " $) cyoa-text! bitefx ouch-- morale3-
		
	dead? if
		+cr ($ "Nach dem Ziehen des zweiten Zahnes " .. cname .. "  horte man ausrufen " .. "YEAH SCREW THIS NOISE" pbold .. "  und dann " .. che .. "  starb spontan." $) +text
	then
	
;

cyoa-choice: cardJ4-dentist cardJ4-mcheck medicalREV ;

: cardJ4-mecheck 
	mechanicroll if
		 ($ "Mit dem zusatzlichen Hebel " .. che .. "  knallt die Autotur zu und zieht den Zahn sauber heraus." $) +text goodfx
		mechanic++ 
	then;
	
		($ "Unvorbereitet auf die plotzliche Kraft des Zuschlagens der Tur " .. che .. "  wird zum Auto gezogen und schlagt " .. chis .. "  kopf gegen die Tur. " $) +text bitefx ouch-- morale--
		
	dead? if
		+cr ($ "Dieses genaue Szenario war tatsachlich eine ziemlich verbreitete Todesart vor der Zombie Apokalypse. Diese Unfalle wurden aus den Nachrichten herausgehalten aufgrund von " .. "GROSSE AUTOKONZERNE" rbold .. "." $) +text
	then
;

cyoa-choice: cardJ4-mechanical
	($ cname .. ", inspiriert von alten Zeichentrickserien, beschliet, " .. chis .. "  zahn an die Autotur zu binden." $) cyoa-text! 
		+cr cardJ4-mecheck mechanicREV
;

: cardJ4-scheck 
	cpet? if
		($ cname .. "  versucht, den kaputten Zahn loszuwerden, indem er auf einen Stein beit. Ein paar Zahne fallen heraus und der Schmerz ist unertraglich. Aber zumindest ist auch der schlechte Zahn ausgefallen." $) cyoa-text! badfx
		nonfatal if
			ouch--
		then morale3- attitude--
	then;
	strengthroll if
		($ cname .. "  schnappt sich eine Zange und geht in die Stadt, zieht den Zahn so fest wie moglich. Mit nichts als Willen, den Schmerz zu bekampfen, " .. che .. "  bekommt den Zahn schnell und mit uberraschend wenig Muhe heraus." $) cyoa-text! goodfx
		strength++ 
	then;
		($ cname .. " ' schnappt sich eine Zange und geht in die Stadt. Die Extraktion verlauft nicht gut. Nach viel Schmerz und Blut kommt der Zahn schlielich heraus." $) cyoa-text! bitefx ouch-- morale2-
		
		dead? if
			+cr ($ "Irgendwie " .. cname .. " stirbt an diesem." $) +text
		then
		
;

cyoa-choice: cardJ4-force cardJ4-scheck strengthREV ;

cyoa: cardJ4-test
	"Mein Hut" cyoa-title!
	choosehuman cicon
	($ cname .. " hat fast lahmende Zahnschmerzen. Ohne Zugang zu den Prazisionswerkzeugen die benotigt werden ist die einzige Option den Zahn herauszuziehen. " .. cHe .. " wagt die aktuellen Optionen ab." $) cyoa-text! basic-tone
	
		choice( "Zwing es raus!" )choice: cardJ4-force
		cnotpet? if
			choice( "Ich war mal ein Zahnarzt!" )choice: cardJ4-dentist
			vehicle.which@ 0 > if choice( "Das hab ich mal in einem Cartoon gesehen!" )choice: cardJ4-mechanical then
		then
;

: cardJ2-broke "Der CD Player ist immer noch kaputt und nervt." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Repariere den CD Player" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " 'lasst einen wirklich langen Furz, der uberhaupt nicht von der Musik der feststeckenden CD ablenkt. Es repariert auch nicht den CD Player." $) cyoa-text! badfx morale2-all
		else
				($ cname .. " sagt " .. everyone .. " zu" .. " KUHL ES!" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "'nasale Stimme kommt genau zur richtigen Zeit wahrend des sich wiederholenden Songs. Eine neue " .. "HIT HIP HOP SONG" rbold .. " 'wird versehentlich erfunden und steigert die Moral der Gruppe erheblich!" $) +crtext goodfx
				morale3+all
			else cool-it++
				"'Das hilft naturlich uberhaupt nicht." +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. " repariert den CD Player mit minimaler Schwierigkeit!" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. " problem gelost!" $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. " entfernt den CD Player und grubelt " $) cyoa-text! 
	mechanicroll if
		($ cHe .. "  herausfindet, was falsch war, und repariert es!" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " ' schafft es nicht es zu reparieren." $) +text cardJ2-broke
	then mechanicREV
;

cyoa-choice: cardJ2-quit
	($ Thegroup .. "  beschliet einfach den CD Player auszuschalten. Das war einfach!" $) cyoa-text!
;

cyoa-bridge: cardJ2-fix4
