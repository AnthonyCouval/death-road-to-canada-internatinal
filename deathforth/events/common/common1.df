			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. "  erra completamente." $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Passagem de Cervos" cyoa-title!
	($ " Um veado parou no meio da estrada. " $) cyoa-text!
	vehicle.which@ 0 > if "Ele olha para o carro." else ($ "Olha para ele " .. thegroup .. "." $) then +text
		choice( "Deixeo sozinho" )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Corre sobre o veado" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( " tenta atingilo com uma arma" )choice: common1-deer-shoot
		then
