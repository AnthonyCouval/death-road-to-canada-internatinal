: cardJ2-broke "Le lecteur de CD reste casse et genant." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Reparer le lecteur CD" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " effectue une longue flatulence, ce qui ne detourne en rien de la musique bloquee du CD. Il ne repare pas non plus le lecteur CD.'," $) cyoa-text! badfx morale2-all
		else
				($ cname .. "  raconte " .. everyone .. " vers" .. " CALME TOI!" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "''la voix nasale arrive juste a temps durant la chanson en boucle. Un nouveau " .. "CHANSON A SUCCES DE HIP HOP" rbold .. " est invente par accident, augmentant considerablement le moral du groupe !" $) +crtext goodfx
				morale3+all
			else cool-it++
				"Ce qui, bien sur, n'aide en rien la situation." +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. " repare le lecteur CD avec un minimum de difficulte" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. " " $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. " en francais  retire le lecteur CD et se demande comment le reparer. " $) cyoa-text! 
	mechanicroll if
		($ cHe .. " repare ce qui ne va pas" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " ne parvient pas a le reparer." $) +text cardJ2-broke
	then mechanicREV
;

