
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " ' coleta os twinkies que nao foram todos esmagados e invadidos por formigas depois que o caminhao capotou. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. "  comeca a mexer na fechadura. " $) cyoa-text!
		cmechanic+ if
			($ "'Nao e problema para " .. chim .. "  abrir." $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Demora um pouco, mas eventualmente a fechadura se abre." +text common1-twinkie-get mechanic++
		else
			($ cHe .. "  nao chega a lugar nenhum e desiste frustrado." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. "  comeca a mastigar a fechadura. " $) 
	else
		($ cname .. "  encontra uma pedra grande e entao balanca forte na fechadura. " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. "  A fechadura se despedaca. " $) +text common1-twinkie-get
		then;
		strengthroll if
			"A fechadura quebra!" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. "  estraga " .. chis .. " dentes se colocam contra o metal solido!" $)
			else
				($ cHe .. " acaba por quebrar " .. chis .. " deixa a mao ficar ruim em vez de abrir o caminhao." $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. " tomando alvo da travessa no fundo do caminhao. " $) cyoa-text!
		cshooting+ if
			"Tiro perfeito! A travessa popa. " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. " consegue fazer o tiro, destruindo a travessa!" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "EXTERMINADAMENTE INFLAVEL" rbold .. " barata dentro. O caminhao e consumido por uma explosao enorme!" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Twinkie Trucking" cyoa-title!
	($ "'Ha um caminhao de Twinkies na estrada. Ele esta de cabeca para baixo, e a porta do fundo esta travada firmemente. " .. Thegroup .. " saliva sobre o butim de xarope de milho processado que deve estar dentro." $) cyoa-text!
		choice( "Ignoralo por razoes de saude" )choice
		mechanicest charchoice( "  tenta arrombalo" )choice: common1-twinkie-mech
		strongest charchoice( "  esmagao com algo pesado" )choice: common1-twinkie-smash
		shootest charchoice( "  atira na fechadura" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
