
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " 'recoge las twinkies que no estaban todo lastimadas y saqueadas por hormigas despues de que el camion se volcara. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. " comienza a jugar con la cerradura " $) cyoa-text!
		cmechanic+ if
			($ "'no es un problema para " .. chim .. " abrirse" $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Toma un poco de tiempo, pero eventualmente la cerradura se rompe." +text common1-twinkie-get mechanic++
		else
			($ cHe .. " no avanza y se rinde en el frustracion." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. "  empieza a morder la cerradura. " $) 
	else
		($ cname .. "  encuentra una piedra grande y luego golpea con fuerza la cerradura. " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. "  La cerradura se rompe. " $) +text common1-twinkie-get
		then;
		strengthroll if
			"La cerradura se rompe!" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. "  estropea " .. chis .. "  dientes contra el metal solido!" $)
			else
				($ cHe .. "  termina rompiendo " .. chis .. "  mano bastante mal en lugar de abrir la camioneta!" $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. "  apunta a la cerradura de la parte trasera de la camioneta. " $) cyoa-text!
		cshooting+ if
			"Disparo perfecto! El pestillo se abre. " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. "  logra hacer el tiro, destruyendo la cerradura!" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "EXTREMADAMENTE INFLAMABLE" rbold .. "  pegajoso dentro. El camion es consumido por una gran explosion!" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Espanol" cyoa-title!
	($ "'Hay un camion de Twinkie en la carretera. Esta al reves y la puerta trasera esta bien cerrada. " .. Thegroup .. "  salpica sobre la generosidad de jarabe de maiz procesado que debe haber dentro." $) cyoa-text!
		choice( "Ignorarlo por razones de salud" )choice
		mechanicest charchoice( "  intenta abrirlo con ganzuas" )choice: common1-twinkie-mech
		strongest charchoice( "  lo rompe con algo pesado" )choice: common1-twinkie-smash
		shootest charchoice( "  dispara a la cerradura" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
	($ Thegroup .. " ' no sabe mucho sobre la conservacion de la caza, pero " .. gthey .. "  cocina torpemente" .. g1s .. "  algo de carne para llevar al coche." $) +cr +text 8 12 rnd trunk-food+ goodfx
;

cyoa-choice: common1-deer-leave
	($ "El venado sale corriendo rapidamente. El tranquilo encuentro levanta " .. thegroup .. "''s espiritus!" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. "  pisa a fondo el acelerador y estrella el coche contra el venado." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. "  dispara al venado" $) cyoa-text!
	vehicle.which@ 0 > if "  desde una de las ventanas del coche. " else ". " then +text
		shooting+roll if
			($ cHe .. "  hace el tiro" $) +crtext 
			vehicle.which@ 0 > if 
				", embolsando el ciervo mientras tambien va en contra de cualquier sentido de etica de cazador."
			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. "  lo pierde por completo." $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Cruce de ciervos" cyoa-title!
	($ "Un ciervo se ha detenido en medio de la carretera. " $) cyoa-text!
	vehicle.which@ 0 > if "Mira fijamente el coche." else ($ "Mira fijamente " .. thegroup .. "." $) then +text
		choice( "Dejalo en paz" )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Atropella al ciervo" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( "  intenta dispararle" )choice: common1-deer-shoot
		then
;

// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. "  decide " .. che .. " ' no necesita puntos de sutura y que es mejor ignorar el problema. " .. cHe .. "  termina sangrando por todas partes y el brazo nunca sana por completo." $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. " intenta suturar " .. vname .. "brazo de '." $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Sin suministros medicos, " .. cname .. "  lo hace mejor " .. che .. "  puede. Hay algunos problemas mas adelante con la infeccion." $) +crtext victim .morale--
			else
				($ cHe .. "  cose el brazo con un dolor minimo." $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. "  termina estropeando los puntos y empeorando la herida. La hemorragia se detiene, pero " .. vname .. "''s brazo sana mal." $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. "  imita algo " .. che .. " ' vi mientras veia la pelicula 'Rambeux'. " .. cHe .. " ' consigue un cuchillo muy caliente y luego lo pone justo en el corte para intentar cauterizar la herida.	Fue una idea bastante mala! El dolor es extremo, aunque la hemorragia se detiene y no hay perdida de fuerza en el brazo." $) cyoa-text! badfx morale3-
;

cyoa: common1-stitches
	image-arm-gash
	"Corte profundo" cyoa-title!
	choosehuman cicon
	cchar to victim
	($ cname .. "  cortes " .. chim .. "yo mismo bastante mal mientras bromeaba en el post apocalipsis. " .. cHe .. "  probablemente necesitara puntos de sutura para " .. chis .. "  brazo." $) cyoa-text!
		choice( "Ignore it / Walk it off" )choice: common1-stitches-no
		medicalest charchoice( "  intenta coserlo" )choice: common1-stitches-med
		cchar charchoice( "  la cauteriza" )choice: common1-stitches-cauter
;

