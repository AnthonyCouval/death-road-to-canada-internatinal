;

cyoa-choice: common1-deer-leave
	($ "O cervo foge rapidamente. O encontro tranquilo levanta " .. thegroup .. "''s espiritos!" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. "  pisa no acelerador e bate o carro no cervo." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. "  atira no cervo" $) cyoa-text!
	vehicle.which@ 0 > if "  de uma das janelas do carro. " else ". " then +text
		shooting+roll if
			($ cHe .. "  acerta o tiro" $) +crtext 
			vehicle.which@ 0 > if 
