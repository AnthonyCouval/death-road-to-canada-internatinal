: cardJ2-broke "O CD player continua quebrado e irritante." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Consertando o CD Player" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " ' da um peido muito longo, o que nao distrai em nada da musica do CD emperrado. Tambem nao conserta o CD player." $) cyoa-text! badfx morale2-all
		else
				($ cname .. "  conta " .. everyone .. "  para" .. "  SE LIGA!" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "' voz nasal acontece na hora certa durante a musica em loop. Um novo " .. "HIT HIP HOP SONG" rbold .. " ' e inventado acidentalmente, aumentando consideravelmente o moral do grupo!" $) +crtext goodfx
				morale3+all
			else cool-it++
				"'Isso, claro, nao ajuda em nada a situacao." +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. "  conserta o CD player com dificuldade minima!" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. "  arranca o CD player do painel e depois o joga pela janela.		Problema resolvido!" $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. "  remove o CD player e fica confuso sobre como conserta lo. " $) cyoa-text! 
	mechanicroll if
		($ cHe .. "  descobre o que estava errado e conserta!" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " ' nao consegue conserta lo." $) +text cardJ2-broke
	then mechanicREV
;

