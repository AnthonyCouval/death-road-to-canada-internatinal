			($ cHe .. " 'las pone en marcha en poco tiempo." $) +text common1-gas-get
		then;
		mechanicroll if
			($ "Tarda un poco, pero " .. che .. " 'logra poner en marcha las bombas." $) +text common1-gas-get mechanic++
		else
			($ cHe .. " 'intenta, pero no consigue nada con ellas." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-gas-climb
	($ cname .. " 'se ofrece para entrar en los tanques. " $) cyoa-text!
		cpet? if
			($ cHe .. " 'queda atrapado alli y se tarda mucho en sacarlo. " .. cname .. " 'sale empapado en gas." $) +text
			morale2- sting-- 
			notsolo if
				($ notcname .. "  retuerce " .. chim .. " 'fuera y recoge el gas!" $)
				+crtext 70 110 rnd trunk-gas+ goodfx
