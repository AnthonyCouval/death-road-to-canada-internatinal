// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. " decide " .. che .. " 'nao precisa de nenhuma costura, e que e melhor ignorar o problema.', " .. cHe .. " acaba sanguessugo por todos os lados, e o braco nunca se cura totalmente.," $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. " tenta coselar " .. vname .. "'seu braco.'" $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Sem suprimentos medicos, , " .. cname .. " " .. che .. " acaba sofrendo com infeccoes." $) +crtext victim .morale--
			else
				($ cHe .. " 'costura o braco com dor minima.'" $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. "  the wound is now infected. " .. vname .. "'s arm heals poorly." $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. "  imita algo " .. che .. "  vi enquanto assistia ao filme 'Rambeux'. " .. cHe .. " 1" $) cyoa-text! badfx morale3-
;

