: cardJ2-broke "El reproductor de CD queda roto y molesto." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Arreglar el Reproductor de CD" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " 'hace un gran pedazo de flatulencia, lo que no desvia en absoluto de la musica del CD atascado. Tambien no arregla el reproductor de CD." $) cyoa-text! badfx morale2-all
		else
				($ cname .. " dice " .. everyone .. " 'a" .. " 'ESCOTELO!" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "su voz nasal sucede justo a tiempo durante la cancion en bucle. Una nueva " .. "CANCION DE HIP-HOP FAMOSA" rbold .. " 'se inventa por error, lo que aumenta considerablemente el animo del grupo!" $) +crtext goodfx
				morale3+all
			else cool-it++
				"Esto, claro, no ayuda en absoluto a la situacion." +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. " 'Arregla el reproductor de CD con dificultad minima!" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. " problema resuelto!" $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. "  quita el reproductor de CD y se rompe la cabeza tratando de arreglarlo. " $) cyoa-text! 
	mechanicroll if
		($ cHe .. " 'figura que estaba mal, y lo repara!" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " no logra repararlo." $) +text cardJ2-broke
	then mechanicREV
;

