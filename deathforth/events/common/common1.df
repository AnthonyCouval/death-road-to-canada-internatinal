// Jon's tooth pain event and revamped version of his radio event

: cardJ4-mcheck 
	medicalroll if
		($ cname .. " realmente sabia lo que estaba haciendo. Usando las herramientas y suministros medicos disponibles, " .. che .. " pudo extraer la muela limpiamente y con relativamente poca dolor. " $) cyoa-text! goodfx
		medical++
	then;
	
		($ cname .. " acabo teniendo muchos problemas. La extraccion dental fue sucia, desordenada y dolorosa. Para empeorar, " .. che .. " extrajo la muela equivocada y tuvo que hacerlo de nuevo. " $) cyoa-text! bitefx ouch-- morale3-
		
	dead? if
		+cr ($ "Despues de extraer la segunda muela " .. cname .. " se escucho gritar " .. "SI A LA MIERDA ESTO RUIDO" pbold .. "  y luego " .. che .. " ' se apago espontaneamente." $) +text
	then
	
;

cyoa-choice: cardJ4-dentist cardJ4-mcheck medicalREV ;

: cardJ4-mecheck 
	mechanicroll if
		 ($ "Utilizando la palanca anadida, " .. che .. " ' golpea la puerta del coche, arrancando el diente de forma limpia." $) +text goodfx
		mechanic++ 
	then;
	
		($ "No preparado para la fuerza repentina del cerrar la puerta, " .. che .. " ' es arrastrado hacia el coche y choca " .. chis .. " ' la cabeza contra la puerta. " $) +text bitefx ouch-- morale--
		
	dead? if
		+cr ($ "Este mismo escenario era en realidad una manera de muerte bastante comun antes del apocalipsis zombi. Estos accidentes se mantuvieron fuera de las noticias debido a " .. "LAS GRANDES EMPRESAS AUTOMOVILISTICAS" rbold .. "." $) +text
	then
;

cyoa-choice: cardJ4-mechanical
	($ cname .. ", utilizando viejos programas de dibujos animados como inspiracion, deciden atar " .. chis .. " ' el diente a la puerta del coche." $) cyoa-text! 
		+cr cardJ4-mecheck mechanicREV
;

: cardJ4-scheck 
	cpet? if
		($ cname .. " ' intenta deshacerse del diente roto mordiendo una roca. Unos pocos dientes se caen, y el dolor es insoportable. Pero al menos tambien se cayo el diente malo." $) cyoa-text! badfx
		nonfatal if
			ouch--
		then morale3- attitude--
	then;
	strengthroll if
		($ cname .. " ' agarra un par de tenazas y se pone a trabajar, tirando del diente con todas sus fuerzas. Utilizando nada mas que su voluntad para luchar contra el dolor, " .. che .. " saca el diente rapidamente y con sorprendentemente poco problema." $) cyoa-text! goodfx
		strength++ 
	then;
		($ cname .. " 'Agarra un par de alicates y se pone a trabajar. La extraccion no va bien. Despues de mucho dolor y sangre, el diente finalmente sale." $) cyoa-text! bitefx ouch-- morale2-
		
		dead? if
			+cr ($ "De alguna manera " .. cname .. " muere de esto." $) +text
		then
		
;

cyoa-choice: cardJ4-force cardJ4-scheck strengthREV ;

