: cardJ2-broke "El reproductor de CD sigue roto y molesto." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Arreglando el reproductor de CD" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " ' realiza un pedo muy largo, que no distrae en absoluto de la musica de los CD atascados. Tampoco arregla el reproductor de CD." $) cyoa-text! badfx morale2-all
		else
				($ cname .. "  cuenta " .. everyone .. "  a" .. "  CALLATE!" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "'la voz nasal ocurre justo en el momento adecuado durante la cancion en bucle. Un nuevo " .. "CANCION DE HIP HOP EXITO" rbold .. " ' se inventa accidentalmente, aumentando considerablemente la moral del grupo!" $) +crtext goodfx
				morale3+all
			else cool-it++
				"'Esto, por supuesto, no ayuda a la situacion en absoluto." +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. " arregla el reproductor de CD con minima dificultad!" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. " arranca el reproductor de CD del tablero y luego lo arroja por la ventana! Problema resuelto!" $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. " quita el reproductor de CD y se pregunta como arreglarlo " $) cyoa-text! 
	mechanicroll if
		($ cHe .. " descubre que estaba mal y lo repara!" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " 'no logra repararlo." $) +text cardJ2-broke
	then mechanicREV
;

