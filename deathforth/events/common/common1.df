
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " coleta os twinkies que nao foram todos esmagados e saqueados por formigas depois que o caminhao virou. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. " comeca a mexer na fechadura " $) cyoa-text!
		cmechanic+ if
			($ "Nao tem problema para " .. chim .. "  para abrir." $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Demora um pouco, mas eventualmente a fechadura se abre." +text common1-twinkie-get mechanic++
		else
			($ cHe .. " nao vai a lugar nenhum, e desiste de frustracao." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. " comeca a morder a fechadura " $) 
	else
		($ cname .. " encontra uma grande pedra e depois balanca com forca na fechadura. " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. " A fechadura se despedaca " $) +text common1-twinkie-get
		then;
		strengthroll if
			"O cadeado se quebra!" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. "  bagunca " .. chis .. "  dentes contra o metal solido!" $)
			else
				($ cHe .. "  acaba destruindo " .. chis .. "  machuca a mao feio em vez de abrir o caminhao!" $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. "  mira no cadeado na parte de tras do caminhao. " $) cyoa-text!
		cshooting+ if
			"Tiro perfeito! A trava se abre. " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. "  consegue acertar o tiro, destruindo o cadeado!" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "EXTREMAMENTE INFLAMAVEL" rbold .. "  gosma la dentro. O caminhao e consumido por uma enorme explosao!" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Caminhao de Twinkies" cyoa-title!
	($ "'Ha um caminhao de Twinkies na estrada. Esta de cabeca para baixo e a porta dos fundos esta trancada. " .. Thegroup .. "  salivar sobre a abundancia de xarope de milho processado que deve estar la dentro." $) cyoa-text!
		choice( "Ignorar por motivos de saude" )choice
		mechanicest charchoice( "  tenta arrombar com uma chave" )choice: common1-twinkie-mech
		strongest charchoice( "  quebra com algo pesado" )choice: common1-twinkie-smash
		shootest charchoice( "  atira no cadeado" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
	($ Thegroup .. " ' nao sabe muito sobre preservacao de caca, mas " .. gthey .. "  cozinha desajeitadamente" .. g1s .. "  um pouco de carne para levar no carro." $) +cr +text 8 12 rnd trunk-food+ goodfx
;

cyoa-choice: common1-deer-leave
	($ "O veado foge rapidamente. O encontro tranquilo levanta " .. thegroup .. "''s espiritos!" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. "  pisa fundo no acelerador e bate o carro no veado." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. "  atira no veado" $) cyoa-text!
	vehicle.which@ 0 > if "  de uma das janelas do carro. " else ". " then +text
		shooting+roll if
			($ cHe .. "  acerta o tiro" $) +crtext 
			vehicle.which@ 0 > if 
				", pegando o veado e tambem indo contra qualquer senso de etica de cacador."
			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. "  erra completamente." $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Deer crossing" cyoa-title!
	($ "Um veado parou no meio da estrada " $) cyoa-text!
	vehicle.which@ 0 > if "Olha para o carro." else ($ "Olha para " .. thegroup .. "." $) then +text
		choice( "Deixe o sozinho" )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Corra sobre o veado" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( " tenta atirar nele" )choice: common1-deer-shoot
		then
;

// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. " decide " .. che .. " 'nao precisa de pontas, e que melhor e ignorar o problema " .. cHe .. " acaba sangrando por todos os lados, e o braco nunca se cura totalmente." $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. " tenta coser " .. vname .. "'seu braco." $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Sem suprimentos medicos, " .. cname .. " faz o melhor " .. che .. " pode. Ha alguns problemas mais tarde com infeccao." $) +crtext victim .morale--
			else
				($ cHe .. "  pontos no braco com o minimo de dor." $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. "  acaba estragando os pontos e piorando a ferida. O sangramento para, mas " .. vname .. "' braco de" $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. "  imita algo " .. che .. " ' viu enquanto assistia ao filme 'Rambeux'. " .. cHe .. " ' pega uma faca bem quente e coloca bem no corte para tentar cauterizar a ferida. Foi uma pessima ideia! A dor e extrema, embora o sangramento pare e nao haja perda de forca no braco." $) cyoa-text! badfx morale3-
;

cyoa: common1-stitches
	image-arm-gash
	"Corte Profundo" cyoa-title!
	choosehuman cicon
	cchar to victim
	($ cname .. "  corta " .. chim .. " se machuca muito enquanto brinca no pos apocalipse. " .. cHe .. "  provavelmente precisara de pontos para " .. chis .. "  braco." $) cyoa-text!
		choice( "Ignore it / Walk it off" )choice: common1-stitches-no
		medicalest charchoice( "  tenta costurar" )choice: common1-stitches-med
		cchar charchoice( "  cauteriza" )choice: common1-stitches-cauter
;

