: cardJ2-broke "O CD player permanece quebrado e irritante." +crtext badfx morale--all ;

cyoa-choice: cardJ2-fix
	"Consertando o CD Player" cyoa-title!
	chumany? not if
		cpetderp cyoa-text! cardJ2-broke
	then;
	cwits- cattitude- notsolo and and if
		cpet? if
			($ cname .. " ' da um peido muito longo, o que nao distrai em nada a musica do CD emperrado. Tambem nao conserta o CD player." $) cyoa-text! badfx morale2-all
		else
				($ cname .. "  conta " .. everyone .. " para" .. " CALMA!" pbold $) cyoa-text!
			cool-it-7 if
				($ cname .. "'uma voz nasal acontece exatamente ao tempo certo durante a musica em loop do grupo. Um novo HIT DE HIPHOP e acidentalmente inventado aumentando consideravelmente a moral do grupo! " .. "CANCaO DE HIP-HOP INVENTADA A CASUALIDADE" rbold .. " 'isso, porem, nao ajuda nada na situacao." $) +crtext goodfx
				morale3+all
			else cool-it++
				"'Isso, claro, nao ajuda em nada a situacao.'" +crtext cardJ2-broke
			then
		then
	then;	
	cmechanic+ if
		($ cname .. " consegue reparar o tocador de CD com facilidade!" $) cyoa-text! goodfx morale++all
	then;
	ctemper- cstrength+ and if
		($ cname .. " tira o tocador de CD do painel e depois o joga fora da janela.	Problema resolvido!" $) cyoa-text! goodfx morale++all strengthREV
	then;
	($ cname .. " retira o tocador de CD e se preocupa com como reparalo. " $) cyoa-text! 
	mechanicroll if
		($ cHe .. " consegue descobrir o que estava errado e o repara!" $) +crtext goodfx mechanic++ morale++all 
	else
		($ cHe .. " 'nao consegue reparar." $) +text cardJ2-broke
	then mechanicREV
;

