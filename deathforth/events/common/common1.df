
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " 'recoge las twinkies que no estaban todo lastimadas y saqueadas por hormigas despues de que el camion se volcara. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. " 'comienza a jugar con la cerradura. " $) cyoa-text!
		cmechanic+ if
			($ "No es problema para " .. chim .. " 'abrirse." $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Toma un poco de tiempo, pero eventualmente la cerradura se rompe." +text common1-twinkie-get mechanic++
		else
			($ cHe .. " 'no avanza y se rinde en frustracion." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. " 'comienza a morder la cerradura. " $) 
	else
		($ cname .. " 'encontra una roca grande y luego la suelta duro contra la cerradura. " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. " 'La cerradura se rompe. " $) +text common1-twinkie-get
		then;
		strengthroll if
			"La cerradura se rompe!" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. " 'molesta " .. chis .. " 'dientes contra el metal solido" $)
			else
				($ cHe .. " 'termina aplastando " .. chis .. " 'da un punetazo en la mano bastante malo en lugar de abrir la caja del camion" $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. " 'apunta al pestillo en la parte posterior del camion. " $) cyoa-text!
		cshooting+ if
			"Tiro perfecto! El pestillo salta abierto. " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. " 'logra dar el tiro, destruyendo el pestillo!" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "EXTREMAMENTE FLAMABLE" rbold .. " 'goo dentro. El camion es consumido por una enorme explosion!" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Camion Twinkie" cyoa-title!
	($ "Hay un camion Twinkie en la carretera. Esta volcado y la puerta trasera esta cerrada con seguro. " .. Thegroup .. " 'saliva sobre el botin de jarabe de maiz procesado que debe estar dentro." $) cyoa-text!
		choice( "Ignoralo por razones de salud" )choice
		mechanicest charchoice( " intenta forzarlo para abrirlo" )choice: common1-twinkie-mech
		strongest charchoice( " lo destroza con algo pesado" )choice: common1-twinkie-smash
		shootest charchoice( " dispara la cerradura" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
	($ Thegroup .. "  no sabe mucho sobre la preservacion de juegos, pero " .. gthey .. " cocinar torpemente" .. g1s .. " algo de carne para meter en el coche." $) +cr +text 8 12 rnd trunk-food+ goodfx
;

cyoa-choice: common1-deer-leave
	($ "El ciervo corre rapidamente. El encuentro tranquilo se levanta. " .. thegroup .. "'espiritus mias!" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. " ' choca a toda velocidad con el coche y atraviesa al ciervo." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. " ' dispara al ciervo" $) cyoa-text!
	vehicle.which@ 0 > if " ' desde una de las ventanas del coche. " else ". " then +text
		shooting+roll if
			($ cHe .. " ' hace el tiro" $) +crtext 
			vehicle.which@ 0 > if 
				", consiguiendo el ciervo mientras tambien va en contra de cualquier etica del cazador."
			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. " ' completamente falla" $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Cruce de Ciervos" cyoa-title!
	($ "Un ciervo ha parado en medio de la carretera. ', " $) cyoa-text!
	vehicle.which@ 0 > if "Mira al coche." else ($ "Mira a ', " .. thegroup .. "." $) then +text
		choice( "Dejemoslo solo." )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Atropellar al ciervo" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( " intenta dispararle" )choice: common1-deer-shoot
		then
;

// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. " decide " .. che .. "  no necesita puntos, y que es mejor ignorar el problema. " .. cHe .. " termina sangrando por todas partes, y el brazo nunca se cura completamente." $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. " ' intenta coser " .. vname .. "' brazo suyo." $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Sin suministros medicos, " .. cname .. " 'hace lo mejor " .. che .. " 'puede. Hay algun problema mas tarde con la infeccion." $) +crtext victim .morale--
			else
				($ cHe .. " 'coser el brazo con poca dolor." $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. " 'termina botchando las suturas y haciendo peor la herida. La hemorragica deja de ocurrir, pero " .. vname .. "' brazo se sanara mal." $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. " 'se asemeja a algo " .. che .. " 'vio mientras veia la pelicula Rambeux. " .. cHe .. " 'obtiene un cuchillo muy caliente y luego lo pone directamente sobre la herida para intentar cauterizarla.	Fue una idea bastante mala! La dolor es intenso, sin embargo, la hemorragica deja de ocurrir y no hay perdida de fuerza en el brazo." $) cyoa-text! badfx morale3-
;

cyoa: common1-stitches
	image-arm-gash
	"Corte profundo" cyoa-title!
	choosehuman cicon
	cchar to victim
	($ cname .. " cortes " .. chim .. "self pretty bad while goofing off in the post-apocalypse. " .. cHe .. " 'probablemente necesite puntos de sutura para " .. chis .. " 'brazo." $) cyoa-text!
		choice( "Ignore it / Walk it off" )choice: common1-stitches-no
		medicalest charchoice( " 'intenta cerrarlo con puntadas" )choice: common1-stitches-med
		cchar charchoice( " 'cauteriza" )choice: common1-stitches-cauter
;

// Lonely Gas Station

: common1-gas-get 
	($ Thegroup .. " 'recoge ese precioso gas. " $) +crtext 70 110 rnd trunk-gas+ goodfx
;

cyoa-choice: common1-gas-mech
