
cyoa{ road{ 

// Twinkie Truck

: common1-twinkie-get
	($ Thegroup .. " ramasse les twinkies qui n'ont pas ete tous ecrases et pilles par les fourmis apres que le camion se soit renverse. " $) +cr +text 8 trunk-food+ goodfx
;

cyoa-choice: common1-twinkie-mech
	($ cname .. " commence a jouer avec la serrure. " $) cyoa-text!
		cmechanic+ if
			($ "'c'est pas un probleme pour " .. chim .. " pour quelle se ouvre." $) +text common1-twinkie-get
		then;
		mechanicroll if
			"Ca prend un peu de temps, mais finalement la serrure casse." +text common1-twinkie-get mechanic++
		else
			($ cHe .. " ne fait pas beaucoup de progres, et abandonne en echange de la frustration." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-twinkie-smash
	cpet? if
		($ cname .. " commence a macher sur la serrure. " $) 
	else
		($ cname .. " trouve une grande pierre et ensuite frappe fort sur la serrure. " $) 
	then cyoa-text!
		cstrength+ if
			($ "WHAAAAAAM!" rbold .. " La serrure explose " $) +text common1-twinkie-get
		then;
		strengthroll if
			"LA SERRURE SE CASSE!" +text common1-twinkie-get strength++
		else
			cpet? if
				($ cHe .. " en fait des dommages a " .. chis .. " dents contre le metal solide!" $)
			else
				($ cHe .. " finit par ecraser " .. chis .. " main en lair plutot que douvrir le camion!" $) 
			then +text bitefx sting-- strength--
		then strengthREV
;

cyoa-choice: common1-twinkie-shoot
	($ cname .. " vise le cadenas sur larriere du camion. " $) cyoa-text!
		cshooting+ if
			"Parfait! Le loquet souvre " +text common1-twinkie-get
		then;
		shootingroll if
			($ cHe .. " reussit a faire le tir, detruisant la serrure !" $) +text common1-twinkie-get shooting++
		else
			($ "The shot misses and goes through the back.
			Sparks from the bullet ignite the " .. "EXTREMEMENT INFLAMMABLE" rbold .. " le camion est consume par une enorme explosion!" $) +text
			bitefx sting--
		then shootingREV
;

cyoa: common1-twinkie
	image-twinkie
	"Twinkie Truckin" cyoa-title!
	($ "'Il y a un camion Twinkie sur la route. Il est a l'envers et la porte de l'arriere est verrouillee " .. Thegroup .. " salive sur la prime de sirop de mais transforme qui doit etre a linterieur." $) cyoa-text!
		choice( "Ignorez le pour des raisons de sante" )choice
		mechanicest charchoice( " essaie de louvrir a la sauvette" )choice: common1-twinkie-mech
		strongest charchoice( " lecrase avec quelque chose de lourd" )choice: common1-twinkie-smash
		shootest charchoice( " casse la baraque" )choice: common1-twinkie-shoot
;

// Deer Hunter

: common1-deer-eat
	($ Thegroup .. " ne sait pas grand chose sur la conservation du gibier, mais " .. gthey .. " cuisiner maladroitement" .. g1s .. "  de la viande a mettre dans la voiture." $) +cr +text 8 12 rnd trunk-food+ goodfx
;

cyoa-choice: common1-deer-leave
	($ "Le cerf senfuit rapidement. La rencontre silencieuse souleve " .. thegroup .. "''s esprits !" $) cyoa-text! morale++all
;

cyoa-choice: common1-deer-runover
	choosehuman
	($ cname .. "  appuie sur le gaz et ecrase la voiture contre le cerf." $) cyoa-text! 
	common1-deer-eat car-plowthrough
;

cyoa-choice: common1-deer-shoot
	($ cname .. "  tire sur le cerf" $) cyoa-text!
	vehicle.which@ 0 > if "  dune des fenetres de la voiture. " else ". " then +text
		shooting+roll if
			($ cHe .. "  fait le tir" $) +crtext 
			vehicle.which@ 0 > if 
				", empochant le cerf tout en allant a lencontre de toute ethique de chasseur."
			else
				"!"
			then +text
			shooting++ common1-deer-eat
		else
			($ cHe .. "  le rate completement." $) +crtext badfx
		then shootingREV
;

cyoa: common1-deer
	image-deer-in-road
	"Passage pour cerfs" cyoa-title!
	($ "Un cerf sest arrete au milieu de la route. " $) cyoa-text!
	vehicle.which@ 0 > if "Il regarde la voiture." else ($ "Il regarde sur " .. thegroup .. "." $) then +text
		choice( "Laisse le tranquille" )choice: common1-deer-leave
		vehicle.which@ 0 > if
			choice( "Renverser le cerf" )choice: common1-deer-runover
		then
		roadtrip-has-firearm? if
			shootest charchoice( "  essaie de labattre" )choice: common1-deer-shoot
		then
;

// Cauterize or stitch wound

cyoa-choice: common1-stitches-no
	($ cname .. " prend la decision " .. che .. " 'ne pense pas avoir besoin de points de suture, et qu'il vaut mieux ignorer le probleme.' " .. cHe .. " finit par saigner partout, et lbras ne guerit jamais completement.," $) cyoa-text! badfx strength-- sting-- 
;

cyoa-choice: common1-stitches-med
	($ cname .. " essaye de suturer " .. vname .. "'son bras.'" $) cyoa-text!
		medical+roll if
			trunk-medical 1 < if
				($ "Sans aucuns fournitures medicales, " .. cname .. " fait le meilleur quil peut " .. che .. " peut. Il y a des problemes plus tard avec linfection." $) +crtext victim .morale--
			else
				($ cHe .. " suture lbras avec peu de douleur." $) +crtext -1 trunk-medical+ medical++
			then medicalREV
		then;
		($ cHe .. " finit par boucher mal les points de suture et en aggraver le sort. Lhemorragie sarrete, mais " .. vname .. "'s arm heals poorly." $) +crtext badfx victim .strength-- victim .morale-- medicalREV
;

cyoa-choice: common1-stitches-cauter
	($ cname .. "  imite quelque chose " .. che .. " ' a vu en regardant le film 'Rambeux'. " .. cHe .. " ' prend un couteau tres chaud et le pose directement sur la coupure pour essayer de cauteriser la plaie.	C'etait une tres mauvaise idee ! La douleur est extreme, bien que le saignement s'arrete et qu'il n'y ait pas de perte de force dans le bras." $) cyoa-text! badfx morale3-
;

cyoa: common1-stitches
	image-arm-gash
	"Coup Profond" cyoa-title!
	choosehuman cicon
	cchar to victim
	($ cname .. " coupes " .. chim .. "self pretty bad while goofing off in the post apocalypse " .. cHe .. " aura probablement besoin de points de suture pour " .. chis .. " " $) cyoa-text!
		choice( "Ignore it / Walk it off" )choice: common1-stitches-no
		medicalest charchoice( " tente de le recoudre" )choice: common1-stitches-med
		cchar charchoice( " cauterise" )choice: common1-stitches-cauter
;

// Lonely Gas Station

: common1-gas-get 
	($ Thegroup .. "  ramasse ce precieux gaz. " $) +crtext 70 110 rnd trunk-gas+ goodfx
;

cyoa-choice: common1-gas-mech
	($ cname .. "  tente de reparer les pompes. " $) cyoa-text!
		cmechanic+ if
			($ cHe .. "  les fait pomper en un rien de temps." $) +text common1-gas-get
		then;
		mechanicroll if
			($ " Cela prend un peu de tatonnement, mais " .. che .. "  fait fonctionner les pompes." $) +text common1-gas-get mechanic++
		else
			($ cHe .. "  essaie, mais narrive a rien avec eux." $) +text
			badfx morale--
		then mechanicREV
;

cyoa-choice: common1-gas-climb
	($ cname .. "  se porte volontaire pour grimper dans les reservoirs. " $) cyoa-text!
		cpet? if
			($ cHe .. "  y reste coince et il faut beaucoup de temps pour sortir. " .. cname .. "  en ressort completement trempe dessence." $) +text
			morale2- sting-- 
			notsolo if
				($ notcname .. "  essore " .. chim .. "  et recupere lessence !" $)
				+crtext 70 110 rnd trunk-gas+ goodfx
			then
		then;
		fitnessroll if
			($ "' Il y a beaucoup de lutte et de chance impliquee, mais " .. che .. "  se faufile dans le reservoir." $) +text common1-gas-get fitness++
		else
			($ cHe .. "  decouvre que grimper dans le reservoir est completement epuisant " .. cname .. " il entre dans le reservoir, mais est blesse en chemin. En sortir est une autre affaire." $) +text
			badfx morale-- ouch--
		then fitnessREV
;

cyoa: common1-gas
	image-lonely-gas
	"Station service solitaire" cyoa-title!
	($ Thegroup .. " trouve une station service loin de la route principale. Quelqu'un a vole le magasin, et il n'y a pas de zombies a voir. Les pompes a essence sont vetustes, et elles ne semblent pas fonctionner." $) cyoa-text!
		choice( "Laissez les pompes tranquilles" )choice
		mechanicest charchoice( " essaye de reparer les pompes" )choice: common1-gas-mech
		dumbest charchoice( " entre dans le reservoir" )choice: common1-gas-climb
;

// Jon's tooth pain event and revamped version of his radio event

: cardJ4-mcheck 
	medicalroll if
		($ cname .. " connaissait vraiment ce quil faisait. En utilisant les outils et les fournitures medicales disponibles, " .. che .. " a pu tirer la molaire de maniere propre et avec relativement peu de douleur " $) cyoa-text! goodfx
		medical++
	then;
	
		($ cname .. " avait beaucoup de mal. Lextraction dentaire etait sale, maladroite et douloureuse. De plus, " .. che .. " a tire la mauvaise molaire et a du le faire a nouveau. " $) cyoa-text! bitefx ouch-- morale3-
		
	dead? if
		+cr ($ "Apres avoir tire la deuxieme molaire " .. cname .. " on entendit dire " .. "OUAI MERDE CET APPORT DE Bruit" pbold .. " et ensuite " .. che .. "  est mort spontanement." $) +text
	then
	
;

cyoa-choice: cardJ4-dentist cardJ4-mcheck medicalREV ;

: cardJ4-mecheck 
	mechanicroll if
		 ($ "Utilisant leffet de levier ajoute, " .. che .. "  claque la portiere de la voiture, arrachant proprement la dent." $) +text goodfx
		mechanic++ 
	then;
	
		($ "Mal prepare a la force soudaine du claquement de la portiere, " .. che .. "  est tire vers la voiture et heurte " .. chis .. "  la tete contre la portiere. " $) +text bitefx ouch-- morale--
		
	dead? if
		+cr ($ "Ce scenario exact etait en fait un moyen assez courant de mourir avant lapocalypse zombie. Ces accidents ont ete tenus a lecart des nouvelles en raison de " .. "GRANDES ENTREPRISES AUTOMOBILES" rbold .. "." $) +text
	then
;

cyoa-choice: cardJ4-mechanical
	($ cname .. ", sinspirant de vieilles emissions de dessins animes, decide dattacher " .. chis .. "  dent a la portiere de la voiture." $) cyoa-text! 
		+cr cardJ4-mecheck mechanicREV
;

: cardJ4-scheck 
	cpet? if
		($ cname .. "  essaie de se debarrasser de la dent cassee en mordant une pierre. Quelques dents tombent et la douleur est atroce. Mais au moins la mauvaise dent est tombee aussi." $) cyoa-text! badfx
		nonfatal if
			ouch--
		then morale3- attitude--
	then;
	strengthroll if
		($ cname .. "  attrape une paire de pinces et se met au travail, tirant la dent aussi fort que possible. En utilisant uniquement sa volonte pour lutter contre la douleur, " .. che .. "  enleve la dent rapidement et avec etonnamment peu de problemes." $) cyoa-text! goodfx
		strength++ 
	then;
		($ cname .. " ' attrape une paire de pinces et se met au travail. L'extraction ne se passe pas bien. Apres beaucoup de douleur et de sang la dent finit par sortir." $) cyoa-text! bitefx ouch-- morale2-
		
		dead? if
			+cr ($ "Certainement " .. cname .. " moult souffre de cela." $) +text
		then
		
;

cyoa-choice: cardJ4-force cardJ4-scheck strengthREV ;

