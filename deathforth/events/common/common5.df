
// EYEBALL Update events

cyoa{ road{ 

// Future Toilet, based on a true story

: common5-toilet-end ($ "The future toilet emits a whirring noise and stops working." $) +crtext goodfx ;

cyoa-choice: common5-toilet-water
	($ "1 " .. cname .. " directly in the eyes." $) text!
	morale--
	solo not if ($ Theothersarename .. "  begeistert von diesem! " $) +crtext cchar ' .morale2+ foreach-team-char-except then
	cchar .shooting@ 6 < if ($ " Aber Moment mal! Das Bidetwasser war mediziniert und angenehm duftend! " .. cname .. "s Sicht wird scharfer!" $) +crtext shooting++ then
	most-hurt damage 0 > if ($ Thegroup .. "  wird vollstandig durch das medikamentose Bidetwasser geheilt!" $) +crtext ' .fullheal foreach-team-char then
	common5-toilet-end
;

cyoa-blank: common5-toilet-note
	"Lied der Toilette" cyoa-title!
	"Die Toilette spielt wunderschone Musik. Die Musik hat keine Worte, doch vermittelt sie eine starke und eindringliche Botschaft der Hoffnung." text!
	morale4+all common5-toilet-end
;

cyoa-choice: common5-toilet-wings
	"Die Toilettendeckel offnet und schließt sich schnell, mit großen WHOOMP-Gerauschen, wahrend der Deckel Luftstoße ausstoßt. Der Deckel offnet und schließt sich etwa 30 Mal hintereinander, dann stoppt." text!
	($ cname .. " 'ist verwirrt uber den Zweck dieser Funktion.'" $) +crtext
	common5-toilet-end gstats{ 30 ' toilets-opened +! }
;

cyoa-choice: common5-toilet-butt
	"Die Klobrille beginnt zu summen und leicht zu leuchten." text!
	($ cname .. "  Der elektronische Toilettensitz gibt. " .. chim .. " 'eine machtige Gesaßmuskulatur" .. "*" pbold .. " 'eine Massage, wahrend die Toilette auf unterschiedlichen Sprachen wiederholt spricht:" $) +crtext
	"'DIE GESASSMUSKULATUR IST DER SCHLUSSEL ZUR FITNESS	DIE GESASSMUSKULATUR IST DER SCHLUSSEL ZUR STARKE	DIE GESASSMUSKULATUR IST DER SCHLUSSEL ZUR KRAFT	DIE GESASSMUSKULATUR IST DAS ZENTRUM DES LEBENS" bbold +crtext
	morale++ strength++ fitness++ common5-toilet-end
	"'*Wortschatznotiz: Gluteal bedeutet ‚Po‘" pbold +crtext
;

cyoa: common5-toilet-rare
	"'Toilette der Vergangenheit der Zukunft" cyoa-title! choosehuman cicon
	($ Thegroup .. " 'entdeckt etwas Unglaubliches, wahrend er ein Haus erkundet: Eine einsatzbereite Toilette. Alle haben ihr Geschaft in den Waldern verrichtet.	Die Toilette hat eine futuristische Bedienoberflache voller Knopfe. " .. cname .. " 'fuhlt sich gezwungen, einen zu drucken, und wahlt den Knopf mit dem Bild von:" $) text!
		choice( "'Strahlen aus Wasser?" )choice: common5-toilet-water
		choice( "'Musiknoten" )choice: common5-toilet-note
		choice( "'Flugelschlagen" )choice: common5-toilet-wings
		choice( "'Einem Po?" )choice: common5-toilet-butt
;

// Long Line

: common5-line-camp ($ "'Mit dem Tag vertan, " .. thegroup .. " 'muss das Lager aufschlagen." $) +crtext +cr fullcamp 1 calendar-day+ ;
: common5-line-shop ($ "'Der Laden ist ein trendiges, hochpreisiges Lebensmittelgeschaft. Die teureren Artikel sind großtenteils verdorben, aber es gibt noch Beute." $) +crtext
	8 12 rnd trunk-food+ 4 6 rnd trunk-medical+ goodfx ;

cyoa-choice: common5-line-pick
	
	cdog? if
		($ cname .. " ' rennt an der Reihe her und sie verlasst dann vollstandig. Stunden spater, " .. cname .. " ' kehrt mit einem Stock in der Mundhohle zuruck. " .. chis .. " " $) text!
		1 WEAPON_TREEBRANCH trunk.weapon+
		morale2+ cchar morale--all-except goodfx
		common5-line-camp
	then;
	
	ccat? if
		($ cname .. " ' tritt aus dem Laden heraus, die Reihe umgehend verlassend. " .. cHe .. " ' tragt ein totgegluhtes Tier mit sich, vielleicht ein Gerbil?" $) text!
		2 trunk-food+ morale2+ goodfx
	then;
	
	cpet? if
		($ cname .. " ' liegt fur eine kleine Pause auf dem Boden. Als Tier, " .. che .. " ' versteht es weder den Begriff einer Reihe, noch lasst es sich tauschen." $) text!
		morale++ goodfx
	then;
	
	cchar oblivious? if
		($ cname .. " ' schneidet die Reihe komplett aus. " .. cHe .. " ' bemerkte nicht, dass es eine Reihe gab, oder Zombies." $) text!
		witsREV attitudeREV common5-line-shop
	then;
	
	ctemper- if
		cloyalty- if
			($ cname .. " ' geht einfach ohne Reihenfolge vorbei. " .. cHe .. " ' folgt NICHT den Regeln!" $) text!
			morale++ aholeREV common5-line-shop
		then;
		($ cname .. " 1 minute " .. chis .. " 20;  // Fehler!" $) text!
		cstrength+ if
			"WARTEN IN DER SCHLANGE IST FUR NERDS!" rbold +crtext
			($ cHe .. " 0 " .. che .. "  wird mude und stolpert weg, schwitzend. Was fur ein Workout!" $) +crtext
			rageREV fitness++ fitnessREV morale++ goodfx
		then;
		($ cHe .. " 0 " .. chim .. " Die anderen Zombies bemerken es schließlich und beginnen anzugreifen. " .. cname .. "  wird gezwungen, zu laufen!" $) +crtext
		sting-- bitefx temperREV 
	then;
	
	ctemper+ if
		($ cname .. "  patiently waits in the long line. It moves forward very slowly, but " .. che .. " 'bleibt einfach stehen und wartet ab." $) text!
		temperREV 
		common5-line-shop common5-line-camp
	then;
	
	($ cname .. " 'steht in der Warteschlange. Es bewegt sich langsam vorwarts, um " .. cname .. "'seine große Verargerung." $) text!
	attitude+roll if 
		($ cname .. " 'versucht, sich positiv darauf zu einzustellen. " .. cHe .. " 'schafft es, durchzuhalten und ins Geschaft zu gelangen!" $) +crtext
		attitudeREV morale-- common5-line-shop
	else
		($ "'Nach Stunden dieses Schauspiels werden die Zombies auf " .. cname .. " 'durch ihr " .. chis .. " 'standiges Gejammer. " .. cHe .. " 'zwingen, wegzulaufen!" $) +crtext
		morale2- sting-- bitefx 
	then temperREV common5-line-camp
;

cyoa-bridge: common5-line-pick4
	"'Wahle, wer in der Zombie-Warteschlange warten muss:" text!
	' common5-line-pick cyoa-team-choice 
;

cyoa-blank: common5-line
	"'Immer noch Warten in der Warteschlange" cyoa-title!
	($ Thegroup .. " 'lauft in eine Horde von Zombies. Sie warten vollig reglos draußen auf einer langen Schlange vor einem Laden. Jeder Zombie starrt geradeaus." $) text!
		choice( "0" )choice
	solo if
		"The shop's sign is faded out and can't be read." +crtext
		soloer charchoice( "warten in der Schlange" )choice: common5-line-pick
	then;
	"Du kannst jemanden schicken, um sich in die Schlange einzureihen, um dieses Ratsel zu losen. Es sieht so aus, als ob es eine lange Wartezeit sein konnte." +crtext
	team-count 3 > if
		' common5-line-pick 2 #cyoa-team-choice
		choice( "'Wahle jemand anderen" )choice: common5-line-pick4
	else
		' common5-line-pick 3 #cyoa-team-choice
	then	
;

// Rock throwing

cyoa-choice: common5-rock-throw
	($ cname .. " ist gezwungen, einen Stein auf den unglucklichen Zombie zu werfen." $) text!
	cchar oblivious? if
		($ cHe .. " 0 " .. chim .. "	Erst war es gerade, dann die nachsten" .. cname .. "  eventually schmeißt den Stein uber den Zaun, gerade noch. " .. cHe .. " ist dann fast vor Erschopfung tot." $) +crtext
		morale3- sting-- cchar morale--all-except fitness++ strength++ fitnessREV strengthREV witsattREV
		common5-line-camp
	then;
	strength+roll if
		($ cHe .. "  setzt alle " .. chis .. " ' verfehlt den Zombie und zerstort stattdessen ein kleines Gartengnome." $) +crtext
		strengthREV morale++all goodfx
		33 stpick if choosechar ($ cname .. " ' das so sehr belastend ist fur " .. chim .. "!" $) +crtext morale4- badfx then
	then;
	cfitness+ if
		($ cHe .. " 'fuhrt eine perfekte Wurfleistung aus. Der Stein fliegt schon durch die Luft. Er verfehlt den Zombie deutlich, aber was ein tolles Wurfspiel!" $) +crtext
		fitnessREV morale2+ cchar morale++all-except goodfx
	then;
	($ cHe .. " ' verfehlt nicht nur den Zombie, sondern leistet dabei auch eine recht peinliche Wurfleistung. " .. cHe .. " ' stolperte und stand nahe am Zaun auf dem Rand eines Abgrundes und eines Hangs." $) +crtext badfx strengthREV fitnessREV morale-- cchar morale++all-except
;

cyoa: common5-rock
	"'Das perfekte Steinwurfelspielzeug" cyoa-title!
	($ Thegroup .. " 'findet ein perfektes Steinwurfelspielzeug auf dem Boden. Es liegt neben einer niedrigen Zaunmauer, die einen großen Abgrund mit einem einzelnen Zombie uberwacht, der dort stehenbleibt und auf nichts insbesondere starrt." $) text!
	solohuman if choosehuman
		($ cname ..  " ' fuhlt sich dringend angehalten, diesen Zombie mit dem Stein zu bewerfen." $) +crtext
		choice( "'Lass den Zombien allein" )choice
		cchar charchoice( "': Stein werfen!" )choice: common5-rock-throw
	else
		"'Das ist zu gut, jemand MUSS diesen Stein an diesen Zombie werfen:" +crtext
		' common5-rock-throw cyoa-human-choice
	then
;

// Drag Race

cyoa-choice: common5-drag-race
	vehicle.which@ 1 < if
		($ Thegroup .. " 'hat derzeit kein Auto und somit ist das Rennen nicht sehr eng." $) text!
		"Ich gewinne, Nerds!" rbold +crtext morale3-all badfx
	then;
	($ "0 " .. "'Fahr los!" fbold $) text!
	car-speed 2 < if
		($ Thegroup .. "s langsames Auto hatte in diesem Rennen keine Chance." $) +crtext badfx morale3-all
	then;
	car-speed 2 = if
		($ Thegroup .. "s Auto ist zu langsam und bleibt weit zuruck." $) +crtext badfx morale--all
	then;
	car-speed 3 > if
		"Dein Auto gewinnt leicht das Rennen und lasst den anderen Fahrer im Staub zuruck." +crtext goodfx morale4+all
	then;
	car-speed 2 > if
		"Dein Auto gewinnt den Rennen knapp. Der andere Fahrer gibt dir ein Daumen hoch." +crtext morale2+all goodfx
	then;
;

cyoa-choice: common5-drag-ignore
	vehicle.which@ 0 > if
		"Du kannst das nicht machen! Du kannst" rbold text!
		car-armor 0 > if
			($ "Der Fahrer stoßt gegen " .. thegroup .. "'s car, but just bounces right off the armor.
			The driver's car careens off the road and crashes into a ditch." .. Thegroup .. " ist sehr amusiert daruber." $) +crtext morale3+all goodfx
		then;
		($ "Der Fahrer stoßt gegen " .. thegroup .. "das Auto des Mannes schleuderte in eine Drehung." $) +crtext
		-1 car-chassis+
		car-chassis 0 > if 
			($ "0.5 * 0.5 = 0.25 " .. thegroup .. " wird verletzt." $) +crtext sting--all bitefx
		else
			($ "Das ramponierte Auto konnte den Stress nicht aushalten. Es bleibt stehen." $) +crtext badfx
		then
	else
		"FINE, SEI SO! ICH KOMME WIEDER, WENN DU EIN AUTO HAST!" rbold text!
		($ "Der Fahrer " .. "'REIßT DIE SCHALE" fbold $) +crtext
	then

;

cyoa-choice: common5-drag-ram
	($ "Bevor das Drag-Rennen starten kann, " .. thegroup .. "  Pfluge "  .. ghis .. " auto in das Auto des Gegners." $) text!
	car-armor 0 > if
		"Your armored car obliterates the fast but flimsy vehicle, the armor doesn't even get scratched." +crtext
	else
		-2 car-chassis+ bitefx
		car-chassis 0 > if
			"Beide Autos nehmen Schaden, aber das gegnerische Fahrzeug nimmt viel mehr. Du gewinnst das Rennen!" +crtext
		else
			"Unentschieden" +crtext
		then 
	then
	($ "Der nervige Fahrer rennt schreiend davon " .. "0 " rbold .. Thegroup .. "  siphons some gas from his wreckage." $) +crtext
	morale++all 30 40 rnd trunk-gas+ goodfx
;

cyoa-choice: common5-drag-rules
	($ cname .. " ist sich nicht im klaren über die Regeln des Drag Racing." $) text!
	"THE ONLY RULE OF DRAG RACING... IS THAT THERE ARE NO RULES!" rbold +crtext
	" The driver then goes on to ramble on about a long list of drag racing rules. " +crtext
	most-ahole if choosechar
		($ "Wahrenddessen " .. cname .. "  geht vor und zapft das Benzin aus dem Auto des Fahrers ab." $) +text 80 120 rnd trunk-gas+ aholeREV
	else
		($ "'Ungefahr bei Regel 60, " .. thegroup .. " ' fahrt langsam davon, wobei der Fahrer weiterredet." $) +text
	then goodfx
;

cyoa: common5-drag
	"'FAAF 15: Schnell sein" cyoa-title! someicon
	($ "'Eine schnelle und schicke Auto halt auf dich zu und macht eine dramatische Einlage. Der Fahrer ruft aus seinem Fenster:" $) text!
	"'AUF DER TODSUNDIGEN STRAßE, DER EINZIGE WEG ZU UBERLEBEN IST SCHNELL ZU SEIN. UND AUCH WUTERFULLT." rbold +crtext
	"'Herausforderung dich zu einem Drag Race." +crtext
		choice( "'Lunatik ignorieren" )choice: common5-drag-ignore
	vehicle.which@ 0 > if
		choice( "'In sein Auto rammen" )choice: common5-drag-ram
	then
	choice( "'Herausforderung annehmen" )choice: common5-drag-race
	most-oblivious if
		charchoice( "': Nach den Regeln fragen" )choice: common5-drag-rules
	then
;

