
// EYEBALL Update events

cyoa{ road{ 

// Future Toilet, based on a true story

: common5-toilet-end ($ "Die Toilette der Zukunft gibt ein surrendes Gerausch von sich und hort auf zu funktionieren." $) +crtext goodfx ;

cyoa-choice: common5-toilet-water
	($ "Die Toilette spritzt eine enorme Menge Wasser aus einem versteckten Bidet. Dies bedeckt den Raum mit Wasser, aber das meiste davon trifft " .. cname .. " direkt in die Augen." $) text!
	morale--
	solo not if ($ Theothersarename .. " freut sich daruber " $) +crtext cchar ' .morale2+ foreach-team-char-except then
	cchar .shooting@ 6 < if ($ "Aber warte! Das Bidetwasser war medizinisch und angenehm duftend " .. cname .. "'wird scharfer!" $) +crtext shooting++ then
	most-hurt damage 0 > if ($ Thegroup .. " wird durch das medizinische Bidetwasser vollstandig geheilt!" $) +crtext ' .fullheal foreach-team-char then
	common5-toilet-end
;

cyoa-blank: common5-toilet-note
	"Lied von der Toilette" cyoa-title!
	"Die Toilette spielt schone Musik. Die Musik hat keine Worte, vermittelt aber eine starke und eindringliche Botschaft der Hoffnung." text!
	morale4+all common5-toilet-end
;

cyoa-choice: common5-toilet-wings
	"'Der Toilettendeckel offnet und schliet sich schnell, mit groen WHOOMP Gerauschen, wahrend der Deckel Luftstoe ausstot. Der Deckel offnet und schliet sich etwa 30 Mal hintereinander und stoppt dann." text!
	($ cname .. "  ist verwirrt uber den Zweck dieser Funktion." $) +crtext
	common5-toilet-end gstats{ 30 ' toilets-opened +! }
;

cyoa-choice: common5-toilet-butt
	"Der Toilettensitz fangt an zu summen und leuchtet leicht." text!
	($ cname .. "  setzt sich hin. Der elektronische Toilettensitz gibt " .. chim .. " " .. "*" pbold .. "  massage, wie die Toilette wiederholt in verschiedenen Sprachen singt:" $) +crtext
	"DIE GESAE SIND DER SCHLUSSEL ZUR FITNESS	DIE GESAE SIND DER SCHLUSSEL ZUR STARKE	DIE GESAE SIND DER SCHLUSSEL ZUR MACHT	DIE GESAE SIND DAS ZENTRUM DES LEBENS" bbold +crtext
	morale++ strength++ fitness++ common5-toilet-end
	"'*Wortschatzhinweis: Gluteal bedeutet 'Hintern'" pbold +crtext
;

cyoa: common5-toilet-rare
	"'Toilette der Zukunft der Vergangenheit" cyoa-title! choosehuman cicon
	($ Thegroup .. "  entdeckt etwas Unglaubliches wahrend er ein Haus erkundet: Eine tatsachlich funktionierende Toilette. Jeder hat sein Geschaft im Wald gemacht.	Die Toilette hat ein futuristisches Bedienfeld voller Knopfe. " .. cname .. "  fuhlt sich gezwungen, einen zu drucken, und wahlt den Knopf mit dem Bild von:" $) text!
		choice( "Wasserstrahlen?" )choice: common5-toilet-water
		choice( "Musiknoten" )choice: common5-toilet-note
		choice( "Flugel schlagen" )choice: common5-toilet-wings
		choice( "Ein Hinterteil?" )choice: common5-toilet-butt
;

// Long Line

: common5-line-camp ($ "Mit dem Tag vergeudet, " .. thegroup .. " muss auf Lager legen." $) +crtext +cr fullcamp 1 calendar-day+ ;
: common5-line-shop ($ "'Das Ladenl local ist ein trendiges hochpreisiges Lebensmittelgeschaft. Die teureren Artikel sind inzwischen verrottet, aber es gibt noch Beute.'," $) +crtext
	8 12 rnd trunk-food+ 4 6 rnd trunk-medical+ goodfx ;

cyoa-choice: common5-line-pick
	
	cdog? if
		($ cname .. "  rennt entlang der Reihe und uberholt sie vollstandig. Stunden spater kehrt " .. cname .. "  mit einem Stock in der Mund zuruck " .. chis .. " " $) text!
		1 WEAPON_TREEBRANCH trunk.weapon+
		morale2+ cchar morale--all-except goodfx
		common5-line-camp
	then;
	
	ccat? if
		($ cname .. " sturmt in den Laden, um die Schlange herum. " .. cHe .. "  tritt heraus, tragend ein totem Nagetier, vielleicht ein Hamster?," $) text!
		2 trunk-food+ morale2+ goodfx
	then;
	
	cpet? if
		($ cname .. "  liegt sich hin fur eine kleine Pause nieder. Als Tier " .. che .. " 'versteht es weder den Begriff einer Reihe, noch lasst es diesen vor.'," $) text!
		morale++ goodfx
	then;
	
	cchar oblivious? if
		($ cname .. " uberspringt die Zeile komplett " .. cHe .. " ich habe die Linie nicht bemerkt, oder die Zombies." $) text!
		witsREV attitudeREV common5-line-shop
	then;
	
	ctemper- if
		cloyalty- if
			($ cname .. " " .. cHe .. " spielt nicht nach den Regeln!" $) text!
			morale++ aholeREV common5-line-shop
		then;
		($ cname .. " dauert etwa eine Minute in der Schl " .. chis .. " " $) text!
		cstrength+ if
			"WARTEN IN DER SCHLANGE IST FUR NERDS!" rbold +crtext
			($ cHe .. " explodes. " .. che .. " wird mude und stolpert weg, schwitzend. Was fur ein Workout!" $) +crtext
			rageREV fitness++ fitnessREV morale++ goodfx
		then;
		($ cHe .. "  greift den Zombie vor sich an " .. chim .. ". Die anderen Zombies bemerken es schlielich und greifen an " .. cname .. "  muss fliehen!" $) +crtext
		sting-- bitefx temperREV 
	then;
	
	ctemper+ if
		($ cname .. "  wartet geduldig in der langen Schlange. Sie bewegt sich nur sehr langsam, aber " .. che .. "  wartet ruhig ab." $) text!
		temperREV 
		common5-line-shop common5-line-camp
	then;
	
	($ cname .. "  wartet in der Schlange. Sie bewegt sich nur Zentimeterweise, zu " .. cname .. "''s groem Arger." $) text!
	attitude+roll if 
		($ cname .. "  versucht positiv zu denken " .. cHe .. "  schafft es durchzuhalten und in den Laden zu gelangen!" $) +crtext
		attitudeREV morale-- common5-line-shop
	else
		($ "Nach Stunden greifen die Zombies " .. cname .. "  wegen " .. chis .. "  standigem Gejammer an " .. cHe .. "  muss fliehen!" $) +crtext
		morale2- sting-- bitefx 
	then temperREV common5-line-camp
;

cyoa-bridge: common5-line-pick4
	"Wahle aus wer in der Zombie Schlange warten darf:" text!
	' common5-line-pick cyoa-team-choice 
;

cyoa-blank: common5-line
	"Immer noch in der Warteschlange" cyoa-title!
	($ Thegroup .. " ' trifft auf eine Horde Zombies. Sie sind vollig gefugig und warten nur in einer groen, einreihigen Schlange vor einem Geschaft. Jeder Zombie starrt geradeaus." $) text!
		choice( "Diese Verrucktheit ignorieren" )choice
	solo if
		"'Das Schild des Ladens ist verblasst und nicht mehr lesbar." +crtext
		soloer charchoice( ": In der Schlange warten" )choice: common5-line-pick
	then;
	"Du kannst jemanden schicken, der sich in die Schlange stellt, um dieses Ratsel zu losen. Es sieht so aus, als konnte es eine lange Wartezeit werden." +crtext
	team-count 3 > if
		' common5-line-pick 2 #cyoa-team-choice
		choice( "Wahle jemand anderen" )choice: common5-line-pick4
	else
		' common5-line-pick 3 #cyoa-team-choice
	then	
;

// Rock throwing

cyoa-choice: common5-rock-throw
	($ cname .. "  ist gezwungen einen Stein auf den unglucklichen Zombie zu werfen." $) text!
	cchar oblivious? if
		($ cHe .. "  wirft den Stein immer wieder versehentlich so dass er zu " .. chim .. " zuruckkehrt. Erst warf er ihn geradeaus, dann traf er beim nachsten Mal den Zaun. Das geht mehrere Stunden so weiter.		" .. cname .. "  wirft den Stein schlielich knapp uber den Zaun " .. cHe .. "  ist zu diesem Zeitpunkt fast tot vor Erschopfung." $) +crtext
		morale3- sting-- cchar morale--all-except fitness++ strength++ fitnessREV strengthREV witsattREV
		common5-line-camp
	then;
	strength+roll if
		($ cHe .. "  setzt all " .. chis .. "  kraft in einen machtigen Wurf nach unten. Der Stein verfehlt den Zombie, vernichtet aber einen kleinen Gartenzwerg." $) +crtext
		strengthREV morale++all goodfx
		33 stpick if choosechar ($ cname .. "  erkennt dass dies " .. chim .. "!" $) +crtext morale4- badfx then
	then;
	cfitness+ if
		($ cHe .. "  fuhrt einen Wurf mit perfekter Form aus. Der Stein segelt anmutig durch die Luft. Er verfehlt den Zombie bei weitem, aber was fur ein schoner Wurf!" $) +crtext
		fitnessREV morale2+ cchar morale++all-except goodfx
	then;
	($ cHe .. "  verfehlt nicht nur, sondern fuhrt auch einen wirklich peinlichen Wurf aus. " .. cHe .. "  stolpert und fallt fast uber den Zaun und die Klippe hinunter." $) +crtext badfx strengthREV fitnessREV morale-- cchar morale++all-except
;

cyoa: common5-rock
	"Der perfekte Stein" cyoa-title!
	($ Thegroup .. "  findet einen perfekten Wurffelsen auf dem Boden. Er befindet sich neben einem niedrigen Zaun, der einen groen Sturz bewacht. Unten an diesem Abgrund steht ein einziger Zombie, der dort steht und ins Leere starrt." $) text!
	solohuman if choosehuman
		($ cname ..  "  verspurt ein brennendes Verlangen den Stein auf diesen Zombie zu werfen." $) +crtext
		choice( "Zombie in Ruhe lassen" )choice
		cchar charchoice( ": Stein werfen!" )choice: common5-rock-throw
	else
		"'Das ist zu schon um es sich entgehen zu lassen. Jemand MUSS diesen Stein auf diesen Zombie werfen:" +crtext
		' common5-rock-throw cyoa-human-choice
	then
;

// Drag Race

cyoa-choice: common5-drag-race
	vehicle.which@ 1 < if
		($ Thegroup .. " ' hat gerade kein Auto, also ist es kein sehr knappes Rennen." $) text!
		"ICH GEWINNE, NERDS!" rbold +crtext morale3-all badfx
	then;
	($ "Das Rennen beginnt! Beide Fahrer " .. "BELASTIGE ES!" fbold $) text!
	car-speed 2 < if
		($ Thegroup .. "''s langsames Auto hatte in diesem Rennen uberhaupt keine Chance." $) +crtext badfx morale3-all
	then;
	car-speed 2 = if
		($ Thegroup .. "''s Auto ist zu langsam und bleibt weit zuruck." $) +crtext badfx morale--all
	then;
	car-speed 3 > if
		"Dein Auto gewinnt das Rennen locker und lasst den anderen Fahrer im Staub zuruck." +crtext goodfx morale4+all
	then;
	car-speed 2 > if
		"Dein Auto gewinnt das Rennen nur knapp. Der andere Fahrer gibt einen Daumen hoch." +crtext morale2+all goodfx
	then;
;

cyoa-choice: common5-drag-ignore
	vehicle.which@ 0 > if
		"'DAS KANNST DU NICHT TUN! DU KANNST EINE DRAG RACE HERAUSFORDERUNG NICHT IGNORIEREN!" rbold text!
		car-armor 0 > if
			($ "Der Fahrer stot gegen " .. thegroup .. "'s car, but just bounces right off the armor.
			The driver's car careens off the road and crashes into a ditch." .. Thegroup .. "  ist sehr amusiert daruber." $) +crtext morale3+all goodfx
		then;
		($ "Der Fahrer stot gegen " .. thegroup .. "'s Auto und bringt es ins Schleudern." $) +crtext
		-1 car-chassis+
		car-chassis 0 > if 
			($ "Das Auto nimmt leichten Schaden und " .. thegroup .. "  wird verletzt." $) +crtext sting--all bitefx
		else
			($ "'Das ramponierte Auto konnte dieser Belastung nicht standhalten. Es bleibt stehen." $) +crtext badfx
		then
	else
		"'SCHON, SEI SO! ICH KOMME WIEDER, WENN DU EIN AUTO HAST!" rbold text!
		($ "Der Fahrer " .. "FAHRT LOS!" fbold $) +crtext
	then

;

cyoa-choice: common5-drag-ram
	($ "Bevor das Drag Race beginnen kann, " .. thegroup .. "  pflugt "  .. ghis .. " ' Auto in das Auto des Gegners." $) text!
	car-armor 0 > if
		"'Ihr Panzerwagen vernichtet das schnelle, aber schwache Fahrzeug, die Panzerung wird nicht einmal zerkratzt." +crtext
	else
		-2 car-chassis+ bitefx
		car-chassis 0 > if
			"'Beide Autos erleiden Schaden, aber das Fahrzeug des Gegners erleidet viel mehr. Du gewinnst das Rennen!" +crtext
		else
			"Beide Autos sind so stark beschadigt dass sie nicht mehr fahren konnen. Das macht das Rennen zu einem Unentschieden!" +crtext
		then 
	then
	($ "Der nervige Fahrer rennt weg und schreit " .. "BETRUGER! " rbold .. Thegroup .. "  zapft etwas Gas aus seinem Wrack ab." $) +crtext
	morale++all 30 40 rnd trunk-gas+ goodfx
;

cyoa-choice: common5-drag-rules
	($ cname .. " ' ist sich uber die Regeln des Drag Racing nicht im Klaren." $) text!
	"THE ONLY RULE OF DRAG RACING... IS THAT THERE ARE NO RULES!" rbold +crtext
	"Der Fahrer faselt dann uber eine lange Liste von Drag Racing Regeln. " +crtext
	most-ahole if choosechar
		($ "Wahrenddessen, " .. cname .. " ' geht hin und zapft das gesamte Benzin aus dem Auto des Fahrers ab." $) +text 80 120 rnd trunk-gas+ aholeREV
	else
		($ "Etwa bei Regel 60, " .. thegroup .. "  fahrt langsam weg wahrend der Fahrer weiter redet." $) +text
	then goodfx
;

cyoa: common5-drag
	"Deutsch sein" cyoa-title! someicon
	($ "Ein schneller und schicker Wagen halt vor dir dramatisch an. Der Fahrer ruft aus seinem Fenster:" $) text!
	"AUF DER TODESSTRASSE IST DER EINZIGE WEG ZU UBERLEBEN, SCHNELL ZU SEIN. UND AUCH WUTEND." rbold +crtext
	"Er fordert dich zu einem Autorennen heraus." +crtext
		choice( "Ignoriere den Wahnsinnigen" )choice: common5-drag-ignore
	vehicle.which@ 0 > if
		choice( "Ramme sein Auto" )choice: common5-drag-ram
	then
	choice( "Nimm die Herausforderung an" )choice: common5-drag-race
	most-oblivious if
		charchoice( ": Frage nach den Regeln" )choice: common5-drag-rules
	then
;

// Radio Station

cyoa-choice: common5-radiodj-siphon
	($ Thegroup .. " ' zapft das Benzin aus dem Generator ab. Es war nicht mehr viel ubrig, aber jedes bisschen hilft.	Der Radiosender wird abgeschaltet." $) text! 15 30 rnd trunk-gas+ goodfx
;

0 value DJ-PICK

cyoa-choice: common5-radiodj-pick

	// This should be expanded later for lots of possible stuff

	cdog? if
		($ cname .. "  sagt etwas in der Art von " .. "BELLEN!" rbold .. "  oder vielleicht " .. "BORK?" rbold .. "'		Hoffentlich hat das viele Leute verwirrt die Radio horen." $) text!
		morale++ goodfx
	then;
	
	cpet? if
		($ cname .. "  greift das Mikrofon an!		Das Mikrofon ist zerstort!		" .. cname .. "  ist sehr erfreut diese Bedrohung losgeworden zu sein." $) text! bitefx morale2+ strengthREV fitnessREV
	then;
	
	cchar oblivious? if
		($ cname .. "  schaltet versehentlich das Mikrofon ein und platzt dann viele peinliche personliche Fakten heraus. Dazu gehoren einige nutzliche Informationen fur Banditen, wie z. B. Standort und aktuelle Menge an Vorraten." $) text!
		witsattREV cchar morale2-all-except
	then;
	
	cchar grating? if
		($ cname .. "  schreit wiederholt " .. "COOL IT!" fbold .. "  ins Mikrofon. Nach elf dieser Schreie fallt die Funkausrustung aus und der Generator brennt." $) text! witsattREV morale++ cchar morale2-all-except 11 to cool-it
	then;
	
	cchar paranoid? if
		($ cname .. "  sendet eine lange, fast atemlose Tirade daruber, wie die Zombie Apokalypse von der Regierung geschaffen wurde. Das Projekt trug den Codenamen " .. "PROJEKT: DINGUS" bbold .. "  und das angestrebte Ziel war es als Deckmantel fur die Herstellung von Jorts zu dienen" .. 
		"*" rbold .. "  illegal. ENDLICH wurde die Wahrheit enthullt!" $) text! witsattREV morale3+ cchar morale--all-except
		"'* Hinweis: Jorts bedeutet 'Jeansshorts'." rbold +crtext
	then;
	
	cloyalty- if
		($ cname .. "  sendet eine hilfreiche Nachricht:" $) text!
		cchar ahole? if
			"CIVILIZATION HAS ENDED!
			LIE, CHEAT, STEAL GAS FROM TOILETS!
			HOMEWORK SUCKS!!!" rbold +crtext
			"Die letzte Zeile dieser Tirade wird so laut geschrien dass das Mikrofon Ruckkopplungen aussendet." +crtext
			aholeREV morale2+
			cchar ' .loyalty-- foreach-team-char-except
		else
			"EVERYONE BUT ME
			... IS A NERD!" rbold +crtext
			loyaltyREV morale++
		then cchar morale--all-except
	then;

	cchar .hiskill-stack depths 0= if 0 else shuffles pops then to DJ-PICK drop
	
	DJ-PICK STAT_MEDICAL = if
		cchar .medical 3 > if
			($ cname .. "  wei dass " .. chis .. "  botschaft Leben retten kann, und rasselt verzweifelt so viel praktisches medizinisches Wissen herunter wie " .. che .. " can in a short time.
			Maybe this can help someone." $) text!
			medicalREV morale++
			' .medical++ foreach-team-char
		then;
		cchar .medical 3 < if
			($ cname .. "  stolz so viel medizinisches Wissen ausspuckt wie " .. che .. " can in a short time.
			It's all really bad advice that makes anyone that hears it actually worse off." $) text!
			medicalREV morale2+
			cchar ' .medical-- foreach-team-char-except
		then; medicalREV
	then
	
	DJ-PICK STAT_MECHANICAL = if
		cchar .mechanical 3 > if
			($ cname .. " broadcasts a short impromptu talk show that is a crash course in car repair. It's informative and entertaining.
			Maybe this can help someone." $) text!
			mechanicREV morale++
			' .mechanic++ foreach-team-char
		then;
		cchar .mechanical 3 < if
			($ cname .. " rambles on about repairing sweet cars and 'crafting level 3 lighting-flame-chainsaws' until the power runs out.
			Anyone that hears this actually gets worse at repairing things." $) text!
			mechanicREV morale2+
			cchar ' .mechanic-- foreach-team-char-except
		then; mechanicREV
	then
	
	DJ-PICK STAT_FITNESS = if
		cfitness+ if
			($ cname .. " creates an intense radio calisthenics show. Whoever is listening joins in with the exercises.
			The total combined sweat from this could fill up a bathtub." $) text!
			fitnessREV morale2+
			' .fitness++ foreach-team-char
		then;
	then
	
	DJ-PICK STAT_SHOOTING = if
		cshooting+ if
			($ cname .. " broadcasts some very effective tips and theories on zombie shooting.
			During the show, " .. cHe .. "  kommt mit neuen Trick Schusse auf." $) text!
			shootingREV morale++
			' .shooting++ foreach-team-char		
		then;	
	then

	DJ-PICK STAT_STRENGTH = if
		cstrength+ if
			($ cname .. "  ein Gedicht vorliest das " .. che .. "  daruber schrieb wie sehr " .. che .. " loves being strong.
			The Four Spirits of Swoleness hear this sweet poem, and they confer their blessing!" $) text!
			strengthREV morale++
			' .strength++ foreach-team-char		
		then;	
	then
	
	cchar charming? if
		($ cname .. " ' nichts mehr ausdenken kann, also " .. che .. " ' singt ein Lied.		Es ist eine groartige Leistung, sicherlich ein neuer Post Zombocalypse Hit Single!" $) text! witsattREV morale2+all
	then;
		
	($ cname .. " ' nichts aufzudenken hat und auch dann still bleibt, wenn der Strom aus." $) text! badfx morale--
;

