	somepet if choosechar then
	($ vname .. "  realmente enfia " .. vhis .. "  nariz em " .. cname .. "  e bufa.	Isso provoca um bom espirro!" $) text! goodfx
;

cyoa-choice: common4-sneeze-spicy
	($ cname .. "  abre um saco de batatas fritas super picantes. " .. cHe .. "  da uma cheirada forte." $) text!
	($ cname .. "  espirra acidentalmente no saco! 	" .. cHe .. "  tambem pode comer o saco inteiro de batatas fritas agora." $) +crtext
	-2 trunk-food+
	morale++
;

cyoa: common4-sneeze
	"O ESPIRRO PERDIDO" cyoa-title! choosehuman cicon
	($ cname .. "  realmente sente que " .. che .. " ' tem que espirrar. Esse sentimento se arrasta, mas nao ha espirro. Isso pode se transformar em uma daquelas vezes em que um espirro e perdido! Combinado com as pressoes do apocalipse zumbi, isso pode quebrar " .. cname .. "''s moral!" $) text!
		choice( "Tente espirrar" )choice: common4-sneeze-try
		choice( "Esmague se no nariz" )choice: common4-sneeze-smash
		numpets 0 > if choice( "Farejar pelos de animais" )choice: common4-sneeze-animal then
		trunk-food 1 > if choice( "Farejar comida picante   Custa 2 alimentos" )choice: common4-sneeze-spicy then
