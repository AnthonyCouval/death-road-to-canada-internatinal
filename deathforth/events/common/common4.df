// Old VHS tape

: common4-vhs-end
	morale++all
	($ "A TV da um curtocircuito apos o filme e e deixada para tras." +crtext
;

: sumo-vhs args( char )
	<- char ispet? if 
		<- char dog? if
			($ <- char .name@ .. "  aprende sumo, apesar de ser um cachorro." $) +crtext
			WEAPON_SUMO <- char .weapon_default!
		then
	then;	
	<- char .weapon_default@ WEAPON_SUMO = if
		($ <- char .name@ .. "  aprende tecnicas superiores de sumo!" $) +crtext
		WEAPON_SUPERSUMO <- char .weapon_default!
	else WEAPON_SUMO <- char .weapon_default! then
;

cyoa-blank: common4-vhs-sumo
	"O Sumo estava entre os 5 principais esportes nos Estados Unidos antes do zombocalipse. Isso levou a muitos filmes de sumo, dramas de TV de sumo, desenhos animados de sumo, e assim por diante." text!
	($ Thegroup .. "  pega alguns movimentos de sumo assistindo ao filme!" $) +crtext
	' sumo-vhs foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-shoot
	($ "A fita de video e uma adaptacao cinematografica do jogo de arcade " .. "DEATHSPLOSIONAL!" fbold .. " ' E melhor do que nenhum filme." $) text!
	
	' .shooting++ foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-fit
	($ Thegroup .. " segue o instrutor de ginastica no video. Todos estao usando faixas de suor neon." $) text!
	
	' .fitness++ foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-morale
	($ "0.0 " .. thegroup .. "  is completely absorbed by it. It's very rare to see a movie anymore." $) text!	morale3+all
	"A TV queima durante a metade da segunda visualizacao, e nunca mais volta a funcionar." +crtext
;

