
(*

- ouija board event "Weegee"
	- Pick a question to ask
	- Different if solo
		- AM I PRETTY? [yes] Morale Boost
		- WHERE'S THE LOOT? Shows hidden supplies nearby
		- 

	
*)

cyoa{ road{ 

// Not a Ouija Board

: weegeetm .. "Tabuleiro Weegee [tm]" pbold .. ;
: weegeecurse args( char ) 13 <- char .seed! ;

: weegee-end
	($ Thegroup .. "  agora se sente incrivelmente azarado. Cuidado com a " .. "Maldicao de Weegee!" rbold $) +crtext
	"SORTE MASSIVAMENTE DIMINUIDA!" fbold +crtext
	"Em algum lugar distante, um goblin ri." pbold +crtext goblin-laugh
	
;

cyoa-choice: common4-weegee-pretty
	choosehuman cicon
	($ "O " weegeetm "  soletra que " .. cname .. "  e " .. "	MUITO BONITO" pbold $) text!
	($ "Legal! " .. cname .. " ' Nao se sente amaldicoado por este adoravel Weegee." $) +crtext
	morale2+
	"Somewhere in the distance, a goblin laughs, but in a nice way. As if to say 'hey.... what's up. have a nice day.'" pbold +crtext goblin-laugh
	choice( "legal" )choice
;

cyoa-choice: common4-weegee-loot
	($ "O " weegeetm "  soletra a localizacao de um esconderijo de saque na casa!" $) text!
	homesearch1
	weegee-end
;

cyoa-choice: common4-weegee-check
	weaselest choosechar cicon
	($ "Quando perguntado quem vai trair o grupo, o " weegeetm "  soletra " $) text!
	weaselest-pass if
		($ cname .. "'nome!" $) +text
		loyaltyREV
		weegee-end
	else
		"Y O U R B U T T" pbold +crtext
		($ "O que isso pode significar? O grupo fica inquieto e a duvida aumenta. Poderia ser a " .. "Maldicao de Weegee?" rbold $) +crtext
		morale--all goblin-laugh
	then
;

cyoa-choice: common4-weegee-canada
	($ "O " weegeetm "  aponta para a palavra " .. "NAO" pbold .. "  com velocidade incrivel. Entao aponta para " .. "NAO" fbold .. "  novamente, enquanto de alguma forma pega fogo." $) text!
	($ "Tanto faz! Essas coisas estao sempre erradas, de qualquer maneira! " .. cname .. "  joga o " weegeetm "  em chamas do outro lado da sala. Isso faz " .. chim .. " tambem pega fogo." $) +crtext
	cchar isfireproof? if 
		($ cname .. " adora ser a prova de fogo!" $) +crtext
		morale++ goodfx
	else
		ouch-- alive? if bitefx then
	then
	alive? if
		"Foi um bom exercicio, pelo menos." +crtext fitness++
	else
		($ "'Nao mexa com o " .. "Maldicao de Weegee!" rbold $) +crtext goblin-laugh
	then
	
;

cyoa: common4-weegee
	"Weegee Board" cyoa-title! choosehuman cicon
	($ Thegroup .. " esta vasculhando uma casa abandonada e encontra um " weegeetm " 'na mesa da cozinha. E um jogo comum usado para se comunicar com " .. "FANTASMAS" fbold $) text!
		choice( "Deixe esse ABSURDO de lado" )choice
		($ "Dizem que essas coisas sao " .. "INCRIVELMENTE AMALDICOADAS COM MA SORTE" rbold .. ", so beware! Still, it may be worth asking it a question..." $) +crtext
		solohuman if
			choice( "EU SOU BONITA?" )choice: common4-weegee-pretty
		else
			choice( "QUEM VAI TRAIR O TIME?" )choice: common4-weegee-check
		then
			choice( "VONTADE" .. I-or-WE .. "FAZER ISSO NO CANADA?" )choice: common4-weegee-canada
			choice( "'ONDE ESTA O BOTIM?" )choice: common4-weegee-loot
;

// Dog Poop

: common4-dogpoo-trueend 16 trunk-food+ goblin-laugh ;
: common4-dogpoo-reward 16 trunk-food+ badfx ;

cyoa-choice: common4-dogpoo-ignore
	($ Thegroup .. "  ignora a caixa de suprimentos e toda a comida dentro dela." .. gThey .. "  encontrar" .. g1s .. "  algumas latas de comida enquanto vasculha o resto da casa, mas deve se perguntar o que " .. gthey .. "  perdeu." $) text!
	2 trunk-food+ goodfx
;

cyoa-choice: common4-dogpoo-pick

	cdog? if
		($ "O coco do cachorro desaparece misteriosamente antes de " .. cname .. "  cutuca a caixa para abri la!" .. cname .. "  parece realmente culpado." $) text!
		morale--
		common4-dogpoo-reward
	then;
	ccat? if
		($ cname .. "  parece especialmente irritado por ter que afastar o coco do cachorro para abrir a caixa. " .. cHe .. "  realmente sente vontade de machucar alguma coisa, mais do que o normal." $) text!
		morale4-
		common4-dogpoo-reward
	then;
	cpet? if
		($ cname .. "  chuta a caixa de suprimentos para abri la. Como um animal, " .. che .. " ' realmente nao se importa com coco de cachorro, entao nao foi um grande obstaculo." $) text!
		16 trunk-food+ goodfx
	then;
	dogpal? if
		($ cname .. "  adora cachorros e nao e estranho pegar coco de cachorro e coloca lo em um saco plastico. Na verdade, isso lembra " .. cname .. "  da civilizacao. Sentindo aquele calor na sua mao, sentindo se vivo!" $) text!
		morale2+ 16 trunk-food+ goodfx
	then;
	cchar oblivious? if
	($ cname .. "  abre a caixa de suprimentos. O que era tao especial?		" .. cHe .. "  nunca percebeu o fezes do cao." $) text!
		solo if
			goodfx
		else
			($ Everyone .. "  se envergonha!" $) +crtext cchar morale2-all-except badfx
		then 16 trunk-food+
	then;
	($ cname .. "  tenta empurrar o fezes fora da caixa com uma peca de madeira, mas isso vai TERRIBILMENTE errado. " .. cHe .. "  se envergonha muito" $) text!
		cattitude+ if
			badfx
			($ cHe .. "  tentar manter um atitude positiva. Um dia, " .. cname .. "  podera olhar para isso e rir. Mas hoje nao." $)
		else
			($ "Isso e, sem duvida, a pior coisa que aconteceu com " .. cname .. "  no apocalipse zombie. " .. cHe .. "  nunca sera o mesmo." $) +crtext
			($ cname .. ":" pbold .. "  PENALIDADE MORALE PERMANENTE!" fbold $)
			-1 STAT_MORALE cchar .bonus+ sfx{ creepy }
		then +crtext morale2- 16 trunk-food+
	
;

cyoa-bridge: common4-dogpoo-stick
	"VOCE GANHOU" cyoa-title! end-music
	"'Toda crianca sabe que o unico jeito infalivel de mover um pedaco de coco de cachorro e usar um galho de arvore. E o removedor de coco perfeito da natureza. Voce canalizou seu senso de fantasia e admiracao infantil para triunfar sobre essa situacao perigosa." text!
	"Parabens!" rainbold +crtext
	"'VOCE ENCONTROU O VERDADEIRO FINAL PARA DEATH ROAD TO CANADA!" fbold +crtext
	choice( "Eu consegui!" )choice: common4-dogpoo-trueend
;

cyoa-bridge: common4-dogpoo-open
	solo if
		soloer choosechar common4-dogpoo-pick
	then;
	($ "Quem deve abrir a caixa?" .. "		AVISO: ISSO E REALMENTE NOJENTO		HA UM COCO DE CACHORRO NA CAIXA" rbold .. "		ISSO PODE SER UM GRANDE PROBLEMA" fbold $) text!
	' common4-dogpoo-pick cyoa-team-choice
;

cyoa: common4-dogpoo
	"VERDADEIRO DESESPERO" cyoa-title! picon!
	($ "Como " .. thegroup .. "  procura uma casa abandonada, um milagre acontece. " .. gThey .. "  encontrar" .. g1s .. "  uma caixa de racao do governo, fechada." $) text!
	($ "Para " .. ghis .. "  HORROR" rbold .. ", acontece que ha um " .. "coco de cachorro" fbold .. "  sentado " .. " BEM NA MACANETA" rbold .. "  da caixa de suprimentos." .. "  Nao tem como abrir sem encostar no coco." rbold $) +crtext
	WEAPON_TREEBRANCH roadtrip-has-weapon? not if
		" Este pode ser o maior desafio ate agora!" +crtext
	then
		choice( " De jeito nenhum, nojento, ignora" )choice: common4-dogpoo-ignore
		solo if
			soloer charchoice( "  abre" )choice: common4-dogpoo-open
		else
			choice( " Pede para alguem abrir" )choice: common4-dogpoo-open
		then
		WEAPON_TREEBRANCH roadtrip-has-weapon? if
			choice( " Usa galho de arvore para abrir" )choice: common4-dogpoo-stick
		then
;

// Losing a sneeze

