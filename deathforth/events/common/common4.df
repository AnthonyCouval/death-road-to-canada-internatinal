	solohuman if
		choosehuman cicon
		($ cname .. " tera que alimentar as notas rasgadas e enrugadas na maquina de vending. Isso sera molesto." $) text!
		cchar charchoice( ": Coloque as notas" )choice: common4-vending-go
		choice( "Deixeo sozinho" )choice
	then;
	($ "Quem deve alimentar as notas quase roladas em maquina de vending?" $) cyoa-text!
	"AVISO: Isso sera um processo muito molesto" rbold +crtext
	' common4-vending-go cyoa-human-choice
	choice( "Deixe de lado apos tudo" )choice
;

: common4-vending-1 1 to common4-vending-num common4-vending-pick ;
: common4-vending-2 2 to common4-vending-num common4-vending-pick ;
: common4-vending-3 3 to common4-vending-num common4-vending-pick ;
: common4-vending-4 4 to common4-vending-num common4-vending-pick ;
: common4-vending-5 5 to common4-vending-num common4-vending-pick ;
: common4-vending-6 6 to common4-vending-num common4-vending-pick ;
: common4-vending-7 7 to common4-vending-num common4-vending-pick ;
: common4-vending-8 8 to common4-vending-num common4-vending-pick ;
: common4-vending-9 9 to common4-vending-num common4-vending-pick ;

cyoa: common4-vending
	"Maquina de Vending Estranha" cyoa-title! someicon
	($ Thegroup .. " corre contra uma maquina de vending muito fora de lugar. Ela parece ter sido feita de pedacos de ferro de lixo. Suas luzes ainda estao acesas.	Tudo que custa na maquina e de $20, e " .. thegroup .. "  tem aproximadamente isso em notas rasgadas e enrugadas." $) text!
		choice( "Deixeo sozinho" )choice
		
		33 %chance if
			choice( "PAQUETE DE DIVERSaO DE CANADEIRO" )choice: common4-vending-1
		else
			50 %chance if
				choice( "CACADARATOS TOOTEM RIFLE" )choice: common4-vending-2
			else
				choice( "CONJUNTO REAL DE REVOLVER" )choice: common4-vending-3
			then
		then
		
		33 %chance if
			choice( "SACOLA DE BATATAS FRITAS" )choice: common4-vending-4
		else
			50 %chance if
				choice( "LATA DE GASOLINA" )choice: common4-vending-5
			else
				choice( "'CAIXA DE BANDAGENS" )choice: common4-vending-6
			then
		then
		
		10 %chance if
			choice( "UM CARRO NOVO!" )choice: common4-vending-7
		else
			50 %chance if
				choice( "UM PAU" )choice: common4-vending-8
			else
				choice( "UM CHAPEU NOVO" )choice: common4-vending-9
			then
		then		
;

} }

