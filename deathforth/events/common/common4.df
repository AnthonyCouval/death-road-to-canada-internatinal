// Old VHS tape

: common4-vhs-end
	morale++all
	($ "'Der Fernseher streikt nach dem Film und bleibt zuruck." +crtext
;

: sumo-vhs args( char )
	<- char ispet? if 
		<- char dog? if
			($ <- char .name@ .. " 'lernt Sumo, obwohl es ein Hund ist." $) +crtext
			WEAPON_SUMO <- char .weapon_default!
		then
	then;	
	<- char .weapon_default@ WEAPON_SUMO = if
		($ <- char .name@ .. " 'lernt fortschrittliche Sumotechniken!" $) +crtext
		WEAPON_SUPERSUMO <- char .weapon_default!
	else WEAPON_SUMO <- char .weapon_default! then
;

cyoa-blank: common4-vhs-sumo
	"'Sumo-Ringen war in Amerika vor der Zombie-Apokalypse unter den Top 5 Sportarten. Das fuhrte zu vielen Sumo-Filmen, Sumo-Fernsehserien, Sumo-Cartoons und so weiter." text!
	($ Thegroup .. " 'holt sich einige Sumo-Bewegungen durch das Anschauen des Films." $) +crtext
	' sumo-vhs foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-shoot
	($ "'Die Videokassette ist eine Filmadaption des Arcade-Spiels " .. "'DEATHSPLOSIONAL!" fbold .. " 'Es ist besser als kein Film uberhaupt." $) text!
	
	' .shooting++ foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-fit
	($ Thegroup .. " 'folgt dem Fitness-Trainer auf der Videokassette. Alle tragen Neonschweißbander." $) text!
	
	' .fitness++ foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-morale
	($ "Der Comedy-Film erweist sich als schrecklich nach den meisten Maßstaben, aber " .. thegroup .. " es wird immer" $) text!	morale3+all
	"'Der Fernseher schaltet sich wahrend der zweiten Wiederholung ab und kehrt nie wieder auf.	" +crtext
;

cyoa: common4-vhs-rare
	"'Video-Kassenspieler" cyoa-title!
	($ Thegroup .. " 'findet jemandes verlassenes Versteck in einem versperrten Haus.	Das ist hier nicht viel ubrig geblieben, außer einer kleinen batteriebetriebenen Fernsehanlage angebunden an ein alter Videokassenspieler.	Es gibt nur noch eine Videokassette im Videotape-Player:" $) text!
