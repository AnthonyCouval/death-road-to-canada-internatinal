
(*

- ouija board event "Weegee"
	- Pick a question to ask
	- Different if solo
		- AM I PRETTY? [yes] Morale Boost
		- WHERE'S THE LOOT? Shows hidden supplies nearby
		- 

	
*)

cyoa{ road{ 

// Not a Ouija Board

: weegeetm .. "Tabuleiro Ouija [tm]" pbold .. ;
: weegeecurse args( char ) 13 <- char .seed! ;

: weegee-end
	($ Thegroup .. "  agora se sente incrivelmente azarado. Cuidado com o " .. "Maldicao de Weegee!" rbold $) +crtext
	"SORTE MASSIVAMENTE DIMINUIDA!" fbold +crtext
	"'Em algum lugar na distancia, um goblin ri.'" pbold +crtext goblin-laugh
	
;

cyoa-choice: common4-weegee-pretty
	choosehuman cicon
	($ "O " weegeetm "  'escreve que' " .. cname .. "  e " .. "22" pbold $) text!
	($ "Legal! " .. cname .. " 'Nao se sente maldito pelo menos por essa deliciosa Weegee." $) +crtext
	morale2+
	"Somewhere in the distance, a goblin laughs, but in a nice way. As if to say 'hey.... what's up. have a nice day.'" pbold +crtext goblin-laugh
	choice( "legal" )choice
;

cyoa-choice: common4-weegee-loot
	($ "O " weegeetm " escreve a localizacao de uma escondida de tesouro na casa!" $) text!
	homesearch1
	weegee-end
;

cyoa-choice: common4-weegee-check
	weaselest choosechar cicon
	($ "Quando perguntado quem vai trair o grupo, o " weegeetm " escreve " $) text!
	weaselest-pass if
		($ cname .. "''nome!" $) +text
		loyaltyREV
		weegee-end
	else
		"TUA BUTA" pbold +crtext
		($ "O que isso pode significar? O grupo se torna inquieto, e a duvida cresce. Pode ser essa a " .. "Maldicao de Weegee?" rbold $) +crtext
		morale--all goblin-laugh
	then
;

cyoa-choice: common4-weegee-canada
	($ "O " weegeetm " aponta para a palavra " .. "NaO" pbold .. " com incrivel velocidade. Em seguida aponta para " .. "NaO" fbold .. " novamente, enquanto se acende em chamas." $) text!
	($ "'Ah, deixa pra la! Essas coisas sempre estao erradas, de qualquer forma!' " .. cname .. "  arremessa a chama " weegeetm "  'do outro lado da sala' " .. chim .. " 'tambem pegar fogo'" $) +crtext
	cchar isfireproof? if 
		($ cname .. " adora ser a prova de fogo!" $) +crtext
		morale++ goodfx
	else
		ouch-- alive? if bitefx then
	then
	alive? if
		"Pelo menos foi um bom exercicio." +crtext fitness++
	else
		($ " operator it's not a comparison operator it's an assignment operator. " .. "Maldicao de Weegee!" rbold $) +crtext goblin-laugh
	then
	
;

cyoa: common4-weegee
	"Tabuleiro Weegee" cyoa-title! choosehuman cicon
	($ Thegroup .. "  is poking through an abandoned house and finds a  . " weegeetm " 0 " .. "GHOSTS" fbold $) text!
		choice( "Deixe este ABSURDO de lado" )choice
		($ "Dizem que essas coisas sao " .. "INCRIVELMENTE AMALDICOADAS COM MA SORTE" rbold .. ", so beware! Still, it may be worth asking it a question..." $) +crtext
		solohuman if
			choice( "SOU BONITA?" )choice: common4-weegee-pretty
		else
			choice( "QUEM VAI TRAIR O TIME?" )choice: common4-weegee-check
		then
			choice( "VAI" .. I-or-WE .. "CHEGAR AO CANADA?" )choice: common4-weegee-canada
			choice( "'ONDE ESTA O SAQUE?" )choice: common4-weegee-loot
;

// Dog Poop

: common4-dogpoo-trueend 16 trunk-food+ goblin-laugh ;
: common4-dogpoo-reward 16 trunk-food+ badfx ;

cyoa-choice: common4-dogpoo-ignore
	($ Thegroup .. "  ignora a caixa de suprimentos e toda a comida dentro dela.	" .. gThey .. "  encontrar" .. g1s .. "  algumas latas de comida enquanto vasculha o resto da casa, mas deve se perguntar o que " .. gthey .. "  perdeu." $) text!
	2 trunk-food+ goodfx
;

cyoa-choice: common4-dogpoo-pick

	cdog? if
		($ "O coco de cachorro desaparece misteriosamente antes de " .. cname .. " portugues" .. cname .. " 'parece realmente culpado.'" $) text!
		morale--
		common4-dogpoo-reward
	then;
	ccat? if
		($ cname .. " parece especialmente irritado por ter que bater a merda de cachorro para abrir a caixa. " .. cHe .. " 'Realmente parece que esta querendo morder algo, mais do que o normal.'" $) text!
		morale4-
		common4-dogpoo-reward
	then;
	cpet? if
		($ cname .. " 0 " .. che .. " 'realmente nao se importa de qualquer maneira para coco de cachorro, entao nao foi muito de um obstaculo.'" $) text!
		16 trunk-food+ goodfx
	then;
	dogpal? if
		($ cname .. " loves dogs, and is no stranger to picking up dog poop and putting it in a plastic bag.		In fact, doing so reminds " .. cname .. " 'da civilizacao. Sentindo o calor em sua mao, sentindose vivo!'" $) text!
		morale2+ 16 trunk-food+ goodfx
	then;
	cchar oblivious? if
	($ cname .. " 	Abre a caixa de suprimentos. Qual e o problema?" .. cHe .. " nunca percebi o coco de cachorro." $) text!
		solo if
			goodfx
		else
			($ Everyone .. " esta com nojo!" $) +crtext cchar morale2-all-except badfx
		then 16 trunk-food+
	then;
	($ cname .. " tenta empurrar a merda do caixao com uma peca de madeira, mas isso acaba sendo HORRIVELmente errado. " .. cHe .. " fica muito envergonhado!" $) text!
		cattitude+ if
			badfx
			($ cHe .. " tenta manter um atitude positiva. Um dia, " .. cname .. " podera olhar isso e rir. Mas hoje nao." $)
		else
			($ "Este e, sem duvida, a pior coisa que aconteceu com " .. cname .. "  no apocalipse zombie. " .. cHe .. " nunca mais sera o mesmo." $) +crtext
			($ cname .. ":" pbold .. " PENALIDADE MORAL PERMANENTE!" fbold $)
			-1 STAT_MORALE cchar .bonus+ sfx{ creepy }
		then +crtext morale2- 16 trunk-food+
	
;

cyoa-bridge: common4-dogpoo-stick
	"VOCE GANHOU" cyoa-title! end-music
	"'Cada crianca sabe que a unica coisa certa para mover uma peca de merda e usar um ramo da arvore. E a remocao de merda perfeita da natureza. Voce canalizou seu senso de humor e maravilha infantil para vencer esta situacao perigosa." text!
	"Parabens!" rainbold +crtext
	"'VOCE ENCONTROU O FIM REAL DE DEATH ROAD TO CANADA!" fbold +crtext
	choice( "Consegui!" )choice: common4-dogpoo-trueend
;

cyoa-bridge: common4-dogpoo-open
	solo if
		soloer choosechar common4-dogpoo-pick
	then;
	($ "Quem deveria abrir a caixa?" .. "'CUIDADO: ISS" rbold .. "iSTO PODERIA SER UM GRANDE NEGOCIO" fbold $) text!
	' common4-dogpoo-pick cyoa-team-choice
;

cyoa: common4-dogpoo
	"VERDADEIRO DESESPERO" cyoa-title! picon!
	($ "Como " .. thegroup .. "  procura uma casa abandonada, um milagre acontece. " .. gThey .. "  encontrar" .. g1s .. "  uma caixa de racao do governo, fechada." $) text!
	($ "Para " .. ghis .. "  TERROR" rbold .. ", acontece que tem um " .. " coco de cachorro" fbold .. "  sentado " .. "EM CIMA DA ALCA" rbold .. "  da caixa de suprimentos." .. "  Nao tem como abrir sem encostar no coco." rbold $) +crtext
	WEAPON_TREEBRANCH roadtrip-has-weapon? not if
		"Esse pode ser o maior desafio, ate agora!" +crtext
	then
		choice( "De jeito nenhum, Eca, Ignorar" )choice: common4-dogpoo-ignore
		solo if
			soloer charchoice( "  abre" )choice: common4-dogpoo-open
		else
			choice( "Mandar alguem abrir" )choice: common4-dogpoo-open
		then
		WEAPON_TREEBRANCH roadtrip-has-weapon? if
			choice( "Use a tree branch to open it" )choice: common4-dogpoo-stick
		then
;

