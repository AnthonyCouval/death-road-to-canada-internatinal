	morale--all badfx
;

cyoa-choice: common4-spider-smoosh
	cchar .trait@ "BERSARKO!" $= if
		($ cname .. "  grita " .. "BERSERKEERRR!" fbold .. " e depois " .. "DESTROI" fbold .. " a aranha. So resta um ralo de fumaca dela." $) text!
