// Dog Poop

: common4-dogpoo-trueend 16 trunk-food+ goblin-laugh ;
: common4-dogpoo-reward 16 trunk-food+ badfx ;

cyoa-choice: common4-dogpoo-ignore
	($ Thegroup .. " 'ignoriert den Versorgungskasten und alles Essen darin.	" .. gThey .. " 'findet" .. g1s .. " 'ein paar Dosen Essen wahrend der Suche im Haus, aber muss sich wundern was " .. gthey .. " 'verfehlt hat." $) text!
	2 trunk-food+ goodfx
;

cyoa-choice: common4-dogpoo-pick

	cdog? if
		($ "'Das Hundekot plotzlich verschwindet vordem " .. cname .. " ' den Kasten offnet!		" .. cname .. " 'ausgesprochen schuldig aussehen." $) text!
		morale--
		common4-dogpoo-reward
	then;
	ccat? if
		($ cname .. " 'besonders wutend erscheint, dass es das Hundekot wegschlagen musste, um den Kasten zu offnen. " .. cHe .. " 'wirklich auf etwas zerren will, mehr als ublich." $) text!
		morale4-
		common4-dogpoo-reward
	then;
	cpet? if
		($ cname .. " 'den Versorgungskasten aufkickt auf. Als Tier " .. che .. " 'kummert es sich weder fur Hundekot, also war es kein großes Hindernis." $) text!
		16 trunk-food+ goodfx
	then;
	dogpal? if
		($ cname .. "  liebt Hunde und ist kein Fremder, Hundekot aufzuheben und in eine Plastiktute zu stecken. Tatsachlich erinnert das daran, dass " .. cname .. "  der Zivilisation. Das Gefuhl, diese Warme in der Hand zu halten, sich lebendig zu fuhlen!" $) text!
		morale2+ 16 trunk-food+ goodfx
	then;
	cchar oblivious? if
	($ cname .. " 	Offnet die Versorgungskiste. Was war das große Ding?" .. cHe .. " ich habe den Hundekot nie bemerkt." $) text!
		solo if
			goodfx
		else
			($ Everyone .. " ist angewidert!" $) +crtext cchar morale2-all-except badfx
		then 16 trunk-food+
	then;
	($ cname .. "  versucht, den Kot mit einem Stuck Holz von der Kiste zu schieben, aber es geht SCHRECKLICH schief. " .. cHe .. " 'ekelt sich ziemlich'" $) text!
		cattitude+ if
			badfx
			($ cHe .. " " .. cname .. " 'werden in der Lage sein, uber dies hinwegzusehen und zu lachen. Aber nicht heute." $)
		else
			($ "'Das ist, ohne Zweifel, das Schlimmste, das bisher passiert ist an " .. cname .. " 'wird sich im Zombie-Apokalypse nie wieder so erholen. " .. cHe .. " 'WIRD FUR IMMER ANDERS SEIN!" $) +crtext
			($ cname .. ":" pbold .. " 'Dauerhafter Moralstrafzug!" fbold $)
			-1 STAT_MORALE cchar .bonus+ sfx{ creepy }
		then +crtext morale2- 16 trunk-food+
	
;

