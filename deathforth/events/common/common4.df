// Old VHS tape

: common4-vhs-end
	morale++all
	($ "A TV da um curto apos o filme e e deixada para tras." +crtext
;

: sumo-vhs args( char )
	<- char ispet? if 
		<- char dog? if
			($ <- char .name@ .. "  aprende sumo, apesar de ser um cachorro." $) +crtext
			WEAPON_SUMO <- char .weapon_default!
		then
	then;	
	<- char .weapon_default@ WEAPON_SUMO = if
		($ <- char .name@ .. "  aprende tecnicas superiores de sumo!" $) +crtext
		WEAPON_SUPERSUMO <- char .weapon_default!
	else WEAPON_SUMO <- char .weapon_default! then
;

cyoa-blank: common4-vhs-sumo
	"Sumo Wrestling estava no top 5 dos esportes na America antes do zombocalipse. Isso levou a muitos filmes de sumo, dramas de TV de sumo, desenhos animados de sumo, e assim por diante." text!
	($ Thegroup .. " pega uns golpes de sumo assistindo ao filme!" $) +crtext
	' sumo-vhs foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-shoot
	($ "O filme e uma adaptacao do jogo de fliperama " .. "DESTRUICAO EXPLOSIVA!" fbold .. " Antes um filme ruim do que nenhum filme." $) text!
	
	' .shooting++ foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-fit
	($ Thegroup .. " segue o instrutor de ginastica no video. Todos estao usando faixas de suor neon." $) text!
	
	' .fitness++ foreach-team-char common4-vhs-end
;

cyoa-blank: common4-vhs-morale
	($ "O filme de comedia acaba sendo pessimo para a maioria dos padroes, mas " .. thegroup .. " ' e completamente absorvido por ele. E muito raro ver um filme hoje em dia." $) text!	morale3+all
	"A TV desliga no meio da segunda exibicao e nunca mais volta a ligar." +crtext
;

cyoa: common4-vhs-rare
	"Video Cassette Player" cyoa-title!
	($ Thegroup .. " ' encontra o esconderijo abandonado de alguem em uma casa fechada.	Nao sobrou muita coisa aqui, exceto uma pequena TV movida a bateria conectada a um antigo videocassete.	Ha apenas um filme no videocassete:" $) text!
