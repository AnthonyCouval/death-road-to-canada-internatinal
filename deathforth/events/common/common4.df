cyoa{ road{ 

// Not a Ouija Board

: weegeetm .. "Ouija Board [tm]" pbold .. ;
: weegeecurse args( char ) 13 <- char .seed! ;

: weegee-end
	($ Thegroup .. "  in the middle of the sentence. " .. "'Fluch des Weegee!" rbold $) +crtext
	"'LUCK BEDINGT SEHR SCHWACH!" fbold +crtext
	"'Irgendwo im Abstand lacht ein Goblin." pbold +crtext goblin-laugh
	
;

cyoa-choice: common4-weegee-pretty
	choosehuman cicon
	($ "'Das " weegeetm " 'schreibt auf, dass " .. cname .. " ' IST " .. "'	V E R Y P R E T T Y" pbold $) text!
	($ "'Gut gemacht! " .. cname .. " 'kann sich von diesem verzauberten Weegee nicht verfluchten fuhlen." $) +crtext
	morale2+
	"Somewhere in the distance, a goblin laughs, but in a nice way. As if to say 'hey.... what's up. have a nice day.'" pbold +crtext goblin-laugh
	choice( "'schon" )choice
;

cyoa-choice: common4-weegee-loot
	($ "'Das " weegeetm " 'schreibt auf, die Position einer verborgenen Beute im Haus an!" $) text!
	homesearch1
	weegee-end
;

cyoa-choice: common4-weegee-check
	weaselest choosechar cicon
	($ "'Wenn nachgefragt wird, wer das Gruppengeheimnis verrat, der " weegeetm " 'schreibt auf " $) text!
	weaselest-pass if
		($ cname .. "'Name!" $) +text
		loyaltyREV
		weegee-end
	else
		"D E I N A R S C H" pbold +crtext
		($ " sign? " .. "Fluch von Weegee?" rbold $) +crtext
		morale--all goblin-laugh
	then
;

cyoa-choice: common4-weegee-canada
	($ "'Das " weegeetm "  'zu dem Wort' " .. "'NEIN" pbold .. " 'mit erstaunlicher Geschwindigkeit. Dann weist es auf " .. "'NEIN" fbold .. " ' noch einmal hin, wahrend es irgendwie in Flammen aufgeht." $) text!
	($ "'Was auch immer! Das ist immer falsch, sowieso! " .. cname .. " 'wirft die brennende " weegeetm " ' durch den Raum. Dadurch fangt auch " .. chim .. " ' Feuer." $) +crtext
	cchar isfireproof? if 
		($ cname .. " ' liebt es, feuerfest zu sein!" $) +crtext
		morale++ goodfx
	else
		ouch-- alive? if bitefx then
	then
	alive? if
		"'Das war immerhin eine gute Ubung." +crtext fitness++
	else
		($ "'Mach dich nicht mit dem " .. "'Fluch des Weegee!" rbold $) +crtext goblin-laugh
	then
	
;

cyoa: common4-weegee
	"Ouija Board" cyoa-title! choosehuman cicon
	($ Thegroup .. "  is poking through an abandoned house and finds a  . " weegeetm " 'Auf dem Kuchentisch liegt es. Es handelt sich um ein haufiges Spiel, um mit " .. "GEISTER" fbold $) text!
		choice( "Lass diesen UNSINN in Ruhe" )choice
		($ "Diese Dinge sollen Geruchten zufolge sein " .. "UNGLAUBLICH VERFLUCHT MIT SCHLECHTEM GLUCK" rbold .. ", so beware! Still, it may be worth asking it a question..." $) +crtext
		solohuman if
			choice( "BIN ICH HUBSCH?" )choice: common4-weegee-pretty
