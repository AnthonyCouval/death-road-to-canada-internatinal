	($ cname .. " ' nao consegue espirrar quando quer. " .. cHe .. " ' perde o espirro! E muito chato!" $) text!
	morale--
	cattitude- if
		($ " Esta e a " .. " pior coisa que pode acontecer com ALGUEM" rbold .. "  no zumbocalipse!" $) +crtext morale-- attitudeREV
	then
;

cyoa-choice: common4-sneeze-smash
	bitefx
	cchar .trait@ "BERSERK!" $= if
		($ cname .. "  grita " .. "BERSERKEERRR!" fbold .. "  e entao soca " .. chim .. "no nariz, o mais forte possivel. Nenhum instinto de autopreservacao." $) text!
		ouch--
	then;
	($ cname .. "  esmaga " .. chis .. "  nariz proprio em uma parede." $) text!
	sting--
	($ "'E um truque antigo! A dor cegante faz " .. cname .. "  esquecer do espirro perdido." $) +crtext
;

