	solo if
		soloer choosechar common4-dogpoo-pick
	then;
	($ "'Wer sollte die Kiste offnen?" .. "'		WARNUNG: DAS IST SEHR EKELHAFT		ES LIEGT HUNDEKOT AUF DER KISTE" rbold .. "'		DAS KONNTE EIN GROSSES GESCHAFT SEIN" fbold $) text!
	' common4-dogpoo-pick cyoa-team-choice
;

cyoa: common4-dogpoo
	"Entdeckt er dann heraus, dass es ein ',\" cyoa-title! picon!
	($ "'Als " .. thegroup .. " entdeckt er dann heraus, dass es ein ',\ " .. gThey .. " 'findet" .. g1s .. " entdeckt er dann heraus, dass es ein ',\" $) text!
	($ "'Zu " .. ghis .. " " rbold .. "entdeckt er dann heraus, dass es ein ',\ " .. "'Hundekot" fbold .. " " .. "Das konnte der großte Herausforderung sein, doch!']" rbold .. " das konnte der großte Herausforderung sein, doch!']" .. " Das konnte der großte Herausforderung sein, doch!']" rbold $) +crtext
