// END rares

// Man trapped under cardboard thievery

0 value common2-cboardtype

: cboardprize
	common2-cboardtype case
		1 of "CASO" endof
		2 of "ESPINGARDA" endof
		3 of "FALCAO" endof
		4 of "REVOLVER DE TIROLHO " endof
		5 of "MACANA DE BATE" endof
		6 of "REVOLVER DE CALADAO" endof
	endcase
;

: cboardget
	common2-cboardtype case
		1 of 1 WEAPON_NIGHTSTICK endof
		2 of 8 trunk-shotgun-shell+ 1 WEAPON_SHOTGUN endof
		3 of 12 trunk-rifle-shell+ 1 WEAPON_RIFLE endof
		4 of 6 trunk-bullet+ 1 WEAPON_DOLT endof
		5 of 1 WEAPON_FIRE endof
		6 of 6 trunk-bullet+ 1 WEAPON_SNUBNOSE endof
	endcase
;

cyoa-choice: common2-cboard-steal
	($ cname .. " 'rouba o homem indefeso de seu " .. cboardprize rbold .. "." $) text! badfx
	cloyalty- if
		($ cHe .. " se sente muito satisfeito com isso." $) +crtext morale++
	else
		cloyalty+ if
			($ cHe .. " se sente incrivelmente culpado por isso!" $) +crtext morale3-
		else
			($ cHe .. " tem um" $) +crtext morale--
		then
	then loyaltyREV
	($ "Pegou um " rbold .. cboardprize rbold $) +crtext cboardget trunk.weapon+
;

: cboard-help-check args( @ )
	<- @ .cloyalty- if <- @ .loyaltyREV <- @ .morale-- then; <- @ .morale++
;

cyoa-choice: common2-cboard-help
	($ Thegroup .. " 'salva o homem dos caixas de papelao que o amarravam. Ele nao pode pagar uma recompensa, mas e realmente grato!" $) text! ' cboard-help-check foreach-team-char goodfx
;

cyoa-choice: common2-cboard-bandit
	($ cname .. " 'rouba o homem indefeso de seu " .. cboardprize rbold .. " e depois leva o resto de suas provisoes!" $) text! badfx
	rob-guilt	
	($ "Pegou um " rbold .. cboardprize rbold $) +crtext cboardget trunk.weapon+ 
	10 20 rnd trunk-bullet+ 5 15 rnd trunk-rifle-shell+ 3 12 rnd trunk-shotgun-shell+
	2 4 rnd trunk-medical+ 1 6 rnd trunk-food+
	aholeREV
;

cyoa-bridge: common2-cboard-steal4
	someicon
	"Quem deve roubar sua arma?" cyoa-text!
	' common2-cboard-steal cyoa-team-choice
;

cyoa: common2-cboard
	"Ajudando alguem em necessidade!" cyoa-title!
	1 6 rnd to common2-cboardtype
	($ "Enquanto vasculhava, " .. thegroup .. " ' encontra um homem coberto de caixas de papelao. Ele diz que esta preso la ha dias! Ele tem um " .. cboardprize rbold .. "  caido no chao fora de seu alcance." $) text!
	choice( "Ajude o a levantar" )choice: common2-cboard-help
	most-ahole if
		charchoice( ": Roube todos os seus suprimentos" )choice: common2-cboard-bandit
		team-count 2 > if
			choice( "Escolha um ladrao" )choice: common2-cboard-steal4
		then;
	then	
		
	solo if
		soloer choosechar choice( "Roube sua arma" )choice: common2-cboard-steal
	else
		"Alguem deveria roubar sua arma?" +crtext
		team-count 3 > if
			choice( "Escolha um ladrao" )choice: common2-cboard-steal4
		then 
		' common2-cboard-steal 3 #cyoa-team-choice
	then
;

