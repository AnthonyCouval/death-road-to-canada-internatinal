// Annoying diplomat

: common2-detoured
	vehicle.which@ 0 > if choice( "Einen Umweg machen" )choice: car-detour else
		// ($ Thegroup .. " ' takes a long detour on foot.' in deutsch is 'macht einen langen Umweg zu Fuß." $) +crtext 
		1 calendar-day+ +cr fullcamp
	then
;

cyoa-bridge: common2-meeting-talk
	($ cname .. "  spricht mit dem nervigen Vertreter." $) text!
	cpet? if
		($ cHe .. " " .. che .. "  meistens nur Leckereien " .. chim .. "0;  and will refuse to do so. " .. cdogcat .. " pass. He then goes to the next guard and says 'I'm  =  and " .. thegroup .. " ' durch!" $) +crtext goodfx morale++all
	then;
	cchar grating? if
		($ cHe .. " ' wirklich gut miteinander! Sie tauschen sich hauptsachlich Witze aus. Fur eine Stunde. " .. "'BERUHIGEN SIE SICH" pbold .. " ' quatscht mit einander. Jeder Stunde wird es etwas gegeben. " .. Thegroup .. " ' hindurchgelassen wird." $) +crtext witsattREV goodfx cool-it++ cool-it++
	then;
	cchar oblivious? if
		($ cHe .. " ' irgendwie nicht aufmerksam, dass der Dingus immer mal die Beleidigungen oder Provokationen vermeint. Das Treffen geht gut und " .. thegroup .. " ' hindurchgelassen wird." $) +crtext witsattREV goodfx
	then;
	cattitude+ if
		($ cHe .. " ' sehr hoflich ist. Nach genugend sinnlosen Fragen, " .. thegroup .. " ' hindurchgelassen wird durch die Barrikade." $) +crtext attitudeREV goodfx
	then;
	cchar angrystrong? if
		($ cHe .. " 'verliert " .. chis .. " ' Zorn aufregen und den Mann in einem Schlag ausknockt. Die Barrikadengewahrten sind beeindruckt von dieser! Einer gibt eine Daumen-Pause. Sie lassen " .. thegroup .. " ' hindurch, trotzdem." $) +crtext attitudeREV strengthREV goodfx morale++all
	then;
	ctemper- if
		($ cHe .. " 'verliert " .. chis .. " ' Zorn aufregen und beginnt zu schreien. Das wirklich erschreckt die Wachen, und sie beginnen zu schießen. " .. Thegroup .. " ' fluchtet sich her!" $) +crtext temperREV bitefx sting--all
		common2-detoured	
	then;
	ctemper+ temperroll or if
		($ cHe .. " schafft es zu kontrollieren " .. chis .. " ' temper! ', ' temper! " .. Thegroup .. " ' ist durch die Barrikade nach weiteren Storungen gelassen.', ' is through the barricade after further annoyance." $) +crtext goodfx
	else
		($ cHe .. " ' versucht zu kontrollieren ', ' tries to control ', " .. chis .. " ' seine Temperatur, aber der Mann ist einfach zu storend. Er rennt und erzahlt den Wachen, die Tur nicht aufzulassen. ', ' his temper, but the man is just too annoying. He runs and tells the guards not to open the door.', " .. Thegroup .. " ' muss eine Methode umgehen!', ' must find a way around!" $) +crtext badfx
		common2-detoured
	then temperREV
;

cyoa-bridge: common2-meeting-talk4
	someicon
	"'Wer soll sich mit dem storenden Menschen auseinandersetzen?', ' Who should deal with the annoying person?" cyoa-text!
	' common2-meeting-talk cyoa-team-choice
;

cyoa: common2-meeting
	"'Diplomatie der Ruhe', ' Cool-it Diplomacy" cyoa-title!
	($ Thegroup .. " ' wird von einer Uberlebensgruppe gehemmt, die sich mit einem Barrikade schutzen. Sie sind misstrauisch und stark bewaffnet. Der Mann, den sie senden, um zu sprechen ist ', ' is blocked by another survivor group that set up a barricade to protect themselves. They are distrusting and heavily armed. The man they send to talk is ', " .. "'EXTREMLY ANNOYING', ' EXTREMLY ANNOYING" rbold .. "." $) text!
		solo if
			($ " ' sagt Dinge wie ', ' says things like ', " .. "'BERUHIGEN SIE SICH" pbold .. " ' und " .. "'SAG ES NICHT SPRAY ES NICHT.', ' Say it don’t spray it." pbold $) +text
			soloer choosechar choice( "'Versuche, dein Temperament zu behalten', ' Try to not lose temper'." )choice: common2-meeting-talk
		else
			" ' Wer sollte versuchen, mit ihm umzugehen?" +text
			team-count 3 > if
				' common2-meeting-talk 2 #cyoa-team-choice
				choice( "'Wahle jemand anderen" )choice: common2-meeting-talk4
			else
				' common2-meeting-talk 3 #cyoa-team-choice
			then
		then
		vehicle.which@ 0 > if smartest charchoice( "': Nehmen Sie einen Umweg" )choice: car-detour then
;

// Park Chatting

: common2-parkchat-tipme args( @ )
	<- @ 40 %chance if 
		.shooting++ 
	else
		50 %chance if 
			.mechanic++
		else
			.medical++
		then
	then
;

cyoa-choice: common2-parkchat-tips
	cpet? if 
		($ cname .. " " .. cbarkmeow .. "'gibt in sehr weise und erfahrene Weise Ratschlage." $) text!
	else
		($ cname .. " ' tauscht Ratschlage zu verschiedenen Themen aus." $) text!
	then
	cwits- if
		"'Der freundliche Uberlebende ist durch den wertlosen Rat verwirrt." +crtext badfx witsREV
	then;
	cwits+ if
		"'Der freundliche Uberlebende wird durch den Rat beleuchtet und gibt großartige Tipps seiner eigenen!" +crtext
		goodfx ' common2-parkchat-tipme foreach-team-char witsREV
	then;
		($ "'Der freundliche Uberlebende schatzt es, und gibt einen kleinen Rat an " .. cname .. " ' zuruck." $) +crtext
		cchar common2-parkchat-tipme goodfx witsREV
;

cyoa-choice: common2-parkchat-pep
	cpet? if 
		($ cname .. " " .. cbarkmeow .. "' in einer beruhigenden Weise gibt." $) text!
	else
		($ cname .. " ' versucht, eine motivierende Ansprache zu geben." $) text!
	then
	cattitude- if
		"'Der freundliche Uberlebende fuhlt sich tatsachlich jetzt schlechter." +crtext badfx attitudeREV
	then;
	cattitude+ if
		"1-2 morale" +crtext
		morale2+all goodfx attitudeREV
	then;
		($ "'Der freundliche Uberlebende gibt einige beruhigende Worte zuruck.'" $) +crtext
		morale++all goodfx attitudeREV
;

cyoa-choice: common2-parkchat-bandit
	($ cname .. "  easily robs the friendly person. He didn't have much, but that will teach him an important lesson in trusting others." $) text! badfx
		2 4 rnd trunk-food+ 50 %chance if 1 3 rnd trunk-medical+ then rob-guilt
		aholeREV
;

cyoa-bridge: common2-parkchat-coolit
	cpet? if
		($ cname .. " " .. "'FARTET" rbold .. " 'an der Person und tat das irgendwie mehrere Minuten lang ohne Unterbrechung fortsetzte." $)
	else
		($ cname .. " 'unterbricht den freundlichen Uberlebenden mitten im Satz, um ihm zu sagen:', " .. "'RUHE ONDOWN." pbold $)
	then text!
	cool-it-7 if
		char-gen to recruitee "'Freundlicher Typ" recruitee .name!
		"'er ist beeindruckt von diesem. Er behauptet, dass das alles nach den Prophezeiungen steht, und dass er seit langem ein Geschenk fur das Gewahlte gehalten hat!" +crtext
		cooled-it goodfx rescue-trainitem morale3+all
	else cool-it++
		($ "'an ihm blickt', " .. cname .. "', ohne zu sprechen, bis er abgeht.', " .. thegroup .. " 'Blatter." $) +crtext
		morale--all badfx
	then witsattREV 
;

cyoa: common2-parkchat
	"'So wie eine normale Tag" cyoa-title!
	($ Thegroup .. " 'trifft er auf eine weitere Person, die nur auf einem Parkbank sitzt. Er scheint freundlich. Sie haben ein Gesprach, genauso wie vor dem Ende der Welt.	Was war das Hauptthema?" $) text!
		smartest charchoice( "'Ratschlagstausch." )choice: common2-parkchat-tips
		gallant charchoice( "motivationsrede" )choice: common2-parkchat-pep
		most-ahole if
			charchoice( "gib mir deine Sachen" )choice: common2-parkchat-bandit
		then
		most-grating if
			charchoice( "abkuhlen" fcoolit )choice: common2-parkchat-coolit
		then
;

// Busted leg

cyoa-choice: common2-busted-med
	($ cname .. " versucht, die gebrochene Bein der Frau zu behandeln." $) text!
	cmedical+ if
		($ cHe .. "  benutzt " .. chis .. " 1x Medical Supplies" $) +crtext morale++ medicalREV medical++ goodfx -1 trunk-medical+ 2 trunk-food+
	then;
	cmedical- if
		($ cHe .. " 'hat keine Ahnung von " .. che .. " 'ist damit beschaftigt, vergeudet einen Tag und mag etwas schlechter machen." $) +crtext medicalREV cloyalty- not if morale-- then loyaltyREV +cr fullcamp badfx -1 trunk-medical+ 1 calendar-day+
	then;
	medicalroll if
		($ cHe .. " 'hilft ihr am Tag bei, ihre Beinverletzung behandeln zu helfen. Sie ist dankbar, aber kann nicht viel sparen!" $) +crtext morale++ medicalREV medical++ goodfx 2 trunk-food+
	else
		($ cHe .. " 'versucht uber den Tag hinweg zu helfen, aber das gelingt ihm nicht. Dennoch uben und wiederholen." $) +crtext medicalREV medical++ cloyalty- not if morale-- then loyaltyREV badfx
	then +cr fullcamp -1 trunk-medical+ 1 calendar-day+ heal-event
;

