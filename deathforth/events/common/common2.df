cyoa-choice: common2-rare-zombmaster-fight
	image-creepy-girl
	($ "O " .. "MESTRE DOS ZUMBIS" rbold .. "  ilumina " .. thegroup .. "  sobre armas brancas com algumas dicas e alguns " .. "TREINAMENTO MUITO PERIGOSO." rbold $) text! goodfx bitefx
	' common2-rare-zomb-workout foreach-team-char sting--all
;

cyoa-choice: common2-rare-zombmaster-shoot
	image-creepy-girl
	($ "O " .. "MESTRE DOS ZUMBIS" rbold .. "  da " .. thegroup .. "  uma otima licao sobre tiro apropriado! Ela da um dever de casa para fazer uma caca de zumbi real e fornece um monte de municao e um rifle!" $) text! goodfx ' .shootingREV foreach-team-char ' .shooting++ foreach-team-char
	60 trunk-bullet+ 40 trunk-rifle-shell+ 30 trunk-shotgun-shell+ 1 WEAPON_RIFLE trunk.weapon+
;

cyoa-choice: common2-rare-zombmaster-tough
	image-creepy-girl
	($ "O " .. "MESTRE DOS ZUMBIS" rbold .. "  grunhe." .. thegroup .. "  como endurecer " .. ghis .. "  a pele contra mordidas de zumbis! Este processo envolve espanca la " .. ghim .. "  para cima." $) text! goodfx bitefx
	' .vitality++ foreach-team-char sting--all
