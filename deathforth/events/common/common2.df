;

cyoa-choice: common2-carhelp-mech
	($ cname .. " 'versucht, das Auto des Mannes zu reparieren." $) text!
	cmechanic+ if
		($ cHe .. "  benutzt " .. chis .. " 'setzt sein Autowissen ein, um es schnell zu reparieren! Er schatzt die Hilfe und gibt einige Vorrate!" $) +crtext morale++ mechanicREV mechanic++ goodfx ammosearch1
	then;
	cmechanic- if
		($ cHe .. " 'hat auch keine Ahnung von " .. che .. " 'verschwendet den Tag damit, herumzualbern." $) +crtext mechanicREV cloyalty- not if morale-- then loyaltyREV +cr fullcamp badfx 1 calendar-day+ heal-event
	then;
	mechanicroll if
		($ cHe .. " 'verbringt einen Tag damit, dem Mann zu helfen, sein Auto zu reparieren. Er gibt einige Vorrate als Belohnung!" $) +crtext morale++ mechanicREV mechanic++ goodfx ammosearch1
	else
		($ cHe .. " 'versucht uber den Tag verteilt zu helfen, ist aber nicht erfolgreich. Trotzdem ist es gute Ubung." $) +crtext mechanicREV mechanic++ cloyalty- not if morale-- then loyaltyREV badfx
	then +cr fullcamp 1 calendar-day+ heal-event
;

cyoa-bridge: common2-carhelp-coolit
	cpet? if
		($ cname .. " 'ignoriert den gestrandeten Mann und beginnt, Ameisen zu essen, die auf dem kalten Boden kriechen." $)
	else
		($ cname .. " 'sagt dem gestrandeten Mann, er soll " .. "'RUHE ONDOWN." pbold $)
	then text!
	cool-it-7 if
		char-gen to recruitee "'Gestrandter Mann" recruitee .name!
		($ "'RICHTIG! DAS AUTO BRAUCHT NUR MEHR KUHLMITTEL, UM WIEDER ZU Laufen!" pbold .. " 'Er bietet Belohnung an!" $) +crtext
		cooled-it goodfx rescue-trainitem morale3+all
	else cool-it++
		($ "'Er beginnt auf jemandem zu schreien: " .. thegroup .. " 'bis " .. gthey .. " 'gehen lassen" .. g1s .. "." $) +crtext
		morale--all badfx
	then witsattREV
;

cyoa-choice: common2-carhelp-bandit
	($ cname .. " 'leicht den von Seiten der Strasse verlassenen Mann aus seinen Vorraten, auch seinen Wrench, leicht entwenden kann." $) text! badfx
		common2-carhelp-win 1 WEAPON_WRENCH trunk.weapon+ rob-guilt aholeREV
;

cyoa: common2-carhelp
	"'Mit dem Auto-Problem helfen" cyoa-title!
	char-gen to recruitee 
	0 recruitee .mechanical!
	0 recruitee .female! ricon
	WEAPON_WRENCH recruitee .weapona!
	($ Thegroup .. " 'trifft einen Mann, der an seinem zerbrochenen Auto arbeitet. Er hat offensichtlich kein Klarheit daruber, was er tut." $) text!
		mechanicest charchoice( "': Hilfe mit dem Wagen reparieren" )choice: common2-carhelp-mech
		most-charming if
			charchoice( "': Sich ihm anschließen lassen" )choice: recruit-yes
		then
		most-grating if
			charchoice( "': Lass es ruhig!" fcoolit )choice: common2-carhelp-coolit
		then
		most-ahole if
			charchoice( "': Seine Vorrate stehlen" )choice: common2-carhelp-bandit
		then
		choice( "'No time to help' in deutsch is 'Keine Zeit, um zu helfen" )choice
;

// Bandit raid

: common2-raid-prize
	1 WEAPON_RIFLE trunk.weapon+ 20 trunk-rifle-shell+ 15 trunk-food+ morale++all goodfx
;

cyoa-choice: common2-raid-calmshot
	($ cname .. " 'nimmt ein Cowboygewehr von den Siedlern und sich in einer hohen Stelle versteckt. Als die Rauber aufkommen, " .. che .. " 3x 1000 caps" $) text! common2-raid-prize
;

: common2-raid-shoot+
	dup humany? 50 %chance and if .shooting++ then
;

: common2-raid-fightprize
	choosehuman alive? if
		"1" +crtext
		common2-raid-prize
		most-calmshot if
			choosechar ($ cname .. " 'macht ein paar verruckte Schusse, die die anderen inspirieren.'" $) +crtext
			' .shooting++ foreach-team-char
		else
			' common2-raid-shoot+ foreach-team-char
		then
	then
;

cyoa-choice: common2-raid-fight
	someicon ($ Thegroup .. "  entscheidet sich, den Siedlern zu helfen, die Banditen direkt zu bekampfen." $) text!
	shootingREVall
	shooting-total 14 > if
		"1" +crtext
		common2-raid-fightprize
	then;
	shooting-total 10 > if
		"The fight is tough, but the group's shooting abilities save the settlement." +crtext
		ouch--all bitefx common2-raid-fightprize
	then;
	shooting-total 6 > if
		"1" +crtext
		ouch12rnd--all bitefx common2-raid-fightprize
	then;
		"'Die Hilfe war nicht genug, um im Schießgefecht gegen die Bande zu helfen. Die Bande vertjaet alle aus dem Siedlung." +crtext ouch2-all bitefx
;

cyoa-choice: common2-raid-mech
	($ cname .. " ' versucht, die Verteidigungsanlagen der Siedlung aufzubauen." $) text!
	cmechanic+ if
		($ cHe .. " " .. chim .. " ' erzahlt den Siedlern, wie man in schwachen Bereichen Festungsanlagen aufbaut. Die zusatzlichen Verteidigungen scheinen dem Bandenangriff sehr hilfreich zu sein! Die Siedler danken fur die Hilfe!" $) +crtext mechanicREV mechanic++ common2-raid-prize
	then;
	mechanicroll if
		($ cHe .. " ' erzahlt den Siedlern, wie man in schwachen Bereichen verteidigt. Die zusatzlichen Verteidigungen scheinen dem Angriff der Bande hilfreich zu sein! Die Siedler danken fur die Hilfe!" $) +crtext mechanicREV mechanic++ common2-raid-prize
	else
		($ cHe .. " ' versucht etwas zu verstarken, aber die Anstrengungen hatten gegen den heftigen Bandenangriff kaum Wirkung. Verletzte Siedler beschuldigen " .. cname .. " ' fur die Zeitverschwendung und weigern sich, das Belohnung zu zahlen." $) +crtext mechanicREV morale--all sting--all bitefx
	then
;

cyoa: common2-raid
	"'Verteidigung gegen Bandenuberfall" cyoa-title!
	($ Thegroup .. " 'bittet um Hilfe von einer Siedlung. Sie sagen, sie werden bald von Banden uberfallen. Wenn " .. thegroup .. " ' mit der Verteidigung hilft, " .. gthey .. " ' wird eine Belohnung in Form von 15 Nahrungsmitteln, Munition und einem Cowboy Gewehr erhalten! Das konnte " .. "very dangerous..." rbold $) text!
		vehicle.which@ 0 > if 
			choice( "'Hilfe leisten, die Bande abschicken" )choice else choice( "Don't help" )choice
		then
		choice( "Helfen Sie ihnen im Schusswechsel" )choice: common2-raid-fight
		mechanicest charchoice( "verteidigung aufbauen" )choice: common2-raid-mech
		most-calmshot if
			charchoice( "ich werde es erledigen" )choice: common2-raid-calmshot
		then
;

