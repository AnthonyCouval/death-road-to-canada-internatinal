// Farm help

: common2-farm-reward
	8 trunk-food+
	most-charming if
		choosechar ($ cname .. " ' uberredet die Bauern, ihm " .. chim .. " 'einen Gabelstapler zu geben!" $) +crtext
		1 WEAPON_PITCHFORK trunk.weapon+
	then
;

cyoa-choice: common2-farm-mechanic
	($ cname .. " ' bietet an ', " .. chis .. " ' Dienstleistungen zur Reparatur von Dingen auf dem Bauernhof an.', " $) text!
	cmechanic+ if	
		($ cHe .. " ' ist so geschickt, dass die große Menge an Arbeit kein großes Problem darstellt." $) +text
	else
		($ "'Es gibt so viele zerbrochene Dinge, dass ', " .. che .. " ' erschopft wird." $) +text cchar tired
	then
	mechanicREV mechanic++
	cchar .mechanical 3 < if
		($ "'Die Bauern weigern sich, eine Belohnung zu geben, nachdem sie herausfinden, dass alles noch zerbrochen ist." $) +crtext badfx
	else
		($ "'Die Bauern sind dankbar fur die Arbeit!" $) +crtext goodfx common2-farm-reward
	then
;

cyoa-choice: common2-farm-bandit
	($ cname .. " 'die Bauern ausraubt. Sie waren ganz uberrascht gegenuber einer Nicht-Zombie-Bedrohung." $) text! badfx
	14 trunk-food+ rob-guilt2 aholeREV
;

