;

cyoa-choice: common2-relax-mech
	($ cname .. "  ensina " .. everyone .. "  o que " .. che .. "  sabe sobre conserto de automoveis e outras maneiras de baguncar acidentalmente as maquinas." $) text!
		cmechanic- if 
			($ cHe .. " ' nao sabe muito sobre isso! E uma licao completamente inutil." $) +crtext badfx
		else
			cchar ' .mechanic++ foreach-team-char-except goodfx
		then mechanicREV morale++
;

cyoa-choice: common2-roofrelax-reveal
	($ cname .. "  revela " .. chis .. "  VERDADEIRA ESSENCIA." $) text!
		allREV goodfx
;

cyoa: common2-roofrelax
	image-tired-survivor 63 to cyoaicon
	($ Thegroup .. "  tem um raro momento de paz para relaxar no telhado de um apartamento." $) text!
	solo if
		soloer choosechar
		($ "Sem ninguem para incomodar " .. chim .. ", " .. che .. "  decide:" $) +crtext
		choice( "CALMA AI" )choice: common2-relax-morale
		choice( "Exercicio para forca" )choice: common2-relax-strength
		choice( "Exercicio para condicionamento fisico" )choice: common2-relax-fitness
	else
		"'Ha tempo para conversar e alguem revela muito sobre si mesmo:" +crtext
		' common2-roofrelax-reveal cyoa-team-choice
	then
;

// Safehouse relaxing

cyoa: common2-saferelax
	image-door-on-brick 27 to cyoaicon
	($ Thegroup .. "  tem um momento de paz para descansar em um esconderijo, nenhum zumbi a vista." $) text!
	solo if
		soloer choosechar
		($ "Sem ninguem para incomodar " .. chim .. ", " .. che .. "  decide:" $) +crtext
		choice( "CALMA AI" )choice: common2-relax-morale
		choice( "Exercicio para forca" )choice: common2-relax-strength
		choice( "Exercicio para condicionamento fisico" )choice: common2-relax-fitness
	else
		"Com algum tempo livre, eles decidem:" +crtext
			50 %chance if 
				choice( "CALMA AI" )choice: common2-relax-morale
			else
				50 %chance if
					choice( "Exercicio para forca" )choice: common2-relax-strength
				else
					choice( "Exercicio para condicionamento fisico" )choice: common2-relax-fitness
				then
			then
			mechanicest charchoice( ": Ensinar habilidades mecanicas" )choice: common2-relax-mech
			medicalest charchoice( ": Ensinar habilidades medicas" )choice: common2-relax-med
	then
;

