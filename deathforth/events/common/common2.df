// Farm help

: common2-farm-reward
	8 trunk-food+
	most-charming if
		choosechar ($ cname .. "  convence os fazendeiros a darem " .. chim .. "  um forcado!" $) +crtext
		1 WEAPON_PITCHFORK trunk.weapon+
	then
;

cyoa-choice: common2-farm-mechanic
	($ cname .. "  oferece " .. chis .. "  servicos consertando coisas ao redor da fazenda. " $) text!
	cmechanic+ if	
		($ cHe .. " ' e tao habilidoso que a grande quantidade de trabalho nao e um grande problema." $) +text
	else
		($ "'Ha tantas coisas quebradas que " .. che .. "  fica exausto." $) +text cchar tired
	then
	mechanicREV mechanic++
	cchar .mechanical 3 < if
		($ "Os fazendeiros se recusam a dar uma recompensa depois que descobrem que tudo ainda esta quebrado." $) +crtext badfx
	else
		($ "Os fazendeiros sao gratos pelo trabalho!" $) +crtext goodfx common2-farm-reward
	then
;

cyoa-choice: common2-farm-bandit
	($ cname .. "  rouba os pacificos fazendeiros. Eles foram completamente pegos de surpresa contra uma ameaca nao zumbi." $) text! badfx
	14 trunk-food+ rob-guilt2 aholeREV
;

cyoa-choice: common2-farm-haul
	someicon
	($ Thegroup .. " ' transporta toneladas de coisas ao redor. Sempre ha coisas para transportar em uma fazenda. E um trabalho exaustivo e " .. gthey .. "  pegue" .. g1s .. "  muito cansado!" $) text! goodfx 
	' .strengthREV foreach-team-char ' .strength++ foreach-team-char common2-farm-reward
	// added STR check for scythe
	strongest-pass if strongest choosechar
		($ "Os fazendeiros notaram " .. cname .. "''s forca incomum e deram " .. chim .. "  uma foice de presente!" $) +crtext
		1 WEAPON_SCYTHE trunk.weapon+
	then
	nosleep
;

cyoa-choice: common2-farm-till
	someicon
	($ Thegroup .. " ' ajuda a arar os campos e plantar novas colheitas. E um trabalho exaustivo e " .. gthey .. "  pegue" .. g1s .. "  muito cansado!" $) text! goodfx 
	' .fitnessREV foreach-team-char ' .fitness++ foreach-team-char common2-farm-reward
	// added FIT check for scythe
	fittest-pass if fittest choosechar
		($ "Os fazendeiros notaram " .. cname .. "''s nivel de condicionamento fisico e deu " .. chim .. "  uma foice de presente!" $) +crtext
		1 WEAPON_SCYTHE trunk.weapon+
	then
	nosleep
;

cyoa: common2-farm
	"Uma Viagem a Fazenda" cyoa-title!
	($ Thegroup .. " 8 food" $) text!
		choice( "Lavoura e plantio" )choice: common2-farm-till
		choice( "Puxando e puxando" )choice: common2-farm-haul
		most-ahole if
			charchoice( "roube a fazenda" )choice: common2-farm-bandit
		then
		mechanicest	charchoice( ": Reparar trabalho" )choice: common2-farm-mechanic
		choice( "Nao tem tempo para ajudar" )choice
;

// Annoying diplomat

: common2-detoured
	vehicle.which@ 0 > if choice( "Fazer um desvio" )choice: car-detour else
		// ($ Thegroup .. "  pega um longo desvio a pe." $) +crtext 
		1 calendar-day+ +cr fullcamp
	then
;

cyoa-bridge: common2-meeting-talk
	($ cname .. "  fala com o representante chato." $) text!
	cpet? if
		($ cHe .. " ' nao e suposto ser capaz de falar, entao " .. che .. "  principalmente so lambe " .. chim .. "eu. O idiota e realmente insultado por ser feito para falar com um " .. cdogcat .. ", mas os guardas se divertem. Eles deixam " .. thegroup .. "  passar!" $) +crtext goodfx morale++all
	then;
	cchar grating? if
		($ cHe .. "  realmente se da bem! Eles principalmente trocam " .. "CALMA AI" pbold .. "  piadas entre si. Por uma hora. " .. Thegroup .. "  e deixado passar." $) +crtext witsattREV goodfx cool-it++ cool-it++
	then;
	cchar oblivious? if
		($ cHe .. " ' de alguma forma nao percebe todas as vezes que o idiota e irritante ou insultuoso. A reuniao vai bem e " .. thegroup .. " is let go." $) +crtext witsattREV goodfx
	then;
	cattitude+ if
		($ cHe .. " consegue ser muito educado. Depois de questionar o suficiente sem sentido " .. thegroup .. " e deixado passar pela barricada." $) +crtext attitudeREV goodfx
	then;
	cchar angrystrong? if
		($ cHe .. " perde " .. chis .. " pass. " .. thegroup .. "  atraves, de qualquer forma." $) +crtext attitudeREV strengthREV goodfx morale++all
	then;
	ctemper- if
		($ cHe .. " perde " .. chis .. "  tempera de imediato e comeca a gritar. Isso realmente assusta os guardas, e eles comecam a atirar. " .. Thegroup .. "  tem que fugir!" $) +crtext temperREV bitefx sting--all
		common2-detoured	
	then;
	ctemper+ temperroll or if
		($ cHe .. "  consegue controlar " .. chis .. "  temperamento! " .. Thegroup .. "  e deixado passar pela barricada apos algum aborrecimento adicional." $) +crtext goodfx
	else
		($ cHe .. "  tenta controlar " .. chis .. "  temperamento, mas o cara e muito chato. Ele corre e diz aos guardas para nao abrirem a porta. " .. Thegroup .. "  tem que encontrar um caminho alternativo!" $) +crtext badfx
		common2-detoured
	then temperREV
;

cyoa-bridge: common2-meeting-talk4
	someicon
	"Quem deve tentar lidar com a pessoa irritante?" cyoa-text!
	' common2-meeting-talk cyoa-team-choice
;

cyoa: common2-meeting
	"Diplomacia Calma" cyoa-title!
	($ Thegroup .. "  e bloqueado por outro grupo de sobreviventes que montou uma barricada para se proteger. Eles sao desconfiados e fortemente armados. A pessoa que eles enviam para falar e " .. "EXTREMAMENTE IRRITANTE" rbold .. "." $) text!
		solo if
			($ "  Ele continua dizendo coisas como " .. "CALMA AI" pbold .. "  e " .. "'Diga nao borrife." pbold $) +text
			soloer choosechar choice( "Tente nao perder a paciencia" )choice: common2-meeting-talk
		else
			"  Quem deveria tentar lidar com ele?" +text
			team-count 3 > if
				' common2-meeting-talk 2 #cyoa-team-choice
				choice( "Escolha outra pessoa" )choice: common2-meeting-talk4
			else
				' common2-meeting-talk 3 #cyoa-team-choice
			then
		then
		vehicle.which@ 0 > if smartest charchoice( ": Faca um desvio" )choice: car-detour then
;

// Park Chatting

: common2-parkchat-tipme args( @ )
	<- @ 40 %chance if 
		.shooting++ 
	else
		50 %chance if 
			.mechanic++
		else
			.medical++
		then
	then
;

cyoa-choice: common2-parkchat-tips
	cpet? if 
		($ cname .. " " .. cbarkmeow .. "s de uma forma muito sabia e experiente." $) text!
	else
		($ cname .. "  trocam conselhos sobre diferentes topicos." $) text!
	then
	cwits- if
		"O sobrevivente amigavel fica confuso com o conselho inutil." +crtext badfx witsREV
	then;
	cwits+ if
		"O sobrevivente amigavel fica iluminado pelo conselho e da otimas dicas proprias!" +crtext
		goodfx ' common2-parkchat-tipme foreach-team-char witsREV
	then;
		($ "O sobrevivente amigavel agradece e da um pequeno conselho a " .. cname .. "  em troca." $) +crtext
		cchar common2-parkchat-tipme goodfx witsREV
;

cyoa-choice: common2-parkchat-pep
	cpet? if 
		($ cname .. " " .. cbarkmeow .. "s de uma forma reconfortante." $) text!
	else
		($ cname .. "  tenta dar um discurso motivacional." $) text!
	then
	cattitude- if
		"O sobrevivente amigavel na verdade se sente pior agora." +crtext badfx attitudeREV
	then;
	cattitude+ if
		"O sobrevivente amigavel se sente muito otimista apos a conversa e faz um discurso inspirador!" +crtext
		morale2+all goodfx attitudeREV
	then;
		($ "O sobrevivente amigavel da algumas palavras tranquilizadoras em troca." $) +crtext
		morale++all goodfx attitudeREV
;

cyoa-choice: common2-parkchat-bandit
	($ cname .. " 'rouba facilmente a pessoa amigavel. Ele nao tinha muito, mas isso lhe ensinara uma licao importante sobre confiar nos outros." $) text! badfx
		2 4 rnd trunk-food+ 50 %chance if 1 3 rnd trunk-medical+ then rob-guilt
		aholeREV
;

cyoa-bridge: common2-parkchat-coolit
	cpet? if
		($ cname .. " comeca " .. "PEIDANDO" rbold .. " para a pessoa, e de alguma forma continua a faze lo por varios minutos sem parar por um momento." $)
	else
		($ cname .. " interrompe o sobrevivente amigavel no meio da frase, para lhe dizer para " .. "SE ACALMAR." pbold $)
	then text!
	cool-it-7 if
		char-gen to recruitee "Cara Amigavel" recruitee .name!
		"'Ele fica pasmo com isso. Ele afirma que tudo isso esta de acordo com as profecias e que ele esta segurando um presente para o Escolhido!" +crtext
		cooled-it goodfx rescue-trainitem morale3+all
	else cool-it++
		($ "Ele olha para " .. cname .. ", sem falar, ate " .. thegroup .. " vai embora." $) +crtext
		morale--all badfx
	then witsattREV 
;

cyoa: common2-parkchat
	"Como um dia normal" cyoa-title!
	($ Thegroup .. " encontra outra pessoa apenas relaxando em um banco de parque. Ele parece amigavel. Eles tem uma conversa, assim como antes do fim do mundo. Qual foi o assunto principal?" $) text!
		smartest charchoice( ": Troca de conselhos" )choice: common2-parkchat-tips
		gallant charchoice( ": Conversa estimulante" )choice: common2-parkchat-pep
		most-ahole if
			charchoice( ": Dando suas coisas para mim" )choice: common2-parkchat-bandit
		then
		most-grating if
			charchoice( ": Acalmando" fcoolit )choice: common2-parkchat-coolit
		then
;

// Busted leg

cyoa-choice: common2-busted-med
	($ cname .. " ' tenta tratar a perna quebrada da mulher." $) text!
	cmedical+ if
		($ cHe .. "  usa " .. chis .. "  conhecimento medico para consertar a perna dela rapidamente! Ela aprecia a ajuda e da o que pode!" $) +crtext morale++ medicalREV medical++ goodfx -1 trunk-medical+ 2 trunk-food+
	then;
	cmedical- if
		($ cHe .. "  nao faz ideia do que " .. che .. "  esta fazendo, desperdica um dia e talvez piore um pouco as coisas." $) +crtext medicalREV cloyalty- not if morale-- then loyaltyREV +cr fullcamp badfx -1 trunk-medical+ 1 calendar-day+
	then;
	medicalroll if
		($ cHe .. " ' leva um dia para ajudar a mulher a tratar sua perna. Ela e grata, mas nao pode poupar muito!" $) +crtext morale++ medicalREV medical++ goodfx 2 trunk-food+
	else
		($ cHe .. " ' tenta ajudar ao longo de um dia, mas nao consegue fazer muito. Ainda assim, a pratica leva a perfeicao." $) +crtext medicalREV medical++ cloyalty- not if morale-- then loyaltyREV badfx
	then +cr fullcamp -1 trunk-medical+ 1 calendar-day+ heal-event
