// Safehouse relaxing

cyoa: common2-saferelax
	image-door-on-brick 27 to cyoaicon
	($ Thegroup .. " 1" $) text!
	solo if
		soloer choosechar
		($ "'Mit niemandem, der sich darum kummert " .. chim .. ", " .. che .. " entscheidet sich fur:" $) +crtext
		choice( "'BERUHIGEN SIE SICH" )choice: common2-relax-morale
		choice( "Kraftubung" )choice: common2-relax-strength
		choice( "Ubung fur Fitness" )choice: common2-relax-fitness
	else
		"Mit etwas Freizeit entscheiden sie sich, zu:" +crtext
			50 %chance if 
				choice( "'BERUHIGEN SIE SICH" )choice: common2-relax-morale
			else
				50 %chance if
					choice( "Kraftubung" )choice: common2-relax-strength
				else
					choice( "Ubung fur Fitness" )choice: common2-relax-fitness
				then
			then
			mechanicest charchoice( "mechanikkenntnisse vermitteln" )choice: common2-relax-mech
			medicalest charchoice( "medizinische Fahigkeiten lehren" )choice: common2-relax-med
	then
;

