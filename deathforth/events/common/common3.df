		($ cHis .. "  senso usual de paranoia sai pela culatra! " .. cHe .. "  fica super assustado. " .. cname .. "  ve um pequeno rato e entao comeca " .. "gritando como um Youtuber" fbold .. "!" $) +crtext morale2- witsattREV sfx{ creepy }
	then;
	cchar oblivious? if
		($ cHe .. " ' nao percebe nada assustador la. Nem mesmo quando um " .. "ESQUELETO DANCANTE ASSUSTADOR" fbold .. " 'pingos do teto e comeca a fazer barulho!'" $) +crtext
		witsattREV homesearch2 goodfx
	then;
	temper+roll if
		($ cHe .. "  mantem a calma, pega o loot " .. che .. " 'Acha, e sai fora!'" $) +crtext
		temperREV homesearch2 goodfx
	then;
		($ cHe .. " perde " .. chis .. " 'Calma e sai de la, dizendo que nao tinha loot!'" $) +crtext
		temperREV morale-- badfx
;

