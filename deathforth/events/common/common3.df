;

cyoa-choice: common3-molotov-4
	($ cname .. " 'stellt 4 Molotow-Cocktails mit 40 Benzin her! Genug, um eine Menge Zombies zu rosten!" $) text! 4 WEAPON_MOLOTOV trunk.weapon+ -40 trunk-gas+
;

cyoa-choice: common3-molotov-6
	($ cname .. " macht 6 Molotow-Cocktails mit 60 Gas! Das sollte einen großen Unterschied machen!" $) text! 6 WEAPON_MOLOTOV trunk.weapon+ -60 trunk-gas+
;

cyoa-choice: common3-molotov-bottle
	($ cname .. " nimmt 6 leere Flaschen mit " .. chim .. ". Sie konnten nutzlich sein?" $) text! 6 WEAPON_BOTTLE trunk.weapon+
;

cyoa: common3-molotov
	smartest choosechar cicon "Molotow-Herstellung" cyoa-title!
	($ Thegroup .. " findet ein zertrummertes Haus mit einigen Flaschen und verfaulten Ragas. Durchsuchen es schien wie eine Abfallaufgabe, aber " .. cname .. " hat eine Idee:" $) text!
	cchar .wits 2 > if
		($ gThey .. " konnte Molotows herstellen, wenn " .. gthey .. " genugend Gas sparen kann!" $) goodfx
	else
		($ gThey .. " konnte die Flaschen mit " .. ghim .. ", um sie gegen Zombies und andere Feinde zu werfen!" $) badfx
	then +crtext witsREV
		
		cchar .wits 2 > if
			trunk-gas 20 < if
				"Leider ist nicht genug Gas da, um zu sparen." +crtext
			then
			trunk-gas 19 > if
				choice( "Herstelle 2 Molotows - 20 Gas" )choice: common3-molotov-2
			then
			trunk-gas 39 > if
				choice( "'Erstellt 4 Molotow-Cocktails - 40 Benzin" )choice: common3-molotov-4
			then
			trunk-gas 59 > if
				choice( "'Erstellt 6 Molotow-Cocktails - 60 Benzin" )choice: common3-molotov-6
			then
		else
			choice( "'Nimm die Flaschen" )choice: common3-molotov-bottle
		then
		choice( "'Verlasse das Haus" )choice
;

cyoa-choice: common3-pipe-go
	($ cname .. " 'versucht fertige Bomben herzustellen." $) text!
	cmechanic+ if
		($ cHe .. " 'schafft es muhelos, und hat am Ende 6 Rohrbomben." $) +crtext goodfx 6 WEAPON_PIPEBOMB trunk.weapon+
	else
		mechanicroll if
			($ cHe .. " " .. chim .. "'endet mit 3 Rohrbomben, wobei die Halfte von ihnen kaputt geht. " .. cHe .. " 'endet damit bei drei Pipebomben, ruiniert aber die Halfte von ihnen." $) +crtext 3 WEAPON_PIPEBOMB trunk.weapon+ goodfx
		else
			($ cHe .. " 'ist mit dieser Art von Arbeit nicht vertraut und macht einen Fehler. Ups!" $) +crtext ouch2- cchar ' .ouch01rnd-- foreach-team-char-except bitefx
		then
	then mechanicREV alive? if mechanic++ then
	
;

