			team-count 3 > if
				' common3-traptown-pick 2 #cyoa-team-choice
				choice( "Elige a alguien mas" )choice: common3-traptown-pick4
			else
				' common3-traptown-pick 3 #cyoa-team-choice
			then
		then
		choice( "Echar!" )choice
		
;

// Creepy Store

cyoa-choice: common3-creepy-go
	($ cname .. " 'entra en la oscura y espeluznante tienda." $) text!
	cchar paranoid? if
		($ cHis .. " 'su habitual paranoia le juega una mala pasada! " .. cHe .. " 'se asusta mucho. " .. cname .. " 've un pequeno raton y luego comienza a " .. "gritar como un youtuber" fbold .. "!" $) +crtext morale2- witsattREV sfx{ creepy }
	then;
	cchar oblivious? if
		($ cHe .. " 'no nota nada aterrador alli. Ni siquiera cuando un " .. "ESQUELETO bailarin SOSPRESO" fbold .. " ' cae del techo y comienza a chocar alrededor!" $) +crtext
		witsattREV homesearch2 goodfx
	then;
	temper+roll if
		($ cHe .. " ' se queda tranquilo, toma el botin " .. che .. " ' encuentra y sale de alli!" $) +crtext
		temperREV homesearch2 goodfx
	then;
		($ cHe .. " ' pierde " .. chis .. " ' se queda tranquilo y sale de alli, reclamando que no habia botin!" $) +crtext
		temperREV morale-- badfx
;

cyoa-choice: common3-creepy-allgo
	someicon ($ Thegroup .. " 'entra en la oscura y espeluznante tienda." $) text!
	angriest-pass if angriest choosechar cicon
		($ cname .. " ' confunde a un maniqui con un zombi y ataca! Esto hace que " .. everyone .. " ' se desmaya y tambien comience a atacar a ciegas. Al final, ningun maniqui quedo de pie." $) +crtext temperREV
		ouch--all bitefx
	else
		"Todos se mantienen tranquilos! La tienda es saqueada sin incidentes." +crtext goodfx
	then ' .temperREV foreach-team-char homesearch2
;

cyoa-choice: common3-creepy-pick
	"Quien debe entrar en esta tienda crepuscula?" text!
	' common3-creepy-go cyoa-team-choice
;

cyoa: common3-creepy
	"Tienda Oscura y Crepuscula" cyoa-title! image-female-zombie-1
	($ Thegroup .. " ' encuentra una tienda inundada. Hay muchos maniquis dentro, casi invisibles en la oscuridad. La tienda esta especialmente crepuscula.	Hay algun suministro podrido que queda alli." $) text!
		choice( "Ignorar este lugar" )choice
		solo if
			soloer charchoice( " 'entra en la tienda" )choice: common3-creepy-go
		else
			choice( "Manda a alguien adentro" )choice: common3-creepy-pick
			choice( "Manda a todos adentro!" )choice: common3-creepy-allgo
		then
;

// Bad Weather Siege

cyoa-choice: common3-badweather-drive
	smartest choosechar
	($ Thegroup .. " 'lanza precaucion al viento y decide continuar con la conduccion." $) text!
	cchar .wits 3 skillcheck? if 
		($ cname .. " 'esfuerza " .. chis .. " 'los ojos y logra evitar chocar con algo por poco!" $) +crtext goodfx witsREV
	else
		car-armor 0 > if
			"El auto embiste algunos obstaculos no vistos en la lluvia. Algunas de sus piezas de armadura se caen." +cr +text goodfx
			-1 ' car-armor +!
		else
			-2 -3 rnd car-chassis+ badfx
			car-chassis 0 > if
				($ "El auto resulta danado al arremeter contra algunos escombros en la carretera, pero sigue funcionando. " .. thegroup .. " 'no vio a tiempo, pero sigue funcionando." $) +crtext
			else
				($ "El auto choca contra el lado de un auto destruido en la carretera y queda destrozado. " .. Thegroup .. " 'comienza a caminar." $) +crtext
			then 
		then
	then
;

cyoa-choice: common3-badweather-fight
	($ Thegroup .. " 'no se instala ni siquiera en esta casa antes de darse cuenta de que una horda grande se esta acercando desde la lluvia torrencial." $) text!
	2.3 15 20 rnd 1 siegecast // 1.7
	"events/siege/region/midopen1.df" $load-region
	choice( "Trata de Sobrevivir" )choice: premission
;

cyoa-choice: common3-badweather-camp
