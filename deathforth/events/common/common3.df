;

cyoa-choice: common3-molotov-4
	($ cname .. " 'hace 4 cocteles molotov con 40 gas! Suficiente para asar a un monton de zombies!" $) text! 4 WEAPON_MOLOTOV trunk.weapon+ -40 trunk-gas+
;

cyoa-choice: common3-molotov-6
	($ cname .. " 'hace 6 cocteles molotov con 60 gas! Esto deberia marcar una gran diferencia!" $) text! 6 WEAPON_MOLOTOV trunk.weapon+ -60 trunk-gas+
;

cyoa-choice: common3-molotov-bottle
	($ cname .. " 'toma 6 botellas vacias con " .. chim .. ". Tal vez les sea util?" $) text! 6 WEAPON_BOTTLE trunk.weapon+
;

cyoa: common3-molotov
	smartest choosechar cicon "Fabricacion de cocteles molotov" cyoa-title!
	($ Thegroup .. " 'encuentra una casa abandonada con algunas botellas y trapos rotos. Buscar en ella parecia una perdida de tiempo, pero " .. cname .. " 'tiene una idea:" $) text!
	cchar .wits 2 > if
		($ gThey .. " 'podrias hacer cocteles molotov, si " .. gthey .. " 'puedes prescindir de la gasolina!" $) goodfx
	else
		($ gThey .. " 'podrias tomar las botellas con " .. ghim .. ", para lanzarlas a los zombies y otros enemigos!" $) badfx
	then +crtext witsREV
		
		cchar .wits 2 > if
			trunk-gas 20 < if
				"Lamentablemente, no hay suficiente gasolina para spares." +crtext
			then
			trunk-gas 19 > if
				choice( "Hace 2 molotovs - 20 gasolina" )choice: common3-molotov-2
			then
			trunk-gas 39 > if
				choice( "Hace 4 molotovs - 40 gasolina" )choice: common3-molotov-4
			then
			trunk-gas 59 > if
				choice( "Hace 6 molotovs - 60 gasolina" )choice: common3-molotov-6
			then
		else
			choice( "Toma las botellas" )choice: common3-molotov-bottle
		then
		choice( "Abandona la casa" )choice
;

