		else
			"Enviar un explorador para buscar botin?" +crtext
			team-count 3 > if
				' common3-traptown-pick 2 #cyoa-team-choice
				choice( "Escoge a alguien mas" )choice: common3-traptown-pick4
			else
				' common3-traptown-pick 3 #cyoa-team-choice
			then
		then
		choice( "Alejate!" )choice
		
;

// Creepy Store

cyoa-choice: common3-creepy-go
	($ cname .. "  entra a la tienda oscura y espeluznante." $) text!
	cchar paranoid? if
		($ cHis .. "  el habitual sentido de paranoia le sale por la culata " .. cHe .. "  se asusta mucho. " .. cname .. "  ve un pequeno raton y luego comienza " .. "gritando como un Youtuber" fbold .. "!" $) +crtext morale2- witsattREV sfx{ creepy }
	then;
	cchar oblivious? if
		($ cHe .. " ' no nota nada aterrador alli. Ni siquiera cuando un " .. "ESQUELETO DE BAILE ESPELUZNANTE" fbold .. "  cae del techo y comienza a hacer ruido!" $) +crtext
		witsattREV homesearch2 goodfx
	then;
	temper+roll if
		($ cHe .. "  mantiene la calma, toma el botin " .. che .. "  encuentra y sale de ahi!" $) +crtext
		temperREV homesearch2 goodfx
	then;
		($ cHe .. "  pierde " .. chis .. "  calma y sale corriendo de alli, afirmando que no habia botin!" $) +crtext
		temperREV morale-- badfx
;

cyoa-choice: common3-creepy-allgo
	someicon ($ Thegroup .. "  entra a la tienda oscura y espeluznante." $) text!
	angriest-pass if angriest choosechar cicon
		($ cname .. "  confunde un maniqui con un zombi y ataca! Esto hace que " .. everyone .. "  se asuste y tambien comience a atacar a ciegas. Al final, no quedo ningun maniqui en pie." $) +crtext temperREV
		ouch--all bitefx
	else
		"Todos mantienen la calma! La tienda es saqueada sin incidentes." +crtext goodfx
	then ' .temperREV foreach-team-char homesearch2
;

cyoa-choice: common3-creepy-pick
	"Quien deberia entrar a la espeluznante tienda?" text!
	' common3-creepy-go cyoa-team-choice
;

cyoa: common3-creepy
	"Tienda oscura y espeluznante" cyoa-title! image-female-zombie-1
	($ Thegroup .. " ' encuentra una tienda inundada. Hay muchos maniquies adentro, apenas visibles en la oscuridad. Toda la tienda es especialmente espeluznante. Puede que queden algunos suministros mohosos alli." $) text!
		choice( "Ignorar este lugar" )choice
		solo if
			soloer charchoice( "  entra en la tienda" )choice: common3-creepy-go
		else
			choice( "Enviar a alguien" )choice: common3-creepy-pick
			choice( "Enviar a todos!" )choice: common3-creepy-allgo
		then
;

// Bad Weather Siege

cyoa-choice: common3-badweather-drive
	smartest choosechar
	($ Thegroup .. "  tira la precaucion al viento y decide seguir conduciendo." $) text!
	cchar .wits 3 skillcheck? if 
		($ cname .. " cepas " .. chis .. " ojos y logra evitar por poco chocar contra algo!" $) +crtext goodfx witsREV
	else
		car-armor 0 > if
			"La automovil se estrella a traves de algun obstaculo desconocido en la lluvia. Algunas piezas de su blindaje caen." +cr +text goodfx
			-1 ' car-armor +!
		else
			-2 -3 rnd car-chassis+ badfx
			car-chassis 0 > if
				($ "Se dana arrastrando algunos escombros que no habia visto a tiempo pero sigue corriendo " .. thegroup .. " 'no la vio a tiempo, pero sigue corriendo." $) +crtext
			else
				($ "La automovil se estrella directamente en el lateral de un coche destrozado en la carretera y queda danada. " .. Thegroup .. " comienza a caminar." $) +crtext
			then 
		then
	then
;

cyoa-choice: common3-badweather-fight
	($ Thegroup .. " no se ha asentado en esta casa antes de saber que una gran horda se acerca desde la lluvia torrencial." $) text!
	2.3 15 20 rnd 1 siegecast // 1.7
	"events/siege/region/midopen1.df" $load-region
	choice( "Intenta Sobrevivir" )choice: premission
;

cyoa-choice: common3-badweather-camp
	someicon ($ Thegroup .. " se desvia por el camino menos transitado y encuentra un lugar para acampar hasta que mejore el clima." $) text! chowday
	"Tiene que pasar un dia extra antes de poder continuar el viaje debido a que el clima no se mejora lo suficiente. " +crtext
	+cr fullcamp heal-event 1 calendar-day+
;

cyoa: common3-badweather
	"Conduccion en mal tiempo" cyoa-title!
	image-water-driving 73 to cyoaicon
	($ "La lluvia empieza a caer. Es dificil ver, y las formas de los zombies acechan justo en el borde de la oscuridad " .. Thegroup .. " no esta seguro de que seria peor: continuar o detenerse aqui." $) text!
	vehicle.which@ 0 > if choice( "Siga Conduciendo" )choice: common3-badweather-drive then
	choice( "Busca refugio en la casa" )choice: common3-badweather-fight
	fullmeal if
		choice( "Baja por el camino lateral, acampa" )choice: common3-badweather-camp
	then 1.4 2 frnd weather! -2 2 frnd wind_force!
;

cyoa-choice: common3-horde-plow
	($ Thegroup .. " decide intentar intentar conducir a traves de la horda!" $) text! car-plowthrough
;

cyoa-choice: common3-horde-fight
	($ Thegroup .. " se esconde en una casa cercana para esperar a que la horda se disperse. Los zombies terminan agitandose y atacan!" $) text!
	2.4 15 20 rnd 1 siegecast // 1.7
	"events/siege/region/midopen1.df" $load-region
	choice( "Intenta Sobrevivir" )choice: premission
;

cyoa: common3-horde
	"Trafico Pesado" cyoa-title!
	image-zombie-hoard-1 9 to cyoaicon
	($ "Hay una gran horda de zombies en el camino por delante " .. Thegroup .. " ' no esta seguro si " .. gthey .. "  podria atravesarlos o no." $) text!
	choice( "Busca refugio en la casa" )choice: common3-horde-fight
	vehicle.which@ 0 > if 
		choice( "Tomar un desvio" )choice: car-detour
		choice( "Ara a traves!" )choice: common3-horde-plow 
	then
;


