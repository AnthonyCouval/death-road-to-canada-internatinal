cyoa-choice: common3-badweather-drive
	smartest choosechar
	($ Thegroup .. "  wirft alle Vorsicht uber Bord und beschliet weiterzufahren." $) text!
	cchar .wits 3 skillcheck? if 
		($ cname .. "  anstrengungen " .. chis .. "  augen und schafft es knapp zu vermeiden in etwas zu sturzen!" $) +crtext goodfx witsREV
	else
		car-armor 0 > if
			"Das Auto stot durch einige unsichtbare Hindernisse im Regen. Ein Teil seiner Rustung fallt ab." +cr +text goodfx
			-1 ' car-armor +!
		else
			-2 -3 rnd car-chassis+ badfx
			car-chassis 0 > if
				($ "Das Auto wird durch einige Trummer beschadigt " .. thegroup .. " ' sah es nicht rechtzeitig, aber es lauft noch." $) +crtext
			else
				($ "Das Auto pflugt direkt in die Seite eines ruinierten Autos auf der Strae und wird zerstort. " .. Thegroup .. "  beginnt zu laufen." $) +crtext
			then 
		then
	then
;

