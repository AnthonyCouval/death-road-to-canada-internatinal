cyoa: common3-traptown
	"Stadt der Fallen" cyoa-title!
	($ Thegroup .. " 1.5" $) text!
		solo if
			($ "'Diese Gegend nach Beute durchsuchen konnte ', " .. "'sehr gefahrlich sein" rbold .. "', insbesondere da er ', " .. soloname .. " 'alleine ist!" $) +crtext
			soloer charchoice( " 'versucht zu erkunden" )choice: common3-traptown-pick
		else
			"'Senden Sie einen Scout, um nach Beute zu suchen?" +crtext
			team-count 3 > if
				' common3-traptown-pick 2 #cyoa-team-choice
				choice( "'Wahle jemand anderen" )choice: common3-traptown-pick4
			else
				' common3-traptown-pick 3 #cyoa-team-choice
			then
		then
		choice( "'Wege clear!" )choice
		
;

// Creepy Store

cyoa-choice: common3-creepy-go
	($ cname .. " ' betritt den dunklen und unheimlichen Laden." $) text!
	cchar paranoid? if
		($ cHis .. " 'seine ubliche Sinnesparanoia ruckgangig! ', " .. cHe .. " ' wird sehr aufgeregt. ', " .. cname .. " ' sieht eine kleine Maus und dann beginnt er ', " .. "'wie ein YouTuber zu schreien." fbold .. "!" $) +crtext morale2- witsattREV sfx{ creepy }
	then;
	cchar oblivious? if
		($ cHe .. " 1. " .. "GRUSELIGER TANZENDER SKELETT" fbold .. " 'Tropfen von der Decke und fangt an zu klappern!'" $) +crtext
		witsattREV homesearch2 goodfx
	then;
	temper+roll if
		($ cHe .. "  ruhig bleiben, die Beute nehmen " .. che .. " 'findet, und kommt da raus!'" $) +crtext
		temperREV homesearch2 goodfx
	then;
		($ cHe .. " 'verliert " .. chis .. " 'Ruhe und raus hier, es gab nichts zu holen!'" $) +crtext
		temperREV morale-- badfx
;

cyoa-choice: common3-creepy-allgo
	someicon ($ Thegroup .. " ' betritt den dunklen und unheimlichen Laden." $) text!
	angriest-pass if angriest choosechar cicon
		($ cname .. " 1. " .. everyone .. " 0" $) +crtext temperREV
		ouch--all bitefx
	else
		"1" +crtext goodfx
	then ' .temperREV foreach-team-char homesearch2
;

cyoa-choice: common3-creepy-pick
	"Wer soll den gruseligen Laden betreten?" text!
	' common3-creepy-go cyoa-team-choice
;

cyoa: common3-creepy
	"'Dunkel und unheimlich Laden" cyoa-title! image-female-zombie-1
	($ Thegroup .. " 'findet ein wassergesattigtes Lager. Es gibt viele Mannequins drauf, kaum sichtbar in der Dunkelheit. Das ganze Lager ist besonders unheimlich.	Es konnten noch vergangliche Versorgungsguter darin zuruckgeblieben sein." $) text!
		choice( "'Diesen Ort ignorieren" )choice
		solo if
			soloer charchoice( " 'geht in den Laden ein" )choice: common3-creepy-go
		else
			choice( "'Senden jemanden hinein" )choice: common3-creepy-pick
			choice( "'Senden alle hinein!" )choice: common3-creepy-allgo
		then
;

// Bad Weather Siege

cyoa-choice: common3-badweather-drive
	smartest choosechar
	($ Thegroup .. " 'wirft Vorsicht auf die Seite und beschließt, weiterzufahren." $) text!
	cchar .wits 3 skillcheck? if 
		($ cname .. " 'ansprengt " .. chis .. " 'Augen weit aufgestrekt und schafft es, in etwas zu kollidieren!" $) +crtext goodfx witsREV
	else
		car-armor 0 > if
			"'Das Auto prallt durch ungesehene Hindernisse im Regen. Ein Teil seines Panzerungsmaterials fallt ab." +cr +text goodfx
			-1 ' car-armor +!
		else
			-2 -3 rnd car-chassis+ badfx
			car-chassis 0 > if
				($ "'Das Auto wird beschadigt, indem es in etwas sturzt, das man nicht rechtzeitig sah, aber noch laufen bleibt. " .. thegroup .. " 'hatte nicht rechtzeitig gesehen, aber es lauft noch." $) +crtext
			else
				($ "'der Wagen rast in die Seite eines Wracks auf der Straße und wird zerstort. " .. Thegroup .. " ' fangt an zu gehen." $) +crtext
			then 
		then
	then
;

cyoa-choice: common3-badweather-fight
	($ Thegroup .. " 'ist noch nicht einmal richtig in diesem Haus eingezogen, bevor sie bemerken, dass eine große Horde auf sie zukommt, die sich aus dem stromenden Regen nahert." $) text!
	2.3 15 20 rnd 1 siegecast // 1.7
	"events/siege/region/midopen1.df" $load-region
	choice( "'Versuche zu uberleben" )choice: premission
;

cyoa-choice: common3-badweather-camp
	someicon ($ Thegroup .. " 'nimmt die weniger befahrene Seitenstraße und findet einen Platz zum Campen, bis das Wetter wieder klar ist." $) text! chowday
	"'Es dauert einen zusatzlichen Tag, bis sich das Wetter genug verbessert hat, um die Reise fortzusetzen. " +crtext
	+cr fullcamp heal-event 1 calendar-day+
;

cyoa: common3-badweather
	"'Autofahren bei schlechtem Wetter" cyoa-title!
	image-water-driving 73 to cyoaicon
	($ "'Es regnet in Stromen. Man sieht schlecht, und die Umrisse von Zombies tauchen am Rand der Dunkelheit auf. " .. Thegroup .. " 'weiß nicht, was schlimmer ware: weiterzufahren oder hier anzuhalten." $) text!
	vehicle.which@ 0 > if choice( "'Weiterfahren" )choice: common3-badweather-drive then
	choice( "'Unterkommen in einem Haus" )choice: common3-badweather-fight
	fullmeal if
		choice( "'Zu einer Nebenstraße abbiegen, campen" )choice: common3-badweather-camp
	then 1.4 2 frnd weather! -2 2 frnd wind_force!
;

cyoa-choice: common3-horde-plow
	($ Thegroup .. " 'versucht, durch die Horde zu fahren!" $) text! car-plowthrough
;

cyoa-choice: common3-horde-fight
	($ Thegroup .. " 'versteckt sich in einem nahegelegenen Haus, um auf die Auflosung der Horde zu warten. Die Zombies werden jedoch unruhig und greifen an!" $) text!
	2.4 15 20 rnd 1 siegecast // 1.7
	"events/siege/region/midopen1.df" $load-region
	choice( "'Versuche zu uberleben" )choice: premission
;

cyoa: common3-horde
	"Schwerer Verkehr" cyoa-title!
	image-zombie-hoard-1 9 to cyoaicon
	($ " Es gibt eine große Horde von Zombies auf der Straße voraus. " .. Thegroup .. "  ist sich nicht sicher, ob " .. gthey .. "  ob sie durch sie hindurchkommen könnten oder nicht." $) text!
	choice( "'Unterkommen in einem Haus" )choice: common3-horde-fight
	vehicle.which@ 0 > if 
		choice( "Einen Umweg machen" )choice: car-detour
		choice( "Durchpflugen!" )choice: common3-horde-plow 
	then
;


