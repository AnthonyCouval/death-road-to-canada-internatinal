cyoa: common3-molotov
	smartest choosechar cicon "Fabricacion de Molotov" cyoa-title!
	($ Thegroup .. "  encuentra una casa destrozada con algunas botellas y trapos andrajosos. Buscar parecia una perdida de tiempo, pero " .. cname .. "  tiene una idea:" $) text!
	cchar .wits 2 > if
		($ gThey .. " " .. gthey .. " puede ahorrar gasolina!" $) goodfx
	else
		($ gThey .. "  podria llevarse las botellas con " .. ghim .. ", para arrojarlas a los zombis y otros enemigos" $) badfx
	then +crtext witsREV
		
		cchar .wits 2 > if
			trunk-gas 20 < if
				"'Lamentablemente, no hay suficiente gas de sobra." +crtext
			then
			trunk-gas 19 > if
				choice( "Hacer 2 cocteles molotov   20 gas" )choice: common3-molotov-2
			then
			trunk-gas 39 > if
				choice( "Hacer 4 cocteles molotov   40 gas" )choice: common3-molotov-4
			then
			trunk-gas 59 > if
				choice( "Hacer 6 cocteles molotov   60 gas" )choice: common3-molotov-6
			then
		else
			choice( "Tomar las botellas" )choice: common3-molotov-bottle
		then
		choice( "Abandonar la casa" )choice
;

