cyoa: common3-molotov
	smartest choosechar cicon "Fazendo Molotov" cyoa-title!
	($ Thegroup .. "  encontra uma casa revirada com algumas garrafas e trapos rasgados. Parecia perda de tempo procurar, mas " .. cname .. "  tem uma ideia:" $) text!
	cchar .wits 2 > if
		($ gThey .. "  poderia fazer coqueteis molotov, se " .. gthey .. "  puder poupar a gasolina!" $) goodfx
	else
		($ gThey .. "  poderia levar as garrafas com " .. ghim .. ", para jogar em zumbis e outros inimigos!" $) badfx
	then +crtext witsREV
		
		cchar .wits 2 > if
			trunk-gas 20 < if
				"'Infelizmente, nao ha gasolina suficiente para poupar." +crtext
			then
			trunk-gas 19 > if
				choice( "Fazer 2 coqueteis molotov - 20 de gasolina" )choice: common3-molotov-2
			then
			trunk-gas 39 > if
				choice( "Fazer 4 coqueteis molotov - 40 de gasolina" )choice: common3-molotov-4
			then
			trunk-gas 59 > if
				choice( "Fazer 6 coqueteis molotov - 60 de gasolina" )choice: common3-molotov-6
			then
		else
			choice( "Pegar as garrafas" )choice: common3-molotov-bottle
		then
		choice( "Sair da casa" )choice
;

