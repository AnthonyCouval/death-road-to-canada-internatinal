// Gloomy Weather

: common3-gloom-attcheck args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		($ <- @ .colourname .. "  mantiene una actitud positiva!" $) +crtext
	then;
	<- @ .cattitude- if
		<- @ .morale2-
	else
		<- @ .morale--
	then	
;

cyoa-choice: common3-gloomy-pick
	($ cname .. "  comienza a hablar, para mejorar el estado de animo." $) text!
	cpet? if
		($ cHe .. "  hace " .. cdogcat .. " ' ruidos. Es una distraccion bastante decente del clima!" $) +crtext morale++	goodfx
	then;
	cchar grating? if
		($ cHe .. "  cuenta " .. everyone .. " a " .. "  TRANQUILICENSE." pbold $) +crtext
		cool-it-7 if
			($ Everyone .. "  se rie de buena gana! Esta frase nunca pasa de moda!" $) +text
			morale2+all goodfx
		else cool-it++
			"Esto NO va bien en absoluto." +text morale3-all badfx
		then witsattREV
	then;
	ctemper- if
		($ cHe .. "  pierde " .. chis .. "  mal humor y empieza a desahogarse sobre el clima! " .. cHe .. "  se siente mejor pero " .. everyone .. "  tiene que soportar las peroratas." $) +crtext badfx
		temperREV morale++ cchar ' .morale-- foreach-team-char-except
	then;
	cattitude+ if
		($ cHe .. "  logra distraer " .. everyone .. "  del clima horrible!" $) +crtext goodfx attitudeREV
	then;
	cattitude- if
		($ cHe .. "  se queja y se queja, tanto del clima como de otros temas no relacionados." $) +crtext badfx
		attitudeREV morale2-all
	then;
	"'No ayuda mucho." +crtext badfx
	' common3-gloom-attcheck foreach-team-char
;

cyoa: common3-gloomy
	"Viaje de clima sombrio" cyoa-title! someicon
	($ "'El clima es gris y lugubre y realmente se esta poniendo " .. thegroup .. "  deprimido. " $) text!
	solo if
		($ cHe .. "  no tiene a nadie con quien quejarse del clima." $) +text
		soloer common3-gloom-attcheck
	else
		"Alguien decide hablar para ayudar con el estado de animo:" +crtext
		' common3-gloomy-pick cyoa-team-choice
	then 0.4 1 frnd weather! -1 1 frnd wind_force!

;

