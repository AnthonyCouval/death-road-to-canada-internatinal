// Gloomy Weather

: common3-gloom-attcheck args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		($ <- @ .colourname .. " 'halt eine positive Einstellung aufrecht!" $) +crtext
	then;
	<- @ .cattitude- if
		<- @ .morale2-
	else
		<- @ .morale--
	then	
;

cyoa-choice: common3-gloomy-pick
	($ cname .. " 'fangt an zu sprechen, um die Stimmung zu verbessern." $) text!
	cpet? if
		($ cHe .. " ' macht " .. cdogcat .. " 'macht Gerausche. Ist eine ganz gute Ablenkung vom Wetter!" $) +crtext morale++	goodfx
	then;
	cchar grating? if
		($ cHe .. " " .. everyone .. " " .. " KUHL ES." pbold $) +crtext
		cool-it-7 if
			($ Everyone .. " 'lacht herzhaft! Dieser Spruch wird nie alt!" $) +text
			morale2+all goodfx
		else cool-it++
			"'Das geht GAR NICHT!" +text morale3-all badfx
		then witsattREV
	then;
	ctemper- if
		($ cHe .. " 'verliert " .. chis .. " 'lasst seinen Arger raus und beschwert sich uber das Wetter! " .. cHe .. " 'fuhlt sich danach besser, aber " .. everyone .. " 'muss die Beschwerden ertragen." $) +crtext badfx
		temperREV morale++ cchar ' .morale-- foreach-team-char-except
	then;
	cattitude+ if
		($ cHe .. " 'lenkt ab " .. everyone .. " 'von dem miesen Wetter!" $) +crtext goodfx attitudeREV
	then;
	cattitude- if
		($ cHe .. " 'quengelt und quengelt, uber das Wetter und andere Themen, die nichts damit zu tun haben." $) +crtext badfx
		attitudeREV morale2-all
	then;
	"'Das hilft nicht viel." +crtext badfx
	' common3-gloom-attcheck foreach-team-char
;

cyoa: common3-gloomy
	"Dusteres Wetter Ausflug" cyoa-title! someicon
	($ "The weather is gray and dingy and it's really getting me down. " .. thegroup .. "  nach unten " $) text!
	solo if
		($ cHe .. " hat niemanden, uber das Wetter zu klagen." $) +text
		soloer common3-gloom-attcheck
	else
		"Jemand entscheidet sich, etwas zu sagen, um die Stimmung zu heben." +crtext
		' common3-gloomy-pick cyoa-team-choice
	then 0.4 1 frnd weather! -1 1 frnd wind_force!

;

