cyoa-choice: common3-fart-smart
	notcchar to victim
	($ cname .. " 'erklart eine komplizierte Reihe von Schlußfolgerungen, die zu einer einzigen Schlussfolgerung kommen:" $) text!
	cattitude+ if
	($ "'Der Fart stammt von einem " .. "GHOST!!! WWWWHHHHOAAAAAAAAA!" fbold $) +crtext witsattREV morale++all goodfx
	then;
	($ "'Es war " .. vname .. " ', der der Farter war!" $) +crtext victim .morale2- badfx
	cloyalty- if
		($ "'Geheimnisvoll war es tatsachlich " .. cname .. " ', der der " .. "'Furzmonster war!" rbold $) +crtext
		loyaltyREV
	then witsREV morale++
;

cyoa: common3-fart
	"'Das Geheimnis der Geisterfahrt" cyoa-title!
	image-bad-smell 33 to cyoaicon
	($ "'Es ist nur ein weiterer Tag auf der Todessstraße, wenn jemand " .. "'im Auto furzt." rbold $) text!
	solo if
		"'Wer konnte das getan haben? Wir werden es vielleicht nie, nie wissen." +crtext 1 trunk-gas+ badfx
	then;
	solohuman if choosepet
		($ "'Wer konnte das getan haben? Es war wahrscheinlich " .. cname .. "'. Was fur ein schlechtes " .. cdogcat .. "!" $) +crtext morale-- badfx
	then;
		"'Drama und Mysterium verzehren das Team.		Wahlen Sie jemanden, um die Schuld zu geben:" +crtext
		most-grating if
			charchoice( " " .. everyone .. " ' um ES AUSZUHALTEN" )choice: common3-fart-coolit
		then
		smartest-pass if 
			smartest charchoice( " lost das Geheimnis" )choice: common3-fart-smart
		then
		' common3-fart-blame cyoa-team-choice
;

// Alligator

: common3-gator-hotwire
	mechanicest choosechar cmechanic+ if cicon
		($ cname .. " schafft es, das Auto zu starten!" $) +crtext mechanicREV goodfx
	else
		($ "'Ohne guten Mechaniker gibt es keinen anderen Weg fur'; " .. thegroup .. "  das Auto starten!" $) +crtext
		car-abandon 1 to gator-ate-key
	then
;

cyoa-choice: common3-gator-str
	($ cname .. " kampft mit dem Alligator um die Schlussel!" $) text!
	strength+roll if
		($ "'Der Alligator wird besiegt von ', " .. cname .. "'seinem rohen Kraft! Es wirft die Schlussel heraus." $) +crtext goodfx strength++
	else
		($ "'Der Alligator gewinnt und es nimmt einen großen Biss aus ', " .. cname .. "'! Es legt die Schlussel ab." $) +crtext ouch2- bitefx
	then alive? if strengthREV then
;

cyoa-choice: common3-gator-fit
	($ cname .. " 'belastigt den Alligator indem es ihm nachjagt und sich davon absetzt, hoffend, dass er die Schlussel verliert." $) text!
	fitness+roll if
		($ "'Der Alligator wird erschopft von ', " .. cname .. "'Energie! Die Schlussel fallen schließlich aus seinem Maul heraus." $) +crtext goodfx fitness++
	else
		($ "'greift in und beißt ', " .. cname .. " " .. che .. " 'kann mude werden! Der Alligator wirft die Schlussel ab." $) +crtext ouch2- bitefx
	then alive? if fitnessREV then
;

cyoa-choice: common3-gator-shoot
	($ cname .. " 'schießt auf den Alligator!" $) text!
	shooting+roll if
		($ "'Der Alligator wird von " .. cname .. "''s treffsichere Aiming! Es lasst die Autoschlussel fallen." $) +crtext goodfx shooting++
		shootingREV
	else
		($ cname .. " '' verfehlt den Schuss. Der Alligator schluckt die Schlussel und watschelt davon." $) +crtext
		shootingREV common3-gator-hotwire
	then
;

cyoa-choice: common3-gator-obliv
	($ cname .. " ' geht einfach hin und nimmt die Autoschlussel einfach aus dem Maul des Alligators! Kein Problem! Der Alligator hat nicht mit dieser entspannten Aktion gerechnet. Es ist so uberrascht, dass es nicht einmal seine Kiefer schnappen ließ. " .. Thegroup .. " ' fahrt das Auto weg!" $) text! goodfx witsattREV
;

cyoa-choice: common3-gator-mech
	someicon
	($ Thegroup .. " ' entscheidet, dass die Autoschlussel nicht das Risiko wert sind, sich mit einem extrem gefahrlichen Alligator auseinanderzusetzen. Der Alligator ist traurig, dass sein Plan nicht aufgeht, und verliert einen Moralpunkt."  $) text!
	common3-gator-hotwire
;

cyoa: common3-gator
	"'Verlegte Schlussel" cyoa-title!
	($ Thegroup .. " ' findet die Autoschlussel nicht! Nach einer Weile, " .. gthey .. " 'Platz" .. g1s .. " die Schlussel liegen in einem " .. "'im offenen Maul eines Alligators" rbold .. "'. Nachdem die meisten Menschen verschwunden sind, haben Alligatoren angefangen, sich entlang der gesamten Ostkuste auszubreiten. Es wird gemunkelt, dass die Alligatoren Zombies und Autoschlussel fressen." $) text!
		strongest charchoice( " ' kampft mit dem Alligator" )choice: common3-gator-str
		fittest charchoice( " ' erschopft den Alligator" )choice: common3-gator-fit
