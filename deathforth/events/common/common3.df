cyoa-choice: common3-fart-smart
	notcchar to victim
	($ cname .. "  explica una complicada secuencia de deduccion que llega a una conclusion:" $) text!
	cattitude+ if
	($ "El pedo fue hecho por un " .. "GHOST!!! WWWWHHHHOAAAAAAAAA!" fbold $) +crtext witsattREV morale++all goodfx
	then;
	($ "Era " .. vname .. "  quien era el pedorro!" $) +crtext victim .morale2- badfx
	cloyalty- if
		($ "En secreto, en realidad era " .. cname .. "  quien fue el " .. "monstruo del pedo!" rbold $) +crtext
		loyaltyREV
	then witsREV morale++
;

cyoa: common3-fart
	"El misterio del pedo fantasma" cyoa-title!
	image-bad-smell 33 to cyoaicon
	($ "'Es solo otro dia en la Carretera de la Muerte cuando alguien " .. "se tira un pedo en el auto." rbold $) text!
	solo if
		"Quien pudo haber hecho esto? Es posible que nunca, nunca lo sepamos." +crtext 1 trunk-gas+ badfx
	then;
	solohuman if choosepet
		($ "Quien pudo haber hecho esto? Probablemente fue " .. cname .. ". Que mal " .. cdogcat .. "!" $) +crtext morale-- badfx
	then;
		"El drama y el misterio consumen al equipo. Elige a alguien a quien culpar:" +crtext
		most-grating if
			charchoice( "  cuenta " .. everyone .. "  que se ENFRIE" )choice: common3-fart-coolit
		then
		smartest-pass if 
			smartest charchoice( "  resuelve el misterio" )choice: common3-fart-smart
		then
		' common3-fart-blame cyoa-team-choice
