	cpet? if
		($ "'Wow! Das ist eine schlechte " .. cdogcat .. "!" $) +crtext morale-- badfx
	then;
	cattitude+ if ($ cHe .. " 'schuttelt den Kopf in einer freundlichen Art." $) +crtext attitudeREV else morale2- then
	"'Das Geruch halt an!" +crtext
	cchar ' .morale-- foreach-team-char-except badfx
	"Was this the real culprit? Who can say..." +crtext
;

cyoa-choice: common3-fart-coolit
	cpet? if
		($ cname .. " 'gibt ein " .. "'EXTREM KLARES ZWEITES FARSTERL" rbold $) text!
		"'Ratsel aufgelost!" +crtext
	else
		($ cname .. " " .. everyone .. " " .. "'RUHE ONDOWN." pbold $) text!
		($ "'PERLEN Nervensafts" rbold .. " 'rollen herab " .. cname .. "'sein Gesicht. Aber nichts kann bewiesen werden!" $) +crtext
	then cool-it++ cchar ' .morale3- foreach-team-char-except witsattREV badfx
