cyoa-choice: common3-molotov-2
	($ cname .. "  macht 2 Molotowcocktails mit 20 Gas! Sei vorsichtig mit denen!" $) text! 2 WEAPON_MOLOTOV trunk.weapon+ -20 trunk-gas+
;

cyoa-choice: common3-molotov-4
	($ cname .. "  macht 4 Molotowcocktails mit 40 Gas! Genug um ein paar Zombies zu rosten!" $) text! 4 WEAPON_MOLOTOV trunk.weapon+ -40 trunk-gas+
;

cyoa-choice: common3-molotov-6
	($ cname .. "  macht 6 Molotowcocktails mit 60 Gas! Das sollte einen groen Unterschied machen!" $) text! 6 WEAPON_MOLOTOV trunk.weapon+ -60 trunk-gas+
;

cyoa-choice: common3-molotov-bottle
	($ cname .. "  nimmt 6 leere Flaschen mit " .. chim .. ". Sie konnten sich als nutzlich erweisen?" $) text! 6 WEAPON_BOTTLE trunk.weapon+
;

cyoa: common3-molotov
	smartest choosechar cicon "Molotowherstellung" cyoa-title!
	($ Thegroup .. "  findet ein verwustetes Haus mit ein paar Flaschen und zerfetzten Lappen. Es schien Zeitverschwendung zu sein, es zu durchsuchen, aber " .. cname .. "  hat eine Idee:" $) text!
	cchar .wits 2 > if
		($ gThey .. "  konnte Molotows herstellen, wenn " .. gthey .. "  benzin ubrig hat!" $) goodfx
	else
		($ gThey .. "  konnte die Flaschen mitnehmen " .. ghim .. ", um sie auf Zombies und andere Feinde zu werfen!" $) badfx
	then +crtext witsREV
		
		cchar .wits 2 > if
			trunk-gas 20 < if
				"'Leider gibt es nicht genug Benzin ubrig." +crtext
			then
			trunk-gas 19 > if
				choice( "Mache 2 Molotows   20 Gas" )choice: common3-molotov-2
			then
			trunk-gas 39 > if
				choice( "Mache 4 Molotows   40 Gas" )choice: common3-molotov-4
			then
			trunk-gas 59 > if
				choice( "Mache 6 Molotows   60 Gas" )choice: common3-molotov-6
			then
		else
			choice( "Nimm die Flaschen" )choice: common3-molotov-bottle
		then
		choice( "Verlasse das Haus" )choice
;

cyoa-choice: common3-pipe-go
	($ cname .. "  versucht die Bomben fertigzustellen." $) text!
	cmechanic+ if
		($ cHe .. "  beendet sie problemlos und erhalt am Ende 6 Rohrbomben." $) +crtext goodfx 6 WEAPON_PIPEBOMB trunk.weapon+
	else
		mechanicroll if
			($ cHe .. "  schafft es sich nicht " .. chim .. "selbst in die Luft zu jagen, teilweise durch Gluck. " .. cHe .. "  erhalt am Ende 3 Rohrbomben und ruiniert die Halfte davon." $) +crtext 3 WEAPON_PIPEBOMB trunk.weapon+ goodfx
		else
			($ cHe .. " ' fuhlt sich bei solchen Arbeiten nicht wohl und macht einen Fehler. Hoppla!" $) +crtext ouch2- cchar ' .ouch01rnd-- foreach-team-char-except bitefx
		then
	then mechanicREV alive? if mechanic++ then
	
;

cyoa: common3-pipe
	someicon "Bomber Zombie" cyoa-title!
	($ Thegroup .. " ' findet ein verlassenes Versteck. Darin befindet sich ein wenig Munition." $) text!
	ammosearch1
	($ "Bei weiterer Suche werden einige unfertige Rohrbomben im Gebaude gefunden. Jemand musste wohl in Eile verschwinden. " .. "Mit ihnen zu spielen konnte wirklich gefahrlich sein!" rbold $) +crtext
		choice( "Lass sie in Ruhe!" )choice
		mechanicest charchoice( ": Mach sie fertig" )choice: common3-pipe-go
;


cyoa-choice: common3-bridgekeep-plow
	someicon
	($ Thegroup .. "  ignoriert einfach den gepanzerten Mann der auf der Brucke steht und kracht hindurch. " .. "DONNER!" rbold $) text!
	car-plowthrough
;

cyoa-choice: common3-bridgekeep-rage
	($ cname .. "  sturmt direkt auf den Sport Ausrustungs Panzermann zu, packt ihn an der Taille und wirft ihn uber die Brucke. " .. "PLATSCH!" fbold $) text! goodfx rageREV morale++
;

cyoa-bridge: common3-bridgekeep-push
	someicon
	($ Thegroup .. "  steigt aus dem Auto und versucht den Bruckenwachter von der Brucke zu stoen." $) text!
	strength-total fitness-total + 15 > if
		($ "Nach einigem Ringen, " .. thegroup .. "  schafft es ihn von der Brucke zu werfen!" $) +crtext goodfx
	else
		($ "'Es ist zwecklos! Der gepanzerte Mann schlagt einfach " .. thegroup .. "  mit seinen Boxhandschuhen! " $) +crtext
			vehicle.which@ 0= if ($ gThey .. "  laufen" .. g1s .. "  an ihm vorbei nach der Tracht Prugel." $) +text then
		sting--all bitefx
		vehicle.which@ 0 > if choice( "Uberfahren Sie ihn mit dem Auto" )choice: common3-bridgekeep-plow then
		 
	then
	' .strengthREV foreach-team-char ' .fitnessREV foreach-team-char
;

: common3-bridgekeep-choice
	choice( "Schubse ihn von der Brucke" )choice: common3-bridgekeep-push
	vehicle.which@ 0 > if choice( "Durchpflugen mit dem Auto" )choice: common3-bridgekeep-plow then
	most-angrystrong if
		charchoice( "kampfe eins gegen eins gegen ihn!" )choice: common3-bridgekeep-rage
	then
;

cyoa-bridge: common3-bridgekeep-coolit
	cpet? if
		($ cname .. " " .. "furze" pbold .. "  in die allgemeine Richtung des Bruckenschutzes." $)
	else
		($ cname .. "  steht am Rand der Brucke und schreit den Bruckenschutz an um " .. "COOL IT." pbold $)
	then text!
	cool-it++
	($ "'Der gepanzerte Mann starrt weiterhin geradeaus. Es ist nicht klar ob er " .. cname .. "." $) +crtext badfx
	common3-bridgekeep-choice
;

cyoa: common3-bridgekeep
	"Silent Guardian" cyoa-title!
	($ "'Ein Mann in provisorischer Rustung aus Sportbekleidung steht Wache in der Mitte einer schmalen Holzbrucke auf der Strae. Er tragt Boxhandschuhe, starrt das Auto an und schweigt, wahrend er den Weg versperrt." $) text!
		common3-bridgekeep-choice
		most-grating if
			charchoice( ": Sagen Sie dem Wachposten er soll sich beruhigen" )choice: common3-bridgekeep-coolit
		then
;

// Ranting raving ambush

cyoa-choice: common3-ranter-rage
	($ "Anstatt von dem Hinterhalt uberrascht zu werden, " .. cname .. " ' sturzt sich auf den Mann als er schimpft und schlagt ihn nieder. Er bekommt nicht einmal Zeit zu reagieren." $) text! goodfx rageREV morale++
	most-ahole if choosechar
		($ cname .. "  nutzt die Gelegenheit um seine Sachen zu stehlen!" $) +crtext
		homesearch1 ammosearch1 rob-guilt2
	then
;

cyoa-choice: common3-ranter-obliv
	($ cname .. " ' nimmt den schimpfenden Mann uberhaupt nicht wahr. Er wird dadurch so verwirrt dass er ruhig wird und davonzieht." .. cname .. " ' hat die ganze Zeit nicht aufgepasst und wird mehr in " .. chis .. "  eigene kleine Welt hineingezogen." $) text! goodfx witsattREV attitude++ wits--
;

cyoa-bridge: common3-ranter-coolit
	cpet? if
		($ cname .. " ' unterbricht die Schimpftiraden des Mannes indem er standig " .. "FARTING" rbold .. "  wenn er versucht zu sprechen." $)
	else
		($ cname .. " ' unterbricht die Schimpftirade des Mannes um ihm zu sagen dass " .. "'SAG ES, SPRUH ES NICHT!" pbold $)
	then text!
	cool-it-7 if
		($ "'Ha! Du bist in Ordnung!" pbold .. " er bietet an seine Fahigkeiten dem Team zur Verfugung zu stellen!" $) +crtext
		cooled-it goodfx recruit-yesno morale3+all
	else cool-it++
		($ "Oh! Sieht so aus als hatten wir einen Zwischenrufer!" pbold .. " er verprugelt " .. cname .. "  mit einem " .. "VERHEEREND EINSICHTSVOLLE SCHIMPFTIRADE!" fbold .. "  Die Hintern Spreng Schimpftirade erzeugt eine kleine Schockwelle!" $) +crtext
		morale4- ouch-- badfx
	then witsattREV 
;

cyoa-choice: common3-ranter-charm
	($ cname .. "  hort dem schimpfenden Mann zu und versucht, mit ihm zu argumentieren. Der Mann beruhigt sich und erklart, dass er dachte, " $) text! 
	solo if
		($ cname .. "  sei ein Bandit!" $)
	else
		"die Gruppe waren Banditen!"
	then  +text goodfx witsattREV morale++
	most-ahole if choosechar
		($ cname .. "  zeigt ihm dass " .. gtheyare .. "  bandit" gaVSs .. "!" $) +crtext
		homesearch1 ammosearch1 rob-guilt2
	then
;

cyoa-choice: common3-ranter-fight ricon
	($ Thegroup .. " ' bekampft den streitsuchtigen Mann! Es ist sehr schnell vorbei, aber er hat noch etwas Schaden angerichtet, bevor er weggelaufen ist." $) text! bitefx ouch--all
;

cyoa-choice: common3-ranter-run ricon
	($ Thegroup .. "  rennt weg! Der streitsuchtige Mann jagt ihnen eine ganze Weile hinterher und verteilt einige " .. "BRUTZELNDE SCHIMPFWORTER" rbold .. "  bevor " .. thegroup .. "  ihn verliert." $) text! badfx morale2-all
;

cyoa: common3-ranter
	"Schimpfender Mann" cyoa-title!
	char-gen to recruitee "George" recruitee .name!
	26 recruitee .specialtype! ricon
	6 recruitee .wits! 0 recruitee .attitude! 0 recruitee .composure! 6 recruitee .loyalty!
	2 recruitee .morale! 6 recruitee .shooting! 6 recruitee .medical! 6 recruitee .mechanical!
	($ Thegroup .. " ' wird von einem schreienden Mann uberfallen. Er schimpft und tobt! Er sieht nicht besonders gefahrlich aus, aber diese Schimpfworter fangen an zu " .. "brutzeln." pbold $) text!
		most-angrystrong if
			charchoice( ": Hau ihn um" )choice: common3-ranter-rage
		then
		most-grating if
			charchoice( "': SAG ES, SPRUHE ES NICHT" fcoolit )choice: common3-ranter-coolit
		then
		most-oblivious if
			charchoice( ": Ignoriere ihn komplett" )choice: common3-ranter-obliv
		then
		most-charming if
			charchoice( ": Hore ihm zu" )choice: common3-ranter-charm
		then
		choice( "Lauf weg!" )choice: common3-ranter-run
		choice( "Bekampfe ihn!" )choice: common3-ranter-fight
;

