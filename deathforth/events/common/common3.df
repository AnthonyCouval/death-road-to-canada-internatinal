cyoa-choice: common3-traptown-pick
	($ cname .. "  his ability. " .. chis .. " fahigkeit" $) text!
	cchar paranoid? if
		($ cHe .. "  ist extrem paranoid bei jedem Schritt und jeder Aktion. Es geht langsam voran, viele Fallen gehen los. " .. cname .. " 'bereit ist fur jeden von ihnen, und vermeidet Verletzungen." $) +crtext goodfx witsattREV mechanic++ ammosearch2 homesearch2
	then;
	cchar oblivious? if
		($ cHe .. " 'stolpernt dahin. Die meisten Fallen treffen ', " .. chim .. ". " .. cname .. " 'wird doch dieses langsame Verfahren uberleben!" $) +crtext bitefx witsattREV sting2- morale2-
		"'Sie sahen so viele Fallen in Aktion war unterrichtend!" +crtext
		' .mechanic++ foreach-team-char ammosearch2 homesearch2
	then;
	wits+roll if
		($ cHe .. " 'sorgsam ist, wo ', " .. che .. "'geht. Viele Falle werden entdeckt und sicher ausgelost.', " .. cname .. " 'vermeidet Verletzungen!" $) +crtext goodfx witsREV mechanic++ ammosearch2 homesearch2
	then;
	mechanic+roll if
		($ cHe .. "  benutzt " .. chis .. " 'Erfahrung mit mechanischen Dingen, um herauszufinden, wo ', " .. che .. " 'ware, wenn er irgendwie verruckt ware.', " .. che .. " 'etwas mit Nussen zu tun hatte. " .. cname .. " vermeidet oder entscharft viele Fallen auf diese Weise!" $) +crtext goodfx mechanicREV mechanic++ ammosearch2 homesearch2
	then;
		($ cHis .. "  Fahigkeit, Fallen zu vermeiden, ist nicht so großartig. " .. cname .. "  wird richtig fertig gemacht! " $) +crtext
		witsREV mechanicREV ouch2- bitefx
;

