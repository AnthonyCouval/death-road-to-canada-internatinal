		solo if
			($ "Explorar esta area em busca de saque pode ser " .. "muito perigoso" rbold .. ", especialmente porque " .. soloname .. "  esta sozinho!" $) +crtext
			soloer charchoice( " tenta explorar" )choice: common3-traptown-pick
		else
			"Envie um escoteiro para explorar por tesouros?" +crtext
			team-count 3 > if
				' common3-traptown-pick 2 #cyoa-team-choice
				choice( "Escolha outra pessoa" )choice: common3-traptown-pick4
			else
				' common3-traptown-pick 3 #cyoa-team-choice
			then
		then
		choice( "Despeje o!" )choice
		
;

