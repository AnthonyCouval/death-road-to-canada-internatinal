// Gloomy Weather

: common3-gloom-attcheck args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		($ <- @ .colourname .. "  mantem um atitude positiva!" $) +crtext
	then;
	<- @ .cattitude- if
		<- @ .morale2-
	else
		<- @ .morale--
	then	
;

cyoa-choice: common3-gloomy-pick
	($ cname .. "  comeca a falar, para melhorar o clima." $) text!
	cpet? if
		($ cHe .. "  faz " .. cdogcat .. " ' faz ruidos. E uma distracao decente o suficiente do tempo que estamos sofrendo com o tempo!" $) +crtext morale++	goodfx
	then;
	cchar grating? if
		($ cHe .. "  conta " .. everyone .. "  para " .. " CALA SEU BICHO." pbold $) +crtext
		cool-it-7 if
			($ Everyone .. "  rindo arderentamente! Essa frase de catch nunca se envelhece!" $) +text
			morale2+all goodfx
		else cool-it++
			"Isso NaO vai funcionar muito bem nesse momento." +text morale3-all badfx
		then witsattREV
	then;
	ctemper- if
		($ cHe .. " perde " .. chis .. "  seu temperamento comeca a ventar sobre o tempo! " .. cHe .. "  se sente melhor mas " .. everyone .. "  tem que suportar as reclamacoes." $) +crtext badfx
		temperREV morale++ cchar ' .morale-- foreach-team-char-except
	then;
	cattitude+ if
		($ cHe .. "  consegue distrair " .. everyone .. "  da mau temperatura!" $) +crtext goodfx attitudeREV
	then;
	cattitude- if
		($ cHe .. "  reclama e reclama, tanto sobre o tempo quanto sobre outros topicos nao relacionados." $) +crtext badfx
		attitudeREV morale2-all
	then;
	"'Nao ajuda muito." +crtext badfx
	' common3-gloom-attcheck foreach-team-char
;

cyoa: common3-gloomy
	"Viagem em clima sombrio" cyoa-title! someicon
	($ "'O tempo esta cinza e sombrio e esta realmente " .. thegroup .. " desanimando. " $) text!
	solo if
		($ cHe .. " nao tem ninguem para reclamar do tempo." $) +text
		soloer common3-gloom-attcheck
	else
		"Alguem decide falar, para ajudar no clima:" +crtext
		' common3-gloomy-pick cyoa-team-choice
	then 0.4 1 frnd weather! -1 1 frnd wind_force!

;

