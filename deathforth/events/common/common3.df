// Gloomy Weather

: common3-gloom-attcheck args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		($ <- @ .colourname .. "  behalt eine positive Einstellung bei!" $) +crtext
	then;
	<- @ .cattitude- if
		<- @ .morale2-
	else
		<- @ .morale--
	then	
;

cyoa-choice: common3-gloomy-pick
	($ cname .. "  fangt an zu reden, um die Stimmung zu verbessern." $) text!
	cpet? if
		($ cHe .. "  macht " .. cdogcat .. " ' Gerausche. Es ist eine ziemlich gute Ablenkung vom Wetter!" $) +crtext morale++	goodfx
	then;
	cchar grating? if
		($ cHe .. " sagt " .. everyone .. "  an " .. "  KUHL ES." pbold $) +crtext
		cool-it-7 if
			($ Everyone .. "  lacht herzlich! Dieser Slogan wird nie alt!" $) +text
			morale2+all goodfx
		else cool-it++
			"Dies kommt uberhaupt NICHT gut an." +text morale3-all badfx
		then witsattREV
	then;
	ctemper- if
		($ cHe .. "  verliert " .. chis .. "  temperament und fangt an uber das Wetter zu schimpfen " .. cHe .. "  fuhlt sich besser aber " .. everyone .. "  muss die Schimpftiraden ertragen." $) +crtext badfx
		temperREV morale++ cchar ' .morale-- foreach-team-char-except
	then;
	cattitude+ if
		($ cHe .. "  schafft es abzulenken " .. everyone .. "  vom beschissenen Wetter!" $) +crtext goodfx attitudeREV
	then;
	cattitude- if
		($ cHe .. "  jammert und jammert, sowohl uber das Wetter als auch uber andere nicht verwandte Themen." $) +crtext badfx
		attitudeREV morale2-all
	then;
	"'Es hilft nicht so viel." +crtext badfx
	' common3-gloom-attcheck foreach-team-char
;

cyoa: common3-gloomy
	"Dusteres Wettertrip" cyoa-title! someicon
	($ "'Das Wetter ist grau und trub und es wird wirklich " .. thegroup .. "  herunterkommen. " $) text!
	solo if
		($ cHe .. "  hat niemandem zu klagen uber das Wetter." $) +text
		soloer common3-gloom-attcheck
	else
		"Jemand entscheidet sich, aufzusprechen, um den Laune" +crtext
		' common3-gloomy-pick cyoa-team-choice
	then 0.4 1 frnd weather! -1 1 frnd wind_force!

;

// Fart Mystery


cyoa-choice: common3-fart-blame
	($ cname .. "  zu heben" $) text!
	cpet? if
		($ "Wow! Was fur ein schlechter " .. cdogcat .. "!" $) +crtext morale-- badfx
	then;
	cattitude+ if ($ cHe .. "  schuttet in einer gutenartigen Weise auf." $) +crtext attitudeREV else morale2- then
	" Ist fur den Faulnisri verantwortlich" +crtext
	cchar ' .morale-- foreach-team-char-except badfx
	"Was this the real culprit? Who can say..." +crtext
;

cyoa-choice: common3-fart-coolit
	cpet? if
		($ cname .. "  lasst einen " .. "SEHR DEUTLICHER ZWEITER FURZ." rbold $) text!
		"Geheimnis aufgeklart!" +crtext
	else
		($ cname .. " sagt " .. everyone .. "  an " .. "COOL IT." pbold $) text!
		($ "PERLEN AUS NERVENSCHWEISS" rbold .. "  rollen " .. cname .. "''s Gesicht herunter. Dennoch kann nichts bewiesen werden!" $) +crtext
	then cool-it++ cchar ' .morale3- foreach-team-char-except witsattREV badfx
;

cyoa-choice: common3-fart-smart
	notcchar to victim
	($ cname .. "  erklart eine komplizierte Reihe von Schlussfolgerungen die zu einer Schlussfolgerung fuhren:" $) text!
	cattitude+ if
	($ "Der Furz wurde von einem " .. "GHOST!!! WWWWHHHHOAAAAAAAAA!" fbold $) +crtext witsattREV morale++all goodfx
	then;
	($ "Es war " .. vname .. "  der den Furz gelassen hat!" $) +crtext victim .morale2- badfx
	cloyalty- if
		($ "Insgeheim war es tatsachlich " .. cname .. "  der " .. "furzmonster war!" rbold $) +crtext
		loyaltyREV
	then witsREV morale++
;

cyoa: common3-fart
	"Das Geheimnis des Geisterfurzes" cyoa-title!
	image-bad-smell 33 to cyoaicon
	($ "'Es ist nur ein weiterer Tag auf dem Todesweg als jemand " .. "im Auto furzt." rbold $) text!
	solo if
		"Wer konnte das getan haben? Wir werden es vielleicht nie erfahren." +crtext 1 trunk-gas+ badfx
	then;
	solohuman if choosepet
		($ "Wer konnte das getan haben? Es war wahrscheinlich " .. cname .. ". Was fur eine schlechter " .. cdogcat .. "!" $) +crtext morale-- badfx
	then;
		"Drama und Geheimnis verzehren das Team.		Wahle jemanden den du beschuldigen kannst:" +crtext
		most-grating if
			charchoice( " sagt " .. everyone .. "  soll sich BERUHIGEN" )choice: common3-fart-coolit
		then
		smartest-pass if 
			smartest charchoice( "  lost das Geheimnis" )choice: common3-fart-smart
		then
		' common3-fart-blame cyoa-team-choice
;

// Alligator

: common3-gator-hotwire
	mechanicest choosechar cmechanic+ if cicon
		($ cname .. "  schafft es das Auto zu uberbrucken!" $) +crtext mechanicREV goodfx
	else
		($ "'Ohne einen guten Mechaniker gibt es fur " .. thegroup .. "  keine andere Moglichkeit das Auto zu starten!" $) +crtext
		car-abandon 1 to gator-ate-key
	then
;

cyoa-choice: common3-gator-str
	($ cname .. "  ringt mit dem Alligator um die Schlussel!" $) text!
	strength+roll if
		($ "Der Alligator wird von " .. cname .. "'s roher Starke besiegt! Er spuckt die Schlussel aus." $) +crtext goodfx strength++
	else
		($ "Der Alligator gewinnt und beit " .. cname .. " schlielich ein groes Stuck aus ihm heraus! Er lasst die Schlussel fallen." $) +crtext ouch2- bitefx
	then alive? if strengthREV then
;

cyoa-choice: common3-gator-fit
	($ cname .. "  belastigt den Alligator, indem er ihn jagt und vor ihm davonlauft, in der Hoffnung, dass er die Schlussel fallen lasst." $) text!
	fitness+roll if
		($ "Der Alligator ist mude von " .. cname .. "''s Energie! Die Schlussel gleiten schlielich aus seinem Maul." $) +crtext goodfx fitness++
	else
		($ "Der Alligator sturzt sich hinein und kaut " .. cname .. "  wenn " .. che .. "  wird mude! Der Alligator lasst die Schlussel fallen." $) +crtext ouch2- bitefx
	then alive? if fitnessREV then
;

cyoa-choice: common3-gator-shoot
	($ cname .. "  schiet auf den Alligator!" $) text!
	shooting+roll if
		($ "Der Alligator wird von " .. cname .. "''s gutem Zielen gesprengt! Er lasst die Autoschlussel fallen." $) +crtext goodfx shooting++
		shootingREV
	else
		($ cname .. "  verfehlt den Schuss. Der Alligator schluckt die Schlussel und watschelt davon." $) +crtext
		shootingREV common3-gator-hotwire
	then
;

cyoa-choice: common3-gator-obliv
	($ cname .. " ' geht einfach hin und schnappt sich die Autoschlussel direkt aus dem Maul des Alligators! Kein Problem! Der Alligator hatte diese beilaufige Aktion uberhaupt nicht erwartet. Er ist so uberrascht dass er seine Kiefer nicht zusammenklappte. " .. Thegroup .. "  fahrt mit dem Auto davon!" $) text! goodfx witsattREV
;

cyoa-choice: common3-gator-mech
	someicon
	($ Thegroup .. " ' entscheidet, dass die Autoschlussel es nicht wert sind, sich mit einem extrem gefahrlichen Alligator anzulegen. Der Alligator ist traurig, dass seine Strategie nicht funktioniert, und verliert einen Punkt Moral."  $) text!
	common3-gator-hotwire
;

cyoa: common3-gator
	"Deutsch sein" cyoa-title!
	($ Thegroup .. " ' kann den Autoschlussel nicht finden! Nach einer Weile " .. gthey .. "  stelle" .. g1s .. "  die Schlussel in einem " .. "'aufgesperrten Maul eines Alligators liegen" rbold .. "'. Nachdem die meisten Menschen verschwunden waren, breiteten sich die Alligatoren an der gesamten Ostkuste aus. Es wird gemunkelt, dass die Alligatoren Zombies und Autoschlussel fressen konnten." $) text!
		strongest charchoice( "  ringt mit dem Alligator" )choice: common3-gator-str
		fittest charchoice( "  ermudet den Alligator" )choice: common3-gator-fit
		most-oblivious if
			charchoice( "  schnappt sich einfach die Schlussel" )choice: common3-gator-obliv
		then
		roadtrip-has-firearm? if
			shootest charchoice( "  erschiet den Alligator" )choice: common3-gator-shoot
		then
		choice( "Lass die Autoschlussel im Stich" )choice: common3-gator-mech
;

// Trap Town

cyoa-choice: common3-traptown-pick
	($ cname .. "  beginnt, die Stadt zu durchsuchen und vermeidet dabei Fallen so gut wie moglich " .. chis .. "  fahigkeit." $) text!
	cchar paranoid? if
		($ cHe .. " ' ist extrem paranoid in Bezug auf jeden Schritt und jede Handlung. Es geht langsam voran, viele Fallen gehen los. " .. cname .. "  ist auf jeden einzelnen vorbereitet und vermeidet Verletzungen." $) +crtext goodfx witsattREV mechanic++ ammosearch2 homesearch2
	then;
	cchar oblivious? if
		($ cHe .. " die meisten Fallen treffen. " .. chim .. ". " .. cname .. " uberlebt irgendwie diesen langen Prozess!" $) +crtext bitefx witsattREV sting2- morale2-
		"Es war lehrreich so viele Fallen in Aktion zu sehen!" +crtext
		' .mechanic++ foreach-team-char ammosearch2 homesearch2
	then;
	wits+roll if
		($ cHe .. "  ist vorsichtig wohin " .. che .. "' geht. Viele Fallen werden entdeckt und sicher ausgelost. " .. cname .. "  vermeidet Verletzungen!" $) +crtext goodfx witsREV mechanic++ ammosearch2 homesearch2
	then;
	mechanic+roll if
		($ cHe .. "  benutzt " .. chis .. "  erfahrung mit mechanischen Dingen um herauszufinden wo " .. che .. "  fallen stellen wurde wenn " .. che .. "  eine Art Spinner war. " .. cname .. "  vermeidet oder entscharft auf diese Weise viele Fallen!" $) +crtext goodfx mechanicREV mechanic++ ammosearch2 homesearch2
	then;
		($ cHis .. " ' Fahigkeit Fallen auszuweichen ist nicht so toll. " .. cname .. "  wird bose geschreddert! " $) +crtext
		witsREV mechanicREV ouch2- bitefx
;

cyoa-bridge: common3-traptown-pick4
	someicon
	"Wer sollte die mit Sprengfallen versehene Stadt auskundschaften?" cyoa-text!
	' common3-traptown-pick cyoa-team-choice
;

cyoa: common3-traptown
	"Stadt der Fallen" cyoa-title!
	($ Thegroup .. "  findet eine Strae in einer Stadt die uberall Fallen hat. Gruben mit Decken daruber. Uberall Schlingen. Spikes auf Fassern. Eine Tur mit einer offensichtlichen Guillotine daruber. Und das sind nur die sichtbaren." $) text!
		solo if
			($ "Die Erkundung dieses Gebiets nach Beute konnte " .. "sehr gefahrlich" rbold .. ", vor allem weil " .. soloname .. "  ist allein!" $) +crtext
			soloer charchoice( "  versucht es zu erkunden" )choice: common3-traptown-pick
		else
			"Soll ich einen Spaher losschicken um nach Beute zu suchen?" +crtext
			team-count 3 > if
				' common3-traptown-pick 2 #cyoa-team-choice
				choice( "Wahle jemand anderen" )choice: common3-traptown-pick4
			else
				' common3-traptown-pick 3 #cyoa-team-choice
			then
		then
		choice( "Fahr weg!" )choice
		
;

// Creepy Store

cyoa-choice: common3-creepy-go
	($ cname .. "  betritt den dunklen und gruseligen Laden." $) text!
	cchar paranoid? if
		($ cHis .. "  das ubliche Gefuhl von Paranoia schlagt fehl " .. cHe .. "  bekommt riesige Angst. " .. cname .. "  sieht eine winzige Maus und fangt dann an " .. "zu kreischen wie ein YouTuber" fbold .. "!" $) +crtext morale2- witsattREV sfx{ creepy }
	then;
	cchar oblivious? if
		($ cHe .. " ' bemerkt dort nichts Beangstigendes. Nicht einmal als ein " .. "GRUSELIGES TANZENDES SKELETT" fbold .. "  von der Decke fallt und zu klappern beginnt!" $) +crtext
		witsattREV homesearch2 goodfx
	then;
	temper+roll if
		($ cHe .. "  bleibt ruhig, nimmt die Beute " .. che .. "  findet und kommt da raus!" $) +crtext
		temperREV homesearch2 goodfx
	then;
		($ cHe .. "  verliert " .. chis .. "  ruhig und rennt von dort raus und behauptet, es gabe keine Beute!" $) +crtext
		temperREV morale-- badfx
;

cyoa-choice: common3-creepy-allgo
	someicon ($ Thegroup .. "  betritt den dunklen und gruseligen Laden." $) text!
	angriest-pass if angriest choosechar cicon
		($ cname .. "  verwechselt eine Schaufensterpuppe mit einem Zombie und greift an! Das lasst " .. everyone .. "  ausflippen und auch blindlings angreifen. Am Ende stand keine Schaufensterpuppe mehr." $) +crtext temperREV
		ouch--all bitefx
	else
		"Alle bleiben ziemlich ruhig! Der Laden wird ohne Zwischenfalle geplundert." +crtext goodfx
	then ' .temperREV foreach-team-char homesearch2
;

cyoa-choice: common3-creepy-pick
	"Wer sollte den gruseligen Laden betreten?" text!
	' common3-creepy-go cyoa-team-choice
;

cyoa: common3-creepy
	"Dunkler und gruseliger Laden" cyoa-title! image-female-zombie-1
	($ Thegroup .. " ' findet einen durchnassten Laden. Drinnen sind viele Schaufensterpuppen, die in der Dunkelheit kaum zu sehen sind. Der ganze Laden ist besonders gruselig. Es konnten noch ein paar schimmelige Vorrate drin sein." $) text!
		choice( "Ignoriere diesen Ort" )choice
		solo if
			soloer charchoice( "  geht in den Laden" )choice: common3-creepy-go
		else
			choice( "Schicke jemanden rein" )choice: common3-creepy-pick
			choice( "Schicke alle rein!" )choice: common3-creepy-allgo
		then
;

