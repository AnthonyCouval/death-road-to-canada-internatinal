// Gloomy Weather

: common3-gloom-attcheck args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		($ <- @ .colourname .. "  mantem uma atitude positiva!" $) +crtext
	then;
	<- @ .cattitude- if
		<- @ .morale2-
	else
		<- @ .morale--
	then	
;

cyoa-choice: common3-gloomy-pick
	($ cname .. "  comeca a falar, para melhorar o clima." $) text!
	cpet? if
		($ cHe .. "  faz " .. cdogcat .. " ' barulhos. E uma distracao suficientemente decente do tempo!" $) +crtext morale++	goodfx
	then;
	cchar grating? if
		($ cHe .. "  conta " .. everyone .. "  para " .. "  CALMA." pbold $) +crtext
		cool-it-7 if
			($ Everyone .. "  ri muito! Esta frase de efeito nunca envelhece!" $) +text
			morale2+all goodfx
		else cool-it++
			"Isso NAO vai bem de jeito nenhum." +text morale3-all badfx
		then witsattREV
	then;
	ctemper- if
		($ cHe .. " perde " .. chis .. "  temperamento e comeca a desabafar sobre o tempo " .. cHe .. "  sente se melhor mas " .. everyone .. "  tem de suportar os discursos." $) +crtext badfx
		temperREV morale++ cchar ' .morale-- foreach-team-char-except
	then;
	cattitude+ if
		($ cHe .. "  consegue distrair " .. everyone .. "  do tempo de merda!" $) +crtext goodfx attitudeREV
	then;
	cattitude- if
		($ cHe .. "  lamenta e lamenta, tanto sobre o tempo como sobre outros topicos nao relacionados." $) +crtext badfx
		attitudeREV morale2-all
	then;
	"'Nao ajuda muito." +crtext badfx
	' common3-gloom-attcheck foreach-team-char
;

cyoa: common3-gloomy
	"Viagem de Tempo Sombrio" cyoa-title! someicon
	($ "'O tempo esta cinzento e sombrio e esta mesmo a ficar " .. thegroup .. "  para baixo. " $) text!
	solo if
		($ cHe .. "  nao tem ninguem para reclamar sobre o tempo." $) +text
		soloer common3-gloom-attcheck
	else
		"Alguem decide falar, para ajudar o clima:" +crtext
		' common3-gloomy-pick cyoa-team-choice
	then 0.4 1 frnd weather! -1 1 frnd wind_force!

;

