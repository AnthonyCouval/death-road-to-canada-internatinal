// Gloomy Weather

: common3-gloom-attcheck args( @ )
	<- @ .attitudeREV
	<- @ .cattitude+ if
		($ <- @ .colourname .. " 'mantiene un buen animo!" $) +crtext
	then;
	<- @ .cattitude- if
		<- @ .morale2-
	else
		<- @ .morale--
	then	
;

cyoa-choice: common3-gloomy-pick
	($ cname .. " 'comienza a hablar, para mejorar el clima." $) text!
	cpet? if
		($ cHe .. "  hace " .. cdogcat .. " 'hace ruidos. Es una distraccion bastante buena del tiempo!" $) +crtext morale++	goodfx
	then;
	cchar grating? if
		($ cHe .. " dice " .. everyone .. " 'a " .. " 'TRanquILIDAD!" pbold $) +crtext
		cool-it-7 if
			($ Everyone .. " 'rie a carcajadas! Esta frase nunca envejece!" $) +text
			morale2+all goodfx
		else cool-it++
			"Esto NO le va bien en absoluto." +text morale3-all badfx
		then witsattREV
	then;
	ctemper- if
		($ cHe .. " ' pierde " .. chis .. " 'enciende su genio y comienza a desahogarse sobre el tiempo! " .. cHe .. " 'se siente mejor pero " .. everyone .. " 'tiene que soportar los alardes." $) +crtext badfx
		temperREV morale++ cchar ' .morale-- foreach-team-char-except
	then;
	cattitude+ if
		($ cHe .. " 'logra distraer " .. everyone .. " 'del mal tiempo!" $) +crtext goodfx attitudeREV
	then;
	cattitude- if
		($ cHe .. " 'queja y se queja, tanto sobre el tiempo como sobre otros temas no relacionados." $) +crtext badfx
		attitudeREV morale2-all
	then;
	"No ayuda mucho." +crtext badfx
	' common3-gloom-attcheck foreach-team-char
;

cyoa: common3-gloomy
	"Viaje al Tiempo Gris" cyoa-title! someicon
	($ "El tiempo esta nublado y sucio y realmente le esta poniendo " .. thegroup .. " 'abajo. " $) text!
	solo if
		($ cHe .. " no tiene a nadie con quien quejarse del clima." $) +text
		soloer common3-gloom-attcheck
	else
		"Alguien decide hablar, para ayudar al ambiente:" +crtext
		' common3-gloomy-pick cyoa-team-choice
	then 0.4 1 frnd weather! -1 1 frnd wind_force!

;

