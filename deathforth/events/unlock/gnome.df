cyoa: unlockgnome gstats{
	"UNOMEY DER ENTSCHLUSSELUNGS GNOME" cyoa-title!
	
	ugnome-found 1 < if
		1 to ugnome-found save
		"'Ich bin Unomey, der Sperrgeist		Du hast mich gefunden!		Ich werde Dir jetzt Sperrungen verkaufen		Lass Dich aus meiner Wohnung heraus!" text!
	else
	
		"HALLO ICH BIN UNOMEY		KENNST DU MICH NICHT?" text!
		
		ugnome-unlocks 2 > if
			"UNLOCKS ARE A BIG PART
			OF GNOME SOCIETY!
			WHOEVER HAS THE MOST UNLOCKS IS KING" text!
		then
		
		gnome-stockleft 18 < if
			"GNOMES INVENTED UNLOCKS!
			WE CALL THEM 'UNCKS' FOR SHORT
			IT SAVES A LOT OF TIME TO CALL THEM THIS" text!
		then
				
		gnome-stockleft 14 < if
			"MORE UNLOCKS
			MORE PROBLEMS
			-- GNOMISH PROVERB" text!
		then
		
		gnome-stockleft 10 < if
