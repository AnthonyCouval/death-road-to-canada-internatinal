cyoa: unlockgnome gstats{
	"UNOMEY EL GNOMO DESBLOQUEADOR" cyoa-title!
	
	ugnome-found 1 < if
		1 to ugnome-found save
		"HOLA! SOY UNOMEY, EL GNOMO DESBLOQUEADOR		ME HAS ENCONTRADO!		AHORA VENDO DESBLOQUEOS		DEJA DE INTRUSAR EN MI CASA, NERD!" text!
	else
	
		"HOLA, SOY UNOMEY		ACASO NO ME CONOCES?" text!
		
		ugnome-unlocks 2 > if
			"UNLOCKS ARE A BIG PART
			OF GNOME SOCIETY!
			WHOEVER HAS THE MOST UNLOCKS IS KING" text!
		then
		
		gnome-stockleft 18 < if
			"GNOMES INVENTED UNLOCKS!
			WE CALL THEM 'UNCKS' FOR SHORT
			IT SAVES A LOT OF TIME TO CALL THEM THIS" text!
		then
				
		gnome-stockleft 14 < if
			"MORE UNLOCKS
			MORE PROBLEMS
			-- GNOMISH PROVERB" text!
		then
		
		gnome-stockleft 10 < if
