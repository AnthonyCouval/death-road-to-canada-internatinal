// Junkyard Pals

cyoa: junkpalsrescue
	"Resgate do Ferro velho" cyoa-title!
	($ Thegroup .. "  escapa do ferro velho!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clem" pbold .. "  sobrevive e mostra otimas dicas de reparo enquanto conserta o carro!" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clem" pbold .. "  nao escapou com voce." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Voce resgatou " .. "Leonard" pbold .. ". Em agradecimento, ele da " .. thegroup .. "  algumas dicas de tiro!" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonard" pbold .. "' as aventuras de ' terminaram." $) +crtext
			morale--all
		then
	then
;

