// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. "  aprende algumas tecnicas de nivel mestre! " .. cHe .. "  agora bate ainda mais forte! Agora " .. chis .. "  maos podem atingir tres alvos por ataque!" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. "  recebe um curso intensivo de artes marciais! " .. cHe .. "  e capaz de aprender um movimento muito util. Agora " .. chis .. " portugues" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Entre na Estrada da Morte" cyoa-title!
		($ rname .. " sai do carro uma vez que ele passa seguro pela cidade lotada." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "Em troca de ajuda, ele ensina " .. cname .. " um forma mais avancada de artes marciais!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "Em troca de ajuda, ele ensina " .. cname .. " com as maos nuas! " .. chis .. " zumbis!" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "Em gratidao por ajudalo " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Derrota Esmagadora" cyoa-title!
		($ Thegroup .. " sentindo um grande sentimento de tristeza sobre " .. rname .. " ser devorado. Sua conhecencia em artes marciais agora esta perdida." $) cyoa-text!
		morale2-all badfx
	then
;

