// Hazmat Guys

cyoa: hazmatrescue
	"Soccorso Hazmat" cyoa-title!
	($ Thegroup .. "  fugge dalla fogna!" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. "  ha lasciato prima di trovare tutti, quindi non cera alcuna ricompensa." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"Lintero Hazmat Team ce lha fatta vivo!" +crtext
			morale2+all
		else
			"Non tutto lHazmat Team ce lha fatta, ma laiuto e stato comunque apprezzato." +crtext
		then

		hazmat1 .alive? if
			($ "Matt" pbold .. "  rivela una scorta di forniture mediche accuratamente sigillate!" $) +crtext
			10 trunk-medical+
		else
			($ "Matt" pbold .. " ' non e sopravvissuto alla sua missione di salvataggio." $) +crtext
		then

		hazmata .alive? if
			($ "Matilda" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "Matilda" pbold .. " ' non e uscito dalla fogna." $) +crtext
		then

		hazmatb .alive? if
			($ "Mattie" pbold .. "  mima alcune tecniche mediche di emergenza." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "Mattie" pbold .. "  e stato lasciato indietro." $) +crtext
		then

;

