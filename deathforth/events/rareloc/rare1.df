// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. "  aprende algumas tecnicas de nivel mestre! " .. cHe .. "  agora acerta ainda mais forte!		Agora " .. chis .. "  as maos podem atingir tres alvos por ataque!" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. "  recebe um curso intensivo de artes marciais! " .. cHe .. "  consegue aprender um movimento muito util.		Agora " .. chis .. "  as maos sao armas anti zumbis!" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Entre na Estrada da Morte" cyoa-title!
		($ rname .. "  sai do carro depois que ele passa com seguranca pela cidade enxameada." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "Em troca da ajuda, ele ensina " .. cname .. "  uma forma ainda mais avancada de artes marciais!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "Em troca da ajuda, ele ensina " .. cname .. "  como esmagar zumbis com " .. chis .. "  maos nuas!" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "Em agradecimento por ajudar " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Derrota Esmagadora" cyoa-title!
		($ Thegroup .. "  sente uma grande perda por " .. rname .. "  sendo comido.		Seu conhecimento em artes marciais se foi." $) cyoa-text!
		morale2-all badfx
	then
;

