// Hazmat Guys

cyoa: hazmatrescue
	"Resgate de Materiais Perigosos" cyoa-title!
	($ Thegroup .. "  escapa do esgoto!" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. "  foi embora antes de encontrar todos, entao nao houve recompensa." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"O Time Hazmat inteiro saiu vivo!" +crtext
			morale2+all
		else
			"Nem todo o Time Hazmat conseguiu sair, mas a ajuda ainda foi apreciada." +crtext
		then

		hazmat1 .alive? if
			($ "Matt" pbold .. "  revela um estoque de suprimentos medicos cuidadosamente lacrados!" $) +crtext
			10 trunk-medical+
		else
			($ "Matt" pbold .. " ' nao sobreviveu a sua missao de resgate." $) +crtext
		then

		hazmata .alive? if
			($ "Matilda" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "Matilda" pbold .. " ' nao conseguiu sair do esgoto." $) +crtext
		then

		hazmatb .alive? if
			($ "Mattie" pbold .. "  faz mimicas de algumas tecnicas medicas de emergencia." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "Mattie" pbold .. "  foi deixado para tras." $) +crtext
		then

;

