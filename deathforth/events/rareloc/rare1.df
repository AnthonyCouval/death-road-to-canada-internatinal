// Hazmat Guys

cyoa: hazmatrescue
	"Rescate de Materiales Peligrosos" cyoa-title!
	($ Thegroup .. " escapa de la alcantarilla!" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. " izquierda antes de encontrar a todos, asi que no hubo recompensa." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"Todo el equipo de materiales peligrosos salio con vida!" +crtext
			morale2+all
		else
			"No todos los miembros del equipo Hazmat lograron salir, pero la ayuda fue apreciada de todos modos." +crtext
		then

		hazmat1 .alive? if
			($ "Matt" pbold .. "  revela un alijo de suministros medicos cuidadosamente sellados!" $) +crtext
			10 trunk-medical+
		else
			($ "Matt" pbold .. " ' no sobrevivio a su mision de rescate." $) +crtext
		then

		hazmata .alive? if
			($ "Matilda" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "Matilda" pbold .. " ' no salio de la alcantarilla." $) +crtext
		then

		hazmatb .alive? if
			($ "Mattie" pbold .. "  hace una pantomima de algunas tecnicas medicas de emergencia." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "Mattie" pbold .. "  fue dejado atras." $) +crtext
		then

;

