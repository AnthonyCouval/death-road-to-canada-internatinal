// Hazmat Guys

cyoa: hazmatrescue
	"Equipe de Desastres Quimicos" cyoa-title!
	($ Thegroup .. "  escapa do esgotamento!" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. "  saiu antes de encontrar todos, entao nao houve recompensa." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"A equipe inteira de Desastres Quimicos saiu ilesa!" +crtext
			morale2+all
		else
			"Nao foram todos da Equipe de Desastres Quimicos que sairam, mas a ajuda foi ainda apreciada." +crtext
		then

		hazmat1 .alive? if
			($ "Matt" pbold .. "  revela um estoque de suministros medicos cuidadosamente selados!" $) +crtext
			10 trunk-medical+
		else
			($ "Matt" pbold .. " 'Nao sobreviveu a missao de resgate." $) +crtext
		then

		hazmata .alive? if
			($ "Matilda" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "Matilda" pbold .. " ' nao conseguiu sair do esgoto." $) +crtext
		then

		hazmatb .alive? if
			($ "Mattie" pbold .. "  faz mimicas de algumas tecnicas medicas de emergencia." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "Mattie" pbold .. "  ficou para tras." $) +crtext
		then

;

