// Junkyard Pals

cyoa: junkpalsrescue
	"Rescatar Jardin de Chatarra" cyoa-title!
	($ Thegroup .. " se escapa del jardin de chatarra!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clem" pbold .. " se salva, y muestra algunas granosos consejos de reparacion mientras arregla el coche!" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clem" pbold .. "  no escapo contigo." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Has rescatado a " .. "Leonardo" pbold .. ". En agradecimiento, el da " .. thegroup .. " s aventuras han terminado" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonardo" pbold .. "''s aventuras han terminado." $) +crtext
			morale--all
		then
	then
;

