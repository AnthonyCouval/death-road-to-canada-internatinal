// Junkyard Pals

cyoa: junkpalsrescue
	"Schrottplatz Rettung" cyoa-title!
	($ Thegroup .. "  entkommt dem Schrottplatz!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clem" pbold .. "  uberlebt und zeigt einige groartige Reparaturtipps, wahrend er das Auto repariert!" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clem" pbold .. "  ist nicht mit dir geflohen." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Du hast gerettet " .. "Leonard" pbold .. ". Als Dank gibt er " .. thegroup .. "  einige Schietipps!" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonard" pbold .. "' Abenteuer sind zu Ende." $) +crtext
			morale--all
		then
	then
;

