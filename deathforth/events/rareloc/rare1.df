// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. "  lernt einige Techniken der Meisterstufe! " .. cHe .. "  trifft jetzt noch harter! Jetzt " .. chis .. "  hande konnen drei Ziele pro Angriff treffen!" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. "  bekommt einen Crashkurs in Kampfkunsten! " .. cHe .. "  kann einen sehr praktischen Zug lernen. Jetzt " .. chis .. "  hande sind Anti Zombie Waffen!" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Betritt den Todesweg" cyoa-title!
		($ rname .. "  steigt aus dem Auto sobald es sicher an der von Schwarmen heimgesuchten Stadt vorbeigefahren ist." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "Im Austausch fur die Hilfe, lehrt er " .. cname .. " eine noch fortgeschrittenere Form der Kampfkunst!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "Im Austausch fur die Hilfe, lehrt er " .. cname .. " wie man Zombies mit " .. chis .. "  bloen Handen zerplatzen lasst!" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "Als Dank dafur dass er geholfen hat " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Vernichtende Niederlage" cyoa-title!
		($ Thegroup .. "  empfindet einen groen Verlust uber " .. rname .. "  gefressen zu werden.		Sein Wissen uber Kampfkunste ist jetzt verschwunden." $) cyoa-text!
		morale2-all badfx
	then
;

