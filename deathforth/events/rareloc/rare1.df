// Junkyard Pals

cyoa: junkpalsrescue
	"Salvataggio in discarica" cyoa-title!
	($ Thegroup .. " fugge dal cantiere di demolizione!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clem" pbold .. " si salva, e offre qualche consiglio di riparazione straordinario mentre ripara lauto!" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clem" pbold .. "  non e fuggito con te." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Hai salvato " .. "Leonard" pbold .. ". In gratitudine, egli ti regala " .. thegroup .. " qualche consiglio di tiro a segno!" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonard" pbold .. "'Le avventure di lui sono terminate." $) +crtext
			morale--all
		then
	then
;

