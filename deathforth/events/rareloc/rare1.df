// Danger Rangers

cyoa: rangerrescue
	"Club dei Fan dei Danger Ranger" cyoa-title!
	($ Thegroup .. " fuggirono in fuga quei sobborghi." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Rosa" pbold .. " scatena il suo Raggio Guaritore Ranger!" $) +crtext
			6 groupheal
		else
			($ "Rosa" pbold .. " non e piu li per sostenere il gruppo." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Arancione" pbold .. " bestows some of her energy and fitness." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Arancione" pbold .. " non ce l'hanno fatta. La loro squadra e ridotta." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Verde" pbold .. "  dona un po della sua famosa forza." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Verde" pbold .. "''s poderosa forza ci manchera." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Blu" pbold .. "  fa un discorso motivazionale stimolante!" $) +crtext
			morale2+all
		else
			($ "Blu" pbold .. "  non fornira piu la leadership." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. "  si sente inadeguato per non aver trovato tutti." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

