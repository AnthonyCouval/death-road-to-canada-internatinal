// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. "  recibe ensenanzas de tecnicas de nivel maestro " .. cHe .. " ahora " .. chis .. " targets per attack" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. " recibe un curso intensivo de artes marciales! " .. cHe .. " is able to learn one very handy move.		Now " .. chis .. "  manos son armas anti zombies!" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Entra en el camino de la muerte" cyoa-title!
		($ rname .. "  sale del coche una vez que conduce con seguridad mas alla de la ciudad invadida." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "A cambio de la ayuda, el ensena " .. cname .. "  una forma aun mas avanzada de artes marciales!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "A cambio de la ayuda, el ensena " .. cname .. "  como aplastar zombis con " .. chis .. "  manos desnudas!" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "En agradecimiento por ayudar " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Derrota aplastante" cyoa-title!
		($ Thegroup .. "  siente una gran sensacion de perdida por " .. rname .. "  ser comido. Su conocimiento de artes marciales ahora se ha ido." $) cyoa-text!
		morale2-all badfx
	then
;

