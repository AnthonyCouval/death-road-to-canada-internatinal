// Hazmat Guys

cyoa: hazmatrescue
	"'Hazmat-Rettung" cyoa-title!
	($ Thegroup .. " ' entkommt dem Abwasser!" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. " 'er war vorher weg, also gab es kein Belohnung." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"'Die gesamte Hazmat-Einheit ist lebendig herausgekommen!" +crtext
			morale2+all
		else
			"'Nicht alle Mitglieder der Hazmat-Einheit sind ausgeschieden, aber dennoch war die Hilfe begrußt." +crtext
		then

		hazmat1 .alive? if
			($ "Schachmatt" pbold .. " 'entdeckt einen Vorrat sorgfaltig versiegelter medizinischer Versorgungsguter!" $) +crtext
			10 trunk-medical+
		else
			($ "Schachmatt" pbold .. " 'uberlebte seine Rettungsmission nicht." $) +crtext
		then

		hazmata .alive? if
			($ "'Matilda" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "'Matilda" pbold .. " 'schaffte es nicht aus dem Abwasserkanal." $) +crtext
		then

		hazmatb .alive? if
			($ "'Mattie" pbold .. " 'demonstriert einige Notfallmedizinische Techniken." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "'Mattie" pbold .. " 'wurde zuruckgelassen." $) +crtext
		then

;

