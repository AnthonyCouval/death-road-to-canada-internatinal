// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. "  bekommt einige Meister-Level-Techniken beigebracht! " .. cHe .. " 'Now it hits even harder!		Now ', " .. chis .. " 'hands can hit three targets per attack!" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. " 'gets a crash course in martial arts! ', " .. cHe .. " 'is able to learn one very handy move.		Now ', " .. chis .. " 'hands are anti-zombie weapons!" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"'Enter the Death Road" cyoa-title!
		($ rname .. " 'gets out of the car once it drives safely past the swarmed city." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "'In return for help, he teaches ', " .. cname .. " 'an even more advanced form of martial arts!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "'In return for help, he teaches ', " .. cname .. " 'how to splat zombies with 	extit{bare hands}!', " .. chis .. " 'In thanks for helping " $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "'Fur Ihre Hilfe danke', " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"' Niederlage in gewaltigem Ausmaß" cyoa-title!
		($ Thegroup .. " ' empfindet tiefe Trauer uber " .. rname .. " ' das Verschlingenlassen. Seine Kenntnisse in Kampfkunst sind nun verloren." $) cyoa-text!
		morale2-all badfx
	then
;

