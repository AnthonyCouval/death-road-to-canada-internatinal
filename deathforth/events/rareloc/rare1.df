// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. " apprend des techniques maitrisees a un niveau avance ! " .. cHe .. " frappe plus fort maintenant ! " .. chis .. " les mains peuvent frapper trois cibles par attaque !" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. " s'initie a des cours intensifs d'arts martiaux ! " .. cHe .. " est en mesure d'apprendre une tres pratique figure de danse ... maintenant " .. chis .. " les mains sont des arme contre les zombies !" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Entrez la route de la mort" cyoa-title!
		($ rname .. " sortie du voiture quand elle conduit surement au-dela de la ville envahie par les foules." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "En echange de l'aide, il enseigne. " .. cname .. " une forme de martial arts encore plus avancee !" $) +crtext WEAPON_MASTERKUNG
			else
				($ "En echange de l'aide, il enseigne. " .. cname .. " comment ecraser les zombies avec : " .. chis .. " mains nues !" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "Je vous remercie pour votre aide. " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Defaite ecrasante" cyoa-title!
		($ Thegroup .. " ressent une grande perte. " .. rname .. " il est devore... ses connaissances en arts martiaux sont desormais perdues." $) cyoa-text!
		morale2-all badfx
	then
;

