// Junkyard Pals

cyoa: junkpalsrescue
	"Recuperation dans la ferraille" cyoa-title!
	($ Thegroup .. " fuyez le dechetterie !" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clemence" pbold .. " survit et donne des excellents conseils de reparation tout en reparant la voiture !" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clemence" pbold .. " n'est pas parvenu a s'enfuir avec toi." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Vrai-essence. " .. "Leonard" pbold .. "en remerciement, il donne. " .. thegroup .. " des conseils de tir :" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonard" pbold .. "les aventures de sont terminees." $) +crtext
			morale--all
		then
	then
;

