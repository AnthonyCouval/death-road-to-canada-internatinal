// Junkyard Pals

cyoa: junkpalsrescue
	"Rescate del desguace" cyoa-title!
	($ Thegroup .. " escapa del desguace!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clementine" pbold .. " sobrevive y muestra algunos consejos de reparacion geniales mientras arregla el coche!" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clementine" pbold .. " ' no escapo contigo." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Rescatado " .. "Leonardo" pbold .. "en agradecimiento, el da " .. thegroup .. " algunos consejos de tiro!" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonardo" pbold .. "s adventures have ended." $) +crtext
			morale--all
		then
	then
;

