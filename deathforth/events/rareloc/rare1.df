// Danger Rangers

cyoa: rangerrescue
	"Clube de Fas do Ranger do Perigo" cyoa-title!
	($ Thegroup .. "  escapa daquele suburbio infestado." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Rosa" pbold .. "  libera seu Raio Ranger Curador!" $) +crtext
			6 groupheal
		else
			($ "Rosa" pbold .. "  nao esta mais la para apoiar o grupo." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Laranja" pbold .. "  concede um pouco de sua energia e boa forma." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Laranja" pbold .. " nao deu. O time deles esta diminuido." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Verde" pbold .. "  da um pouco de sua famosa forca." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Verde" pbold .. "' sua forca poderosa fara falta." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Azul" pbold .. "  faz um discurso motivacional inspirador!" $) +crtext
			morale2+all
		else
			($ "Azul" pbold .. "  nao fornecera mais lideranca." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. "  se sente inadequado por nao encontrar todos." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

