// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. " 'recibe tecnicas de nivel maestro! " .. cHe .. " 'ahora golpea aun mas fuerte! Ahora " .. chis .. " 'puede alcanzar tres objetivos con un solo ataque" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. " 'recibe un curso acelerado de artes marciales " .. cHe .. " 'es capaz de aprender un movimiento muy util. Ahora " .. chis .. " 'las manos se convierten en armas contra zombis!" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Entra a la Death Road" cyoa-title!
		($ rname .. " 'se baja del coche una vez que pasa seguramente la ciudad llena de zombies." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "En agradecimiento por la ayuda, ensena a " .. cname .. " 'un curso de artes marciales de nivel mas avanzado!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "En agradecimiento por la ayuda, ensena a " .. cname .. " 'muestra como splear zombies con " .. chis .. " 'las manos desnudas!" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "Spanish: " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Derrota Aplastante" cyoa-title!
		($ Thegroup .. " siente una gran perdida por " .. rname .. " 0" $) cyoa-text!
		morale2-all badfx
	then
;

