// Hazmat Guys

cyoa: hazmatrescue
	"Recuperation d'urgence en matiere d'hazard" cyoa-title!
	($ Thegroup .. " fonce au-dela des egouts !" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. " avant de trouver tous, donc il n'y a eu aucun recompense." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"Toute l'equipe hazmat s'est sortie vivante !" +crtext
			morale2+all
		else
			"Aide bienvenue malgre le fait que tous les membres de l'equipe d'interventions aux matieres dangereuses n'ont pas pu sortir." +crtext
		then

		hazmat1 .alive? if
			($ "Matthieu" pbold .. " atteint vers la poke du chat mais est interrompu" $) +crtext
			10 trunk-medical+
		else
			($ "Matthieu" pbold .. " n'a pas survecu a sa mission de sauvetage." $) +crtext
		then

		hazmata .alive? if
			($ "Matilde" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "Matilde" pbold .. " n'a pas reussi a sortir des egouts." $) +crtext
		then

		hazmatb .alive? if
			($ "Matty" pbold .. " je realise des mimiques de techniques medicales d'urgence." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "Matty" pbold .. " est reste derriere." $) +crtext
		then

;

