// Hazmat Guys

cyoa: hazmatrescue
	"Gefahrgut Rettung" cyoa-title!
	($ Thegroup .. "  entkommt der Kanalisation!" $) text!
	hazmat1 0= hazmata 0= hazmatb 0= or or if
		($ Thegroup .. "  verlie den Ort, bevor alle gefunden waren, so dass es keine Belohnung gab." $) +crtext
		morale--all badfx
	then;
		0 hazmat1 .party! 0 hazmata .party! 0 hazmatb .party!
		hazmat1 .alive? hazmata .alive? hazmatb .alive? and and if
			"Das gesamte Gefahrgut Team hat es lebend geschafft!" +crtext
			morale2+all
		else
			"Nicht alle vom Gefahrgut Team haben es geschafft, aber die Hilfe wurde trotzdem gewurdigt." +crtext
		then

		hazmat1 .alive? if
			($ "Matt" pbold .. "  enthullt einen Vorrat an sorgfaltig versiegelten medizinischen Hilfsgutern!" $) +crtext
			10 trunk-medical+
		else
			($ "Matt" pbold .. " ' uberlebte seinen Rettungseinsatz nicht." $) +crtext
		then

		hazmata .alive? if
			($ "Matilda" pbold .. " shows off a cache of food. It may or may not have been stashed in the sewer..." $) +crtext
			20 trunk-food+
		else
			($ "Matilda" pbold .. " ' schaffte es nicht aus der Kanalisation." $) +crtext
		then

		hazmatb .alive? if
			($ "Mattie" pbold .. " pantomimen einige medizinische Notfalltechniken." $) +crtext
			' .medical++ foreach-team-char
		else
			($ "Mattie" pbold .. "  wurde zuruckgelassen." $) +crtext
		then

;

