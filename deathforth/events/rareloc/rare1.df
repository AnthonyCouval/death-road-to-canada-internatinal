// Danger Rangers

cyoa: rangerrescue
	"Est vraiment etonne/e" cyoa-title!
	($ Thegroup .. " se renverse et se comporte tout adorablement." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Chevre" pbold .. " libere son rayon de guerison de rangers !" $) +crtext
			6 groupheal
		else
			($ "Chevre" pbold .. " il n'y a plus de lui pour appuyer le groupe." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Orange = orange" pbold .. " elle transmet une partie de son energie et de sa vitalite." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Orange = orange" pbold .. " n'ont pas reussi. leur equipe est reduite." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Santa claus" pbold .. " accorde une partie de sa renommee en matiere de force a quelqu'un." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Santa claus" pbold .. "la puissante force de lui sera regretee." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Bleu" pbold .. " 'accedez a vos reserves intarissables d'energie et de motivation. vous avez l'interieur fort et les outils necessaires pour surmonter tous les obstacles. faites confiance en votre pouvoir interne et ne vous laisse" $) +crtext
			morale2+all
		else
			($ "Bleu" pbold .. " le commandement ne sera plus assure." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. " se sent insuffisant a cause de ne pas trouver tout le monde." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

