// Junkyard Pals

cyoa: junkpalsrescue
	"Resgate no Jardim de Lixo" cyoa-title!
	($ Thegroup .. " fuga do jardim de lixo!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "Clem" pbold .. " survive e mostra algumas otimas dicas de reparo enquanto ele corrige o carro!" $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "Clem" pbold .. " nao te escapou." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Salvaste " .. "Leonard" pbold .. ". Em agradecimento, ele te da " .. thegroup .. "  algumas dicas de tiro!" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonard" pbold .. "''as aventuras seu terminaram." $) +crtext
			morale--all
		then
	then
;

