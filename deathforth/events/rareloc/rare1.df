		gas-draw
;

// Danger Rangers

cyoa: rangerrescue
	"'Fan-Club des Gefahren-Rangers" cyoa-title!
	($ Thegroup .. " 'entkommt dem uberlaufenden Viertel." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "'Pink" pbold .. " ' verpfeift ihr Heilrangerstrahl!" $) +crtext
			6 groupheal
		else
			($ "'Pink" pbold .. " ' ist nicht mehr da, um die Gruppe zu unterstutzen." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "'Orange" pbold .. " ' schenkt etwas von ihrer Energie und Fitness aus." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "'Orange" pbold .. " ' ist nicht mehr da. Ihr Team ist schwacher." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "'Grun" pbold .. " ' schenkt etwas seiner beruhmten Kraft." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "'Grun" pbold .. "'' starke Kraft wird vermissen." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "'Blau" pbold .. " ' gibt eine inspirierende Pep-Talk!" $) +crtext
			morale2+all
		else
			($ "'Blau" pbold .. " ' wird nicht mehr Fuhrung leisten." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. " ' fuhlt sich unzureichend, weil sie jemandes nicht gefunden hat." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

