// Danger Rangers

cyoa: rangerrescue
	"Club de Fanes Danger Ranger" cyoa-title!
	($ Thegroup .. " 'escapa de ese suburbio lleno de insectos." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Rosa" pbold .. " 'desata su Rayo de Enfermera Sanadora!" $) +crtext
			6 groupheal
		else
			($ "Rosa" pbold .. " ya no esta alli para apoyar al grupo." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Naranja" pbold .. " 'le otorga alguna de su energia y aptitud fisica." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Naranja" pbold .. " 'no lo hizo. Su equipo se ha debilitado." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Verde" pbold .. " 'otorga alguna de su famosa fuerza." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Verde" pbold .. "'vastia fuerza desaparecera." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Azul" pbold .. " 'te da un discurso inspirador!" $) +crtext
			morale2+all
		else
			($ "Azul" pbold .. " 'dejara de proporcionar liderazgo." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. " 'se siente insuficiente por no haber encontrado a todos." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

