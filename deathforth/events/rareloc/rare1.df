// Bryce Li

cyoa-choice: bruce-picked 
	cchar .weapon_default@ WEAPON_KUNG = if
		($ cname .. "  impara alcune tecniche di livello master! " .. cHe .. "  ora colpisce ancora piu forte!		Ora " .. chis .. "  le mani possono colpire tre bersagli per attacco!" $) text!
		WEAPON_MASTERKUNG
	else
		($ cname .. "  riceve un corso accelerato di arti marziali! " .. cHe .. "  e in grado di imparare una mossa molto pratica.		Ora " .. chis .. "  le mani sono armi anti zombie!" $) text!
		WEAPON_KUNG
	then cchar .weapon_default! goodfx
;

cyoa: brucerescued
	rescue to recruitee ricon
	0 rescue .party!
	recruitee .alive? if
		"Intra nella Death Road" cyoa-title!
		($ rname .. "  scende dallauto una volta che ha superato la citta invasa." $) text!
		solohuman if
			soloer choosechar
			cchar .weapon_default@ WEAPON_KUNG = if
				($ "In cambio dellaiuto, insegna " .. cname .. "  una forma ancora piu avanzata di arti marziali!" $) +crtext WEAPON_MASTERKUNG
			else
				($ "In cambio dellaiuto, insegna " .. cname .. "  come spiaccicare gli zombie a " .. chis .. "  mani nude!" $) +crtext WEAPON_KUNG
			then cchar .weapon_default! goodfx
		else		
			($ "In segno di gratitudine per laiuto dato a " .. rhim .. ", " .. rhe .. " offers to teach one person how to splat zombies bare-handed.
			Who should get the training?" $) +crtext
			' bruce-picked cyoa-human-choice
		then
	else
		"Sconfitta schiacciante" cyoa-title!
		($ Thegroup .. "  prova un grande senso di perdita per " .. rname .. "  essere stato mangiato. Ora le sue conoscenze di arti marziali sono andate." $) cyoa-text!
		morale2-all badfx
	then
;

