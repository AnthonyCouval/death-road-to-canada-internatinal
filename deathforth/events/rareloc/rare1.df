	"Materiais Perigosos" cyoa-title!
	char-gen to recruitee 23 recruitee .specialtype! ricon
	($ Thegroup .. "  esta dirigindo por uma cidade vazia quando ve um homem na estrada. Ele esta em uma roupa de Hazmat e esta acenando " .. thegroup .. "  por ajuda. Quando o carro se aproxima, ele abre um bueiro e entra no esgoto." $) cyoa-text! basic-tone
		1.5 19 22 rnd 2 3 rnd zforecast
		// mission{ ' door-no-escape to main-event-mission-location-leave }
		"events/rareloc/region/hazmat.df" $load-region
		choice( "Va atras dele!" )choice: premission
		gas-draw
;

// Danger Rangers

cyoa: rangerrescue
	"Clube de Fas do Ranger Perigo" cyoa-title!
	($ Thegroup .. "  escapou que invadiu o suburbio." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Rosa" pbold .. "  desencadeia seu Raio Ranger de Cura!" $) +crtext
			6 groupheal
		else
			($ "Rosa" pbold .. "  nao esta mais la para apoiar o grupo." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Laranja" pbold .. "  concede um pouco de sua energia e condicionamento fisico." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Laranja" pbold .. " ' nao conseguiu. Sua equipe esta diminuida." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Verde" pbold .. "  concede um pouco de sua famosa forca." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Verde" pbold .. "' a poderosa forca fara falta." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Azul" pbold .. "  faz um discurso motivacional inspirador!" $) +crtext
			morale2+all
		else
			($ "Azul" pbold .. "  nao fornecera mais lideranca." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. "  sente se inadequado por nao encontrar todos." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

