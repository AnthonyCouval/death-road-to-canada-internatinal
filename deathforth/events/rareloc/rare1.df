// Lab Office

cyoa: rare1-lab
	"Laboratorio Ominoso" cyoa-title!
	($ Thegroup .. " passa por um complexo de escritorios. A sinalizacao diz " .. "OMNI-BIO-CORP LABS" rbold .. ", o que soa muito mal para ser uma empresa real." $) cyoa-text! basic-tone
	1.7 8 18 rnd 1 2 rnd zforecast // former 2.5, 2
	"events/rareloc/region/laboffice.df" $load-region
		choice( "Investigue o escritorio" )choice: premission
		gas-draw
;

// Cabin Bunker

