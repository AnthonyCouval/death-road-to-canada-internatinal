// Danger Rangers

cyoa: rangerrescue
	"Club de fans del guardabosques del peligro" cyoa-title!
	($ Thegroup .. "  escapa de ese suburbio abarrotado." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Rosa" pbold .. "  desata su rayo sanador!" $) +crtext
			6 groupheal
		else
			($ "Rosa" pbold .. "  ya no esta ahi para apoyar al grupo." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Naranja" pbold .. "  otorga algo de su energia y estado fisico." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Naranja" pbold .. " ' no lo logro. Su equipo esta disminuido." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Verde" pbold .. "  concede algo de su famosa fuerza." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Verde" pbold .. "' su poderosa fuerza sera extranada." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Azul" pbold .. "  da una charla motivacional inspiradora!" $) +crtext
			morale2+all
		else
			($ "Azul" pbold .. "  ya no proporcionara liderazgo." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. "  se siente inadecuado por no encontrar a todos." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

