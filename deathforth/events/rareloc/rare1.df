// Danger Rangers

cyoa: rangerrescue
	"Danger Ranger Fan Club" cyoa-title!
	($ Thegroup .. " entkommt diesem uberfullten Vorort." $) text!
	rangerp 0= not if
		0 rangerp .party!
		rangerp .alive? if
			($ "Pink" pbold .. " setzt ihren Healing Ranger Ray ein!" $) +crtext
			6 groupheal
		else
			($ "Pink" pbold .. " ist nicht mehr da um die Gruppe zu unterstutzen." $) +crtext
		then
	then
	rangero 0= not if
		0 rangero .party!
		rangero .alive? if
			($ "Orange" pbold .. " schenkt etwas von ihrer Energie und Fitness." $) +crtext
			' .fitness++ foreach-team-char
		else
			($ "Orange" pbold .. " 'hat es nicht geschafft. Ihr Team ist geschwacht." $) +crtext
		then
	then
	rangerg 0= not if
		0 rangerg .party!
		rangerg .alive? if
			($ "Grun" pbold .. " gibt etwas von seiner beruhmten Starke." $) +crtext
			' .strength++ foreach-team-char
		else
			($ "Grun" pbold .. "'s machtige Starke wird vermisst werden." $) +crtext
		then
	then
	rangerb 0= not if
		0 rangerb .party!
		rangerb .alive? if
			($ "Blau" pbold .. "  halt eine inspirierende Motivationsrede!" $) +crtext
			morale2+all
		else
			($ "Blau" pbold .. "  wird keine Fuhrung mehr ubernehmen." $) +crtext
		then
	then
	rangerp 0= rangero 0= rangerg 0= rangerb 0= or or or if
		($ Thegroup .. "  fuhlt sich unzulanglich weil er nicht alle gefunden hat." $) +crtext
		morale--all badfx
	else
		goodfx
	then
;

