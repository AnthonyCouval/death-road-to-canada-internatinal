// Junkyard Pals

cyoa: junkpalsrescue
	"'Abfallplatz-Rettung" cyoa-title!
	($ Thegroup .. " entkommt dem Schrottplatz!" $) text! goodfx
	junkpal1 0= not if
		0 junkpal1 .party!
		junkpal1 .alive? if
			($ "'Clem" pbold .. " " $) +crtext
			car-weld-rnd car-tune-rnd
			' .mechanic++ foreach-team-char
		else
			($ "'Clem" pbold .. " 'entkam nicht mit dir." $) +crtext
			morale--all
		then
	then
	junkpal2 0= not if
		0 junkpal2 .party!
		junkpal2 .alive? if
			($ "Du hast gerettet " .. "Leonard" pbold .. "'. In Dankbarkeit gibt er " .. thegroup .. " einige Schießtipps!" $) +crtext
			30 trunk-rifle-shell+
			' .shooting++ foreach-team-char
		else
			($ "Leonard" pbold .. "''s Abenteuer sind beendet." $) +crtext
			morale--all
		then
	then
;

