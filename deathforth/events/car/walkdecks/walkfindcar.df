uses base.df
uses charfilt.df
uses charstats.df
// uses road.df

cyoa{ road{

: walkwandermore 
 	// Removed due to not really being a sensible choice, confusing to new players
	// choice( "Vagueie por mais um dia" )choice 
;

cyoa: carfind-tunnel
	"Tunel de Preservacao de Carros" cyoa-title!
	someicon
	($ Thegroup .. " ' caminha ate a entrada de um tunel escuro, cheio de carros. Deve haver algum combustivel e um carro funcionando la dentro. Tambem muitos zumbis dos quais voce nao pode escapar facilmente." $) cyoa-text! basic-tone
		1.7 2 frnd 18 23 rnd 1 3 rnd zforecast
		"events/car/region/car-tunnel.df" $load-region
		choice( "Procurar tunel" )choice: premission
		walkwandermore
;

cyoa: carfind-dirt
	"Estrada de Terra para Fora daqui" cyoa-title!
	someicon
	($ Thegroup .. "  ve a estrada de terra se estender ao longe. Ha muitos carros destruidos e muitos zumbis. No final esta um carro que parece que pode pegar." $) cyoa-text! basic-tone
		1.7 2 frnd 13 23 rnd 1 3 rnd zforecast
		"events/car/region/car-dirt.df" $load-region
		choice( "Tente chegar ao carro" )choice: premission
		walkwandermore
;

cyoa: carfind-gravel
	"Estrada Acidentada para a Seguranca" cyoa-title!
	someicon
	($ Thegroup .. "  esta caminhando por uma estrada de cascalho quando " .. gthey .. "  ve" .. g1s .. " ' um grupo de carros destruidos a frente. Provavelmente ha gasolina em alguns carros, e um ainda pode funcionar." $) cyoa-text! basic-tone
		1.7 2 frnd 9 23 rnd 1 3 rnd zforecast
		"events/car/region/car-gravel.df" $load-region
		choice( "Investigue os carros" )choice: premission
		walkwandermore
;

cyoa: carfind-country
	"Longa Caminhada pelo Campo" cyoa-title!
	someicon
	($ Thegroup .. " is walking through the countryside and stumbles on burnt out cars, with zombies roaming around them. This is a good opportunity to check if one of the cars still works." $) cyoa-text! basic-tone
		1.5 1.7 frnd 7 23 rnd 1 3 rnd zforecast
		"events/car/region/car-country.df" $load-region
		choice( "Investigue esses carros" )choice: premission
		walkwandermore
;

cyoa: carfind-city
	"Ultima Rua do Mundo" cyoa-title!
	someicon
	($ "Ha uma cidade adiante, podre de zumbis " .. Thegroup .. " sabe que andar por uma rua da cidade assim e uma ma ideia, mas um carro espera no final dela."  $) cyoa-text! basic-tone
		2 2.5 frnd 8 23 rnd 2 3 rnd zforecast
		"events/car/region/car-city.df" $load-region
		choice( "Passe por cima mesmo" )choice: premission
		walkwandermore
;

cyoa: carfind-long
	"Longa Estrada a Percorrer" cyoa-title!
	someicon
	($ Thegroup .. " encontra uma longa estrada cheia de carros abandonados e os zumbis que talvez costumavam ser seus donos. Deve haver muito gas ainda em seus tanques, e deve haver um carro que ainda funcione."  $) cyoa-text! basic-tone
		1.7 2 frnd 8 12 rnd 2 3 rnd zforecast
		"events/car/region/car-long.df" $load-region
		choice( "Procurar por um bom carro" )choice: premission
		walkwandermore
;

cyoa: walkfind-junk
	"Ultima parada sucata e salvamento" cyoa-title!
	($ Thegroup .. "  caminha ate um ferro velho com um carro parado na frente. O carro parece que vai funcionar e pode ser o ultimo que voce vera por um tempo." $) cyoa-text! basic-tone
	1.1 1.4 frnd 8 18 frnd 0 2 rnd zforecast
	"events/car/region/junk-car.df" $load-region
		choice( "Explore Ferro velho" )choice: premission
		walkwandermore
;

cyoa: walkfind-rest
	"Bom lugar para descansar" cyoa-title!
	($ Thegroup .. "  encontra um ponto de descanso apos horas caminhando pela estrada. Bem na frente dele esta um carro que parece poder ser dirigido. As chaves devem estar em algum lugar proximo." $) cyoa-text! basic-tone
	1.3 7 22 frnd 1 3 rnd zforecast
	"events/car/region/rest-car.df" $load-region
		choice( "Verifique aquele carro" )choice: premission
		walkwandermore
;

cyoa: walkfind-cabin
	"Cabana no fim da estrada" cyoa-title!
	($ "Depois de horas de caminhada " .. thegroup .. " ' encontra um carro parado ao lado de uma estrada rural. Esta em otimo estado, mas as chaves estao faltando. Uma cabana e visivel ao norte." $) cyoa-text! basic-tone
	1.5 7 22 frnd 1 3 rnd zforecast
	"events/car/region/cabin-car.df" $load-region
		choice( "Procure as chaves" )choice: premission
		walkwandermore
;

} }



