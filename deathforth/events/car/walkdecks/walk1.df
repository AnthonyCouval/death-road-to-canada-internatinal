
// Might need to add trunk-food 0 > if then checks to taking food away. I added this to walk-hole.

cyoa{ road{ 

cyoa: walk-stubtoe
	image-stubbed-toe
	"ZEH STOSS" cyoa-title!
	someone choosechar
	($ "Beim Wandern, " .. cname .. "  stot sich " .. chis .. " ' Zeh schlimm. Es ist keine groe Sache, aber es ist wirklich argerlich." $) cyoa-text! 
	bitefx morale-- sting--
;
cyoa: walk-hole
	image-backpack-hole
	"Bodenlose Grube" cyoa-title!
	choosehuman
	($ "Beim Wandern, " .. cname .. " ' bemerkt ein Loch in " .. chis .. "  rucksack nicht." $) cyoa-text!
	nosupplyloss2? if
	// cchar .weaponb@ 0 = and
		($ "Es gab so wenig Vorrate dass keine verloren gingen." .. cname .. "  fuhlt sich dabei wirklich selbstgefallig!" $) +crtext morale2+ goodfx
	else
		($ "Als " .. che .. "  erkennt dies, es gab Stunden der Moglichkeit, dass Vorrate durchfallen." $) +crtext
		morale-- badfx supplyloss2
	then
;

cyoa: walk-carrock
	image-car-looking-rock
	"SO NAH" cyoa-title!
	someone choosechar
	($ "Wahrend " .. cname .. "  dachte dass " .. che .. "  entdeckte ein Auto in der Ferne. Erst als " .. che .. "  in die Nahe davon kam " .. che .. "  erkannte dass es nur ein riesiger Felsen war. Schade." $) cyoa-text! badfx morale--all 
;
cyoa: walk-dirty
	image-wet-bag
	"Alles schmutzig" cyoa-title!
	choosehuman
	($ "Wahrend " .. cname .. " ' nicht darauf achtet wo " .. che .. "  hingeht und in eine tiefe Wasserpfutze fallt. " .. cHis .. "  ausrustung ist vollstandig untergetaucht und bringt Vorrate in Gefahr." $) cyoa-text! supplyloss2 morale2- badfx
;


cyoa-choice: walk-branchbreak-go
	($ cname .. "  beschliet auf einen Baum zu klettern um einen besseren Uberblick uber die Umgebung zu erhalten." $) cyoa-text!
	cfitness+ if
		($ cHe .. " ' klettert mit wenig Muhe auf den Baum. Es ist eine lustige Ubung! " .. cHe .. "  entdeckt etwas in der Ferne." $) +crtext morale++ goodfx fitnessREV
	then;
	40 %chance if
		 ($ "Auf dem Weg nach oben bricht der Ast, der " .. chis .. "  gewicht hielt, und " .. chim .. "  fallt eine gute Strecke!" $) +crtext fitnessREV morale-- bitefx ouch--
	then;
		($ cHis .. "  plan funktioniert gut. " .. cHe .. "  entdeckt etwas in der Ferne und bekommt eine majestatische Aussicht zu sehen!" $) +crtext fitnessREV morale++ goodfx
;
: walk-branchbreak-no 2 ' walk-countdown +! badfx ;

cyoa: walk-branchbreak
	image-tree
	"DER AST" cyoa-title!
	choosehuman
	($ "Wahrend einer Wanderung durch den Wald " .. thegroup .. "  verirrt sich ziemlich. Wenn " .. gthey .. "  weiter" .. g1s .. "  so weitermacht, " .. gthey .. "' wird viel Zeit verschwenden." $) cyoa-text! 
		fittest charchoice( "  klettert auf einen Baum um sich umzuschauen" )choice: walk-branchbreak-go
		choice( "Blindlings wandern weiter" )choice: walk-branchbreak-no
;
cyoa: walk-nicewalk
	image-uneventful-way
	"Schoner Spaziergang" cyoa-title!
	($ "" .. Thegroup .. " weiter wandelt durchs Land. Es ist sehr unauffallig aber zumindest gibt es keine Zombies oder Gefahren in Sicht. Fur den ersten Mal seit langem konnen sie ihre Wache abschalten und in irgendeiner Form friedlich gehen." $) cyoa-text! basic-tone
;
cyoa: walk-mold
	image-white-van
	"Fur immer gut" cyoa-title!
	($ "" .. Thegroup .. " entdeckt ein Backofenwagen, der an der Seite der Strae stehenblieb. Der Wagen ist unbehebbar, das Brot ist verdorben und essbar, aber es gibt noch etwas Sukuchen, die noch gut sind." $) cyoa-text! morale++all 4 trunk-food+ goodfx
;
cyoa: walk-pain1
	"Sprung ins Ungewisse" cyoa-title!
	($ Thegroup .. " wird von Zombiehorden zwischengestoen, wahrend sie auf einer Klippenstrae fahren. Ohne ein Auto war das einzige Moglichkeit, sich zu entkommen, indem man sprang und hoffte, dass es gut geht. Der Fall war schmerzhaft, aber nicht todlich." $) cyoa-text! sting--all bitefx
;
cyoa: walk-pain2
	"Katzenangriff!" cyoa-title!
	someicon
	image-feral-cat
	
	($ cname .. " " .. che .. " ' wird plotzlich von einer verwilderten Katze zerfleischt! Wilde Katzen uberziehen die Vereinigten Staaten nach der Zombocalypse. Du siehst sie nie, aber sie beobachten immer. Warten auf jede Schwache." $) cyoa-text! sting-- morale-- bitefx
;
cyoa: walk-pain3
	"Raues Gelande" cyoa-title! someicon
	($ Thegroup .. " 'lauft in eine besonders raues Stuck Land hinein. Es gibt nichts Lebensgefahrliches daran, es ist ein langweiliger und peinlicher Spaziergang " .. gThey .. "  bekommt" .. g1s .. " durch viele kleine Unfalle gezeichnet" $) cyoa-text! sting--all morale--all bitefx
;
cyoa: walk-rainmorale
	"Schreckliches Wetter" cyoa-title! someicon
	($ "Das Wetter schlagt plotzlich um. " .. Thegroup .. "  wird im Freien uberrascht und muss in Bewegung bleiben. " .. gTheyare .. "  ist gezwungen in erbarmlichen Bedingungen herumzustolpern." $) cyoa-text!
	roadtrip-has-umbrella? if
		umbrella-count numhums >= if 
			solohuman if choosehuman 
				($ "'Es ist gut dass " .. cname .. "  einen Regenschirm mitgebracht hat!" $)
			else "Es gibt genug Regenschirme fur alle!" then +crtext "Es hatte viel schlimmer kommen konnen!" +crtext goodfx
		else 
			umbrella-count 2 < if "Ein Regenschirm hilft, aber nur ein wenig." else "Es gibt nicht genug Regenschirme um alle vollstandig vor dem Regen zu schutzen!"
			then +crtext morale--all badfx
		then
	else
		morale2-all badfx
	then
	1.2 1.6 frnd weather! -2 2 frnd wind_force!
;
cyoa: walk-meal
	image-caught-fish
	"Kostenloses Essen" cyoa-title!
	choosehuman
	($ "Beim Spaziergang entlang eines groen Baches " .. cname .. "  bemerkt einige Fische im Wasser. " .. cname .. "  versucht sein Gluck, geht ins Wasser und versucht, einige Fische mit der Hand zu fangen. Wie durch ein Wunder " .. cname .. "  ist in der Lage einen anstandigen Wels zu fangen! " $) cyoa-text! morale++all +6 trunk-food+ goodfx
;

cyoa-choice: walk-bandit1-paranoid
	($ cname .. "  einen Hinterhalt vorausgesehen, also " .. che .. "  gesetzt " .. chis .. "  plant kurz vor dem Erscheinen der Banditen auf." $) cyoa-text!
	solo if
		($ cHe .. "  rennt sofort los als die Banditen anfangen Forderungen zu stellen. Als die Banditen jagen " .. chim .. " stolpern sie uber Aste und Schlingen, die auf dem Weg aufgestellt wurden." $)
	else
		($ "Wenn die Banditen ihre Forderungen an " .. cname .. ", " .. theothers .. "  schleichen" .. g2s .. " ' hinter ihnen in einem Gegenangriff. Sie sind davon uberrascht und ergeben sich." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-choice: walk-bandit1-charm
	($ cname .. " ' uberzeugt die Banditen, den Angriff nicht durchzufuhren. Sie wollten es sowieso nicht." $) cyoa-text! goodfx
	trunk-food 4 > if
		($ "Aus Mitleid " .. cname .. "  gibt ihnen etwas zu essen." $) +crtext -5 trunk-food+ morale++
	else
		($ cname .. " ' will ihnen helfen, kann aber nicht viel ubrig lassen." $) +crtext  morale--
	then
;

cyoa: walk-bandit1
	"Widerwillige Banditen" cyoa-title!
	($ "Ohne Auto, " .. thegroup .. " ist eine leichte Beute fur Banditen." .. Thegroup .. " wird von ungeschickten Banditen uberfallen die sich fur den Raub entschuldigen. Sie sind wahrscheinlich neu in diesem Geschaft." .. "Sie verlangen " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid if 
			charchoice( " but got this " )choice: walk-bandit1-paranoid
		then
		most-charming if
			charchoice( "  grunde mit ihnen" )choice: walk-bandit1-charm
		then
;

cyoa-choice: walk-bandit2-coolit
	cpet? if 
		($ cname .. "  irgendwie schafft es mit " .. chis .. "  tiermund zu sagen" .. "  COOL ES " pbold .. "zu den gnadenlosen Banditen." $)
	else
		($ cname .. "  sagt den gnadenlosen Banditen" .. "  KUHL ES." pbold $)
	then cyoa-text!
	($ "Dies verwandelt sie in morderische Banditen. Kein Wunder war hier moglich." $) +crtext
	cool-it++ sting--all ouch2- witsattREV bitefx
;

cyoa-choice: walk-bandit2-pay
	($ Thegroup .. "  zahlt." $) text!
	ezbandit? if
		($ gThey .. "  verlieren" .. g1s .. "  ein Stuck von " .. ghis .. "  vorrate. " .. gTheyare .. "  noch am Leben, vorerst." $) 
	else
		($ gThey .. "  verlieren" .. g1s .. " " .. ghis .. "  waffen und alles andere " .. gthey .. "  hatte. " .. gTheyare .. "  noch am Leben, vorerst." $) 
	then +crtext badfx
	 ezbandit? not if 0weaponsall trunk.clear then bandit-all morale3-all someicon
;

: bandit2-ezbandit ezbandit? if "  ein Viertel " else "  halfte " then ;

cyoa-choice: walk-bandit2-paranoid
	($ cname .. "  einen Hinterhalt erwartet hatte, also " .. che .. "  sich schon fruher einen Fluchtplan ausgedacht hatte." $) cyoa-text!
	($ gHe .. "  wirft schnell" .. bandit2-ezbandit .. "von " .. ghis .. "  vorrate auf den Boden, um abzulenken, und dann " .. " RENNT WEG!" rbold .. "  als ein Kugelhagel vorbeizischt." $) +crtext
	sting-- 
	($ "Der Plan war wirklich simpel und alles andere als perfekt, aber es hatte viel schlimmer kommen konnen." $) +crtext goodfx witsattREV bandit-supplies2
;

cyoa-choice: walk-bandit2-gourmand
	"'Gut! Was auch immer! Ist mir egal!" text! goodfx
	($ cname .. "  watschelt zu jedem Banditen und frisst dann die Waffe, die der Bandit halt. " .. cHe .. "  macht das bei jedem einzelnen Banditen. Alle sind zu verwirrt, um sich zu bewegen oder uberhaupt auf das Geschehen zu reagieren." $) +crtext
	($ cname .. "  erklart spater dass " .. che .. "  war " .. "''wirklich hungrig'" pbold .. "." $) +crtext 
	morale3+
	cchar .vitality@ 6 < if 
		1.5 cchar .scale.body.x!
		cchar .vitality++ -1 STAT_DEXTERITY cchar .bonus+
	then
;

cyoa-bridge: walk-bandit2-eatpick
	' gourmandcheck pick-team-char 0= if false then; pickchar true if choosechar then
	cicon
	"Was?	Was meinen Sie mit " text! "'Iss die Waffen der Banditen" pbold +text "?" +text
	"'Das macht ja uberhaupt keinen Sinn!" +crtext
	cchar charchoice( " ' isst die Waffen des Banditen! " .. "MACH ES!" rbold )choice: walk-bandit2-gourmand
	choice( "Du hast mich gehort! Iss all diese Waffen!" )choice: walk-bandit2-gourmand
	choice( "'Iss die Waffen! Es ist der perfekte Konter!" )choice: walk-bandit2-gourmand
	choice( "Iss Waffen, aber benutze Stealth Taktiken" )choice: walk-bandit2-gourmand
;

cyoa: walk-bandit2
	"Gnadenlose Banditen" cyoa-title!
	($ "Ohne Auto, " .. thegroup .. "  ist eine leichte Beute fur Banditen. Eine Gruppe uberfallt dich und verlangt deine Vorrate." $) text!
	ezbandit? if
		($ "Alle Lebensmittel und Benzin sowie die Halfte deiner Munition und medizinischen Vorrate." rbold $)
	else
		($ "Alle Waffen, Munition, Benzin, Vorrate. Alles." rbold .. "das ist auergewohnlich grausam, auf einem Niveau, zu dem die meisten Banditen nicht sinken wurden. Sie wissen, dass es ein Todesurteil ist." $)
	then +crtext 1 500 tone
	choice( "Erfullen Sie ihre Forderungen" )choice: walk-bandit2-pay
	bandit-fight
		numgourmands 0 > if
			choice( "'Iss die Waffen der Banditen" )choice: walk-bandit2-eatpick
		then
		most-paranoid if 
			charchoice( ": Fallenlassen" .. bandit2-ezbandit .. "beute machen und abhauen" )choice: walk-bandit2-paranoid
		then
		most-grating if
			charchoice( " ' sagt 'Cool It' als letzte Worte" )choice: walk-bandit2-coolit
		then
;

cyoa-choice: walk-bandit3-burn
	($ cname .. "  verspricht den Banditen dass wenn " .. che .. " ' nicht gehen gelassen wird, " .. che .. "  zuruckkommen und die Brucke niederbrennen wird." $) cyoa-text!
	50 %chance if
		($ "Die Banditen beschlieen auf Nummer sicher zu gehen und sofort anzugreifen. " .. Thegroup .. "  schafft es, dem Kampf zu entkommen, ohne dass Vorrate gestohlen werden." $) +crtext
		solo if 
			sting2-
		else
			sting-- sting--all
		then bitefx 
	else
		($ "Die Banditen sind von dem Gedanken eingeschuchtert, ihren Lebensunterhalt zu verlieren, also lassen sie " .. thegroup .. "  passieren.		Sobald " .. che .. "  die Brucke uberquert, " .. che .. "  zundet sie an." $) +crtext morale++ goodfx
	then aholeREV
;

cyoa-choice: walk-bandit3-paranoid
	($ cname .. "  einen Hinterhalt erwartet hatte, also " .. che .. "  ging allein uber die Brucke und trug nichts.	Die Banditen sind genervt von " .. chis .. "  mangel an Zeug und beschissener Einstellung, also werfen sie " .. chim .. " ' uber die Brucke. Es ist kein schlimmer Sturz." $) cyoa-text!
	sting-- 
	($ "Wahrend das passiert, " .. theothers .. "  schleichen" .. g2s .. " an der Brucke mit den Versorgungen!" $) +crtext goodfx witsattREV
;

cyoa: walk-bandit3
	"Tollbrucker Diebe" cyoa-title!
	($ Thegroup .. "  wird auf einer schwachsinnigen Brucke uber einen Fluss gestellt.	Die Diebe fordern eine Gebuhr fur den Durchlass.	" .. "Sie wollen " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid notsolo and if 
			charchoice( " but got this " )choice: walk-bandit3-paranoid
		then
		most-ahole if
			charchoice( "  droht die Brucke in Brand zu setzen" )choice: walk-bandit3-burn
		then
;

cyoa-choice: walk-bandit4-fight
	($ Thegroup .. "  ist mude und im Offen stehen als " .. gthey .. "  kampfen" .. g1s .. "'. Glucklicherweise sind diese Diebe nicht sehr gefahrlich. Nach einer kurzen Auseinandersetzung fliehen sie." $) cyoa-text! someicon bitefx sting--all
;
cyoa-choice: walk-bandit4-angry
	($ "Die Diebe sind " .. "VOLLIG AUFGEJAGT" rbold .. " von " .. cname .. "!	Sie stolpern sich in einem wilden Rausch um zu fliehen!	" .. Thegroup .. "  ist erleichtert einen Kampf wahrend mude zu vermeiden." $) cyoa-text! goodfx temperREV strengthREV morale++
;
cyoa-choice: walk-bandit4-obliv
	cpet? if
		($ cname .. "  macht eine laute bedrohende Gerauschquelle. Aus dem Gerausch heraus stellt sich heraus dass es sich um Tierverwirrung handelte." $) cyoa-text!
	else
		($ cname .. "  sagt so etwas wie " .. "WAS HAST DU GESAGT?" pbold .. "  aber wirklich " .. che .. " ' habe sie einfach nicht gehort." $) cyoa-text!
	then
	($ "Die Banditen verstehen das falsch und sind " .. "SEHR EINGESCHUCHTERT" rbold .. " von " .. cname .. "! Sie fliehen und zerstreuen sich!	" .. cname .. "  hat keine Ahnung was passiert ist ist aber froh dass die Banditen weg sind." $) +crtext goodfx witsattREV morale++
;

cyoa: walk-bandit4
	"Feige Banditen" cyoa-title!
	($ Thegroup .. "  wird von schwachen Banditen ausgeraubt. Die Banditen sind von einer Art, die normalerweise kein Problem ware, wenn " .. thegroup .. " ' ein Auto hatte oder nicht mude ware den ganzen Tag zu Fu zu gehen.	" .. "Die Banditen fordern nervos " rbold $) cyoa-text! 1 500 tone
	bandit-demand choice( "Weigere dich und kampfe!" )choice: walk-bandit4-fight
	most-angrystrong if 
		charchoice( "  erschreckt die Banditen" )choice: walk-bandit4-angry
	then
	most-oblivious if
		charchoice( ": WAS HAST DU GESAGT?" )choice: walk-bandit4-obliv
	then
	// badfx morale--all bandit-ammomed bandit-food 0bweaponsall
;

: walk-boot-get 1 WEAPON_THROWINGBOOT trunk.weapon+ goodfx ;

cyoa: walk-boot
	image-stubbed-toe
	"Free Boot" cyoa-title!
	($ Thegroup .. " ' entdeckt einen alten Stiefel auf der Strae. Er sieht nicht so aus, als ob er jemandem passen wurde, aber hey, er ist kostenlos! Das ist sicher ein Symbol fur Gluck." $) cyoa-text!
		choice( "Hol dir den Stiefel" )choice: walk-boot-get
		choice( "Stiefel ignorieren" )choice
;

: walk-caryell-check args( @ )
	<- @ oblivious? if
		($ <- @ .colourname .. " 'hat nichts davon bemerkt." $) +crtext 
		<- @ <- @ .witsattREV
	then;
	<- @ .morale--
;

cyoa: walk-caryell
	image-angry-car
	"Unsterblicher amerikanischer Zeitvertreib" cyoa-title!
	($ Thegroup .. "  sieht ein Auto die Strae entlang rasen. Vielleicht kann dieser Fremde ihnen helfen! Ohne langsamer zu werden, schreit der Fahrer " .. FLASHY .. "NEEEEEEEEEEEEEEEEEEEERD" .. gS .. "!" .. WHITE $) cyoa-text!
	"Und dann ist das Auto weg." +crtext ' walk-caryell-check foreach-human-char badfx
;

} }
