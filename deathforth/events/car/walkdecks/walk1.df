
// Might need to add trunk-food 0 > if then checks to taking food away. I added this to walk-hole.

cyoa{ road{ 

cyoa: walk-stubtoe
	image-stubbed-toe
	"Dedos" cyoa-title!
	someone choosechar
	($ "Enquanto andava, , " .. cname .. " estourou " .. chis .. " 'dedo do pe mal. Nao e uma grande coisa, mas e muito irritante.'," $) cyoa-text! 
	bitefx morale-- sting--
;
cyoa: walk-hole
	image-backpack-hole
	"Poco Infinito" cyoa-title!
	choosehuman
	($ "Enquanto andava, , " .. cname .. " 'nao percebe um buraco na ' " .. chis .. " sacola." $) cyoa-text!
	nosupplyloss2? if
	// cchar .weaponb@ 0 = and
		($ "Havia tantas poucas suprimentos que nenhum foi perdido.		" .. cname .. " se sente muito orgulhoso disso!" $) +crtext morale2+ goodfx
	else
		($ "Quando " .. che .. " percebe isto, havia horas de oportunidade para suprimentos cairem." $) +crtext
		morale-- badfx supplyloss2
	then
;

cyoa: walk-carrock
	image-car-looking-rock
	"TaO PERTO" cyoa-title!
	someone choosechar
	($ " Enquanto caminhava por uma estrada de terra, " .. cname .. "  pensou que " .. che .. "  avistou um carro ao longe. So quando " .. che .. "  aproximouse dele que " .. che .. " 'percebi que era apenas uma pedra gigante. Que pena.'" $) cyoa-text! badfx morale--all 
;
cyoa: walk-dirty
	image-wet-bag
	"Todos sujos" cyoa-title!
	choosehuman
	($ "Ao caminhar pela floresta, " .. cname .. " ' nao presta atencao por onde " .. che .. "  esta indo e cai em uma poca funda de agua. " .. cHis .. "  equipamento fica totalmente submerso, colocando suprimentos em perigo." $) cyoa-text! supplyloss2 morale2- badfx
;


cyoa-choice: walk-branchbreak-go
	($ cname .. "  decide subir em uma arvore para obter um melhor ponto de vista da area circundante." $) cyoa-text!
	cfitness+ if
		($ cHe .. " ' sobe na arvore com pouco esforco. E um exercicio divertido! " .. cHe .. "  avista algo a distancia." $) +crtext morale++ goodfx fitnessREV
	then;
	40 %chance if
		 ($ "No caminho para cima, o galho que estava segurando " .. chis .. "  peso quebra, fazendo " .. chim .. "  cair uma boa distancia!" $) +crtext fitnessREV morale-- bitefx ouch--
	then;
		($ cHis .. "  plano da certo. " .. cHe .. "  avista algo a distancia e consegue ver uma vista majestosa!" $) +crtext fitnessREV morale++ goodfx
;
: walk-branchbreak-no 2 ' walk-countdown +! badfx ;

cyoa: walk-branchbreak
	image-tree
	"O GALHO" cyoa-title!
	choosehuman
	($ "Ao caminhar pela floresta, " .. thegroup .. " " .. gthey .. " manter" .. g1s .. " assim, por exemplo, " .. gthey .. "''ira desperdicar muito tempo." $) cyoa-text! 
		fittest charchoice( " sube em um arvore para olhar ao redor" )choice: walk-branchbreak-go
		choice( "Continua errando sem rumo" )choice: walk-branchbreak-no
;
cyoa: walk-nicewalk
	image-uneventful-way
	"Bela Caminhada" cyoa-title!
	($ "" .. Thegroup .. " continua caminhando pela campanha. E muito descontraido mas pelo menos nao ha zumbis ou perigos a vista. Por primeira vez em um longo tempo eles podem deixar de guardar a pena e andar em algum tipo de paz." $) cyoa-text! basic-tone
;
cyoa: walk-mold
	image-white-van
	"Bom para sempre" cyoa-title!
	($ "" .. Thegroup .. " encontra uma caminhaopao que se enganchou no lado da estrada. O caminhao e irreparavel, o pao esta amassado e incomestivel, mas ha algumas cakes doces que ainda estao boas." $) cyoa-text! morale++all 4 trunk-food+ goodfx
;
cyoa: walk-pain1
	"Salto de Fe" cyoa-title!
	($ Thegroup .. " se encontra preso entre hordas de zumbis numa estrada de montanha. Sem carro, a unica forma de escapar era saltando e esperando por melhor. A queda foi dolorosa, mas nao fatal." $) cyoa-text! sting--all bitefx
;
cyoa: walk-pain2
	"Ataque de Gato!" cyoa-title!
	someicon
	image-feral-cat
	
	($ cname .. " esta caminhando pela estrada quando " .. che .. " ' e subitamente atacada por um gato selvagem! Gatos selvagens revestem os Estados Unidos apos o zumbocalipse. Voce nunca os ve, mas eles estao sempre observando. Esperando por qualquer fraqueza." $) cyoa-text! sting-- morale-- bitefx
;
cyoa: walk-pain3
	"Terreno acidentado" cyoa-title! someicon
	($ Thegroup .. " ' percorre um trecho de estrada particularmente acidentado. Nao ha nada de ameacador nisso, e apenas uma caminhada longa e miseravel. " .. gThey .. "  pega" .. g1s .. "  machucado e ferido em varios pequenos acidentes." $) cyoa-text! sting--all morale--all bitefx
;
cyoa: walk-rainmorale
	"Clima Horrivel" cyoa-title! someicon
	($ "O tempo muda repentinamente para pior. " .. Thegroup .. "  e pego ao ar livre e deve continuar se movendo. " .. gTheyare .. "  forcado a tropecar em condicoes miseraveis." $) cyoa-text!
	roadtrip-has-umbrella? if
		umbrella-count numhums >= if 
			solohuman if choosehuman 
				($ "'E uma coisa boa que " .. cname .. "  trouxe um guardachuva!" $)
			else "Ha guardachuvas suficientes para todos!" then +crtext "As coisas poderiam ter sido muito piores!" +crtext goodfx
		else 
			umbrella-count 2 < if "Ter um guardachuva ajuda, mas so um pouco." else "Nao ha guardachuvas suficientes para proteger totalmente a todos da chuva torrencial!"
			then +crtext morale--all badfx
		then
	else
		morale2-all badfx
	then
	1.2 1.6 frnd weather! -2 2 frnd wind_force!
;
cyoa: walk-meal
	image-caught-fish
	"Free Me" cyoa-title!
	choosehuman
	($ "Walking beside a large stream " .. cname .. " notices some fish in the water. " .. cname .. " tries his luck, goes into the water and tries to catch some fish by hand. By some miracle, " .. cname .. " catches a decent sized Catfish! " $) cyoa-text! morale++all +6 trunk-food+ goodfx
;

cyoa-choice: walk-bandit1-paranoid
	($ cname .. " foresaw an ambush, so " .. che .. " sets " .. chis .. " plans up shortly before the bandits appeared." $) cyoa-text!
	solo if
		($ cHe .. " runs immediately as the bandits start making demands. When they chase " .. chim .. ", they trip over branches and snares that were set on the path." $)
	else
		($ "When the bandits make their demands of " .. cname .. ", " .. theothers .. " sneaks" .. g2s .. " 'sets up a counter-ambush behind them. They're surprised by this and surrender." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-choice: walk-bandit1-charm
	($ cname .. " ' convence os bandidos a nao continuarem com o ataque. Eles nao queriam, de qualquer forma." $) cyoa-text! goodfx
	trunk-food 4 > if
		($ "Sentindo pena, " .. cname .. "  da a eles um pouco de comida." $) +crtext -5 trunk-food+ morale++
	else
		($ cname .. " ' quer ajudalos, mas nao pode poupar muito." $) +crtext  morale--
	then
;

cyoa: walk-bandit1
	"Bandidos Relutantes" cyoa-title!
	($ "Sem um carro, " .. thegroup .. "  e um alvo facil para bandidos." .. Thegroup .. "  e emboscado por bandidos desajeitados que se desculpam pelo roubo. Eles provavelmente sao novos nisso." .. "Exigem " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid if 
			charchoice( "  esperava isso" )choice: walk-bandit1-paranoid
		then
		most-charming if
			charchoice( "  raciocina com eles" )choice: walk-bandit1-charm
		then
;

cyoa-choice: walk-bandit2-coolit
	cpet? if 
		($ cname .. "  de alguma forma consegue falar com " .. chis .. "  boca de animal para dizer" .. "  RELAXA " pbold .. "aos impiedosos bandidos." $)
	else
		($ cname .. " 'Vai se foder seus filhos da puta!'" .. " CALA SEU BICHO." pbold $)
	then cyoa-text!
	($ "'Isso os transforma em bandidos assassinos. Nenhum milagre aqui foi possivel.'" $) +crtext
	cool-it++ sting--all ouch2- witsattREV bitefx
;

cyoa-choice: walk-bandit2-pay
	($ Thegroup .. " paga." $) text!
	ezbandit? if
		($ gThey .. " perder" .. g1s .. " um pedaco de " .. ghis .. " " .. gTheyare .. " ainda vivo, por enquanto." $) 
	else
		($ gThey .. " perder" .. g1s .. " " .. ghis .. " armas e tudo mais " .. gthey .. " " .. gTheyare .. " ainda vivo, por enquanto." $) 
	then +crtext badfx
	 ezbandit? not if 0weaponsall trunk.clear then bandit-all morale3-all someicon
;

: bandit2-ezbandit ezbandit? if "  um quarto " else "  metade " then ;

cyoa-choice: walk-bandit2-paranoid
	($ cname .. "  esperava uma emboscada, entao " .. che .. "  pensei em um plano de fuga mais cedo." $) cyoa-text!
	($ gHe .. "  rapidamente joga" .. bandit2-ezbandit .. "de " .. ghis .. "  suprimentos no chao como uma distracao, entao " .. "CORRE PARA LONGE!" rbold .. "  enquanto uma saraivada de balas passa zunindo." $) +crtext
	sting-- 
	($ "O plano era realmente simples e longe de ser perfeito, mas as coisas poderiam ter sido muito piores." $) +crtext goodfx witsattREV bandit-supplies2
;

cyoa-choice: walk-bandit2-gourmand
	"'Tudo bem! Tanto faz! Eu nao me importo!" text! goodfx
	($ cname .. "  ginga ate cada bandido e come a arma que o bandido esta segurando. " .. cHe .. "  faz isso com cada bandido. Todos estao muito confusos com isso para se moverem ou ate mesmo reagir ao que esta acontecendo." $) +crtext
	($ cname .. "  depois explica que " .. che .. "  estava " .. "''estava muito fome'" pbold .. "." $) +crtext 
	morale3+
	cchar .vitality@ 6 < if 
		1.5 cchar .scale.body.x!
		cchar .vitality++ -1 STAT_DEXTERITY cchar .bonus+
	then
;

cyoa-bridge: walk-bandit2-eatpick
	' gourmandcheck pick-team-char 0= if false then; pickchar true if choosechar then
	cicon
	"O que?	O que voce quer dizer, " text! "'Come o Armamento do Bandido" pbold +text "?" +text
	"'Isso nao faz sentido nada!" +crtext
	cchar charchoice( " ' come o armamento do bandido! " .. "FACA ISSO!" rbold )choice: walk-bandit2-gourmand
	choice( "Voce ouviume! Come tudo aqueles armamentos!" )choice: walk-bandit2-gourmand
	choice( "'Come os armamentos! E a melhor contraofensiva!" )choice: walk-bandit2-gourmand
	choice( "Come armas, mas use taticas furtivas" )choice: walk-bandit2-gourmand
;

cyoa: walk-bandit2
	"Bandeirantes Selvagens" cyoa-title!
	($ "Sem um carro, " .. thegroup .. " voce e facil presa para bandidos. Um grupo os ataca e te exige suas provisoes." $) text!
	ezbandit? if
		($ "Toda a comida e gasolina, mais metade da sua municao e suprimentos medicos." rbold $)
	else
		($ "Todas as armas, municao, gasolina, suprimentos. Tudo." rbold .. "'		Isso e excepcionalmente cruel, a um nivel que a maioria dos bandidos nao se rebaixaria. Eles sabem que e uma sentenca de morte." $)
	then +crtext 1 500 tone
	choice( "De suas demandas" )choice: walk-bandit2-pay
	bandit-fight
		numgourmands 0 > if
			choice( "'Come o Armamento do Bandido" )choice: walk-bandit2-eatpick
		then
		most-paranoid if 
			charchoice( ": Largue" .. bandit2-ezbandit .. "de saque e corra" )choice: walk-bandit2-paranoid
		then
		most-grating if
			charchoice( " ' diz 'Calma' como ultimas palavras" )choice: walk-bandit2-coolit
		then
;

cyoa-choice: walk-bandit3-burn
	($ cname .. "  promete aos bandidos que se " .. che .. " ' nao for liberado, " .. che .. "  voltara e queimara a ponte." $) cyoa-text!
	50 %chance if
		($ "Os bandidos decidem jogar pelo seguro e simplesmente atacam imediatamente. " .. Thegroup .. "  consegue escapar da luta, sem suprimentos roubados." $) +crtext
		solo if 
			sting2-
		else
			sting-- sting--all
		then bitefx 
	else
		($ "Os bandidos ficam intimidados com o pensamento de perder seu sustento, entao deixam " .. thegroup .. "  passar.		Uma vez que " .. che .. "  cruza a ponte " .. che .. "  acendeo em fogo." $) +crtext morale++ goodfx
	then aholeREV
;

cyoa-choice: walk-bandit3-paranoid
	($ cname .. "  esperava uma emboscada, entao " .. che .. " 0.0 " .. chis .. "  'falta de coisa e ma vontade, entao eles jogam' " .. chim .. " 'Sobre a ponte. Nao e uma queda ruim.'" $) cyoa-text!
	sting-- 
	($ "Enquanto isso esta acontecendo, " .. theothers .. " sneaks" .. g2s .. " 'Passe a ponte com os suprimentos!'" $) +crtext goodfx witsattREV
;

cyoa: walk-bandit3
	"Bandeirantes de Ponte de Toll" cyoa-title!
	($ Thegroup .. " e emboscado enquanto cruza um ponte ralado sobre um rio. Os bandidos exigem uma taxa para passar.	" .. " Eles querem " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid notsolo and if 
			charchoice( "  esperava isso" )choice: walk-bandit3-paranoid
		then
		most-ahole if
			charchoice( " amenaca incendiar a ponte" )choice: walk-bandit3-burn
		then
;

cyoa-choice: walk-bandit4-fight
	($ Thegroup .. " esta cansado e e preso no aberto como " .. gthey .. " lutamos" .. g1s .. "'. So que esses bandidos nao sao muito perigosos. Depois de uma breve luta, eles fugem.	" $) cyoa-text! someicon bitefx sting--all
;
cyoa-choice: walk-bandit4-angry
	($ "Sao " .. "UTTERLY TERRIFIED" rbold .. " por " .. cname .. "! Eles se afastam em uma corrida para escapar!	" .. Thegroup .. " se alivia de evitar uma briga enquanto cansado.	" $) cyoa-text! goodfx temperREV strengthREV morale++
;
cyoa-choice: walk-bandit4-obliv
	cpet? if
		($ cname .. " faz um som assustador. Acaba sendo apenas ruidas de confusao animal.	" $) cyoa-text!
	else
		($ cname .. " diz algo como " .. "O QUE VOCE DIZ?" pbold .. "  mas realmente " .. che .. " ' simplesmente nao os ouviu." $) cyoa-text!
	then
	($ "Os bandidos entendem mal isso e estao " .. "MUITO INTIMIDADOS" rbold .. " por " .. cname .. "! Eles fogem e se espalham!	" .. cname .. "  nao tem ideia do que aconteceu mas esta feliz que os bandidos se foram." $) +crtext goodfx witsattREV morale++
;

cyoa: walk-bandit4
	"Bandidos covardes" cyoa-title!
	($ Thegroup .. "  e roubado por bandidos fracos. Os bandidos sao do tipo que normalmente nao seria um problema, se " .. thegroup .. " ' tivesse um carro ou nao estivesse cansado de andar o dia todo.	" .. "Os bandidos exigem nervosamente " rbold $) cyoa-text! 1 500 tone
	bandit-demand choice( "Recuse e lute!" )choice: walk-bandit4-fight
	most-angrystrong if 
		charchoice( "  aterroriza os bandidos" )choice: walk-bandit4-angry
	then
	most-oblivious if
		charchoice( ": O QUE VOCE DISSE?" )choice: walk-bandit4-obliv
	then
	// badfx morale--all bandit-ammomed bandit-food 0bweaponsall
;

: walk-boot-get 1 WEAPON_THROWINGBOOT trunk.weapon+ goodfx ;

cyoa: walk-boot
	image-stubbed-toe
	"Bota Livre" cyoa-title!
	($ Thegroup .. " ' avista uma bota velha na estrada. Nao parece do tamanho de ninguem, mas ei, e gratis!	Este e certamente um simbolo de boa sorte." $) cyoa-text!
		choice( "Pegue essa bota;" )choice: walk-boot-get
		choice( "Ignorar bota;" )choice
;

: walk-caryell-check args( @ )
	<- @ oblivious? if
		($ <- @ .colourname .. " 'nao percebeu nada disso.';" $) +crtext 
		<- @ <- @ .witsattREV
	then;
	<- @ .morale--
;

cyoa: walk-caryell
	image-angry-car
	"Passatempo Imortal Americano;" cyoa-title!
	($ Thegroup .. " encontra um carro correndo pela estrada. Talvez este estranho possa ajudalos!	Sem frear, o motorista grita: ; " .. FLASHY .. "NEEEEEEEEEEEEEEEEEEEERD" .. gS .. "!" .. WHITE $) cyoa-text!
	"E entao o carro desaparece.;" +crtext ' walk-caryell-check foreach-human-char badfx
;

} }
