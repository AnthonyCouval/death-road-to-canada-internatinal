
// Might need to add trunk-food 0 > if then checks to taking food away. I added this to walk-hole.

cyoa{ road{ 

cyoa: walk-stubtoe
	image-stubbed-toe
	"AVANT DE COMMENCER, VEUILLEZ NOTER QUE LES TRAD" cyoa-title!
	someone choosechar
	($ "En marchant. " .. cname .. " stubs " .. chis .. " cela me derange un peu la cheville. c'est pas grave, mais c'est vraiment genant." $) cyoa-text! 
	bitefx morale-- sting--
;
cyoa: walk-hole
	image-backpack-hole
	"Abyss sans fond" cyoa-title!
	choosehuman
	($ "En marchant. " .. cname .. " epee de chevalier pour 25 nourriture " .. chis .. " rucksack." $) cyoa-text!
	nosupplyloss2? if
	// cchar .weaponb@ 0 = and
		($ "Il n'y avait si peu de fournitures qu'aucune n'a ete perdue." .. cname .. " ressent vraiment une grande fierte quant a cela !" $) +crtext morale2+ goodfx
	else
		($ "Dans le temps que " .. che .. " ceci a ete constate, il y a eu des heures durant lesquelles les approvisionnements pouvaient s'effondrer." $) +crtext
		morale-- badfx supplyloss2
	then
;

cyoa: walk-carrock
	image-car-looking-rock
	"C'EST TRES PROCHE." cyoa-title!
	someone choosechar
	($ "En marchant le long d'une piste de terre, " .. cname .. " pensait que " .. che .. " on a repere une voiture a l'horizon. seulement quand " .. che .. " les frais de passage du bandit sont reduits. " .. che .. " c'etait juste une grosse pierre. quel dommage." $) cyoa-text! badfx morale--all 
;
cyoa: walk-dirty
	image-wet-bag
	"Tout sale" cyoa-title!
	choosehuman
	($ "Achat une chaussure attachee a un baton. " .. cname .. " ne tienne pas compte de l'endroit " .. che .. " tombe et se jette dans une profonde boue d'eau. " .. cHis .. " l'engrenage est entierement immerge, les approvisionnements sont menaces." $) cyoa-text! supplyloss2 morale2- badfx
;


cyoa-choice: walk-branchbreak-go
	($ cname .. " mon personne decide de gravir un arbre pour obtenir une meilleure vue du environnement alentour." $) cyoa-text!
	cfitness+ if
		($ cHe .. " mon singe monte dans l'arbre sans peine. c'est une bonne exercice amusante! " .. cHe .. " voici quelque chose que je remarque a l'horizon." $) +crtext morale++ goodfx fitnessREV
	then;
	40 %chance if
		 ($ "Sur le chemin vers le haut, la branche qui tenait " .. chis .. " les poids s'ecrasent, entrainant " .. chim .. " tomber une grande distance !" $) +crtext fitnessREV morale-- bitefx ouch--
	then;
		($ cHis .. " le plan fonctionne bien. " .. cHe .. " voici quelque chose loin dans la distance, et voila une magnifique vue qui s'offre a ses yeux !" $) +crtext fitnessREV morale++ goodfx
;
: walk-branchbreak-no 2 ' walk-countdown +! badfx ;

cyoa: walk-branchbreak
	image-tree
	"LA BRANCHE" cyoa-title!
	choosehuman
	($ "En marchant dans la foret, " .. thegroup .. " se perd un peu. si " .. gthey .. " nourriture" .. g1s .. " 1. n'appuyez pas sur le bouton rouge avant d'etre sur " .. gthey .. "je vais depenser beaucoup de temps." $) cyoa-text! 
		fittest charchoice( " monte dans un arbre pour voir autour de lui" )choice: walk-branchbreak-go
		choice( "Continue a errer aveuglement" )choice: walk-branchbreak-no
;
cyoa: walk-nicewalk
	image-uneventful-way
	"Promenade agreable" cyoa-title!
	($ "" .. Thegroup .. " continuant de marcher par la campagne. il est tres ennuyeux, mais au moins il n'y a pas de zombies ou de dangers en vue. pour la premiere fois depuis longtemps, ils sont en mesure de relacher leur garde et de marcher dans quelque forme de paix." $) cyoa-text! basic-tone
;
cyoa: walk-mold
	image-white-van
	"Toujours bien, toujours heureux" cyoa-title!
	($ "" .. Thegroup .. " voici un camionnette de boulangerie qui s'est arrete par la route a cause d'une panne irreparable. les pains sont sale et indigestes, mais il y a encore quelques gateaux sucres qui sont toujours bon." $) cyoa-text! morale++all 4 trunk-food+ goodfx
;
cyoa: walk-pain1
	"Saut de foi" cyoa-title!
	($ Thegroup .. " sur le bord d'une route, il est assiege par des horde de zombies. sans voiture, la seule facon d'echapper etait en sautant et en esperant pour le mieux. la chute etait douloureuse, mais pas fatale." $) cyoa-text! sting--all bitefx
;
cyoa: walk-pain2
	"Tres epais !" cyoa-title!
	someicon
	image-feral-cat
	
	($ cname .. " marche le long de la route quand. " .. che .. " soudainement, vous etes attaque par un chat felin sauvage !. . 	les chats felins recouvrent les etats-unis apres le zombocalypse. vous ne les voyez jamais, mais ils sont toujours derriere vous. en attendant toute faiblesse." $) cyoa-text! sting-- morale-- bitefx
;
cyoa: walk-pain3
	"Mechanic montee" cyoa-title! someicon
	($ Thegroup .. " se rencontre avec une section de route particulierement rugueuse. il n'y a rien de menacant la-dessus, c'est juste une longue et mechante marche. " .. gThey .. " obtenir" .. g1s .. " gonfle et ecrase a de multiples petites accroches." $) cyoa-text! sting--all morale--all bitefx
;
cyoa: walk-rainmorale
	"Mauvais temps horrible" cyoa-title! someicon
	($ "La meteo soudainement se deteriore. " .. Thegroup .. " est pris au depouillement et doit continuer de se deplacer. " .. gTheyare .. " s'egrener dans des conditions miserables." $) cyoa-text!
	roadtrip-has-umbrella? if
		umbrella-count numhums >= if 
			solohuman if choosehuman 
				($ "C'est une bonne chose que " .. cname .. " se perd un peu. si" $)
			else "Il y a assez d'parasols pour tous !" then +crtext "Cela pouvait etre beaucoup plus mal!" +crtext goodfx
		else 
			umbrella-count 2 < if "Avoir un parapluie cela fait quelque chose, mais c'est peu." else "Il manque de parapluies pour couvrir tous les gens du pluie !"
			then +crtext morale--all badfx
		then
	else
		morale2-all badfx
	then
	1.2 1.6 frnd weather! -2 2 frnd wind_force!
;
cyoa: walk-meal
	image-caught-fish
	"Offre de repas gratuit" cyoa-title!
	choosehuman
	($ "En marcheant le long d'un grand ruisseau. " .. cname .. " voici quelques poissons dans l'eau. " .. cname .. " essaye leur chance, s'immersent dans l'eau et essaient de attraper des poissons a la main. par quel miracule, " .. cname .. " peut attraper une catche de taille decente de catfish ! " $) cyoa-text! morale++all +6 trunk-food+ goodfx
;

cyoa-choice: walk-bandit1-paranoid
	($ cname .. " apercut un guet-apens. " .. che .. " definir " .. chis .. " se prepare brievement avant que les voleurs apparaissent." $) cyoa-text!
	solo if
		($ cHe .. " 'je ne peux pas vous donner ce que vous demandez, je suis simplement un passant.' " .. chim .. "ils se heurtent aux branches et a des pieges poses sur la route." $)
	else
		($ "Lorsque les voleurs formulent leurs exigences : " .. cname .. ", " .. theothers .. " accrocher discretement" .. g2s .. " derriere eux dans une embuscade a l'arriere. ils sont pris par surprise et se rendent." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-choice: walk-bandit1-charm
	($ cname .. " convainc les voleurs de ne pas poursuivre l'attaque. ils n'en avaient pas reellement l'intention." $) cyoa-text! goodfx
	trunk-food 4 > if
		($ "Sentiment de compassion. " .. cname .. " donnez-leurs quelque nourriture." $) +crtext -5 trunk-food+ morale++
	else
		($ cname .. " veut les aider, mais ne peut pas depense beaucoup de temps." $) +crtext  morale--
	then
;

cyoa: walk-bandit1
	"Forge dans le feu" cyoa-title!
	($ "Sans voiture, " .. thegroup .. " une belle oie assise est une proie facile pour les voleurs." .. Thegroup .. " cette personne est prise en embuscade par des voleurs genants qui se disent desoles pour le vol. ils sont probablement des debutants ... ." .. "Prendre le deck numero 2 chaque jour. " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid if 
			charchoice( " attendu ceci" )choice: walk-bandit1-paranoid
		then
		most-charming if
			charchoice( " raisonner avec eux" )choice: walk-bandit1-charm
		then
;

cyoa-choice: walk-bandit2-coolit
	cpet? if 
		($ cname .. " parvient a parler avec quelqu'un. " .. chis .. " bouche d'animal pour parler" .. " FAITES PAS CHAUD. " pbold .. "aux bandits implacables :" $)
	else
		($ cname .. " donnez-leur savoir la misericorde envers les bandits cruels." .. " FAITES ATTENTION A NE PAS S'ECH" pbold $)
	then cyoa-text!
	($ "Cela les transforme en voleurs meurtriers. aucun miracle n'etait possible ici." $) +crtext
	cool-it++ sting--all ouch2- witsattREV bitefx
;

cyoa-choice: walk-bandit2-pay
	($ Thegroup .. " paye a hauteur." $) text!
	ezbandit? if
		($ gThey .. " perdre" .. g1s .. " une morceau de " .. ghis .. " approvisionnements. " .. gTheyare .. " en vie, pour le moment." $) 
	else
		($ gThey .. " perdre" .. g1s .. " " .. ghis .. " armes et tout le reste " .. gthey .. " eut. " .. gTheyare .. " en vie, pour le moment." $) 
	then +crtext badfx
	 ezbandit? not if 0weaponsall trunk.clear then bandit-all morale3-all someicon
;

: bandit2-ezbandit ezbandit? if " beaucoup d'obama " else " desole, je n'ai " then ;

cyoa-choice: walk-bandit2-paranoid
	($ cname .. " on s'attendait a une embuscade. " .. che .. " j'ai elabore un plan d'evasion auparavant." $) cyoa-text!
	($ gHe .. " rapidement jette" .. bandit2-ezbandit .. "instructions : de " .. ghis .. " fournitures au sol pour servir de distraction. " .. "FUITE!" rbold .. " des balles de plomb volent vers toi a la rafale." $) +crtext
	sting-- 
	($ "Le plan etait vraiment simple et loin de parfait, mais les choses pouvaient avoir bien plus mal tourne." $) +crtext goodfx witsattREV bandit-supplies2
;

cyoa-choice: walk-bandit2-gourmand
	"Bien sur, pas d'importance, je m'en fiche !" text! goodfx
	($ cname .. " se fait sur les genoux jusqu'a chacun des brigands et mange ensuite la arme qu'ils tiennent. " .. cHe .. " chaque bandit doit faire cela. chaque personne est trop confuse par ces evenements pour bouger ou reagir a la suite de celles-ci." $) +crtext
	($ cname .. " plus tard expliquera que " .. che .. " etait " .. "'vraiment famish'" pbold .. "." $) +crtext 
	morale3+
	cchar .vitality@ 6 < if 
		1.5 cchar .scale.body.x!
		cchar .vitality++ -1 STAT_DEXTERITY cchar .bonus+
	then
;

cyoa-bridge: walk-bandit2-eatpick
	' gourmandcheck pick-team-char 0= if false then; pickchar true if choosechar then
	cicon
	"What?
	What do you mean, " text! "Manger les armes du voleur" pbold +text "?" +text
	"Ceci n'a aucun sens!" +crtext
	cchar charchoice( " mange les armes du voleur ! " .. "FAIS-LE !" rbold )choice: walk-bandit2-gourmand
	choice( "Mangez tous ces armes !" )choice: walk-bandit2-gourmand
	choice( "Manger les armes ! c'est l'ideal contre !" )choice: walk-bandit2-gourmand
	choice( "Mangez des armes, mais utilisez des techniques de dissimulation." )choice: walk-bandit2-gourmand
;

cyoa: walk-bandit2
	"Bande de voleurs impitoyables" cyoa-title!
	($ "Sans voiture, " .. thegroup .. " vous etes facile proie pour des voleurs. un groupe vous cerne et vous exige vos approvisionnements." $) text!
	ezbandit? if
		($ "Toute nourriture et gaz, ainsi que la moitie de vos munitions et fournitures medicales." rbold $)
	else
		($ "Tout : armes, munitions, gaz, vivres. tout cela." rbold .. "ceci est extraordinairement cruel, a un niveau que la plupart des voleurs ne s'y preteront jamais. ils sont au courant qu'il s'agit d'une peine de mort." $)
	then +crtext 1 500 tone
	choice( "I. liberte d'expression et de reunion pacifique" )choice: walk-bandit2-pay
	bandit-fight
		numgourmands 0 > if
			choice( "Mangez les armes des voleurs." )choice: walk-bandit2-eatpick
		then
		most-paranoid if 
			charchoice( ": jeter" .. bandit2-ezbandit .. "le butin et fuir" )choice: walk-bandit2-paranoid
		then
		most-grating if
			charchoice( " dit 'cool it' comme dernier mot" )choice: walk-bandit2-coolit
		then
;

cyoa-choice: walk-bandit3-burn
	($ cname .. " promet aux voleurs qu'si " .. che .. " ne la laissez pas aller. " .. che .. " il reviendra et brulera le pont a nouveau." $) cyoa-text!
	50 %chance if
		($ "Les voleurs decident de ne rien retarder et d'attaquer immediatement. " .. Thegroup .. " s'en echappe de la bagarre sans rien vole ni emporte." $) +crtext
		solo if 
			sting2-
		else
			sting-- sting--all
		then bitefx 
	else
		($ "Les voleurs sont effrayes a l'idee de perdre leur source de revenu. " .. thegroup .. " une seule fois " .. che .. " traverse le pont. " .. che .. " allume la flamme dessus." $) +crtext morale++ goodfx
	then aholeREV
;

cyoa-choice: walk-bandit3-paranoid
	($ cname .. " on s'attendait a une embuscade. " .. che .. " seul et portant rien, j'ai traverse le pont. ... les voleurs en sont deranges par " .. chis .. " manque de choses et mauvaise humeur, donc ils jettent. " .. chim .. " sur le pont. c'est pas une chute mauvaise." $) cyoa-text!
	sting-- 
	($ "Alors que cela se produit, " .. theothers .. " accrocher discretement" .. g2s .. " traverse le pont avec les approvisionnements !" $) +crtext goodfx witsattREV
;

cyoa: walk-bandit3
	"Voleurs de passerelle routiere a frais" cyoa-title!
	($ Thegroup .. " le heros est pris en embuscade en traversant un pont instable sur une riviere... . les voleurs exigent un peage pour passer... ." .. "Ils veulent " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid notsolo and if 
			charchoice( " attendu ceci" )choice: walk-bandit3-paranoid
		then
		most-ahole if
			charchoice( " menace de bruler le pont" )choice: walk-bandit3-burn
		then
;

cyoa-choice: walk-bandit4-fight
	($ Thegroup .. " est fatigue et pris au depouillement dans l'ouvert. " .. gthey .. " lutter" .. g1s .. "c'est heureux que ces voleurs ne soient pas tres dangereux. apres un combat bref, ils prennent la fuite." $) cyoa-text! someicon bitefx sting--all
;
cyoa-choice: walk-bandit4-angry
	($ "Les voleurs sont : " .. "TOTALLEMENT EPERDUEMENT" rbold .. " reel " .. cname .. "ils se jettent l'un sur l'autre en courant pour s'enfuir!" .. Thegroup .. " se soulage de ne pas devoir s'engager dans un combat a l'epuise." $) cyoa-text! goodfx temperREV strengthREV morale++
;
cyoa-choice: walk-bandit4-obliv
	cpet? if
		($ cname .. " c'est un bruit menacant, mais c'etait juste des bruits de confusion d'un animal." $) cyoa-text!
	else
		($ cname .. " 'dire quelque chose comme ...' " .. "QU'AVEZ-VOUS DIT ?" pbold .. " en realite " .. che .. " je n'ai pas entendu ce qu'ils disent." $) cyoa-text!
	then
	($ "Confondus par cette instruction et agissent de maniere erronee. " .. "TRES INTIMIDE" rbold .. " reel " .. cname .. "ils s'enfuient et se dispersent!" .. cname .. " a ete mordu par une castor au pied. les deux ont explose en morceaux." $) +crtext goodfx witsattREV morale++
;

cyoa: walk-bandit4
	"Vieilles-tues-par-nous:" cyoa-title!
	($ Thegroup .. " est depouille par des voleurs faibles. les voleurs sont de ce genre qui n'aurait normalement pas pose de probleme, si " .. thegroup .. " je parie que les etreints sont responsables de ces zombies." .. "Demandez nerveusement : 10 000 dollars. " rbold $) cyoa-text! 1 500 tone
	bandit-demand choice( "Refuter et combattre !" )choice: walk-bandit4-fight
	most-angrystrong if 
		charchoice( " effraye les voleurs" )choice: walk-bandit4-angry
	then
	most-oblivious if
		charchoice( "QU'EST-CE QUE TU AS DIT ?" )choice: walk-bandit4-obliv
	then
	// badfx morale--all bandit-ammomed bandit-food 0bweaponsall
;

: walk-boot-get 1 WEAPON_THROWINGBOOT trunk.weapon+ goodfx ;

cyoa: walk-boot
	image-stubbed-toe
	"Libre demarche" cyoa-title!
	($ Thegroup .. " ceci est certainement un symbole de bonne chance." $) cyoa-text!
		choice( "Prends ce botton" )choice: walk-boot-get
		choice( "Rimes malades" )choice
;

: walk-caryell-check args( @ )
	<- @ oblivious? if
		($ <- @ .colourname .. " n'ai pas remarque cela." $) +crtext 
		<- @ <- @ .witsattREV
	then;
	<- @ .morale--
;

cyoa: walk-caryell
	image-angry-car
	"Loisir immortel americain" cyoa-title!
	($ Thegroup .. " vous avez apercu une voiture qui roule a grande vitesse sur la route. peut-etre que ce passant pourra nous aider ! " .. FLASHY .. "NEEEEEEEEEEEEEEEEEEEERD" .. gS .. "!" .. WHITE $) cyoa-text!
	"Et alors, la voiture a disparu." +crtext ' walk-caryell-check foreach-human-char badfx
;

} }
