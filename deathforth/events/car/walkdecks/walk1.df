
// Might need to add trunk-food 0 > if then checks to taking food away. I added this to walk-hole.

cyoa{ road{ 

cyoa: walk-stubtoe
	image-stubbed-toe
	" ORTEIL STUB" cyoa-title!
	someone choosechar
	($ " En marchant, " .. cname .. "  se cogne " .. chis .. " ' l'orteil gravement. Ce n'est pas grave, mais c'est vraiment ennuyeux." $) cyoa-text! 
	bitefx morale-- sting--
;
cyoa: walk-hole
	image-backpack-hole
	"Puits sans fond" cyoa-title!
	choosehuman
	($ " En marchant, " .. cname .. " ' ne remarque pas un trou dans " .. chis .. "  sac a dos." $) cyoa-text!
	nosupplyloss2? if
	// cchar .weaponb@ 0 = and
		($ "Il y avait si peu de fournitures quaucune na ete perdue.		" .. cname .. "  se sent vraiment suffisant a ce sujet !" $) +crtext morale2+ goodfx
	else
		($ "Au moment ou " .. che .. "  se rend compte de cela, il y avait eu des heures dopportunite pour que les fournitures tombent." $) +crtext
		morale-- badfx supplyloss2
	then
;

cyoa: walk-carrock
	image-car-looking-rock
	"TELLEMENT PROCHE" cyoa-title!
	someone choosechar
	($ "En marchant sur un chemin de terre, " .. cname .. "  pensait que " .. che .. "  a repere une voiture au loin. Ce nest que lorsque " .. che .. "  sest approche de celui ci que " .. che .. "  a realise que ce netait quun rocher geant. Quel dommage." $) cyoa-text! badfx morale--all 
;
cyoa: walk-dirty
	image-wet-bag
	"Tout sale" cyoa-title!
	choosehuman
	($ "Alors quil marche dans la foret, " .. cname .. " 'ne prend pas garde ou il va et tombe dans une profonde puddle d'eau " .. che .. " son equipement est totalement immerge mettant en danger ses approvisionnements " .. cHis .. "  decide de monter dans un arbre pour avoir une vue plus large de lenvironnement.," $) cyoa-text! supplyloss2 morale2- badfx
;


cyoa-choice: walk-branchbreak-go
	($ cname .. " monte dans larbre avec peu deffort. Cest une bonne exercice" $) cyoa-text!
	cfitness+ if
		($ cHe .. " 'voit quelque chose a distance. " .. cHe .. " sur le chemin vers le haut la branche qui portait" $) +crtext morale++ goodfx fitnessREV
	then;
	40 %chance if
		 ($ "Poids considerable se casse, faisant " .. chis .. " tomber une bonne distance " .. chim .. " son plan fonctionne bien" $) +crtext fitnessREV morale-- bitefx ouch--
	then;
		($ cHis .. " le plan fonctionne bien " .. cHe .. " apercoit quelque chose au loin, et a la chance de voir un paysage majestueux!" $) +crtext fitnessREV morale++ goodfx
;
: walk-branchbreak-no 2 ' walk-countdown +! badfx ;

cyoa: walk-branchbreak
	image-tree
	"LA BRANCHE" cyoa-title!
	choosehuman
	($ "En francais En randonnee dans les bois, " .. thegroup .. " " .. gthey .. "  continue" .. g1s .. "  continue comme ca, " .. gthey .. "''perdra beaucoup de temps." $) cyoa-text! 
		fittest charchoice( "  grimpe dans un arbre pour observer les environs" )choice: walk-branchbreak-go
		choice( "Continue a errer aveuglement" )choice: walk-branchbreak-no
;
cyoa: walk-nicewalk
	image-uneventful-way
	"Belle promenade" cyoa-title!
	($ "" .. Thegroup .. "  continue de marcher a travers la campagne. Cest tres calme mais au moins il ny a pas de zombies ou de dangers en vue. Pour la premiere fois depuis longtemps ils peuvent baisser leur garde et marcher dans une certaine forme de paix." $) cyoa-text! basic-tone
;
cyoa: walk-mold
	image-white-van
	"Bon pour toujours" cyoa-title!
	($ "" .. Thegroup .. "  repere un camion de boulangerie qui est tombe en panne sur le bord de la route. Le camion est irreparable, le pain est moisi et immangeable, mais il y a des gateaux sucres qui sont encore bons." $) cyoa-text! morale++all 4 trunk-food+ goodfx
;
cyoa: walk-pain1
	"Saut de la foi" cyoa-title!
	($ Thegroup .. "  se retrouve coince entre des hordes de zombies sur une route a flanc de falaise. Sans voiture, le seul moyen de sechapper etait de sauter et desperer le meilleur. La chute a ete douloureuse, mais pas fatale." $) cyoa-text! sting--all bitefx
;
cyoa: walk-pain2
	"Attaque de chat !" cyoa-title!
	someicon
	image-feral-cat
	
	($ cname .. "  marche le long de la route lorsque " .. che .. " ' est brutalement attaque par un chat feroce! Les chats fers, qui recouvrent les Etats Unis apres l'apocalypse zombie, ne sont jamais a vue, mais ils sont toujours derriere toi. Attendant qu'il y ait une faiblesse." $) cyoa-text! sting-- morale-- bitefx
;
cyoa: walk-pain3
	"Terrain difficile" cyoa-title! someicon
	($ Thegroup .. " repete une partie particulierement dure de la route. Il n'y a rien de menacant la dessus, c'est juste une longue et mechante marche " .. gThey .. "  obtenir" .. g1s .. " est blesse et ecrase dans plusieurs petites accidents." $) cyoa-text! sting--all morale--all bitefx
;
cyoa: walk-rainmorale
	"Mauvais temps" cyoa-title! someicon
	($ "Le temps soudainement devient mauvais. " .. Thegroup .. "  est pris au plein air, et doit continuer a marcher. " .. gTheyare .. " force de se balader dans des conditions deplaisantes." $) cyoa-text!
	roadtrip-has-umbrella? if
		umbrella-count numhums >= if 
			solohuman if choosehuman 
				($ "'C'est une bonne chose que " .. cname .. "  ait apporte un parapluie!" $)
			else "Il y a assez de parapluies pour tous!" then +crtext "Les choses auraient pu etre bien pires!" +crtext goodfx
		else 
			umbrella-count 2 < if "Avoir un parapluie aide, mais seulement un peu." else "Il ny a pas assez de parapluies pour proteger completement tout le monde de laverse!"
			then +crtext morale--all badfx
		then
	else
		morale2-all badfx
	then
	1.2 1.6 frnd weather! -2 2 frnd wind_force!
;
cyoa: walk-meal
	image-caught-fish
	"Repas gratuit" cyoa-title!
	choosehuman
	($ "En marchant le long dun grand ruisseau " .. cname .. "  remarque quelques poissons dans leau. " .. cname .. "  tente sa chance, va dans leau et essaie dattraper des poissons a la main. Par miracle, " .. cname .. "  parvient a attraper un poisson chat de taille decente! " $) cyoa-text! morale++all +6 trunk-food+ goodfx
;

cyoa-choice: walk-bandit1-paranoid
	($ cname .. "  a prevu une embuscade, donc " .. che .. "  a defini " .. chis .. "  des plans peu de temps avant lapparition des bandits." $) cyoa-text!
	solo if
		($ cHe .. "  court immediatement alors que les bandits commencent a faire des demandes. Lorsque les bandits poursuivent " .. chim .. ", ils trebuchent sur des branches et des pieges qui ont ete places sur le chemin." $)
	else
		($ "Lorsque les bandits font leurs demandes " .. cname .. ", " .. theothers .. " se faufiler" .. g2s .. " 'derriere eux dans une contre embuscade. Ils sont surpris par cela et se rendent." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-choice: walk-bandit1-charm
	($ cname .. " 'convainc les bandits de ne pas poursuivre l'attaque. Ils ne le voulaient pas, de toute facon." $) cyoa-text! goodfx
	trunk-food 4 > if
		($ "Pris de pitie " .. cname .. " leur donne de la nourriture." $) +crtext -5 trunk-food+ morale++
	else
		($ cname .. " 'veut les aider, mais ne peut pas se permettre grand chose." $) +crtext  morale--
	then
;

cyoa: walk-bandit1
	"Bandits reticents" cyoa-title!
	($ "Sans voiture " .. thegroup .. " est une cible facile pour les bandits." .. Thegroup .. " est pris en embuscade par des bandits maladroits qui sexcusent pour le vol. Ils sont probablement nouveaux dans ce domaine." .. "Ils demandent " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid if 
			charchoice( " bonjour je suis un texte en francais" )choice: walk-bandit1-paranoid
		then
		most-charming if
			charchoice( " raisons avec eux" )choice: walk-bandit1-charm
		then
;

cyoa-choice: walk-bandit2-coolit
	cpet? if 
		($ cname .. " parvient en quelque sorte a parler avec " .. chis .. " gueule danimal pour dire" .. " CALME TOI " pbold .. "aux impitoyables bandits." $)
	else
		($ cname .. " dit aux impitoyables bandits de" .. "  CALMER LE JEU." pbold $)
	then cyoa-text!
	($ "Cela les transforme en bandits meurtriers. Aucun miracle netait possible ici." $) +crtext
	cool-it++ sting--all ouch2- witsattREV bitefx
;

cyoa-choice: walk-bandit2-pay
	($ Thegroup .. " paie." $) text!
	ezbandit? if
		($ gThey .. " perdre" .. g1s .. " un morceau de " .. ghis .. " fournitures. " .. gTheyare .. " toujours en vie, pour le moment." $) 
	else
		($ gThey .. " perdre" .. g1s .. " " .. ghis .. " armes et tout le reste " .. gthey .. " eu. " .. gTheyare .. " toujours en vie, pour le moment." $) 
	then +crtext badfx
	 ezbandit? not if 0weaponsall trunk.clear then bandit-all morale3-all someicon
;

: bandit2-ezbandit ezbandit? if "  un quart " else "  moitie " then ;

cyoa-choice: walk-bandit2-paranoid
	($ cname .. "  sattendait a une embuscade, donc " .. che .. "  a pense a un plan devasion plus tot." $) cyoa-text!
	($ gHe .. "  lance rapidement" .. bandit2-ezbandit .. " de " .. ghis .. "  fournitures sur le sol comme une distraction, puis " .. " SENFUIT !" rbold .. "  alors quune pluie de balles passe en sifflant." $) +crtext
	sting-- 
	($ " Le plan etait vraiment simple et loin detre parfait, mais les choses auraient pu etre bien pires." $) +crtext goodfx witsattREV bandit-supplies2
;

cyoa-choice: walk-bandit2-gourmand
	"' Bien ! N'importe quoi ! Je m'en fiche !" text! goodfx
	($ cname .. "  se dandine vers chaque bandit, puis mange larme que le bandit tient. " .. cHe .. "  fait cela pour chaque bandit. Tout le monde est trop confus par cela pour bouger, ou meme reagir a la procedure." $) +crtext
	($ cname .. "  explique plus tard que " .. che .. " etait " .. "tres faim" pbold .. "." $) +crtext 
	morale3+
	cchar .vitality@ 6 < if 
		1.5 cchar .scale.body.x!
		cchar .vitality++ -1 STAT_DEXTERITY cchar .bonus+
	then
;

cyoa-bridge: walk-bandit2-eatpick
	' gourmandcheck pick-team-char 0= if false then; pickchar true if choosechar then
	cicon
	"Quest ce que tu veux dire, " text! "Manger les armes du Bandit" pbold +text "?" +text
	"Cela n'a aucun sens!" +crtext
	cchar charchoice( " mange les armes du bandit ! " .. "Fais le!" rbold )choice: walk-bandit2-gourmand
	choice( "Tu las entendu! Mange tous ces armes!" )choice: walk-bandit2-gourmand
	choice( "Manger des armes! C'est le contre attaquant parfait" )choice: walk-bandit2-gourmand
	choice( "Manger des armes, mais utilise des tactiques de furtivite" )choice: walk-bandit2-gourmand
;

cyoa: walk-bandit2
	"Bandits sans pitie" cyoa-title!
	($ "Sans voiture " .. thegroup .. " est une proie facile pour les bandits. Un groupe vous tend une embuscade et vous demande vos provisions." $) text!
	ezbandit? if
		($ "Of all ammo and medical supplies" rbold $)
	else
		($ "Tout les armes, munitions, essence, provisions. Tout." rbold .. "c'est exceptionnellement cruel, a un niveau auquel la plupart des bandits ne descendront pas. Ils savent que c'est une sentence de mort." $)
	then +crtext 1 500 tone
	choice( "Donner satisfaction a leurs revendications" )choice: walk-bandit2-pay
	bandit-fight
		numgourmands 0 > if
			choice( "Manger les armes du Bandit" )choice: walk-bandit2-eatpick
		then
		most-paranoid if 
			charchoice( ": Lacher" .. bandit2-ezbandit .. " de butin et de courir" )choice: walk-bandit2-paranoid
		then
		most-grating if
			charchoice( " ' dit Calme toi comme derniers mots" )choice: walk-bandit2-coolit
		then
;

cyoa-choice: walk-bandit3-burn
	($ cname .. "  promet aux bandits que si " .. che .. " ' n'est pas relache, " .. che .. "  reviendra et brulera le pont." $) cyoa-text!
	50 %chance if
		($ "Les bandits decident de jouer la securite et dattaquer immediatement. " .. Thegroup .. "  parvient a sechapper du combat, sans quaucune provision ne soit volee." $) +crtext
		solo if 
			sting2-
		else
			sting-- sting--all
		then bitefx 
	else
		($ "Les bandits sont intimides a lidee de perdre leurs moyens de subsistance, alors ils laissent " .. thegroup .. "  passer.		Une fois " .. che .. "  traverse le pont, " .. che .. "  lincendie." $) +crtext morale++ goodfx
	then aholeREV
;

cyoa-choice: walk-bandit3-paranoid
	($ cname .. "  sattendait a une embuscade, donc " .. che .. "  traversa le pont seul, ne portant rien.	Les bandits sont agaces par " .. chis .. "  manque de trucs et une attitude merdique, alors ils balancent " .. chim .. " ' par dessus le pont. Ce n'est pas une mauvaise chute." $) cyoa-text!
	sting-- 
	($ "Pendant que cela se passe, " .. theothers .. " se faufiler" .. g2s .. "  passe le pont avec les fournitures !" $) +crtext goodfx witsattREV
;

cyoa: walk-bandit3
	"Bandits du pont a peage" cyoa-title!
	($ Thegroup .. "  est pris en embuscade alors quil traverse un pont branlant au dessus dune riviere. Les bandits exigent un peage pour traverser." .. "Veulent " rbold $) cyoa-text! 1 500 tone
	bandit-demand-fight
		most-paranoid notsolo and if 
			charchoice( " bonjour je suis un texte en francais" )choice: walk-bandit3-paranoid
		then
		most-ahole if
			charchoice( "  menace de bruler le pont" )choice: walk-bandit3-burn
		then
;

cyoa-choice: walk-bandit4-fight
	($ Thegroup .. "  est fatigue et pris en plein air comme " .. gthey .. " combat" .. g1s .. "'. Il est heureux que ces bandits ne soient pas si dangereux. Apres un court combat, ils s'enfuient." $) cyoa-text! someicon bitefx sting--all
;
cyoa-choice: walk-bandit4-angry
	($ "Les bandits sont " .. "UTTERLY TERRIFIED" rbold .. "  par " .. cname .. "! Ils trebuchent sur eux memes dans une course pour sechapper !" .. Thegroup .. "  est soulage deviter un combat alors quil est epuise." $) cyoa-text! goodfx temperREV strengthREV morale++
;
cyoa-choice: walk-bandit4-obliv
	cpet? if
		($ cname .. "  fait un bruit qui semble menacant. Il savere que ce netait que des bruits de confusion animale." $) cyoa-text!
	else
		($ cname .. "  dit quelque chose comme " .. "QUEST CE QUE TU AS DIT ?" pbold .. "  mais vraiment " .. che .. " ' ne les ai tout simplement pas entendus." $) cyoa-text!
	then
	($ "Les bandits comprennent mal cela et sont " .. "TRES INTIMIDES" rbold .. "  par " .. cname .. "! Ils fuient et se dispersent !	" .. cname .. "  na aucune idee de ce qui sest passe mais est heureux que les bandits soient partis." $) +crtext goodfx witsattREV morale++
;

cyoa: walk-bandit4
	"Bandits laches" cyoa-title!
	($ Thegroup .. "  est vole par des bandits faibles. Les bandits sont dun genre qui ne serait normalement pas un probleme, si " .. thegroup .. " ' avait une voiture ou n'etait pas fatigue de marcher toute la journee.	" .. "Les bandits demandent nerveusement " rbold $) cyoa-text! 1 500 tone
	bandit-demand choice( "Refusez et combattez !" )choice: walk-bandit4-fight
	most-angrystrong if 
		charchoice( "  terrifie les bandits" )choice: walk-bandit4-angry
	then
	most-oblivious if
		charchoice( ": QUE AVEZ VOUS DIT ?" )choice: walk-bandit4-obliv
	then
	// badfx morale--all bandit-ammomed bandit-food 0bweaponsall
;

: walk-boot-get 1 WEAPON_THROWINGBOOT trunk.weapon+ goodfx ;

cyoa: walk-boot
	image-stubbed-toe
	"Botte gratuite" cyoa-title!
	($ Thegroup .. " ' apercoit une vieille botte sur la route. Elle n'a pas l'air d'etre a la taille de quelqu'un, mais bon, c'est gratuit ! C'est surement un symbole de chance." $) cyoa-text!
		choice( "Prendre cette botte" )choice: walk-boot-get
		choice( "Ignorer la botte" )choice
;

: walk-caryell-check args( @ )
	<- @ oblivious? if
		($ <- @ .colourname .. " ' n'a rien remarque de tout cela." $) +crtext 
		<- @ <- @ .witsattREV
	then;
	<- @ .morale--
;

cyoa: walk-caryell
	image-angry-car
	"Le passe temps americain immortel" cyoa-title!
	($ Thegroup .. "  apercoit une voiture qui fonce sur la route. Peut etre que cet etranger peut les aider ! Sans ralentir, le conducteur crie " .. FLASHY .. "NEEEEEEEEEEEEEEEEEEEERD" .. gS .. "!" .. WHITE $) cyoa-text!
	"Et puis la voiture est partie." +crtext ' walk-caryell-check foreach-human-char badfx
;

} }
