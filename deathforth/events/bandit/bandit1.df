
cyoa{ road{

: bandithurt
	ezbandit? if 
		ouch--all
	else
		ouch12rnd--all
	then
;

: haggler-morale 
	haggler? if else morale--all then
;

: bandit-give-in
	($ Thegroup .. "  decide di andare sul sicuro. " .. gThey .. "  dare" .. g1s .. " ' nelle richieste dei banditi." $) cyoa-text! badfx
;
cyoa-choice: bandit-pay-foodtoll
	someicon bandit-give-in bandit-toll-pay haggler-morale 
;
cyoa-choice: bandit-pay-lowtoll
	someicon bandit-give-in eat-all bandit-ammomed2 haggler-morale
;
cyoa-choice: bandit-pay-badtoll
	someicon bandit-give-in bandit-ammomed 
		// check if in car
		vehicle.which@ 0 if
			bandit-gas
		else
			bandit-gastank
		then morale2-all
;
cyoa-choice: bandit-start-fight
	($ Thegroup .. "  sa che ogni singolo di " .. ghis .. " le provviste sono essenziali per " .. ghis .. " possibilita di raggiungere il Canada. Quindi " .. gthey .. " combatti" .. g1s .. "." $) cyoa-text! someicon bitefx
		ezbandit? if 
			ouch--all
		else
			70 %chance if
				ouch12rnd--all
			then;
			ouch2-all
		then
;

: bandit-demand
	bandit-toll? if
		($ bandit-toll .. " cibo " .. "per passare in sicurezza." rbold $) +text
		choice( "Paga il pedaggio di " .. bandit-toll .. " cibo" )choice: bandit-pay-foodtoll
	else
		trunk-food 4 > if
			ezbandit? if 
				($ "il resto del tuo cibo e un quarto delle tue munizioni e scorte mediche." rbold $) 
			else
				($ "il resto del tuo cibo e meta delle tue munizioni e scorte mediche." rbold $) 
			then +text
			choice( "Cedi alle richieste dei banditi" )choice: bandit-pay-lowtoll
		else
			ezbandit? if
				($ "tutta la tua benzina extra e meta delle tue munizioni e scorte mediche!" rbold $)
			else
				($ "tutte le tue munizioni, scorte mediche e serbatoi di benzina extra!" rbold $)
			then +text
			choice( "Cedi alle dure richieste dei banditi" )choice: bandit-pay-badtoll
		then
	then
;
: bandit-fight
	choice( "Rifiuta e combatti!" )choice: bandit-start-fight
;
: bandit-demand-fight
	bandit-demand bandit-fight
;

cyoa-choice: bandit-charm-sucker
	($ cname .. "  inganna i banditi. Mentre aspettano che i rifornimenti vengano lanciati, " .. ghe .. " ' corre verso la macchina e sfreccia via. I banditi riescono a sparare all'auto mentre sfreccia via! Hanno solo il tempo di sfiorarla." $) cyoa-text! -1 car-chassis+ bitefx
	car-chassis 0 > if
		"La macchina prende uno o due buchi in piu, ma per il resto va bene." +crtext
	then loyaltyREV
;

cyoa-choice: bandit-charm-tp
	($ Thegroup .. ", non volendo o non potendo risparmiare altre provviste, decide di buttare via " .. ghis .. "  armi extra dal bagagliaio.	" .. "ARMI EXTRA PERSE!" rbold $) cyoa-text! trunk.clear badfx
;

cyoa-choice: bandit-charm-morale
	($ Thegroup .. ", non volendo o non potendo risparmiare altre provviste, decide di perdere alcuni preziosi oggetti personali per i banditi.	" .. "CHE DELUSIONE!" rbold $) cyoa-text! badfx morale3-all
;

cyoa-bridge: bandit-start-charm
	($ cname .. "  offre un compromesso ai banditi. " .. gHe .. "  lascera qualcosa che i banditi vogliono sulla strada, poi andra via. Nessun danno verra a nessuna delle due parti." $) cyoa-text! witsattREV 
	"Le provviste saranno:" +crtext
		trunk-food bandit-toll2 1 - > if
			choice( bandit-toll2 .. " cibo" )choice: bandit-toll-pay2
		then
		cloyalty- if
			choice( "CIAO, NERDS!!" )choice: bandit-charm-sucker
		then
		trunk-bullet 1 > trunk-rifle-shell 1 > trunk-shotgun-shell 1 > or or if
			choice( "Meta delle tue munizioni" )choice: bandit-ammo2
		then
		trunk-gas 100 > if
			choice( "Italiano" )choice: bandit-gastank
		then
		trunk-medical 1 > if
			choice( "Meta delle sue approvvigionamenti medici" )choice: bandit-med2
		then
		choice( "Tesori personali" )choice: bandit-charm-morale
		trunk.count 0 > if
			choice( "Armi da carro armato tutte" )choice: bandit-charm-tp
		then
;
: bandit-charm
	most-charming if 
		charchoice( " propone una compromissione" )choice: bandit-start-charm
	then
;

// Toll road

cyoa-bridge: bandit-tollroad-coolit
	cpet? if
		($ cname .. " farti sul ponte di tortura." $)
	else
		($ cname .. " dice ai ladri del ponte di" .. " Raffreddati." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Ladro del ponte" recruitee .name!
		($ "Questo veramente fa pensare agli ladri. 	Hanno stato troppo avidi?	Essi offrono un regalo in ordine di cominciare a riparare le cose!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Gli ladri rifiutano di abbassare il ponte finche non ricevono la maggior parte di " .. thegroup .. "'suoi approvvigionamenti." $) +crtext badfx bandit-mostgas morale2-all
	then witsattREV 
;

cyoa-choice: bandit-tollroad-shoot
	($ Thegroup .. "  estrae " .. ghis .. "  arma" .. gs .. "  contro i banditi." $) cyoa-text!
	($ gThey .. " " .. gare .. "  cosi velocemente che i banditi vengono sopraffatti e costretti ad abbassare il ponte.	" .. Thegroup .. "  attraversa il ponte incolume." $) +crtext goodfx
;

cyoa-choice: bandit-tollroad-jump
	($ cname .. "  e veramente furioso per aver dovuto pagare il pedaggio del ponte, anche dopo la civilta. " .. cHe .. "  corre dritto verso il ponte retratto, facendo volare lauto oltre." $) cyoa-text! temperREV car-jump
;

cyoa: bandit-tollroad

	($ "I banditi stanno sorvegliando un ponte retratto su un fiume. Chiedono il pagamento di un pedaggio a chiunque voglia passare.	" .. "Vogliono " rbold $) cyoa-text!
		bandit-demand-fight
		shooting-total 12 > if
			choice( "Estrarre armi" )choice: bandit-tollroad-shoot
		then
		most-grating if
			charchoice( " ' dice ai banditi di 'Calmarsi'" fcoolit )choice: bandit-tollroad-coolit
		then
		smartest-pass if
			smartest charchoice( ": Prendi una deviazione invece" )choice: car-detour
		then
		angriest-pass if 
			angriest choosechar chumany? if
				cchar charchoice( "': SALTIAMOLO" )choice: bandit-tollroad-jump
			then
		then
;

// Quiet Ambush

cyoa-choice: bandit-quiet-paranoid
	($ cname .. " chiama subito i banditi al loro bluff. Prima che i banditi possano urlare, " .. cname .. "  sta gia facendo un sacco di rumore. Mentre i banditi vanno nel panico e gli zombi si precipitano dentro, " .. thegroup .. "  se ne va da una strada laterale. Vale la pena pianificare costantemente per il peggio!" $) cyoa-text! goodfx morale++ morale++all witsREV attitudeREV
;

cyoa-choice: bandit-quiet-angry
	($ "Senza preavviso e senza una sola parola, " .. cname .. "  corre verso il bandito piu vicino e lo prende a pugni in faccia, mettendolo fuori combattimento. Poi " .. cname .. " ' fa la stessa cosa a un secondo bandito prima di calmarsi. 	Tutti sono sbalorditi. I restanti tre banditi decidono che dovrebbero prendere in silenzio i loro amici e andarsene." $) cyoa-text! goodfx morale++ morale++all temperREV strengthREV
;

cyoa-choice: bandit-quiet-pay
	($ Thegroup .. "  paga ai banditi la loro estorsione, invece di rischiare la carneficina." $) cyoa-text!
	-10 trunk-food+ badfx
;

: bandit-quiet-roll
	50 %chance if
		"Uno dei banditi urla e il caos esplode mentre gli zombi sciamano fuori. La fuga e stata disordinata." +crtext ouch--all badfx
	else
		($ "'I banditi erano tutti chiacchiere e non fanno un suono quando il loro bluff viene chiamato. " .. Thegroup .. "  torna alla macchina mentre i banditi si allontanano." $) +crtext goodfx
	then
;

cyoa-choice: bandit-quiet-nofood
	choosehuman cicon
	($ cname .. "  spiega che " .. ghe .. " ' non ha abbastanza cibo." $) cyoa-text! 
	trunk-food 4 > if
		"'I banditi non sembrano voler rischiare la carneficina, quindi accettano di prendere meno cibo." +crtext eat-all
	else
		"I banditi pensano che questa sia una bugia!" +crtext
		bandit-quiet-roll
	then
;

cyoa-choice: bandit-quiet-yell
	choosehuman cicon
	($ cname .. " ' chiama il bluff del bandito." $) cyoa-text! 
	bandit-quiet-roll
;

cyoa-bridge: bandit-quiet-coolit
	cpet? if
		($ cname .. "  scoreggia davvero debolmente ai banditi." .. " Silent... but deadly." pbold $)
	else	
		($ "In un debole sussurro " .. cname .. " sparare a vista" .. " Raffreddati." pbold $) 
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Bandito Silenzioso" recruitee .name!
		($ ", otherwise they attack. " .. cname .. " " $)
		+crtext 
		rescue-trainitem 
		cooled-it goodfx
	else cool-it++
		"I banditi sono, per quello che sembra un lungo periodo, colti di sorpresa da questo." +crtext 
		bandit-quiet-roll
	then witsattREV 
;

cyoa: bandit-quiet
	// NEED IMAGE 
	someicon
	($ "Cinque banditi tendono unimboscata " .. thegroup .. "  mentre " .. gtheyare .. " ' sta per intrufolarsi in una casa infestata da zombi. I banditi parlano a mezza voce, per paura di attirare l'attenzione dei non morti. Dicono di consegnare loro delle provviste, altrimenti urleranno." $) cyoa-text!
		trunk-food 9 > if
			choice( "Dai 10 cibo" )choice: bandit-quiet-pay
		else
			choice( "'Non ho il cibo!" )choice: bandit-quiet-nofood
		then		
		most-angrystrong if 
			charchoice( "  scatena una furia" )choice: bandit-quiet-angry
		then
		most-paranoid if 
			charchoice( "  aveva gia un piano" )choice: bandit-quiet-paranoid
		then
		most-grating if 
			charchoice( " ' dice ai banditi di 'Calmarsi'" fcoolit )choice: bandit-quiet-coolit
		then
		choice( "Chiama bluff" )choice: bandit-quiet-yell
;

// Buff Bandits

cyoa-choice: bandit-buff-pose
	($ cname .. "  sfida i banditi a una gara di pose." $) cyoa-text!
	cstrength+ if
		($ cHe .. "  fa unincredibile routine di flessioni, affinata dallallenamento. I banditi perdono chiaramente e devono lasciare " .. thegroup .. " ' andare a causa del codice del bodybuilder." $) +crtext goodfx morale2+all strengthREV
	then;
	
	cstrength- if
		90 %chance if
			($ cHe .. "  non e neanche lontanamente vicino alla massa muscolare dei banditi. " .. cHe .. " ' perde la gara di pose quasi all'istante. I banditi prendono molte provviste extra, anche quelle di cui non hanno bisogno, per insegnare " .. thegroup .. "  una lezione sui deboli che sfidano i muscolosi. " $) +crtext morale--all bandit-most bandit-gastank badfx
		else
			($ "Per uno strano scherzo del destino, " .. che .. " ' vince in qualche modo la posa a causa di una scappatoia poco conosciuta nel regolamento dei giudici di bodybuilding. I banditi sono piuttosto arrabbiati per questo, ma le loro mani sono legate dalle loro regole." $) +crtext strength++ goodfx morale++all
		then strengthREV
	then;
	
	strengthroll if
		($ cHe .. "  riesce in qualche modo a battere i banditi in una gara di bodybuilding, sia nei segmenti di flessione che nel segmento di talento! " .. Thegroup .. "  viene lasciato andare." $)
		+crtext goodfx strength++ morale++all strengthREV
	then;
	($ cname .. " ' perde la posa. E umiliante e i banditi prendono molte provviste come punizione per la tracotanza di averli sfidati." $) +crtext
		morale-- badfx bandit-ammomed2 bandit-food
;

cyoa-bridge: bandit-buff-coolit
	cpet? if
		($ cname .. "  scoreggia sui banditi e anche sul concetto di bodybuilding." $)
	else	
		($ cname .. "  dice ai buffi banditi di" .. " Raffreddati." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Bandito buffo" recruitee .name!
		($ "Sono colpiti da questo atteggiamento audace!		Dichiarano " .. chim .. "  il loro campione onorario!		I banditi offrono un regalo:" $)
		+crtext morale3+all rescskill1 rescue-reward-shuffle recruit-choice cooled-it goodfx
	else cool-it++
		($ "I banditi sono infuriati da questo atteggiamento negativo e sfogano la loro rabbia su " .. thegroup .. "''s auto. La colpiscono tutti a mani nude, facendo seri danni." $) +crtext -2 car-chassis+ badfx	
		morale--all bitefx
		"I buffi banditi scappano, flessandosi. Anche il cane." +crtext	
	then witsattREV
;

cyoa-choice: bandit-buff-obliv
	($ cname .. "  chiede ai buffi banditi del loro " .. "Fasce da polso" pbold .. "  e " .. "	CONSTANTE SOLLEVAMENTO MANUBRI MENTRE SI PARLA." pbold .. "I banditi parlano di una straordinaria tradizione perduta di " .. "BODYBUILDING" rbold .. "  che ando perduta quando cadde la civilta.	Mostrano cosa intendono con unemozionante esibizione di muscoli. Nel frattempo, i banditi si dimenticano di derubare " .. thegroup .. "." $) cyoa-text!
	morale2+all goodfx witsattREV
;

cyoa: bandit-buff
	image-buff-bandit
	"'Indossano tutti fasce per il sudore" cyoa-title!
	($ "Appaiono i banditi, tutti con fasce per il sudore e tutti super palestrati, persino il loro cane. Chiedono polvere proteica!	" .. "Accetteranno anche " rbold $) cyoa-text!
		bandit-demand
		strongest charchoice( "  li sfida a una gara di pose" )choice: bandit-buff-pose
		most-grating if
			charchoice( " ' dice ai banditi di Calmarsi" fcoolit )choice: bandit-buff-coolit
		then
		most-oblivious if
			charchoice( " ' chiede cosa c'e con gli abiti" )choice: bandit-buff-obliv
		then
		bandit-fight
		
;

// Greedy bandits that demand all of your stuff

: bandit-allofit-ez
	ezbandit? if "meta del" else "tutti" then
;

cyoa-choice: bandit-allofit-pay
	($ "I banditi frugano nelle cose di " .. thegroup .. "' e prendono tutto il cibo e " .. bandit-allofit-ez .. "  le provviste.	" .. Thegroup .. " ' non e felice di questo, ma almeno i banditi non hanno rubato la macchina o addirittura aspirato la benzina. In cambio della cooperazione, hanno anche restituito tutte le armi." $) cyoa-text! badfx
	bandit-most
	morale2-all
;
: bandit-allofit-check args( chara ) 
	<- chara .cfitness+ if
		<- chara .sting--
	else
		<- chara .cfitness- if
			<- chara .ouch2-
		else
			<- chara .ouch-- 50 %chance if <- chara .sting-- then
		then
	then <- chara .fitnessREV
;
cyoa-choice: bandit-allofit-run
	($ Thegroup .. "  decide di correre, pensando che le provviste duramente conquistate valgano il rischio. I banditi iniziano a sparare!" $) cyoa-text!
	solo if
		cfitness+ if
			($ cname .. "  corre e corre, senza stancarsi " .. cHe .. "  viene solo sfiorato." $) +crtext sting--
		else
			cfitness- if
				($ cname .. " runs a few feet before getting winded. 
				" .. cHe .. "  viene colpito duramente." $) +crtext ouch2-
			else
				($ cname .. "  corre fino a " .. che .. "  puo " .. cHe .. "  viene colpito." $) +crtext ouch-- 50 %chance if sting-- then
			then
		then
	else
		' bandit-allofit-check foreach-team-char
	then bitefx
;
cyoa-bridge: bandit-allofit-coolit
	cpet? if
		($ cname .. "  scoreggia sui banditi, dando loro tutti " .. chis .. "  gas puzzolente." $)
	else	
		($ cname .. " sparare a vista" .. " Raffreddati." pbold $) 
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Bandito redento" recruitee .name!
		($ "'Questo ferma i banditi sui loro passi. E se avessero vissuto le loro vite sbagliate per tutto questo tempo? Le cose sarebbero potute andare meglio se semplicemente si fossero raffreddati? Decidono di fare solo cose buone da ora in poi e offrono un regalo!" .. Thegroup .. "  e illeso, con " .. ghis .. "  gli spiriti sollevati." $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Soon as they see you. " .. thegroup .. " cerca di scappare." $) +crtext 
		notsolo if 
			($ "Da un lato, la maggior parte dei proiettili ha colpito " .. cname .. "." $) +crtext
		then
		ouch2- sting--all bitefx
	then witsattREV 
;

cyoa-choice: bandit-allofit-paranoid
	($ "Sempre diffidente, " .. cname .. " era in guardia prima dellimboscata. " .. cHe .. "  corre verso un riparo vicino ma facile da trascurare. " $) cyoa-text!
	notsolo if ($ Everyone .. "  segue " .. chim .. "." $) +text then
	"I banditi perdono il loro vantaggio schiacciante e scappano dopo un breve scontro." +crtext bitefx sting--all witsattREV
;

cyoa: bandit-allofit
	"I tuoi soldi o la tua vita" cyoa-title! someicon
	($ "I banditi catturano " .. thegroup .. "  di sorpresa, in un posto davvero terribile per " .. ghim .. "  combattere. Chiedono che tutto il cibo e le " .. bandit-allofit-ez .. "  scorte vengano consegnate!	" .. "I TUOI SOLDI O LA TUA VITA!" rbold $) cyoa-text!
		most-charming if 
			charchoice( " propone una compromissione" )choice: bandit-start-charm
		else
			choice( "Rinunciare alle scorte" )choice: bandit-allofit-pay
		then
		most-grating if 
			charchoice( " ' dice ai banditi di 'Calmarsi'" fcoolit )choice: bandit-allofit-coolit
		then
		most-paranoid if 
			charchoice( ": Possiamo combatterli!" )choice: bandit-allofit-paranoid
		then
		choice( "Correre per salvarsi!" )choice: bandit-allofit-run
;		

} }


