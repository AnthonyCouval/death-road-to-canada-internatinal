
cyoa{ road{

cyoa-choice: bandit-car-plow
	($ Thegroup .. "  decide di lanciare la macchina contro la barricata dei banditi e Lord Humungoid.	" .. gThey .. "  prende" .. g1s .. " sopravvivere" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;

cyoa-choice: bandit-car-paranoid
	($ cname .. " ha escogitato un piano durante il discorso." $) cyoa-text!
	cpet? if
		($ cHe .. "  inizia a suonare il clacson. I banditi si divertono alla vista di un animale che fa questo. Decidono di lasciare " .. thegroup .. "  passare. Tutto fa parte del piano!" $)
	else
		($ cHe .. "  improvvisamente schiaccia il pedale del gas e svolta bruscamente. Lauto sfreccia attraverso un piccolo varco nella barricata, poi continua ad andare. 		Quando i banditi reagiscono, " .. thegroup .. "  ha gia troppo vantaggio per essere inseguito." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-bridge: bandit-car-coolit
	cpet? if
		($ "Dopo una pausa drammatica, " .. cname .. "  scoreggia. Risuona." $)
	else
		($ cname .. "  dice a Lord Humungoid di" .. " Raffreddati." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Humungoid" recruitee .name!
		($ "Lord Humungoid ha un enorme cambiamento di cuore. 		" .. "THERE HAS BEEN TOO MUCH VIOLENCE... TOO MUCH PAIN. BUT I HAVE AN HONORABLE COMPROMISE!" rbold .. " 		Offre un dono e un passaggio sicuro!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "'A Lord Humungoid non piace questo. I banditi prendono la macchina e tutte le " .. thegroup .. "''s forniture che trovano." $) +crtext badfx car-abandon bandit-all morale2-all
	then witsattREV 
;

cyoa-choice: bandit-car-pay
	image-buff-bandit
	($ Thegroup .. " ' esce dalla macchina e se ne va illeso. Le ultime parole di Lord Humungoid prima di sgommare in " .. thegroup .. "''s sono " .. "PUZZATE PIU TARDI, SFIGATI!" rbold $) cyoa-text! car-abandon bandit-gas
;
	

cyoa: bandit-car
	"Banditi collezionisti di auto" cyoa-title!
	($ "I banditi hanno bloccato la strada stretta piu avanti con le loro auto. Il loro capo, " .. "Lord Humungoid" pbold .. "  dice:	" .. "ANDATE VIA! DATEMI LA VOSTRA AUTO! IL VOSTRO OLIO! LA BENZINA! E VI RISPARMIERO LA VITA.	ASPETTO LA VOSTRA RISPOSTA." rbold $) cyoa-text!
		choice( "Rinunciate allauto e a tutta la benzina" )choice: bandit-car-pay
		bandit-fight
		most-paranoid if
			charchoice( "  ha gia un piano" )choice: bandit-car-paranoid
		then
		most-grating if
			charchoice( " ' dice ai banditi di 'Calmarsi'" fcoolit )choice: bandit-car-coolit
		then
		choice( "Facciamoci largo con lauto!" )choice: bandit-car-plow
;

// Medical bandits

cyoa-choice: bandit-medic-help
	($ cname .. "  si offre di curare il bandito ferito con le sue " .. chis .. "  conoscenze mediche. " $) cyoa-text!
	medicalroll cmedical+ or if
		($ cHe .. "  rimette tutto a posto! I banditi lasciano andare " .. thegroup .. " " $) +text goodfx
	else
		($ cHe .. " ' finisce per peggiorare le cose! I banditi non sono molto contenti." $) +text
		bandithurt bitefx
	then medicalREV medical++
;

cyoa-choice: bandit-medic-paranoid
	cpet? if
		($ cname .. "  sapeva che qualcosa non andava proprio bene tutto il tempo. A causa di " .. chis .. " " .. cgrowlhiss .. "ing, " .. everyone .. "  e avvertito precocemente. " .. gTheyare .. "  pronto a combattere prima dei briganti siano, e scappare illeso." $) 
	else
		($ "Sospettoso come sempre, " .. cname .. "  nasconde la macchina e i rifornimenti prima di approcciarsi al insediamento. " .. cHe .. " 'afferma di non avere nulla di prezioso.	Il falso commerciante ne dubita, ma non riesce a trovare nulla. Allora lasciano andare " .. thegroup .. " " $)
	then cyoa-text! witsattREV goodfx
;

cyoa-bridge: bandit-medic-coolit
	cpet? if
		($ cname .. "  fartisce in una nota che suona come un violino da violetta." $)
	else
		($ cname .. " ordina ai falso commercianti di " .. " Raffreddati." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Falso Commerciante" recruitee .name!
		($ "Il brigante ferito sembra essere veramente commosso da questo!		" .. "Wow! Mi sento cosi meglio ora!" rbold .. "		Ella si alza, fa un giro sul carro e ordina che tu sia portato un regalo!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "I briganti guardano stupiti il " .. thegroup .. "  per un po. Poi prendono la maggior parte di " .. ghis .. "  forniture." $) +crtext badfx bandit-mostgas morale2-all
	then witsattREV 
;

: bandit-medic-pay
		-8 trunk-medical+
;
	

cyoa: bandit-medic
	"Trappola per commercianti" cyoa-title!
	($ Thegroup .. "  controlla quello che sembra essere un insediamento di commercianti. " .. gTheyare .. "  invece vengono attaccati dai banditi che lo hanno preso in consegna. Uno di loro sembra ferito." .. "Chiedono " rbold $) cyoa-text!
	solo if
		soloer ispet? if
			($ "... oh wait, it's just an animal. False alarm!" rbold .. "
			The bandits let " .. soloname .. "  vai!" $) +text goodfx
		then;
	then
	trunk-medical 7 > if
		($ "8 forniture mediche!" rbold $) +text
		choice( "Dai loro 8 forniture mediche" )choice: bandit-medic-pay
	else
		bandit-demand
	then
		medicalest charchoice( "  offre di curare la ferita" )choice: bandit-medic-help
		most-grating if
			charchoice( " ' dice ai banditi di 'Calmarsi'" fcoolit )choice: bandit-medic-coolit
		then
		most-paranoid if
			charchoice( "  ho visto arrivare questo" )choice: bandit-medic-paranoid
		then
		bandit-fight
;

// Weapon thieves

cyoa-choice: bandit-weapon-paranoid
	solo if 
		($ cname .. "  sentivo dei sospetti sul campeggio, quindi " .. che .. "  ho lasciato lauto in moto. " .. cHe .. "  finge di cedere alle loro richieste, ma corre allauto in attesa non appena " .. che .. " ' puo. Le armi dei banditi sono troppo pessime per poter fare qualcosa alla macchina una volta che " .. che .. "' entra, quindi " .. che .. "  SCATTA VIA." rbold $) cyoa-text! goodfx
	else
		($ cname .. "  sentivo dei sospetti sul campeggio, quindi " .. che .. "  volevano aspettare vicino alla macchina. Mentre " .. everyone .. "  viene rapinata, la macchina attraversa a tutta velocita il campeggio, travolge una tenda e poi si ferma. I banditi sono spaventati, quindi scappano!" $) cyoa-text!
		cpet? if 
			cdog? if 
				($ cname .. "  si e chiuso accidentalmente " .. chim .. " dentro la macchina e abbaia per essere lasciato uscire." $)
			else
				($ Everyone .. "  non e sicuro se " .. cname .. "  abbia fatto tutto questo di proposito o meno." $)
			then +crtext
		else
			cloyalty- if
				($ notcname .. "  e davvero sorpreso " .. cname .. " ' non ha provato a rubare la macchina. " .. cHe .. "  sembra nervoso per un momento." $) +crtext
			then loyaltyREV
		then
	then goodfx witsattREV morale++all
;

cyoa-bridge: bandit-weapon-coolit
	cpet? if
		($ cname .. "  scoreggia e ne fa una questione davvero importante." $)
	else
		($ cname .. " sparare a vista" .. " Raffreddati." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Bandito scadente" recruitee .name!
		($ "	In italian I banditi prendono davvero a cuore questa cosa." .. "Si! Chi ha bisogno di armi quando abbiamo lamicizia?" rbold .. "offrono un regalo!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "I banditi iniziano tutti ad attaccare " .. cname .. ". Si dimenticano delle armi." $) +crtext badfx ouch2-
	then witsattREV 
;

: bandit-weapon-check args( @ ) 
	<- @ .cstrength+ if
		($ <- @ .colourname .. " combatte i banditi con " .. <- @ xhis .. " forza!" $) +crtext
		<- @ .strengthREV
	else
		<- @ .cstrength- if
			<- @ .ouch-- <- @ .sting--
		else
			<- @ .ouch--
		then
	then
;

cyoa-choice: bandit-weapon-fight
	($ Thegroup .. " 'decide di tentare la fortuna in uno scontro. Le armi dei banditi sono rozze, ma sono comunque pericolose." $) cyoa-text! 
		bitefx ' bandit-weapon-check foreach-team-char
;

cyoa-choice: bandit-weapon-pay
	($ Thegroup .. " finisce per dare tutte " .. ghis .. " armi ai banditi. I banditi gli danno " .. ghim .. " alcune vecchie armi, forse sentendosi un po in colpa." $) cyoa-text! 0weaponsall trunk.clear morale2-all someicon badfx
	3 WEAPON_NAIL trunk.weapon+ 2 WEAPON_REBAR trunk.weapon+ 1 WEAPON_FISHING trunk.weapon+
	1 WEAPON_PARKING trunk.weapon+
;

cyoa-choice: bandit-weapon-calmshot
	($ cname .. " punta " .. ghis .. " 'l'arma alla velocita della luce e fa esplodere tutte le fragili armi dei banditi. I banditi iniziano a scappare!" $) cyoa-text! goodfx shootingREV temperREV shooting++
;
	

cyoa: bandit-weapon
	"Banditi Ladri di Armi" cyoa-title! someicon
	($ "Mentre " .. thegroup .. " esplora un campeggio " .. gtheyare .. " 	Assalito da banditi. Brandiscono armi improvvisate." .. "Ti chiedono tutte le tue armi." rbold .. "'		Questo e ridicolo. Neanche i banditi di solito lo fanno, poiche e una condanna a morte lenta ma sicura." $) cyoa-text!
		calmshot-check if
			charchoice( " quickdraw e fuochi" )choice: bandit-weapon-calmshot
		else
			choice( "Combatti i banditi!" )choice: bandit-weapon-fight
		then
		most-paranoid if
			solo if charchoice( " ha lasciato la macchina accesa" )choice: bandit-weapon-paranoid
			else charchoice( " arriva in soccorso" )choice: bandit-weapon-paranoid then
		then
		most-grating if
			charchoice( " ' dice ai banditi di 'Calmarsi'" fcoolit )choice: bandit-weapon-coolit
		then
		bandit-charm
		choice( "Arrendetevi e consegnate tutte le armi!" )choice: bandit-weapon-pay
;

cyoa-choice: bandit-loyal-pick
	cicon
	($ cname .. " viene mandato per distrarre i banditi in modo che lascino il posto di blocco della loro auto." $) cyoa-text!
	cloyalty- if
		($ cname .. " si accende allistante " .. everyone .. " e si unisce ai banditi, invece." $) +crtext
		loyaltyREV decruit-bad
		($ Thegroup .. " viene attaccato " .. gThey .. " tentativo" .. g1s .. " to escape..." $) +crtext
		bandithurt
	then;
	($ cname .. " 'non vende " .. everyone .. " ai banditi " .. cHe .. " li distrae da lontano e corre. Loro lo inseguono e lauto riesce a passare." $) +crtext
	ctemper- if
		($ cname .. " va nel panico mentre si ritira " .. cHe .. "  finisce per non seguire il piano, e si fa male!" $) +crtext cchar .sting12rnd-- bitefx
	else
		goodfx
	then
		($ cname .. "  si intrufolera in seguito al posto di blocco e si riunira " .. everyone .. "." $) +crtext
		loyaltyREV
		
;

cyoa-choice: bandit-loyal-plow
	($ Thegroup .. "  decide di arare lauto proprio attraverso il posto di blocco." .. gThey .. "  prende" .. g1s .. " sopravvivere" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;
: bandit-loyal-plow? choice( "Ara lauto attraverso!" )choice: bandit-loyal-plow ;

cyoa-choice: bandit-loyal-paranoid
	($ cname .. "  ha creato un piano di backup prima di essere individuato." $) cyoa-text!
	($ cHe .. " ' si ritira in un edificio in rovina, dove i banditi non possono vedere " .. chim .. ". Quando si avvicinano, " .. che .. "  salta fuori da una finestra sul retro, si intrufolera " .. chis .. "  fino allauto e scappa attraverso un posto di blocco ormai vuoto!" $) +crtext goodfx
;

cyoa-bridge: bandit-loyal-pick4
	someicon
	"Chi dovrebbe essere mandato a distrarre i banditi?" cyoa-text!
	' bandit-loyal-pick cyoa-team-choice
;

cyoa: bandit-loyal
	"Di chi ci si puo fidare?" cyoa-title! someicon
	solo if
		($ cname .. "  un gruppo di banditi che hanno istituito un posto di blocco sulla strada. " .. cHe .. "  non ha un buon modo per distrarli e alla fine viene individuato." .. "Chiedono " rbold $) cyoa-text!
		bandit-demand
		most-paranoid if
			charchoice( "  ha un piano per ingannarli" )choice: bandit-loyal-paranoid
		then
		bandit-charm
		bandit-loyal-plow?
		bandit-fight
	else
		($ Thegroup .. "  individua un gruppo di banditi che hanno istituito un posto di blocco sulla strada. Qualcuno del team potrebbe essere inviato a distrarre i banditi in modo che lauto possa passare. 		Dovrebbe essere qualcuno calmo e " .. "affidabile" rbold .. "." $) cyoa-text!
		team-count 3 > if
			' bandit-loyal-pick 2 #cyoa-team-choice
			choice( "Scegli qualcun altro" )choice: bandit-loyal-pick4
		else
			' bandit-loyal-pick 3 #cyoa-team-choice
		then
			bandit-loyal-plow?
	then
;

