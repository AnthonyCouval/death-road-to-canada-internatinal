
cyoa{ road{

cyoa-choice: bandit-car-plow
	($ Thegroup .. "  decide atravesar el bloqueo de bandidos y a Lord Humungoid con el coche." .. gThey .. "  toma" .. g1s .. "  mucho dano, pero sobrevive" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;

cyoa-choice: bandit-car-paranoid
	($ cname .. "  ideo un plan durante el discurso." $) cyoa-text!
	cpet? if
		($ cHe .. "  empieza a tocar la bocina. Los bandidos se divierten al ver a un animal haciendo esto. Deciden dejar que " .. thegroup .. "  pase. Todo forma parte del plan!" $)
	else
		($ cHe .. "  de repente pisa a fondo el acelerador y gira con fuerza. El coche atraviesa un pequeno hueco en el bloqueo y sigue adelante. Cuando los bandidos reaccionan, " .. thegroup .. "  ya tiene demasiada ventaja como para merecer la pena perseguirlo." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-bridge: bandit-car-coolit
	cpet? if
		($ "Despues de una pausa dramatica, " .. cname .. "  se tira un pedo. Retumba." $)
	else
		($ cname .. "  le dice a Lord Humungoid que" .. "  TRANQUILICENSE." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Humanoide" recruitee .name!
		($ "El Senor Humanoide tiene un gran cambio de corazon.		" .. "THERE HAS BEEN TOO MUCH VIOLENCE... TOO MUCH PAIN. BUT I HAVE AN HONORABLE COMPROMISE!" rbold .. "		Ofrece un regalo y pasaje seguro!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "'El Senor Humanoide no le gusta esto. Los ladrones se llevan el coche y todo lo que encuentran de " .. thegroup .. "''s suministros." $) +crtext badfx car-abandon bandit-all morale2-all
	then witsattREV 
;

cyoa-choice: bandit-car-pay
	image-buff-bandit
	($ Thegroup .. " ' sale del coche y camina sin lastimarse. Las ultimas palabras de Lord Humanoide antes de salir en " .. thegroup .. "''s coche son " .. "HUELEMOSOTROS, NERDOS!" rbold $) cyoa-text! car-abandon bandit-gas
;
	

cyoa: bandit-car
	"Ladrones de Coches" cyoa-title!
	($ "Los ladrones han bloqueado la estrecha carretera adelante con sus coches. Su jefe, " .. "El Senor Humanoide" pbold .. "  habla:" .. "SOLO ALEJATE! DAME TU COCHE! TU ACEITE! LA GASOLINA! Y TE PERDONARE LA VIDA. ESPERO TU RESPUESTA." rbold $) cyoa-text!
		choice( "Entregar el coche y toda la gasolina" )choice: bandit-car-pay
		bandit-fight
		most-paranoid if
			charchoice( " ya tiene un plan" )choice: bandit-car-paranoid
		then
		most-grating if
			charchoice( " ' le dice a Bandits que 'se enfrie'" fcoolit )choice: bandit-car-coolit
		then
		choice( "Atraviesalos!" )choice: bandit-car-plow
;

// Medical bandits

cyoa-choice: bandit-medic-help
	($ cname .. " ofrece curar al bandido herido con " .. chis .. " conocimientos medicos " $) cyoa-text!
	medicalroll cmedical+ or if
		($ cHe .. " arregla las cosas de inmediato! Los bandidos dejan " .. thegroup .. " ir." $) +text goodfx
	else
		($ cHe .. " 'termina empeorando las cosas! Los bandidos no estan muy contentos." $) +text
		bandithurt bitefx
	then medicalREV medical++
;

cyoa-choice: bandit-medic-paranoid
	cpet? if
		($ cname .. " sabia que algo andaba muy mal todo el tiempo. Debido a " .. chis .. " " .. cgrowlhiss .. "ing " .. everyone .. " se le advierte con anticipacion " .. gTheyare .. " listo para luchar antes que los bandidos, y escapar ileso." $) 
	else
		($ "Siempre sospechoso " .. cname .. "  escondio el auto y los suministros antes de acercarse al asentamiento. " .. cHe .. " ' afirma no tener nada de valor. Los bandidos dudan de esto, pero no pueden encontrar nada. Terminan dejando " .. thegroup .. " ir." $)
	then cyoa-text! witsattREV goodfx
;

cyoa-bridge: bandit-medic-coolit
	cpet? if
		($ cname .. "  se tira pedos en un tono que suena como un violin diminuto." $)
	else
		($ cname .. "  les dice a los falsos comerciantes que " .. "  TRANQUILICENSE." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Falso Comerciante" recruitee .name!
		($ "El bandido herido parece realmente conmovido por esto!		" .. "GUAU! Me siento mucho mejor ahora!" rbold .. "		Ella se pone de pie, hace una voltereta y ordena que te traigan un regalo!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Los bandidos simplemente miran el " .. thegroup .. "  por un rato.		Luego toman la mayor parte de " .. ghis .. "  suministros." $) +crtext badfx bandit-mostgas morale2-all
	then witsattREV 
;

: bandit-medic-pay
		-8 trunk-medical+
;
	

cyoa: bandit-medic
	"Trampa del comerciante" cyoa-title!
	($ Thegroup .. "  comprueba lo que parece ser un asentamiento de comerciantes. " .. gTheyare .. "  en cambio emboscado por bandidos que lo han tomado. Uno de ellos parece herido." .. "Ellos exigen " rbold $) cyoa-text!
	solo if
		soloer ispet? if
			($ "... oh wait, it's just an animal. False alarm!" rbold .. "
			The bandits let " .. soloname .. "  vayanse!" $) +text goodfx
		then;
	then
	trunk-medical 7 > if
		($ "8 suministros medicos!" rbold $) +text
		choice( "Dales 8 suministros medicos" )choice: bandit-medic-pay
	else
		bandit-demand
	then
		medicalest charchoice( "  ofrece curar heridas" )choice: bandit-medic-help
		most-grating if
			charchoice( " ' le dice a Bandits que 'se enfrie'" fcoolit )choice: bandit-medic-coolit
		then
		most-paranoid if
			charchoice( "  vio esto venir" )choice: bandit-medic-paranoid
		then
		bandit-fight
;

// Weapon thieves

cyoa-choice: bandit-weapon-paranoid
	solo if 
		($ cname .. "  sintio sospechas sobre el campamento, asi que " .. che .. "  dejo el auto en marcha. " .. cHe .. "  finge ceder a sus demandas, pero corre al auto que espera tan pronto como " .. che .. " ' puede.		Las armas de los bandidos son demasiado malas para hacerle algo al auto una vez " .. che .. "''s en, asi que " .. che .. "  ARRANCA." rbold $) cyoa-text! goodfx
	else
		($ cname .. "  sintio sospechas sobre el campamento, asi que " .. che .. "  queria esperar cerca del auto.		Mientras " .. everyone .. "  esta siendo robado, el auto atraviesa el campamento, arrolla una tienda y luego se detiene. Los bandidos se asustan, asi que huyen!" $) cyoa-text!
		cpet? if 
			cdog? if 
				($ cname .. " accidentalmente bloqueado " .. chim .. "se queda en el coche y ladra para que lo dejen salir." $)
			else
				($ Everyone .. "  no esta seguro de si " .. cname .. "  hizo todo esto a proposito o no." $)
			then +crtext
		else
			cloyalty- if
				($ notcname .. "  esta realmente sorprendido " .. cname .. " ' no intento robar el auto. " .. cHe .. "  parece nervioso por un momento." $) +crtext
			then loyaltyREV
		then
	then goodfx witsattREV morale++all
;

cyoa-bridge: bandit-weapon-coolit
	cpet? if
		($ cname .. "  se tira pedos y hace un gran alboroto al respecto." $)
	else
		($ cname .. " les pida tranquilidad a los ladrones" .. "  TRANQUILICENSE." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Bandido de mierda" recruitee .name!
		($ "Los bandidos realmente se toman esto en serio.		" .. "Si! Quien necesita armas cuando tenemos amistad?" rbold .. "		Ofrecen un regalo!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Todos los bandidos comienzan a atacar " .. cname .. ". Se olvidan de las armas." $) +crtext badfx ouch2-
	then witsattREV 
;

: bandit-weapon-check args( @ ) 
	<- @ .cstrength+ if
		($ <- @ .colourname .. "  lucha contra los bandidos con " .. <- @ xhis .. "  fuerza!" $) +crtext
		<- @ .strengthREV
	else
		<- @ .cstrength- if
			<- @ .ouch-- <- @ .sting--
		else
			<- @ .ouch--
		then
	then
;

cyoa-choice: bandit-weapon-fight
	($ Thegroup .. " ' decide arriesgarse en una pelea. Las armas de los bandidos son toscas, pero siguen siendo peligrosas." $) cyoa-text! 
		bitefx ' bandit-weapon-check foreach-team-char
;

cyoa-choice: bandit-weapon-pay
	($ Thegroup .. "  acaba dando todas " .. ghis .. "  armas a los bandidos. Los bandidos le dan " .. ghim .. "  algunas armas viejas, quiza sintiendose algo culpables." $) cyoa-text! 0weaponsall trunk.clear morale2-all someicon badfx
	3 WEAPON_NAIL trunk.weapon+ 2 WEAPON_REBAR trunk.weapon+ 1 WEAPON_FISHING trunk.weapon+
	1 WEAPON_PARKING trunk.weapon+
;

cyoa-choice: bandit-weapon-calmshot
	($ cname .. "  apunta " .. ghis .. " ' arma a velocidades del rayo y destroza todas las armas endebles de los bandidos. Los bandidos empiezan a correr!" $) cyoa-text! goodfx shootingREV temperREV shooting++
;
	

cyoa: bandit-weapon
	"Bandidos Ladrones de Armas" cyoa-title! someicon
	($ "Mientras " .. thegroup .. "  explora un campamento " .. gtheyare .. "  le tienden una emboscada unos bandidos. Blandiendo armas improvisadas." .. "Te exigen todas tus armas." rbold .. "' Esto es ridiculo. Ni siquiera los bandidos suelen hacer esto, ya que es una sentencia de muerte lenta pero segura." $) cyoa-text!
		calmshot-check if
			charchoice( "  desenfunda y dispara" )choice: bandit-weapon-calmshot
		else
			choice( "Lucha contra los bandidos!" )choice: bandit-weapon-fight
		then
		most-paranoid if
			solo if charchoice( " dejo el carro encendido" )choice: bandit-weapon-paranoid
			else charchoice( " viene al rescate" )choice: bandit-weapon-paranoid then
		then
		most-grating if
			charchoice( " ' le dice a Bandits que 'se enfrie'" fcoolit )choice: bandit-weapon-coolit
		then
		bandit-charm
		choice( "Rindanse todos los armas!" )choice: bandit-weapon-pay
;

cyoa-choice: bandit-loyal-pick
	cicon
	($ cname .. " is sent to distract the bandits so they leave their car checkpoint." $) cyoa-text!
	cloyalty- if
		($ cname .. " enciende instantaneamente " .. everyone .. " y se une a los bandidos, en su lugar." $) +crtext
		loyaltyREV decruit-bad
		($ Thegroup .. "  es atacado! " .. gThey .. "  intenta" .. g1s .. " to escape..." $) +crtext
		bandithurt
	then;
	($ cname .. " ' no vende " .. everyone .. "  a los bandidos! " .. cHe .. "  los distrae desde lejos y corre. Lo persiguen, y el auto puede pasar." $) +crtext
	ctemper- if
		($ cname .. "  entra en panico mientras se retira. " .. cHe .. "  termina no siguiendo el plan correctamente, y se lastima!" $) +crtext cchar .sting12rnd-- bitefx
	else
		goodfx
	then
		($ cname .. "  se cuela por el punto de control mas tarde y se reincorpora " .. everyone .. "." $) +crtext
		loyaltyREV
		
;

cyoa-choice: bandit-loyal-plow
	($ Thegroup .. "  decide arar el auto a traves del punto de control.	" .. gThey .. "  toma" .. g1s .. "  mucho dano, pero sobrevive" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;
: bandit-loyal-plow? choice( "Arar el auto!" )choice: bandit-loyal-plow ;

cyoa-choice: bandit-loyal-paranoid
	($ cname .. "  hizo un plan de respaldo antes de ser visto." $) cyoa-text!
	($ cHe .. " ' se retira a un edificio en ruinas, donde los bandidos no pueden ver " .. chim .. ". Cuando se acercan, " .. che .. " salta por una ventana trasera, se escabulle " .. chis .. " camino al coche, y escapa a traves de un puesto de control ahora vacio!" $) +crtext goodfx
;

cyoa-bridge: bandit-loyal-pick4
	someicon
	"Quien deberia ser enviado para distraer a los bandidos?" cyoa-text!
	' bandit-loyal-pick cyoa-team-choice
;

cyoa: bandit-loyal
	"Quien puede ser confiable?" cyoa-title! someicon
	solo if
		($ cname .. "  un grupo de bandidos que han establecido un puesto de control en la carretera. " .. cHe .. "  no tiene una buena manera de distraerlos, y finalmente es descubierto.		" .. "Ellos exigen " rbold $) cyoa-text!
		bandit-demand
		most-paranoid if
			charchoice( "  tiene un plan para enganarlos" )choice: bandit-loyal-paranoid
		then
		bandit-charm
		bandit-loyal-plow?
		bandit-fight
	else
		($ Thegroup .. "  ve a un grupo de bandidos que han establecido un puesto de control en la carretera. Alguien del equipo podria ser enviado a distraer a los bandidos para que el automovil pueda pasar.		Tendria que ser alguien tranquilo y " .. "confiable" rbold .. "." $) cyoa-text!
		team-count 3 > if
			' bandit-loyal-pick 2 #cyoa-team-choice
			choice( "Escoge a alguien mas" )choice: bandit-loyal-pick4
		else
			' bandit-loyal-pick 3 #cyoa-team-choice
		then
			bandit-loyal-plow?
	then
;

