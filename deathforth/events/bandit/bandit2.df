
cyoa{ road{

cyoa-choice: bandit-car-plow
	($ Thegroup .. " 'decide conducir el coche directamente a traves del bloqueo de bandidos y Lord Humungoid." .. gThey .. " tomar" .. g1s .. " 'sobrevivir" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;

cyoa-choice: bandit-car-paranoid
	($ cname .. " figured out a plan while the speech was happening." $) cyoa-text!
	cpet? if
		($ cHe .. "  go. " .. thegroup .. " todo parte del plan!" $)
	else
		($ cHe .. " 	they are too late. The car is gone. " .. thegroup .. " ' ya tiene demasiada cabeza de ventaja para ser perseguido." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-bridge: bandit-car-coolit
	cpet? if
		($ "Despues de un dramatico pausa, ', " .. cname .. " ' farta. Ecoja." $)
	else
		($ cname .. " 'dice a Humungoid que" .. " 'TRanquILIDAD!" pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Humungoid" recruitee .name!
		($ "El Senor Humungoid tiene una gran cambio de corazon.		" .. "THERE HAS BEEN TOO MUCH VIOLENCE... TOO MUCH PAIN. BUT I HAVE AN HONORABLE COMPROMISE!" rbold .. "		Ofrece un regalo y pasaje seguro!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "El Senor Humungoid no le gusta esto. Los bandidos toman el coche y todas las suministros que encuentran.', " .. thegroup .. "suministros que encuentran." $) +crtext badfx car-abandon bandit-all morale2-all
	then witsattREV 
;

cyoa-choice: bandit-car-pay
	image-buff-bandit
	($ Thegroup .. " ' sale del coche y se va sin lastimio. Las ultimas palabras de Humungoid antes de salir a toda velocidad en " .. thegroup .. "'s car are ' = 'el coche de el es " .. "SMELL YA LATER, NERDS!' = 'HUELE HASTA LUEGO, NERDS!" rbold $) cyoa-text! car-abandon bandit-gas
;
	

cyoa: bandit-car
	"Car Collecting Bandits' = 'Ladrones de Coches" cyoa-title!
	($ "Bandits have blocked the narrow road ahead with their cars. Their leader, ' = 'Los ladrones han bloqueado el estrecho camino por delante con sus coches. Su lider, " .. "Lord Humungoid' = 'Senor Humungoid" pbold .. " ' speaks:	' = 'habla:" .. "JUST WALK AWAY! GIVE ME YOUR CAR! YOUR OIL! THE GASOLINE! AND I WILL SPARE YOUR LIVES.	I AWAIT YOUR ANSWER.' = 'SOLO VAYASE! DEME SU COCHE! SU ACEITE! LA GASOLINA! Y YO PERDONARE SUS VIDAS. ESPERO SU RESPUESTA." rbold $) cyoa-text!
		choice( "Give up the car and all gas' = 'Abandone el coche y todo el combustible" )choice: bandit-car-pay
		bandit-fight
		most-paranoid if
			charchoice( " ' already has a plan' = 'ya tiene un plan" )choice: bandit-car-paranoid
		then
		most-grating if
			charchoice( " 'le dice a los Banditos: 'Tranquilos" fcoolit )choice: bandit-car-coolit
		then
		choice( "Plow through them!' = 'Abriendose paso entre ellos!" )choice: bandit-car-plow
;

// Medical bandits

cyoa-choice: bandit-medic-help
	($ cname .. " ' offers to heal the injured bandit with ' = 'ofrece curar al bandido herido con " .. chis .. " ' medical know-how. ' = 'conocimientos medicos. " $) cyoa-text!
	medicalroll cmedical+ or if
		($ cHe .. " ' fixes things right up! The bandits let ' = 'arregla todo! Los bandidos dejan " .. thegroup .. " 'van a ir." $) +text goodfx
	else
		($ cHe .. " 'acaba haciendose peor! Los bandidos no estan muy felices." $) +text
		bandithurt bitefx
	then medicalREV medical++
;

cyoa-choice: bandit-medic-paranoid
	cpet? if
		($ cname .. " 'sabian que algo estaba muy mal desde el principio. Debido a ', " .. chis .. " " .. cgrowlhiss .. "sospechasos, ', " .. everyone .. " 'es advertido temprano. ', " .. gTheyare .. " 'estan listos para pelear antes que los banditos lo sean, y escapan hermosa." $) 
	else
		($ "Siempre sospechoso, ', " .. cname .. " 'oculto el coche y los suministros antes de acercarse al asentamiento. ', " .. cHe .. " 'dice tener nada de valor.	Los banditos dudan de esto, pero no pueden encontrarlo. Terminan dejandolo ir', " .. thegroup .. " 'van a ir." $)
	then cyoa-text! witsattREV goodfx
;

cyoa-bridge: bandit-medic-coolit
	cpet? if
		($ cname .. " 'dice a los traficantes falsos que" $)
	else
		($ cname .. " 'les dice que " .. " 'TRanquILIDAD!" pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Falso Comerciante" recruitee .name!
		($ "0x0000000000000000," .. "WOW! Me siento mucho mejor ahora!" rbold .. "se levanta, da una voltereta y ordena que te traigan un regalo!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Los bandidos solo miran a lo lejos durante un rato. Despues toman la mayor parte de las suministros. " .. thegroup .. " 'pero luego se les escapa que han sido emboscados por bandidos que se habian apoderado de ella. Uno de ellos parece herido. " .. ghis .. " ' suministros." $) +crtext badfx bandit-mostgas morale2-all
	then witsattREV 
;

: bandit-medic-pay
		-8 trunk-medical+
;
	

cyoa: bandit-medic
	"Asentamiento de comerciante" cyoa-title!
	($ Thegroup .. " 'el comerciante Trap se detiene para ver lo que parece ser un asentamiento de comerciante. " .. gTheyare .. " 'en vez de encontrar a un comerciante, fueron emboscados por bandidos que habian tomado el control. Uno de ellos tiene una herida visible." .. "Exigen " rbold $) cyoa-text!
	solo if
		soloer ispet? if
			($ "... oh wait, it's just an animal. False alarm!" rbold .. "
			The bandits let " .. soloname .. " 'Vamos!" $) +text goodfx
		then;
	then
	trunk-medical 7 > if
		($ "8 suministros medicinales!" rbold $) +text
		choice( "Danos 8 suministros medicinales" )choice: bandit-medic-pay
	else
		bandit-demand
	then
		medicalest charchoice( " 'ofrece curar la herida" )choice: bandit-medic-help
		most-grating if
			charchoice( " 'le dice a los Banditos: 'Tranquilos" fcoolit )choice: bandit-medic-coolit
		then
		most-paranoid if
			charchoice( " 'lo vi venir desde lejos" )choice: bandit-medic-paranoid
		then
		bandit-fight
;

// Weapon thieves

cyoa-choice: bandit-weapon-paranoid
	solo if 
		($ cname .. "  sospecho del campamento, asi que " .. che .. "  dejo el coche encendido. " .. cHe .. " hace como que accede a sus demandas, pero se dirige corriendo hacia el coche que esta esperando mientras " .. che .. " eso ocurre.		La banda de malechores tiene armas demasiado cutres como para hacerle algo al coche una vez " .. che .. "ha subido, asi que " .. che .. " ACELERA A FONDO." rbold $) cyoa-text! goodfx
	else
		($ cname .. "  sospecho del campamento, asi que " .. che .. " queria esperar cerca del coche.		Mientras " .. everyone .. " esta siendo asaltado, el coche atraviesa el campamento, aplasta una tienda de campana y se detiene. Los malechores se asustan y huyen!" $) cyoa-text!
		cpet? if 
			cdog? if 
				($ cname .. " ha quedado encerrado en el coche y aulla para que le dejen salir. " .. chim .. "se ha encerrado sin querer y aulla para que le dejen salir." $)
			else
				($ Everyone .. " no esta seguro de si " .. cname .. " ha hecho todo esto a proposito o no." $)
			then +crtext
		else
			cloyalty- if
				($ notcname .. " esta realmente sorprendido de " .. cname .. " que no haya intentado robar el coche. " .. cHe .. " parece nervioso durante un momento." $) +crtext
			then loyaltyREV
		then
	then goodfx witsattREV morale++all
;

cyoa-bridge: bandit-weapon-coolit
	cpet? if
		($ cname .. " ' toota y se hace un gran caso de ello." $)
	else
		($ cname .. " 'dice a los banditos que" .. " 'TRanquILIDAD!" pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Caballero Malo" recruitee .name!
		($ "Los bandidos realmente lo toman a corazon.		" .. "Si! Quien necesita armas cuando tenemos amistad?" rbold .. "les ofrece un regalo!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Los banditos comienzan a atacar " .. cname .. ". Se olvidan de las armas." $) +crtext badfx ouch2-
	then witsattREV 
;

: bandit-weapon-check args( @ ) 
	<- @ .cstrength+ if
		($ <- @ .colourname .. " 'lucha contra los bandidos con " .. <- @ xhis .. " ' fuerza!" $) +crtext
		<- @ .strengthREV
	else
		<- @ .cstrength- if
			<- @ .ouch-- <- @ .sting--
		else
			<- @ .ouch--
		then
	then
;

cyoa-choice: bandit-weapon-fight
	($ Thegroup .. " 'se decide tomar riesgos en una pelea.		Los armas de los banditos son rudimentarias, pero aun peligrosas." $) cyoa-text! 
		bitefx ' bandit-weapon-check foreach-team-char
;

cyoa-choice: bandit-weapon-pay
	($ Thegroup .. " 'acaba entregando todo " .. ghis .. "  los bandidos dan " .. ghim .. " le algunas armas viejas, tal vez sintiendo algo de culpa." $) cyoa-text! 0weaponsall trunk.clear morale2-all someicon badfx
	3 WEAPON_NAIL trunk.weapon+ 2 WEAPON_REBAR trunk.weapon+ 1 WEAPON_FISHING trunk.weapon+
	1 WEAPON_PARKING trunk.weapon+
;

cyoa-choice: bandit-weapon-calmshot
	($ cname .. "  objetivos " .. ghis .. " el guerrero mueve su arma a velocidades relampago y destruye todas las armas endebles de los bandidos. Los bandidos empiezan a correr!" $) cyoa-text! goodfx shootingREV temperREV shooting++
;
	

cyoa: bandit-weapon
	"Bandidos Ladrones de Armas" cyoa-title! someicon
	($ "Como " .. thegroup .. " 'explora un campamento, " .. gtheyare .. " 'es emboscado por bandidos. Branden m makeshift weapons.	" .. "Exigen todas sus armas." rbold .. "		Esto es ridiculo. Incluso los bandidos rara vez lo hacen, ya que es una sentencia de muerte lenta pero segura." $) cyoa-text!
		calmshot-check if
			charchoice( " 'quickdraws y dispara" )choice: bandit-weapon-calmshot
		else
			choice( "Lucha contra los bandidos!" )choice: bandit-weapon-fight
		then
		most-paranoid if
			solo if charchoice( " 'dejo el coche en marcha" )choice: bandit-weapon-paranoid
			else charchoice( " 'viene al rescate" )choice: bandit-weapon-paranoid then
		then
		most-grating if
			charchoice( " 'le dice a los Banditos: 'Tranquilos" fcoolit )choice: bandit-weapon-coolit
		then
		bandit-charm
		choice( "Entregue todas las armas!" )choice: bandit-weapon-pay
;

cyoa-choice: bandit-loyal-pick
	cicon
	($ cname .. " 'es enviado a distraer a los bandidos para que dejen su puesto de control de coche." $) cyoa-text!
	cloyalty- if
		($ cname .. " 'se enciende instantaneamente " .. everyone .. " 'y se une a los bandidos, en su lugar." $) +crtext
		loyaltyREV decruit-bad
		($ Thegroup .. " 'es atacado! " .. gThey .. " ' intenta" .. g1s .. " to escape..." $) +crtext
		bandithurt
	then;
	($ cname .. " ' no vende " .. everyone .. " ' se las oculta a los banditos! " .. cHe .. " ' se distrae de ellos desde lejos y huye. Ellos la persiguen, y el coche logra pasar." $) +crtext
	ctemper- if
		($ cname .. " ' se desmaya al retirarse. " .. cHe .. " ' termina no siguiendo el plan, y se lastima!" $) +crtext cchar .sting12rnd-- bitefx
	else
		goodfx
	then
		($ cname .. " ' se esquiva al rededor de la retenia mas tarde, y vuelve a unirse " .. everyone .. "." $) +crtext
		loyaltyREV
		
;

cyoa-choice: bandit-loyal-plow
	($ Thegroup .. " ' decide conducir el coche directamente a traves de la retenia.	" .. gThey .. " tomar" .. g1s .. " 'sobrevivir" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;
: bandit-loyal-plow? choice( "Conduzca el coche a traves!" )choice: bandit-loyal-plow ;

cyoa-choice: bandit-loyal-paranoid
	($ cname .. " ' hizo un plan de reserva antes de ser descubierto." $) cyoa-text!
	($ cHe .. " ' se refugia en un edificio ruinoso, donde los banditos no pueden verla " .. chim .. ". Cuando estan mas cerca, " .. che .. " salta por una ventana trasera, se escabulle " .. chis .. " camino al coche, y escapa a traves de un puesto de control ahora vacio!" $) +crtext goodfx
;

cyoa-bridge: bandit-loyal-pick4
	someicon
	"Quien deberia ser enviado para distraer a los bandidos?" cyoa-text!
	' bandit-loyal-pick cyoa-team-choice
;

cyoa: bandit-loyal
	"Quien puede ser confiable?" cyoa-title! someicon
	solo if
		($ cname .. "  un grupo de bandidos que han establecido un punto de control en la carretera. " .. cHe .. "  no tiene una buena manera de distraerlos, y eventualmente es descubierto." .. "Exigen " rbold $) cyoa-text!
		bandit-demand
		most-paranoid if
			charchoice( " tiene un plan para enganarlos" )choice: bandit-loyal-paranoid
		then
		bandit-charm
		bandit-loyal-plow?
		bandit-fight
	else
		($ Thegroup .. " 1 " .. "fiable" rbold .. "." $) cyoa-text!
		team-count 3 > if
			' bandit-loyal-pick 2 #cyoa-team-choice
			choice( "Elige a alguien mas" )choice: bandit-loyal-pick4
		else
			' bandit-loyal-pick 3 #cyoa-team-choice
		then
			bandit-loyal-plow?
	then
;

