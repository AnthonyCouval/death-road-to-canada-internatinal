
cyoa{ road{

cyoa-choice: bandit-car-plow
	($ Thegroup .. "  beschliet mit dem Auto durch die Straensperre der Banditen und Lord Humungoid zu fahren." .. gThey .. "  nehmen" .. g1s .. "  viel Schaden, uberleben aber" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;

cyoa-choice: bandit-car-paranoid
	($ cname .. "  hat wahrend der Rede einen Plan ausgearbeitet." $) cyoa-text!
	cpet? if
		($ cHe .. "  fangt an die Hupe zu hupen. Die Banditen amusieren sich uber den Anblick eines Tieres das dies tut. Sie beschlieen " .. thegroup .. " alles Teil des Plans!" $)
	else
		($ cHe .. " 	they are already far behind. " .. thegroup .. "  hat schon zu viel Vorsprung um eine Verfolgung wert zu sein." $)
	then +crtext morale++all witsattREV goodfx
;

cyoa-bridge: bandit-car-coolit
	cpet? if
		($ " Nach einer dramatischen Pause, " .. cname .. "  furze. Sie hallen wider." $)
	else
		($ cname .. "  sagt Lord Humungoid zu" .. "  KUHL ES." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Humungoid" recruitee .name!
		($ "Lord Humungoid hat einen groen Sinneswandel.		" .. "THERE HAS BEEN TOO MUCH VIOLENCE... TOO MUCH PAIN. BUT I HAVE AN HONORABLE COMPROMISE!" rbold .. "		Er bietet ein Geschenk und sicheren Durchgang an!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "'Lord Humungoid gefallt das nicht. Die Banditen nehmen das Auto und alle von " .. thegroup .. "''s Vorrate die sie finden." $) +crtext badfx car-abandon bandit-all morale2-all
	then witsattREV 
;

cyoa-choice: bandit-car-pay
	image-buff-bandit
	($ Thegroup .. " ' steigt aus dem Auto und geht unversehrt davon. Lord Humungoids letzte Worte bevor er in " .. thegroup .. "''s Auto davonbraust sind " .. "BIS SPATER, NERDS!" rbold $) cyoa-text! car-abandon bandit-gas
;
	

cyoa: bandit-car
	"Autosammelnde Banditen" cyoa-title!
	($ ", is a tough opponent. You should be careful. " .. "Herr Humungoid" pbold .. " spricht:" .. "	In deutsch GEH EINFACH WEG! GIB MIR DEIN AUTO! DEIN OL! DAS BENZIN! UND ICH WERDE EUCH DAS LEBEN SCHENKEN.	ICH ERWARTE EURE ANTWORT." rbold $) cyoa-text!
		choice( "Of the worlds CO emissions" )choice: bandit-car-pay
		bandit-fight
		most-paranoid if
			charchoice( "  hat bereits einen Plan" )choice: bandit-car-paranoid
		then
		most-grating if
			charchoice( " ' sagt Banditen sie sollen sich beruhigen" fcoolit )choice: bandit-car-coolit
		then
		choice( "Pflugt durch sie hindurch!" )choice: bandit-car-plow
;

// Medical bandits

cyoa-choice: bandit-medic-help
	($ cname .. "  bietet an den verletzten Banditen mit " .. chis .. "  medizinischem Know how zu heilen. " $) cyoa-text!
	medicalroll cmedical+ or if
		($ cHe .. "  repariert die Dinge sofort! Die Banditen lassen " .. thegroup .. "  gehen." $) +text goodfx
	else
		($ cHe .. " ' macht am Ende alles noch schlimmer! Die Banditen sind nicht sehr glucklich." $) +text
		bandithurt bitefx
	then medicalREV medical++
;

cyoa-choice: bandit-medic-paranoid
	cpet? if
		($ cname .. "  wusste die ganze Zeit dass etwas nicht stimmte. Aufgrund " .. chis .. " " .. cgrowlhiss .. " des " .. everyone .. "  wird fruhzeitig gewarnt. " .. gTheyare .. "  ist bereit zu kampfen, bevor die Banditen da sind, und entkommt unverletzt." $) 
	else
		($ "Immer misstrauisch, " .. cname .. "  versteckte das Auto und die Vorrate bevor er sich der Siedlung naherte. " .. cHe .. " ' behauptet, nichts Wertvolles zu haben. Die Banditen bezweifeln das, konnen aber nichts finden. Sie lassen am Ende " .. thegroup .. "  gehen." $)
	then cyoa-text! witsattREV goodfx
;

cyoa-bridge: bandit-medic-coolit
	cpet? if
		($ cname .. "  furzt in einer Tonhohe die wie eine winzige Geige klingt." $)
	else
		($ cname .. "  fordert die falschen Handler auf " .. "  KUHL ES." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee "Falscher Handler" recruitee .name!
		($ "Der verletzte Bandit ist davon wirklich bewegt!		" .. "WOW! Ich fuhle mich jetzt so viel besser!" rbold .. "		Sie steht auf, macht einen Radschlag und befiehlt, dass dir ein Geschenk gebracht wird!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ "Die Banditen starren nur auf die " .. thegroup .. "  eine Weile.		Dann nehmen sie das meiste von " .. ghis .. "  liefert." $) +crtext badfx bandit-mostgas morale2-all
	then witsattREV 
;

: bandit-medic-pay
		-8 trunk-medical+
;
	

cyoa: bandit-medic
	"Handlerfalle" cyoa-title!
	($ Thegroup .. "  schaut sich an was wie eine Handlersiedlung aussieht. " .. gTheyare .. "  stattdessen von Banditen uberfallen die sie ubernommen haben. Einer von ihnen sieht verletzt aus.	" .. "Sie verlangen " rbold $) cyoa-text!
	solo if
		soloer ispet? if
			($ "... oh wait, it's just an animal. False alarm!" rbold .. "
			The bandits let " .. soloname .. "  gehen!" $) +text goodfx
		then;
	then
	trunk-medical 7 > if
		($ "acht Medizinpaketen!" rbold $) +text
		choice( "Gib sie acht Medizinpaketen" )choice: bandit-medic-pay
	else
		bandit-demand
	then
		medicalest charchoice( " bietet sich an Verletzung zu heilen" )choice: bandit-medic-help
		most-grating if
			charchoice( " ' sagt Banditen sie sollen sich beruhigen" fcoolit )choice: bandit-medic-coolit
		then
		most-paranoid if
			charchoice( " hat das kommende Schadensereignis vorausgesehen" )choice: bandit-medic-paranoid
		then
		bandit-fight
;

// Weapon thieves

cyoa-choice: bandit-weapon-paranoid
	solo if 
		($ cname .. " verdachtigte den Lagerplatz und " .. che .. " lie den Wagen laufen. " .. cHe .. " simuliert, sich den Forderungen zu ergeben, aber zum wartenden Auto fluchtet sie genau dann, " .. che .. " 'sobald sie es kann.		Die Bande kann die Wagenwaffen nichts anzutun sobald sie " .. che .. "'darin ist, also " .. che .. " RASEND AUSFAHRT." rbold $) cyoa-text! goodfx
	else
		($ cname .. " verdachtigte den Lagerplatz und " .. che .. " hatte warten wollen.		Wahrend " .. everyone .. " geplundert wird, fahrt der Wagen durch den Lagerplatz, zerstort ein Zelt und halt an. Die Bande ist verunsichert, daher fliehen sie!" $) cyoa-text!
		cpet? if 
			cdog? if 
				($ cname .. "  hat sich versehentlich " .. chim .. " im Auto eingesperrt und bellt um herausgelassen zu werden." $)
			else
				($ Everyone .. "  ist sich nicht sicher ob " .. cname .. "  das alles absichtlich getan hat oder nicht." $)
			then +crtext
		else
			cloyalty- if
				($ notcname .. "  ist wirklich uberrascht " .. cname .. " ' hat nicht versucht das Auto zu stehlen. " .. cHe .. "  sieht fur einen Moment nervos aus." $) +crtext
			then loyaltyREV
		then
	then goodfx witsattREV morale++all
;

cyoa-bridge: bandit-weapon-coolit
	cpet? if
		($ cname .. "  furzt und macht eine wirklich groe Sache daraus." $)
	else
		($ cname .. "  sagt den Banditen sie sollen" .. "  KUHL ES." pbold $)
	then cyoa-text!
	cool-it-7 if
		char-gen to recruitee " Crappy Bandit" recruitee .name!
		($ " Die Banditen nehmen sich das wirklich zu Herzen." .. " Ja! Wer braucht Waffen wenn wir Freundschaft haben?" rbold .. " sie bieten ein Geschenk an!" $)
		+crtext morale3+all rescue-trainitem cooled-it goodfx
	else cool-it++
		($ " Die Banditen greifen alle an " .. cname .. ". Sie vergessen die Waffen." $) +crtext badfx ouch2-
	then witsattREV 
;

: bandit-weapon-check args( @ ) 
	<- @ .cstrength+ if
		($ <- @ .colourname .. "  kampft mit den Banditen mit " .. <- @ xhis .. "  starke!" $) +crtext
		<- @ .strengthREV
	else
		<- @ .cstrength- if
			<- @ .ouch-- <- @ .sting--
		else
			<- @ .ouch--
		then
	then
;

cyoa-choice: bandit-weapon-fight
	($ Thegroup .. " ' beschliet, sein Gluck in einem Kampf zu versuchen. Die Waffen der Banditen sind primitiv, aber sie sind immer noch gefahrlich." $) cyoa-text! 
		bitefx ' bandit-weapon-check foreach-team-char
;

cyoa-choice: bandit-weapon-pay
	($ Thegroup .. "  gibt am Ende alle " .. ghis .. "  waffen an die Banditen. Die Banditen geben " .. ghim .. "  ihm ein paar alte Waffen, vielleicht aus Schuldgefuhlen." $) cyoa-text! 0weaponsall trunk.clear morale2-all someicon badfx
	3 WEAPON_NAIL trunk.weapon+ 2 WEAPON_REBAR trunk.weapon+ 1 WEAPON_FISHING trunk.weapon+
	1 WEAPON_PARKING trunk.weapon+
;

cyoa-choice: bandit-weapon-calmshot
	($ cname .. "  zielt " .. ghis .. " ' Waffe mit Lichtgeschwindigkeit und sprengt alle mickrigen Waffen der Banditen in die Luft. Die Banditen fangen an zu rennen!" $) cyoa-text! goodfx shootingREV temperREV shooting++
;
	

cyoa: bandit-weapon
	" Waffenraubende Banditen" cyoa-title! someicon
	($ "Als " .. thegroup .. "  ein Camp erkundet, " .. gtheyare .. "  von Banditen uberfallen. Sie schwingen provisorische Waffen." .. "Sie verlangen alle deine Waffen." rbold .. "'Das ist lacherlich. Nicht einmal Banditen tun das normalerweise, da es ein langsames, aber sicheres Todesurteil ist." $) cyoa-text!
		calmshot-check if
			charchoice( "  schnellzuge und Feuer" )choice: bandit-weapon-calmshot
		else
			choice( " Bekampft die Banditen!" )choice: bandit-weapon-fight
		then
		most-paranoid if
			solo if charchoice( "  lie das Auto laufen" )choice: bandit-weapon-paranoid
			else charchoice( "  kommt zur Rettung" )choice: bandit-weapon-paranoid then
		then
		most-grating if
			charchoice( " ' sagt Banditen sie sollen sich beruhigen" fcoolit )choice: bandit-weapon-coolit
		then
		bandit-charm
		choice( " Gebt alle Waffen ab!" )choice: bandit-weapon-pay
;

cyoa-choice: bandit-loyal-pick
	cicon
	($ cname .. "  wird geschickt um die Banditen abzulenken damit sie ihren Autokontrollpunkt verlassen." $) cyoa-text!
	cloyalty- if
		($ cname .. "  schaltet sich sofort ein " .. everyone .. "  und schliet sich stattdessen den Banditen an." $) +crtext
		loyaltyREV decruit-bad
		($ Thegroup .. "  wird angegriffen! " .. gThey .. "  versuch" .. g1s .. " to escape..." $) +crtext
		bandithurt
	then;
	($ cname .. " ' verkauft nicht " .. everyone .. "  an die Banditen! " .. cHe .. "  lenkt sie von weitem ab und rennt. Sie verfolgen ihn, und das Auto kann vorbeifahren." $) +crtext
	ctemper- if
		($ cname .. "  gerat beim Ruckzug in Panik " .. cHe .. "  letztendlich den Plan nicht befolgt und verletzt wird!" $) +crtext cchar .sting12rnd-- bitefx
	else
		goodfx
	then
		($ cname .. "  schleicht sich spater um den Kontrollpunkt herum und schliet sich wieder an " .. everyone .. "." $) +crtext
		loyaltyREV
		
;

cyoa-choice: bandit-loyal-plow
	($ Thegroup .. "  beschliet mit dem Auto direkt durch den Kontrollpunkt zu pflugen.	" .. gThey .. "  nehmen" .. g1s .. "  viel Schaden, uberleben aber" .. g1s .. "." $) cyoa-text! sting12rnd--all bitefx car-plowthrough someicon
;
: bandit-loyal-plow? choice( "Pfluge das Auto durch!" )choice: bandit-loyal-plow ;

cyoa-choice: bandit-loyal-paranoid
	($ cname .. "  hatte einen Sicherungsplan erstellt bevor er entdeckt wurde." $) cyoa-text!
	($ cHe .. " ' zieht sich in ein zerstortes Gebaude zuruck, in dem die Banditen nicht sehen konnen " .. chim .. ". Wenn sie naher kommen, " .. che .. "  springt durch ein hinteres Fenster hinaus, schleicht sich " .. chis .. "  weg zum Auto und entkommt durch einen jetzt leeren Kontrollpunkt!" $) +crtext goodfx
;

cyoa-bridge: bandit-loyal-pick4
	someicon
	"Wer soll geschickt werden um die Banditen abzulenken?" cyoa-text!
	' bandit-loyal-pick cyoa-team-choice
;

cyoa: bandit-loyal
	"Wem kann man vertrauen?" cyoa-title! someicon
	solo if
		($ cname .. "  eine Gruppe von Banditen die einen Kontrollpunkt auf der Strae eingerichtet haben. " .. cHe .. "  hat keine gute Moglichkeit, sie abzulenken, und wird schlielich entdeckt.		" .. "Sie verlangen " rbold $) cyoa-text!
		bandit-demand
		most-paranoid if
			charchoice( "  hat einen Plan sie auszutricksen" )choice: bandit-loyal-paranoid
		then
		bandit-charm
		bandit-loyal-plow?
		bandit-fight
	else
		($ Thegroup .. " 	In deutsch  spots a group of bandits that have set up a checkpoint on the road. Someone from the team could be sent to distract the bandits so the car can get through. 		It would have to be someone calm and   " .. "vertrauenswurdig" rbold .. "." $) cyoa-text!
		team-count 3 > if
			' bandit-loyal-pick 2 #cyoa-team-choice
			choice( "Wahle jemand anderen" )choice: bandit-loyal-pick4
		else
			' bandit-loyal-pick 3 #cyoa-team-choice
		then
			bandit-loyal-plow?
	then
;

