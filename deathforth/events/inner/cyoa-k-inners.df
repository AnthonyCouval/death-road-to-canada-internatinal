uses road.df *)

cyoa{ road{

cyoa: inner8-cyoa
	"'Bewohnter Hochhaus" cyoa-title!
	someicon
	($ Thegroup .. " 1" $) cyoa-text! basic-tone
	1.3 1.5 frnd 9 15 frnd 0 1 rnd zforecast
	"events/inner/region/hellhotel.df" $load-region
		choice( "Besuche das Apartmentgebaude" )choice: premission
		gas-draw		
;

cyoa: inner7-cyoa
	
	"Großes Haus auf dem Hugel" cyoa-title!
	someicon
	($ Thegroup .. " fahrt in eine Stadt, die bis auf den Grund niedergebrannt wurde. Auf einem Hugel in der Nahe steht ein großes Haus. Schon von weitem sieht es so aus, als ware es komplett von Zombies uberrannt." $) cyoa-text! basic-tone
	1.3 1.5 frnd 9 15 frnd 0 1 rnd zforecast
	"events/inner/region/hardopen1.df" $load-region
		choice( "'Besuchen das Haus" )choice: premission
		gas-draw		
;

cyoa: inner6-cyoa
	
	"'Durchlochertes Haus" cyoa-title!
	someicon
	($ Thegroup .. " 'sieht von weitem ein Haus. Bei naherer Betrachtung sieht es so aus, als ob hier zwischen Menschen ein großer Schusswechsel passiert ist. Aus dem Haus kommen viele Gerausche der Untoten zu horen. " $) cyoa-text! basic-tone
	0.9 1.2 frnd 9 15 frnd 0 1 rnd zforecast
	"events/inner/region/midopen1.df" $load-region
		choice( "'Das Haus durchsuchen" )choice: premission
		gas-draw
;

cyoa: inner5-cyoa
	
	"'Haus an der Straße" cyoa-title!
	someicon
