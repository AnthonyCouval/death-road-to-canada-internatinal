: gimmestatupdate  
($ cname .. "  agora tem " .. cchar .strength@ .. "  de forca e " .. cchar .fitness@ .. "  de condicionamento fisico." $) cyoa-text! goodfx
;
cyoa-choice: gimmestata
	6 STAT_STRENGTH cchar .stat! 6 STAT_FITNESS cchar .stat!
	gimmestatupdate
;
cyoa-choice: gimmestatb
	6 STAT_FITNESS cchar .stat!
	gimmestatupdate
;
cyoa-choice: gimmestatc
	6 STAT_STRENGTH cchar .stat!
	gimmestatupdate
;
cyoa: gimmestats
	"Carregando e travando" cyoa-title! 0 to cyoaicon cyoaicon
